  "MS_GRC_PROD_REL_SUMM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PROD_REL_SUMM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_PROD_REL_SUMM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_METRIC_PENDING_TASK_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_METRIC_PENDING_TASK_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_METRIC_PENDING_TASK_V\" (\"OBJECT_ID\", \"OBJECT_NAME_URL\", \"OBJECT_NAME\", \"EVENT_ID\", \"DD_OBJECT_TYPE\", \"STATUS\", \"STATUS_STORED\", \"DUE_BY\", \"PAST_DUE_DATE\", \"PREVIOUS_VALUE\", \"CURRENT_VALUE\", \"INDI_MEASURE_DIS_VAL\", \"INDI_MEASURE_STORED\", \"FREQUENCY\", \"VALIDITY\", \"VALID_FROM\", \"STORED_RISK_CATEGORY\", \"RISK_CATEGORY\", \"ASSESSMENT_TYPE\", \"ASSESSMENT_TYPE_STORED\", \"ASSESSMENT_FOR\", \"ASSESSMENT_FOR_STORED\", \"METRIC_TYPE\", \"METRIC_TYPE_STORED\", \"DATA_COLL_TYPE\", \"CRITICAL_RISING_THRESH\", \"CRITICAL_DROPPING_THRESH\", \"NON_CRITICAL_RISING_THRESH\", \"NON_CRITICAL_DROPPING_THRESH\", \"EXPECTED_VALUE\", \"OWN_ORGNIZATIONS\", \"OWNERS\", \"PROCESS_INSTANCE_ID\", \"INSTANCE_ID\", \"CURRENT_ASSIGNEE_STORED\", \"CURRENT_ASSIGNEE\", \"PREV_CLR\", \"CUR_CLR\") AS \n  SELECT b.object_id,\n  null     as object_name_url,\na.object_name,\na.event_id,\n  --(SELECT ms_Grc_utilities.Get_Edit_Url(a.DD_OBJECT_TYPE, a.PROCESS_INSTANCE_ID, a.iNSTANCE_ID,\u0027 \u0027,a.event_id)\n  --FROM dual  )\n  A.dd_object_type,                                                                                                                                                                                        \n  decode( A.OBJ_STATUS,\u0027M_REASSIGN\u0027,\u0027Data Entry - Pending\u0027,\u0027M_CLR_PEND\u0027,\u0027Data Entry - Pending\u0027,\n                MS_APPS_UTILITIES.GET_DISPLAY_VALUE (MS_GRC_UTILITIES.ACTIVE_ENT_ID ,\u0027MS_GRC_Metrics_Status_Custom\u0027, A.OBJ_STATUS))                                   AS STATUS,\n  decode(a.OBJ_STATUS,\u0027M_CLR_PEND\u0027,\u0027MET_PEND\u0027,\u0027M_REASSIGN\u0027,\u0027MET_PEND\u0027,a.OBJ_STATUS)                                                    as status_stored,\n  (SELECT TRUNC(a.NEXT_SCHEDULED_DATE)FROM dual)                                                                                                                        AS DUE_BY,\n  (SELECT TRUNC(SYSDATE - a.NEXT_SCHEDULED_DATE)FROM dual )                                                                                                    AS past_due_date,\n  a.previous_value,\n  a.current_value,\n  ms_APPS_utilities.get_DISPLAY_VALUE(MS_GRC_UTILITIES.ACTive_ENT_ID ,\u0027MS GRC KI Indicator Measurement\u0027,A.INDICATOR_MEASUREMENT) AS indi_measure_dis_val,\n  A.INDICATOR_MEASUREMENT                                                                                                                                                                    as indi_measure_stored,\n  MS_APPS_UTILITIES.GET_DISPLAY_VALUE (MS_GRC_UTILITIES.ACTIVE_ENT_ID , \u0027MS GRC KI Frequency\u0027, A.FREQUENCY )         AS FREQUENCY,\n B.VALID_UNTIL                                                                                                                                              AS VALIDITY,\n b.valid_from                                                                                                                                            as valid_from,\n  a.Risk_category as stored_risk_category,\n   (SELECT ms_grc_utilities.get_risk_category_comma_sep(a.Risk_category) from dual) as Risk_Category,\n  MS_APPS_UTILITIES.GET_DISPLAY_VALUE(MS_GRC_UTILITIES.ACTIVE_ENT_ID,\u0027MS GRC KI Applies To Items\u0027,A.APPLIES_TO ) AS ASSESSMENT_TYPE,\n  A.applies_to                                                                                                                                                                                                 AS ASSESSMENT_TYPE_stored,\n  (SELECT ms_grc_kpi_kri_utilities.get_kpi_kri_ati_name (A.applies_to,A.ati_object_id )FROM DUAL )                                      AS ASSESSMENT_FOR,\n  a.ati_object_id                                                                                                                                                                                             as assessment_for_stored,\n  (SELECT ms_apps_utilities.get_display_value(MS_GRC_UTILITIES.ACTive_ENT_ID,\u0027MS GRC KI Type\u0027,b.kpi_kri_flag)\n  FROM dual )                                                                                                                                                                                                 AS metric_type,\n  b.kpi_kri_flag                                                                                                                                                                                               as metric_type_stored,\n  NVL(ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.ACTive_ENT_ID,\u0027MS GRC KI ATI Manual Automated\u0027,b.MANUAL_ENTRY_AUTOMATED ),\u0027Both\u0027) AS data_coll_type,\n  a.CRITICAL_RISING_THRESH,\n  a.CRITICAL_DROPPING_THRESH,\n  a.NON_CRITICAL_RISING_THRESH,\n  a.NON_CRITICAL_DROPPING_THRESH,\n  a.EXPECTED_VALUE,\n  (SELECT (RTRIM(XMLAGG(XMLELEMENT(e,\n    (SELECT org_entity_name\n    FROM SI_ORG_ENTITIES_T\n    WHERE org_entity_id \u003d g.owner_organizations )\n    || \u0027,\u0027 )).EXTRACT (\u0027//text()\u0027),\u0027,\u0027))\n  FROM ms_grc_kpi_kri_defn_ogn g\n  WHERE G.OBJECT_ID \u003d A.OBJECT_ID)                                                                                                                                                           AS OWN_ORGNIZATIONS,\n  (SELECT (RTRIM(XMLAGG(XMLELEMENT(E,(SELECT FIRST_NAME||\u0027 \u0027||LAST_NAME FROM SI_USERS_T\n    where user_name\u003dg.owners) || \u0027,\u0027 )).EXTRACT (\u0027//text()\u0027),\u0027,\u0027 ))\n   FROM ms_grc_kpi_kri_defn_own g\n   WHERE g.object_id \u003d a.object_id)                                                                                                                                                          AS owners,\n   a.process_instance_id,\n   a.instance_id,\n  (SELECT (RTRIM(XMLAGG(XMLELEMENT(e, (select user_name from si_users_t where user_id\u003dc.user_id)|| \u0027,\u0027 )).EXTRACT (\u0027//text()\u0027),\u0027,\u0027 )) \n  FROM si_event_assignments c\n  WHERE c.process_instance_id \u003d a.process_instance_id)                                                                                                                 as current_assignee_stored,\n (SELECT (RTRIM(XMLAGG(XMLELEMENT(e,(select first_name||\u0027 \u0027||last_name from si_users_t where user_id\u003dc.user_id)|| \u0027,\u0027 )).EXTRACT (\u0027//text()\u0027),\u0027,\u0027 ))\n FROM si_event_assignments c\n WHERE c.process_instance_id \u003d A.process_instance_id)                                                                                                                  AS current_assignee,\n  (SELECT MS_GRC_UTILITIES.FN_KPI_KRI_PREVAL_COLOR_CODE(B.OBJECT_ID, A.EVENT_ID) FROM  DUAL)                                              AS PREV_CLR,\n\t(SELECT Ms_Grc_Utilities.Fn_Kpi_Kri_Curval_Color_Code(b.object_id, a.event_id) From Dual)                                                             As cur_Clr\n FROM ms_grc_kpi_kri_data_entry_v A,\n ms_grc_kpi_kri_defn b\nWHERE a.process_instance_id IN\n  (SELECT process_instance_id\n  FROM si_event_assignments\n  --WHERE user_id   \u003d ms_apps_utilities.get_user_id(:LOGIN_ID)\n  where infolet_id IN\n    (SELECT metric_id\n    FROM si_metrics_t\n    WHERE metric_name \u003d \u0027MS_GRC_KPI_KRI_DATA_ENTRY\u0027\n    )\n  AND a.instance_id \u003d\n    (SELECT MAX (instance_id)\n    FROM ms_grc_kpi_kri_data_entry_v\n    WHERE process_instance_id \u003d a.process_instance_id\n    ) )\nAND A.OBJECT_ID\u003dB.OBJECT_ID\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PRODUCT_DET_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PRODUCT_DET_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_PRODUCT_DET_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_FINANCIAL_ACCOU_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_FINANCIAL_ACCOU_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_FINANCIAL_ACCOU_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"OBJ_CATEGORY\", \"OBJ_TYPE\", \"ACCOUNT_BALANCE\", \"ACCOUNT_BALANCE_AS_OF\") AS \n  SELECT a.OBJECT_ID,\n    a.OBJECT_NAME,\n    regexp_replace(a.DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\n    a.DESCRIPTION                                  AS DESCRIPTION_HTM,\n    (SELECT MS_CONCAT(OWNERS)\n    FROM ms_Grc_financial_accou_OWN B\n    WHERE B.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS OWNERS,\n    (SELECT MS_CONCAT(OWNER_ORGANIZATIONS)\n    FROM ms_Grc_financial_accou_ORG B\n    WHERE B.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS OWNER_ORGANIZATIONS,\n    a.RESTRICT_ACCESS_TO,\n    a.obj_STATUS,\n    a.VALID_FROM,\n    a.VALID_UNTIL,\n    a.RTF_aTTACH,\n    a.DD_OBJECT_TYPE,\n    a.DD_PROCESS_INSTANCE_ID,\n    a.DD_INSTANCE_ID,\n    a.obj_category,\n    a.obj_type ,\n    a.account_balance,\n    a.acc_balance_As_of AS account_balance_As_of\n  FROM ms_Grc_financial_accou a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DBOARD_ORG_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DBOARD_ORG_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DBOARD_ORG_V\" (\"NAME\", \"OBJECT_ID\", \"OBJECT_NAME\", \"ATI_OBJECT_ID\", \"FREQUENCY\", \"EXPECTED_VALUE\", \"PREVIOUS_VALUE\", \"CURRENT_VALUE\", \"CURRENT_DATE_VALUE\", \"CHANGE\", \"LAST_3ASMNT\", \"LAST_6ASMNT\", \"LAST_12ASMNT\", \"APPLIES_TO\", \"DD_OBJECT_TYPE\", \"RESTRICT_ACCESS_TO\", \"OWNER_ORGANIZATIONS\", \"EVENT_ID\", \"RISK_CATEGORIES\", \"ASSESSED_ORGS\", \"METRIC_DEFN_URL\", \"CHANGE_NUMBER\") AS \n  SELECT b.object_name AS NAME,\n    a.object_id,\n    c.org_entity_name AS object_name,\n    a.ati_object_id,\n    ms_apps_utilities.get_display_value (ms_grc_kpi_kri_utilities.get_ent_id (), \u0027MS GRC KI Frequency\u0027, frequency ) frequency,\n    a.expected_value,\n    a.previous_value,\n    a.current_value,\n    a.current_date_value,\n    ms_grc_kpi_kri_utilities.for_breached_rpt (current_value, previous_value )                AS CHANGE,\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 3 )  AS last_3asmnt,\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 6 )  AS last_6asmnt,\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 12 ) AS last_12asmnt,\n    applies_to,\n    a.dd_object_type,\n    a.restrict_access_to,\n    (SELECT ms_concat (owner_organizations)\n    FROM ms_grc_kpi_kri_defn_ogn d\n    WHERE a.object_id \u003d d.object_id\n    ) AS owner_organizations,\n    event_id,\n    (SELECT ms_concat(risk_category)\n    FROM MS_GRC_KPI_KRI_DEFN_RCG e\n    WHERE e.object_id \u003d b.object_id\n    ) AS RISK_CATEGORIES,\n    a.ati_object_id ASSESSED_ORGS,\n    Ms_Grc_Utilities.Get_Edit_Url(b.DD_OBJECT_TYPE, b.dd_process_instance_id,b.dd_instance_id , NVL(b.dd_edit_flag,\u0027Y\u0027),b.object_name) AS Metric_defn_url,\n    ROUND((current_value - previous_value),2)                                                                                          AS change_number\n  FROM ms_grc_kpi_kri_data_en a,\n    ms_grc_kpi_kri_defn b,\n    si_org_entities_t c\n  WHERE current_date_value IN\n    (SELECT MAX (current_date_value)\n    FROM ms_grc_kpi_kri_data_en\n    WHERE object_id   \u003d a.object_id\n    AND ati_object_id \u003d a.ati_object_id\n    )\n  AND a.object_id               \u003d b.object_id\n  AND TO_CHAR (c.org_entity_id) \u003d a.ati_object_id\n  AND applies_to                \u003d \u00271\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_ORG_PROFILE_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ORG_PROFILE_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_ORG_PROFILE_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"ORG_LEVEL\", \"ORG_PARENT\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"BASEL_II_ENTITY\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT OBJECT_ID,\n    OBJECT_NAME,\n    regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\n    DESCRIPTION                                  AS DESCRIPTION_HTM,\n    (SELECT MS_CONCAT(OWNER_ORGANIZATIONS)\n    FROM MS_GRC_ORG_PROFILE_ORG B\n    WHERE B.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS OWNER_ORGANIZATIONS,\n    (SELECT MS_CONCAT(OWNERS)\n    FROM MS_GRC_ORG_PROFILE_OWN B\n    WHERE B.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS OWNERS,\n    ORG_LEVEL,\n    ORG_PARENT,\n    RESTRICT_ACCESS_TO,\n    obj_STATUS,\n    VALID_FROM,\n    VALID_UNTIL,\n   BASEL_II_ENTITY,\n    RTF_ATTACH,\n    DD_OBJECT_TYPE,\n    DD_PROCESS_INSTANCE_ID,\n    DD_INSTANCE_ID\n  FROM ms_grc_ORG_PROFILE a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PROCESS_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PROCESS_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_PROCESS_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"DESCRIPTION\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"TYPE\", \"CATEGORY\", \"OBJECT_LEVEL\", \"CONFIDENTIALITY\", \"INTEGRITY\", \"PROCESS_AVAILABILITY\", \"BUSINESS_CRITICALITY\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"OWNER_ORG_IDS\", \"RESTRICT_ACCESS_TO\", \"PARENT\", \"STATUS\", \"RELATED_RISKS\", \"RELATED_CONTROLS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_REQUIREMENTS\", \"RELATED_FRAMEWORK_REFERENCES\", \"RELATED_OBJECTIVES\", \"RELATED_FINANCIAL_ACCOUNTS\", \"RELATED_REFERENCES\", \"RELATED_FUNCTIONS\", \"APPLIES_TO_ORGANIZATIONS\", \"CORE_OBJECT_TYPE\", \"DISPLAY_NAME\", \"DD_OBJECT_TYPE\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"EXPIRED_FLAG\", \"EDIT\") AS \n  SELECT A.OBJECT_NAME,\n    a.object_id,\n    DESCRIPTION,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_core_object_own d,\n      si_users_t usr ,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND USR.ENTERPRISE_ID \u003d E.ENTERPRISE_ID\n    AND A.OBJECT_ID       \u003d D.OBJECT_ID\n    ) AS OWNERS,\n    VALID_UNTIL,\n    VALID_FROM,\n    core_TYPE     AS TYPE,\n    core_category AS CATEGORY,\n    OBJECT_LEVEL,\n    confidentiality,\n    INTEGRITY,\n    AVAILABILITY_PROCESS AS PROCESS_AVAILABILITY,\n    business_criticality AS Business_Criticality,\n    b.dd_process_instance_id,\n    b.dd_instance_id,\n    NVL (b.dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM MS_GRC_cORE_OBJECT_OGN E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS owner_org_ids,\n    restrict_access_to,\n    (select OBJECT_ID\n    FROM ms_grc_core_object\n    WHERE OBJECT_ID \u003d a.OBJECT_PARENT\n    )          AS parent,\n    obj_status AS STATUS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_RISKS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_CONTROLS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_AREAS_OF_COMPLIANCE,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_requirement base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_REQUIREMENTS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_model_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_MODEL_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_FRAMEWORK_REFERENCES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_objectives base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_OBJECTIVES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_financial_accou base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_FINANCIAL_ACCOUNTS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_FINANCIAL_ACCOUNTS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_REFERENCES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_function base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_FUNCTIONS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      si_org_entities_t base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.org_entity_id\n    ) AS APPLIES_TO_ORGANIZATIONS,\n    (SELECT display_name\n    FROM ms_apps_entity_v B\n    WHERE B.DB_TABLE_NAME \u003d A.DD_OBJECT_TYPE\n    ) AS CORE_OBJECT_TYPE,\n    SUBSTR(OBJECT_NAME,1,30) DISPLAY_NAME,\n    a.dd_object_type AS dd_object_type,\n    \u0027Y\u0027              AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(a.dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(a.dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(A.DD_EDIT_FLAG,\u0027Y\u0027)                                    AS EDIT_ACTION_PARAMETERS,\n    MS_GRC_UTILITIES.INCLUDE_EXPIRED(A.VALID_FROM,A.VALID_UNTIL) AS EXPIRED_FLAG,\n    (SELECT ms_Grc_utilities.Get_Edit_Url(a.DD_OBJECT_TYPE, a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID, NVL(a.DD_EDIT_FLAG,\u0027Y\u0027), a.object_name)\n    FROM DUAL\n    ) AS EDIT\n  FROM ms_grc_core_object a,\n    MS_GRC_PROCESS b\n  WHERE A.OBJECT_ID    \u003d B.OBJECT_ID\n  and B.DD_OBJECT_TYPE \u003d \u0027MS_GRC_PROCESS\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_OBJ_REL_CONFIG_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_OBJ_REL_CONFIG_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_OBJ_REL_CONFIG_V\" (\"TYP_OBJECT_ID\", \"MANDATE_TEMP\", \"RELATED_TO\", \"MANDATE_REL_TO\", \"OBJECT_ACTION\", \"OBJ_TYPE\", \"OBJECT_ID\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\") AS \n  select \"TYP_OBJECT_ID\",\"MANDATE_TEMP\",\"RELATED_TO\",\"MANDATE_REL_TO\",\"OBJECT_ACTION\",\"OBJ_TYPE\",\"OBJECT_ID\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\" from SI_100994_T \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_QUESTION_PROCEDURE_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_QUESTION_PROCEDURE_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_QUESTION_PROCEDURE_DV\" (\"ORB_REL_SOURCE_ID\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"STAKE_HOLDERS\", \"TYPE_OF_OWNER\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"NATURE_OF_RELATIONSHIP\", \"NATURE_OF_REL_PTOA\", \"CAUSE_CONSEQUENCE\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"CHECK_USER_AVAIL\", \"ATTACHMENTS\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"TYPE_QUEST\", \"OBJ_STATUS_TEMP\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"CURRENCY\", \"OBJ_STATUS\", \"OBJ_MODIFIED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_CREATED_ON\", \"OBJ_CREATED_BY\", \"APPLIES_TO_AOC\", \"VALID_FROM\", \"APPLIES_TO_CORE_OBJECTS\", \"RTF_ATTACH\", \"APPLIES_TO_REQUIREMENTS\", \"LIST_OF_RESPONSE\", \"OBJECTIVE\", \"VALID_UNTIL\", \"APPLIES_TO_CONTROLS\", \"QUESTION\", \"RESTRICT_ACCESS_TO\", \"RESPONSE_TYPE\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"OBJECT_NAME\", \"OBJECT_ID\", \"OBJECT_ACTION\", \"MANUAL_AUTOMATIC\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"INFOLET_SOURCE\", \"CATEGORY_QUEST\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"QUESTION_HTM\", \"OBJECTIVE_HTM\") AS \n  SELECT \"ORB_REL_SOURCE_ID\",\n    \"TEMP_FIELD_OBJ_ID\",\n    \"ORB_RPT_NAME\",\n    \"STAKE_HOLDERS\",\n    \"TYPE_OF_OWNER\",\n    \"PTOA_INPUT\",\n    \"ATO_ORB_TEMP\",\n    \"ORB_KEY_RISK\",\n    \"NATURE_OF_RELATIONSHIP\",\n    \"NATURE_OF_REL_PTOA\",\n    \"CAUSE_CONSEQUENCE\",\n    \"ADDITIONAL_COLUMN5\",\n    \"ADDITIONAL_COLUMN4\",\n    \"ORB_FORM_URL\",\n    \"ADDITIONAL_DETAILS_WITH_ID\",\n    \"ATTRIBUTE_COLUMN_H2\",\n    \"ATTRIBUTE_COLUMN_H3\",\n    \"ADDITIONAL_COLUMN_H1\",\n    \"ADDITIONAL_COLUMN3\",\n    \"ADDITIONAL_COLUMN2\",\n    \"ADDITIONAL_COLUMN1\",\n    \"ADDITIONAL_DETAILS\",\n    \"APPLICABLE_TO\",\n    \"CHECK_USER_AVAIL\",\n    \"ATTACHMENTS\",\n    \"ROW_NUM\",\n    \"TGT_OBJ_TYPE\",\n    \"SELF_REL_TYPE\",\n    \"PARENT_REL_INST_ID\",\n    \"REL_COMMENTS\",\n    \"REL_VALID_UNTIL\",\n    \"REL_VALID_FROM\",\n    \"TGT_OBJ_ID\",\n    \"TGT_OBJ_NAME\",\n    \"REL_CONFIG_ID\",\n    \"REL_INST_ID\",\n    \"ASSOCIATED_WITH_TYPE\",\n    \"CONFIGURED_OBJECT_TYPES\",\n    \"REL_SOURCE_OBJECT_ID\",\n    \"REL_SOURCE_ID\",\n     ms_apps_utilities.get_display_valueS (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Question Type\u0027,\n               type_quest,\n               \u0027,\u0027\n            )\n               \"TYPE_QUEST\",\n            \"OBJ_STATUS_TEMP\",\n            \"OWN_APP_CONFIG_TEMP\",\n            \"OWN_APP_CONFIG\",\n            \"CURRENCY\",\n            ms_grc_utilities.get_status_name (dd_current_user_name,\n                                              obj_status)\n               \"OBJ_STATUS\",\n            \"OBJ_MODIFIED_ON\",\n            \"OBJ_MODIFIED_BY\",\n            \"OBJ_CREATED_ON\",\n            \"OBJ_CREATED_BY\",\n            \"APPLIES_TO_AOC\",\n            \"VALID_FROM\",\n            \"APPLIES_TO_CORE_OBJECTS\",\n            \"RTF_ATTACH\",\n            \"APPLIES_TO_REQUIREMENTS\",\n            \"LIST_OF_RESPONSE\",\n            \"OBJECTIVE\",\n            \"VALID_UNTIL\",\n            \"APPLIES_TO_CONTROLS\",\n            \"QUESTION\",\n            ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Restrict Access To\u0027,\n               restrict_access_to\n            )\n               restrict_access_to,\n            ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Response Type\u0027,\n               response_type\n            )\n               response_type,\n            MS_APPS_UTILITIES.GET_DISPLAY_VALUES (\n               MS_GRC_UTILITIES.ACTIVE_ENT_ID,\n               \u0027ORGENTITY\u0027,\n               OWNER_ORGANIZATIONS,\n               \u0027,\u0027\n            )\n               OWNER_ORGANIZATIONS,\n            MS_GRC_UTILITIES.get_users (owners) owners,\n            \"OBJECT_NAME\",\n            \"OBJECT_ID\",\n            ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Action\u0027,\n               object_action\n            )\n               object_action,\n            ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.ACTIVE_ENT_ID,\n               \u0027MS GRC Question Manual Automatic\u0027,\n               manual_automatic\n            )\n               manual_automatic,\n            MS_APPS_UTILITIES.GET_USER_FULL_NAME (LEVEL_2_APPROVER)\n               level_2_approver,\n            ms_apps_utilities.get_user_full_name (level_1_approver)\n               LEVEL_1_APPROVER,\n            \"INFOLET_SOURCE\",\n            ms_apps_utilities.get_display_valueS (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Question Category\u0027,\n               category_quest,\n               \u0027,\u0027\n            )\n               \"CATEGORY_QUEST\",\n            \"ACTION_COMMENTS\",\n            \"DD_PROCESS_CODE\",\n            \"DD_CURRENT_STAGE\",\n            \"DD_ENTERPRISE_INFO\",\n            MS_APPS_UTILITIES.GET_USER_FULL_NAME (DD_EVENT_USER_NAME)\n               DD_EVENT_USER_NAME,\n            MS_APPS_UTILITIES.GET_USER_FULL_NAME (DD_CURRENT_USER_NAME)\n               dd_current_user_name,\n    \"DD_OBJECT_TYPE\",\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    \"CREATED_BY\",\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"QUESTION_HTM\",\n    \"OBJECTIVE_HTM\"\n    FROM MS_GRC_QUESTION_PROCEDURE_V\n  WHERE   DD_CURRENT_STAGE !\u003d \u0027NONE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DATA_ENTRY_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DATA_ENTRY_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DATA_ENTRY_RV\" (\"METRIC_DEFINITION_ID\", \"METRIC_DEFN_NAME\", \"EVENT_ID\", \"STATUS\", \"ASSESSMENT_TYPE\", \"DUE_BY\", \"ASSIGNED_TO\", \"INDICATOR_MEASUREMENT\", \"OWNER_ORG_IDS\", \"RESTRICT_ACCESS_TO\", \"DATA_ENTRY_OWNER\", \"DATA_ENTRY_APPROVER\", \"DATA_DEFINITION_OWNER\", \"CURRENT_VALUE\", \"NON_CRITICAL_RISING_THRESH\", \"CRITICAL_RISING_THRESH\", \"NON_CRITICAL_DROPPING_THRESH\", \"CRITICAL_DROPPING_THRESH\", \"CURRENT_DATE_VALUE\", \"ATI_OBJECT_ID\", \"ASSESSMENT_FOR\", \"ATI_COMMENTS\", \"EXPECTED_VALUE\", \"FREQUENCY\", \"RISING_DROPPING_INDICATOR\", \"PROCESS_INSTANCE_ID\", \"DD_OBJECT_TYPE\", \"PREVIOUS_VALUE\", \"PAST_DUE_DATE\", \"DAYS_PAST_DUE\", \"DD_EDIT_FLAG\", \"METRIC_EVENT_ID\", \"COLOR_CODE\", \"CURRENT_PERIOD_AS_TEXT\", \"PREVIOUS_PERIOD\", \"CLR_CODE\", \"DATA_COLLECTION_TYPE\", \"PROVIDED_ON\", \"CREATION_DATE\", \"FREQUENCY_STOREDVAL\", \"NEXT_SCHEDULED_DATE\", \"PID_DEFN\", \"INSTANCEID_DEFN\", \"DD_EDIT_FLAG_DEFN\", \"DEFN_OBJECT_TYPE\", \"DEFN_PID\", \"DEFN_INSTID\", \"DATA_EN_PID\", \"DATA_INST_ID\", \"DATA_EN_OBJECT_TYPE\", \"DATA_EN_EDIT_FLAG\", \"RISK_CATEGORIES\", \"EVENT_URL\", \"EDIT\", \"ASSESSED_ORGS\", \"ASSESSED_ORG_IDS\") AS \n  SELECT B.object_id AS METRIC_DEFINITION_ID ,\n    b.object_name    AS METRIC_DEFN_NAME,\n    A.EVENT_ID       AS EVENT_ID,\n (case when a.CURRENT_DATE_VALUE \u003c trunc(sysdate) and a.OBJ_STATUS \u003c\u003e \u0027MET_COM\u0027 then\n   \u0027M_DUE\u0027\n   else\n  a.OBJ_STATUS\n   end ) as status,\n    a.applies_to         AS ASSESSMENT_TYPE,\n    A.CURRENT_DATE_VALUE AS DUE_BY,\n    (SELECT (RTRIM(XMLAGG(XMLELEMENT(e,(select user_name from si_users_t where user_id\u003dc.user_id)|| \u0027,\u0027 )).EXTRACT (\u0027//text()\u0027),\u0027,\u0027 ))\n FROM si_event_assignments c\n WHERE c.process_instance_id \u003d A.process_instance_id) as assigned_to,\n    b.indicator_measurement AS INDICATOR_MEASUREMENT,\n     a.OWNER_ORGANIZATIONS as owner_org_ids,\n     a.RESTRICT_ACCESS_TO,\n    (\n    SELECT ms_concat(DISTINCT(usr.user_name)) AS DATA_ENTRY_OWNER\n    FROM\n      si_users_t usr,\n      si_ent e\n    WHERE usr.user_name \u003d a.DATA_ENTRY_OWNER\n    AND a.object_id     \u003d b.object_id\n    AND e.enterprise_id \u003d usr.enterprise_id\n    ) AS DATA_ENTRY_OWNER,\n    (\n    SELECT ms_concat(DISTINCT(usr.user_name)) AS DATA_ENTRY_APPROVER\n    FROM\n      si_users_t usr,\n      si_ent e\n    WHERE usr.user_name \u003d a.data_entry_approver\n    AND a.object_id     \u003d b.object_id\n    AND e.enterprise_id \u003d usr.enterprise_id\n    ) AS DATA_ENTRY_APPROVER,\n   /* (\n    SELECT ms_concat(distinct(usr.user_name)) AS data_defn_owners\n      FROM ms_grc_kpi_kri_defn_own d, si_users_t usr, si_ent e\n     WHERE usr.user_name \u003d d.owners\n       AND usr.enterprise_id \u003d e.ENTERPRISE_ID\n       AND a.OBJECT_ID \u003d D.OBJECT_ID\n       and a.object_id \u003d b.object_id\n    )*/a.DEFINITION_OWNERS AS DATA_DEFINITION_OWNER,\n    round(a.current_value,2) as current_value,\n    round(a.non_critical_rising_thresh,2) as non_critical_rising_thresh,\n    round(a.critical_rising_thresh,2) as critical_rising_thresh,\n    round(a.non_critical_dropping_thresh,2) as non_critical_dropping_thresh,\n    round(a.critical_dropping_thresh,2) as critical_dropping_thresh,\n    a.current_date_value,\n    a.ati_object_id,\n    (SELECT ms_grc_kpi_kri_utilities.get_kpi_kri_ati_name (a.applies_to, a.ati_object_id )\n    FROM DUAL\n    ) AS ASSESSMENT_FOR,\n    a.ati_comments,\n    round(a.expected_value,2) as expected_value,\n    a.frequency,\n    a.rising_dropping_indicator,\n    a.PROCESS_INSTANCE_ID,\n  --  a.INSTANCE_ID, --a.dd_instance_id,\n    a.dd_object_type,\n    round(a.Previous_Value,2)  AS PREVIOUS_VALUE,\n    DECODE(SIGN(TRUNC(A.CURRENT_DATE_VALUE -sysdate)),1,\u0027Days Past Due is\u0027\n    ||TO_CHAR(TRUNC(A.CURRENT_DATE_VALUE ) - TRUNC(sysdate),\u002799\u0027)\n    ||\u0027 days\u0027,                             -1,\u0027Days Past Due is\u0027\n    ||TO_CHAR(TRUNC(A.CURRENT_DATE_VALUE)  -TRUNC(sysdate),\u002799\u0027)\n    ||\u0027 days\u0027, 0,\u0027Days Past Due is\u0027\n    ||TO_CHAR(TRUNC(A.CURRENT_DATE_VALUE)-TRUNC(sysdate),\u002799\u0027)) AS past_due_date,\n    (TRUNC(A.CURRENT_DATE_VALUE)         - TRUNC(sysdate))      AS days_past_due,\n    nvl(a.LATEST_FLAG,\u0027Y\u0027) AS DD_EDIT_FLAG, -- NVL (a.dd_edit_flag, \u0027Y\u0027)                                   AS DD_EDIT_FLAG,\n    a.event_id                                                  AS metric_event_id,\n    (SELECT ms_grc_utilities.FN_KPI_KRI_preval_COLOR_CODE(B.object_id,a.event_id)\n    FROM dual\n    ) AS color_code,\n    (SELECT ms_grc_utilities.GET_VALUE_IN_DATE_FORMAT(current_date_value, B.FREQUENCY)\n    FROM dual\n    ) AS CURRENT_PERIOD_AS_TEXT,\n    (SELECT ms_grc_utilities.get_value_in_date_format(previous_period, B.FREQUENCY)\n    FROM dual\n    ) AS PREVIOUS_PERIOD,\n    (\n    SELECT ms_grc_utilities.FN_KPI_KRI_CURVAL_color_code(B.object_id,a.event_id)\n    FROM dual\n    )                        AS clr_code,\n    b.manual_entry_automated AS data_collection_type,\n    A.PROVIDED_ON,\n    a.CREATION_DATE,\n    B.FREQUENCY              AS FREQUENCY_STOREDVAL,\n    B.NEXT_SCHEDULED_DATE    AS NEXT_SCHEDULED_DATE,\n    B.dd_PROCESS_INSTANCE_ID AS PID_DEFN,\n    B.dd_instance_id         AS INSTANCEID_DEFN,\n    NVL(b.dd_edit_flag,\u0027Y\u0027)  AS DD_EDIT_FLAG_DEFN,\n    B.DD_OBJECT_TYPE         AS DEFN_OBJECT_TYPE,\n    b.dd_process_instance_id AS defn_pid,\n    b.dd_instance_id         AS defn_instid,\n    a.PROCESS_INSTANCE_ID AS data_en_pid, --a.dd_process_instance_id AS data_en_pid,\n    a.INSTANCE_ID            as data_inst_id,  --a.dd_instance_id         AS data_inst_id,\n    a.dd_object_type         AS data_en_object_type,   --a.dd_edit_flag           AS data_en_edit_flag,\n    a.LATEST_FLAG AS data_en_edit_flag,\n  (SELECT ms_concat(risk_category)--ms_concat(ms_apps_utilities.get_display_values ( ms_grc_utilities.active_ent_id, \u0027MS GRC Risk Category\u0027, risk_category, \u0027,\u0027 ))\n    FROM MS_GRC_KPI_KRI_DEFN_RCG e\n    WHERE e.object_id \u003d b.object_id\n    ) AS RISK_CATEGORIES,\n    (SELECT ms_Grc_utilities.Get_Edit_Url(a.dd_object_type, a.process_instance_id,a.instance_id,NVL(a.LATEST_FLAG,\u0027Y\u0027),A.EVENT_ID)\n    FROM dual\n    ) AS EVENT_URL,--changed dd_edit_flag to LATEST_FLAG, instance_id,process_instance_id\n    (SELECT Ms_Grc_Utilities.Get_Edit_Url(B.DD_OBJECT_TYPE, b.dd_process_instance_id, B.dd_instance_id , NVL(b.dd_edit_flag,\u0027Y\u0027),b.object_name)\n    FROM Dual\n    ) AS Edit,\n    a.ATI_ASSESSED_ORGS as assessed_orgs,\n    a.ATI_ASSESSED_ORG_IDS as assessed_org_ids\n  FROM /*ms_grc_kpi_kri_data_en*/ms_grc_kpi_kri_data_entry_v a,\n    ms_grc_kpi_kri_defn b\n  WHERE a.instance_id \u003d\n    (SELECT MAX (instance_id)\n    FROM ms_grc_kpi_kri_data_entry_v\n    WHERE process_instance_id \u003d a.process_instance_id\n    )\n    and\n  a.object_id \u003d b.object_id\n  and a.OBJ_STATUS not in (\u0027M_REJ\u0027,\u0027M_REASSIGN\u0027)\norder by metric_defn_name,due_by\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REL_DENORM_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REL_DENORM_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_REL_DENORM_PUB_V\" (\"SRC_OBJ_TYPE\", \"SRC_OBJ_ID\", \"SRC_OBJ_NAME\", \"TGT_OBJ_TYPE\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_COMMENTS\", \"APPLICABLE_TO\", \"REL_DIR_TYPE\", \"ADDN_DETLS\") AS \n  SELECT Src_Obj_Type       AS Src_Obj_Type,\n           Src_Obj_Id         AS Src_Obj_Id,\n           Src_Obj_Name       AS Src_Obj_Name,\n           Tgt_obj_type       AS Tgt_obj_type,\n           Tgt_Obj_Id         AS Tgt_Obj_Id,\n           Tgt_Obj_Name       AS Tgt_Obj_Name, \n           rel_comments       AS rel_comments,\n           applicable_to      AS applicable_to,\n           \u0027DIRECT\u0027           AS REL_DIR_TYPE,\n           ADDITIONAL_DETAILS AS ADDN_DETLS\n      FROM MS_GRC_RELATIONSHIP_DETAILS\n     UNION\n    SELECT Tgt_obj_type       AS Src_Obj_Type,\n           Tgt_Obj_Id         AS Src_Obj_Id,\n           Tgt_Obj_Name       AS Src_Obj_Name,\n           Src_Obj_Type       AS Tgt_obj_type,\n           Src_Obj_Id         AS Tgt_Obj_Id,\n           Src_Obj_Name       AS Tgt_Obj_Name, \n           rel_comments       AS rel_comments,\n           applicable_to      AS applicable_to,\n           \u0027INDUCED\u0027           AS REL_DIR_TYPE,\n           ADDITIONAL_DETAILS AS ADDN_DETLS\n      FROM MS_GRC_RELATIONSHIP_DETAILS\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_RISK_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_RISK_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_RISK_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"CATEGORY_RISK\", \"TYPE_RISK\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM MS_GRC_RISK_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM MS_GRC_RISK_OGN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \nRESTRICT_ACCESS_TO, obj_STATUS,\nVALID_FROM, VALID_UNTIL, OBJECT_LEVEL,\n(SELECT MS_CONCAT(PRI_OBJECT_PARENT) FROM MS_GRC_RISK_PRI B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OBJECT_PARENT, \n(SELECT MS_CONCAT(CATEGORY_RISK) FROM MS_GRC_RISK_CAT B WHERE B.OBJECT_ID \u003d A.OBJECT_ID) as CATEGORY_RISK,\nTYPE_RISK, RTF_ATTACH,\nDD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID\nFROM ms_grc_risk a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_ORB_RELATIONS_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ORB_RELATIONS_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_ORB_RELATIONS_PUB_V\" (\"OBJ_REL_ID\", \"SRC_OBJ_TYPE\", \"SRC_OBJ_ID\", \"TGT_OBJ_TYPE\", \"TGT_OBJ_ID\", \"REL_COMMENTS\", \"APPLICABLE_TO\", \"REL_DIR_TYPE\", \"ADDN_DETLS\") AS \n  SELECT /*+ FIRST_ROWS(10) */rel_orb.REL_INST_ID AS OBJ_REL_ID,\n    defn.SRC_OBJ_TYPE        AS src_obj_type,\n    rel.REL_SOURCE_OBJECT_ID AS src_obj_id,\n    defn.dest_obj_type       AS tgt_obj_type,\n    rel_orb.tgt_obj_id       AS tgt_obj_id,\n    rel_orb.rel_comments     AS rel_comments,\n    rel_orb.applicable_to    AS applicable_to,\n    \u0027Related\u0027                AS REL_DIR_TYPE,\n    Additional_details       AS ADDN_DETLS\n  FROM ms_Grc_rel_defn defn,\n    ms_Grc_orb_relations rel,\n    ms_Grc_orb_relations_orb rel_orb\n  WHERE rel.rel_source_id \u003d rel_orb.rel_source_id\n  AND defn.rel_id         \u003d rel_orb.rel_config_id\n  UNION ALL\n  SELECT /*+ FIRST_ROWS(10) */ REL_ORB.REL_INST_ID AS OBJ_REL_ID,\n    defn.dest_obj_type       AS src_obj_type,\n    rel_orb.tgt_obj_id       AS src_obj_id,\n    defn.SRC_OBJ_TYPE        AS tgt_obj_type,\n    rel.REL_SOURCE_OBJECT_ID AS tgt_obj_id,\n    rel_orb.rel_comments     AS rel_comments,\n    rel_orb.applicable_to    AS applicable_to,\n    \u0027Induced\u0027                AS REL_DIR_TYPE,\n    Additional_details       AS ADDN_DETLS\n  FROM ms_Grc_rel_defn defn,\n    ms_Grc_orb_relations rel,\n    ms_Grc_orb_relations_orb rel_orb\n  WHERE REL.REL_SOURCE_ID \u003d REL_ORB.REL_SOURCE_ID\n  AND defn.rel_id         \u003d rel_orb.rel_config_id\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
