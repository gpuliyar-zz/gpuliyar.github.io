ANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    \"CREATED_BY\",\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"DESCRIPTION_HTM\"\n  FROM\nMS_GRC_PROCESS_V\n  WHERE   DD_CURRENT_STAGE !\u003d \u0027NONE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_RISK_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_RISK_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_RISK_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DESCRIPTION\", \"DD_OBJECT_TYPE\", \"RISK_CATEGORIES\", \"TYPE_RISK\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"RESTRICT_ACCESS_TO\", \"OVER_RIDE_ACTION\", \"OBJECT_TYPE\", \"EDIT_ACTION_PARAMETERS\", \"OBJECT_LEVEL\", \"DISPLAY_NAME\", \"OBJECT_NAME_URL\", \"RELATED_FRAMEWORK_REFERENCES\", \"RELATED_CORE_OBJECTS\", \"RELATED_RISKS\", \"RELATED_PROCESS\", \"RELATED_ASSETS\", \"RELATED_ASSET_CLASSES\", \"RELATED_CONTROL\", \"RELATED_REQUIREMENT\", \"RELATED_REGULATORY_BODY\", \"RELATED_FUNCTION\", \"RELATED_REFERENCE\", \"APPLIES_TO_ORGANIZATIONS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_FINANCIAL_ACCOUNTS\", \"CORE_OBJECT_TYPE\", \"RELATED_OBJECTIVES\", \"OWNER_ORG_IDS\", \"OBJ_STATUS\", \"PRI_OBJECT_PARENT\", \"OBJECT_PARENT\", \"EXPIRED_FLAG\", \"KEY_RISK\") AS \n  SELECT a.object_name,\n    a.object_id,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_risk_own d,\n      si_users_t usr ,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND usr.enterprise_id \u003d e.ENTERPRISE_ID\n    AND a.OBJECT_ID       \u003d D.OBJECT_ID\n    ) AS OWNERS,\n    valid_until,\n    valid_from,\n    description,\n    dd_object_type,\n    /* (  SELECT ms_concat(displayed_value)\n    FROM z lovvalue,ms_grc_risk_cat c\n    WHERE a.object_id \u003d c.object_id\n    and  lov_name   \u003d \u0027MS GRC Risk Category\u0027\n    AND stored_value \u003d category_risk\n    ) AS*/\n    (\n    SELECT ms_concat(CATEGORY_RISK)\n    FROM ms_grc_risk_cat c\n    WHERE a.object_id \u003d c.object_id\n    ) AS RISK_CATEGORIES,\n    /* (  SELECT displayed_value\n    FROM z lovvalue\n    WHERE lov_name   \u003d \u0027MS GRC Risk Classification Type\u0027\n    AND stored_value \u003d type_risk\n    ) AS*/\n    TYPE_RISK,\n    dd_process_instance_id,\n    dd_instance_id,\n    NVL (dd_edit_flag, \u0027Y\u0027) AS dd_edit_flag,\n    restrict_access_to,\n    \u0027Y\u0027            AS OVER_RIDE_ACTION,\n    DD_OBJECT_TYPE AS OBJECT_TYPE,\n    \u0027proc\u003d\u0027\n    || TO_CHAR (dd_process_instance_id)\n    || \u0027|instid\u003d\u0027\n    || TO_CHAR (dd_instance_id)\n    || \u0027|edit_flag\u003d\u0027\n    || NVL (dd_edit_flag, \u0027Y\u0027) AS EDIT_ACTION_PARAMETERS,\n    /* (  SELECT displayed_value\n    FROM z lovvalue\n    WHERE lov_name   \u003d \u0027MS_GRC_LEVEL\u0027\n    AND stored_value \u003d object_level\n    ) AS*/\n    object_level,\n    SUBSTR (object_name, 1, 30) display_name,\n    ms_grc_utilities.get_edit_url (dd_object_type, dd_process_instance_id, dd_instance_id, NVL (dd_edit_flag, \u0027Y\u0027), a.object_name ) AS object_name_url,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_model_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_MODEL_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_framework_references,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type    \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type IN(\u0027MS_GRC_ASSET\u0027,\u0027MS_GRC_ASSET_CLASS\u0027,\u0027MS_GRC_PROCESS\u0027)\n    AND orb.src_obj_id    \u003d a.object_id\n    AND orb.tgt_obj_id    \u003d base.object_id\n    ) AS Related_core_objects,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_risks,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_process,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_assets,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_asset_classes,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_control,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      MS_GRC_REQUIREMENT base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_requirement,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      MS_GRC_REGULATORY_BODY base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REGULATORY_BODY\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_regulatory_body,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_function base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_function,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      MS_GRC_REFERENCE base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_reference,\n    (SELECT ms_concat(base.org_entity_ID)\n    FROM ms_grc_orb_relations_pub_v orb,\n      si_org_entities_t base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.org_entity_id\n    ) AS applies_to_organizations,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_areas_of_compliance,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_financial_accou base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_FINANCIAL_ACCOUNTS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_FINANCIAL_ACCOUNTS,\n    (SELECT ms_grc_utilities.Get_Relationships (object_id, dd_object_type, \u0027MS_GRC_PROCESS\u0027,\u0027OBJECT_TYPE\u0027)\n    FROM DUAL\n    ) AS core_object_type,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      MS_GRC_OBJECTIVES base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_objectives,\n    (SELECT ms_concat (owner_organizations)\n    FROM ms_grc_risk_ogn e\n    WHERE e.object_id \u003d a.object_id\n    )            AS owner_org_ids,\n    a.OBJ_STATUS AS OBJ_STATUS,\n    (SELECT ms_concat(object_name)\n    FROM MS_GRC_RISK\n    WHERE object_id IN\n      (SELECT b.pri_objecT_parent FROM MS_GRC_RISK_PRI B WHERE a.OBJECT_ID\u003dB.OBJECT_ID\n      )\n    )                                                        AS PRI_OBJECT_PARENT,\n    a.object_parent as object_parent,\n    MS_GRC_UTILITIES.INCLUDE_EXPIRED(VALID_FROM,VALID_UNTIL) AS EXPIRED_FLAG,\n    KEY_RISK\n  FROM MS_GRC_RISK a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_FUNCTION_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_FUNCTION_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_FUNCTION_V\" (\"ORB_REL_SOURCE_ID\", \"REF_OBJECT_ID\", \"STAKE_HOLDERS\", \"OBJ_OBJECT_ID\", \"TYPE_OF_OWNER\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"NATURE_OF_RELATIONSHIP\", \"NATURE_OF_REL_PTOA\", \"CAUSE_CONSEQUENCE\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"CHECK_USER_AVAIL\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"OBJ_STATUS_TEMP\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_CREATED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_MODIFIED_ON\", \"REF_ID\", \"REFERENCE_TYPE\", \"REF_COMMENTS\", \"REFERENCE_NAME\", \"OTH_OBJ_ID\", \"OBJECT_LEVEL\", \"ATTACHMENTS\", \"OBJ_STATUS\", \"OBJ_CREATED_BY\", \"RTF_ATTACH\", \"RELATIONSHIP_DATE\", \"RELATIONSHIPCOMMENTS\", \"RELATIONSHIP\", \"OTHER_NAME\", \"VALID_UNTIL\", \"VALID_FROM\", \"RESTRICT_ACCESS_TO\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"OBJECT_NAME\", \"OBJECT_ID\", \"OBJECT_ACTION\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"DESCRIPTION\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  select \"ORB_REL_SOURCE_ID\",\"REF_OBJECT_ID\",\"STAKE_HOLDERS\",\"OBJ_OBJECT_ID\",\"TYPE_OF_OWNER\",\"PTOA_INPUT\",\"ATO_ORB_TEMP\",\"ORB_KEY_RISK\",\"NATURE_OF_RELATIONSHIP\",\"NATURE_OF_REL_PTOA\",\"CAUSE_CONSEQUENCE\",\"TEMP_FIELD_OBJ_ID\",\"ORB_RPT_NAME\",\"ADDITIONAL_COLUMN5\",\"ADDITIONAL_COLUMN4\",\"ORB_FORM_URL\",\"ADDITIONAL_DETAILS_WITH_ID\",\"ATTRIBUTE_COLUMN_H2\",\"ATTRIBUTE_COLUMN_H3\",\"ADDITIONAL_COLUMN_H1\",\"ADDITIONAL_COLUMN3\",\"ADDITIONAL_COLUMN2\",\"ADDITIONAL_COLUMN1\",\"ADDITIONAL_DETAILS\",\"APPLICABLE_TO\",\"CHECK_USER_AVAIL\",\"ROW_NUM\",\"TGT_OBJ_TYPE\",\"SELF_REL_TYPE\",\"PARENT_REL_INST_ID\",\"REL_COMMENTS\",\"REL_VALID_UNTIL\",\"REL_VALID_FROM\",\"TGT_OBJ_ID\",\"TGT_OBJ_NAME\",\"REL_CONFIG_ID\",\"REL_INST_ID\",\"ASSOCIATED_WITH_TYPE\",\"CONFIGURED_OBJECT_TYPES\",\"REL_SOURCE_OBJECT_ID\",\"REL_SOURCE_ID\",\"OBJ_STATUS_TEMP\",\"OWN_APP_CONFIG_TEMP\",\"OWN_APP_CONFIG\",\"OBJ_CREATED_ON\",\"OBJ_MODIFIED_BY\",\"OBJ_MODIFIED_ON\",\"REF_ID\",\"REFERENCE_TYPE\",\"REF_COMMENTS\",\"REFERENCE_NAME\",\"OTH_OBJ_ID\",\"OBJECT_LEVEL\",\"ATTACHMENTS\",\"OBJ_STATUS\",\"OBJ_CREATED_BY\",\"RTF_ATTACH\",\"RELATIONSHIP_DATE\",\"RELATIONSHIPCOMMENTS\",\"RELATIONSHIP\",\"OTHER_NAME\",\"VALID_UNTIL\",\"VALID_FROM\",\"RESTRICT_ACCESS_TO\",\"OWNER_ORGANIZATIONS\",\"OWNERS\",\"OBJECT_NAME\",\"OBJECT_ID\",\"OBJECT_ACTION\",\"LEVEL_2_APPROVER\",\"LEVEL_1_APPROVER\",\"DESCRIPTION\",\"ACTION_COMMENTS\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\",\"DESCRIPTION_HTM\" from SI_100988_T \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_CORE_OBJECT_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_CORE_OBJECT_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_CORE_OBJECT_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"REFERENCE_ID\", \"LEGACY_ID\", \"CORE_CATEGORY\", \"CORE_TYPE\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM MS_GRC_CORE_OBJECT_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM MS_GRC_CORE_OBJECT_OGN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \nRESTRICT_ACCESS_TO, obj_STATUS,\nVALID_FROM, VALID_UNTIL, OBJECT_LEVEL,\nOBJECT_PARENT, reference_id, LEGACY_ID, core_category, core_type,  \nRTF_aTTACH, DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID\nFROM ms_grc_core_object a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_STANDARD_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_STANDARD_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_STANDARD_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"STANDARD_TYPE\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \na.OBJECT_ID, a.OBJECT_NAME, regexp_replace(a.DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\na.DESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM Ms_Grc_Area_Of_Complia_Own B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM Ms_Grc_Area_Of_Complia_Org B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS,\nc.STANDARD_TYPE, \na.RESTRICT_ACCESS_TO, a.obj_STATUS,\na.VALID_FROM, a.VALID_UNTIL,   \na.RTF_aTTACH, a.DD_OBJECT_TYPE, a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID\nFROM Ms_Grc_Standard c , MS_GRC_AREA_OF_COMPLIA a\nWHERE c.object_id \u003d a.object_id\nand a.dd_object_type \u003d \u0027MS_GRC_STANDARD\u0027\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_ASSET_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ASSET_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_ASSET_RV\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"OWNERS\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"ASSET_TYPE\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"DISPLAY_OBJECT_PARENT\", \"CONFIDENTIALITY\", \"INTEGRITY\", \"ASSET_AVAILABILITY\", \"SERIAL_NUMBER\", \"RELATED_RISKS\", \"PHYSICAL_LOCATION\", \"BUSINESS_CRITICALITY\", \"RELATED_REFERENCES\", \"BELONG_TO_ASSET_CLASS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_CONTROLS\", \"RELATED_PROCESSES\", \"RELATED_QUESTION_PROCEDURES\", \"RESTRICT_ACCESS_TO\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"OWNER_ORG_IDS\", \"OBJECT_NAME_URL\", \"EXPIRED_FLAG\", \"APPLIES_TO_ORGANIZATIONS\", \"DISPLAY_NAME\", \"OBJECT_TYPE\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\") AS \n  SELECT b.object_id,\n    b.object_name,\n    b.description,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_core_object_own d,\n      si_users_t usr, si_ent e\n    WHERE usr.user_name \u003d d.owners\n    and  usr.enterprise_id \u003d e.ENTERPRISE_ID\n    AND b.OBJECT_ID     \u003d D.OBJECT_ID\n    AND a.object_id     \u003d b.object_id\n    ) AS OWNERS,\n    b.OBJ_STATUS,\n    b.valid_from,\n    b.valid_until,\n    a.Asset_type,\n    b.object_level,\n    b.OBJECT_PARENT,\n       (Select a.object_name\n\t\t\t  From   ms_grc_core_object a\n\t\t\t  Where  b.OBJECT_PARENT \u003d a.object_id) As DISPLAY_OBJECT_PARENT,\n    a.confidentiality,\n    a.integrity,\n    a.availability_asset AS ASSET_AVAILABILITY,\n    a.SERIAL_NUMBER,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    )                      AS Related_risks,\n    a.physical_location    AS physical_location,\n    a.business_criticality AS business_criticality,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_references,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Belong_to_Asset_Class,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Areas_of_Compliance,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Controls,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_processes,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_question_proc base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_question_procedures,\n    b.restrict_access_to,\n    a.dd_object_type,\n    a.dd_process_instance_id,\n    a.dd_instance_id,\n    NVL (a.dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM MS_GRC_ASSET_OGN E\n    WHERE E.OBJECT_ID \u003d B.OBJECT_ID\n    )                                                                                                                                      AS owner_org_ids,\n    ms_grc_utilities.get_edit_url (a.dd_object_type, a.dd_process_instance_id, a.dd_instance_id, NVL (a.DD_EDIT_FLAG, \u0027Y\u0027), b.OBJECT_NAME) AS object_name_url,\n    ms_Grc_utilities.INCLUDE_eXPIRED(B.VALID_FROM,B.VALID_UNTIL)                                                                           AS EXPIRED_FLAG,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      si_org_entities base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.org_entity_id\n    )                          AS Applies_to_Organizations,\n    SUBSTR(b.object_name,1,30) AS Display_name,\n    a.dd_object_type           AS Object_Type,\n    \u0027Y\u0027                        AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(a.dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(a.dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(a.dd_edit_flag,\u0027Y\u0027) AS EDIT_ACTION_PARAMETERS\n  FROM ms_grc_asset a,\n    ms_grc_core_object b\n  WHERE a.object_id    \u003d b.object_id\n  AND a.dd_object_type \u003d \u0027MS_GRC_ASSET\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_CONTROL_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_CONTROL_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_CONTROL_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"LEGACY_ID\", \"COMPLIANCE_CATEGORY\", \"COMPLIANCE_TYPE\", \"KEY_COMPLIANCE\", \"FREQUENCY\", \"FREQUENCY_DATE\", \"START_AFTER\", \"DUE_BY\", \"NEXT_SCHEDULED_DATE\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT OBJECT_ID,\n    OBJECT_NAME,\n    regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\n    DESCRIPTION                                  AS DESCRIPTION_HTM,\n    (SELECT MS_CONCAT(OWNERS)\n    FROM MS_GRC_COMPLIANCE_OBJE_OWN B\n    WHERE B.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS OWNERS,\n    (SELECT MS_CONCAT(OWNER_ORGANIZATIONS)\n    FROM MS_GRC_COMPLIANCE_OBJE_OGN B\n    WHERE B.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS OWNER_ORGANIZATIONS,\n    RESTRICT_ACCESS_TO,\n    obj_STATUS,\n    VALID_FROM,\n    VALID_UNTIL,\n    OBJECT_LEVEL,\n    OBJECT_PARENT,\n    LEGACY_ID,\n    NULL AS COMPLIANCE_CATEGORY,\n    NULL AS COMPLIANCE_TYPE,\n    decode(KEY_COMPLIANCE,\u0027yes\u0027,1,\u0027no\u0027,2) AS KEY_COMPLIANCE,\n    FREQUENCY,\n    FREQUENCY_DATE,\n    START_AFTER,\n    DUE_BY,\n    NEXT_SCHEDULED_DATE,\n    RTF_aTTACH,\n    DD_OBJECT_TYPE,\n    DD_PROCESS_INSTANCE_ID,\n    DD_INSTANCE_ID\n  FROM ms_grc_compliance_obje a\n  WHERE DD_OBJECT_TYPE \u003d \u0027MS_GRC_CONTROL\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DATA_ENTRY_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DATA_ENTRY_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DATA_ENTRY_DV\" (\"FREQUENCY\", \"ACCESS_RIGHTS\", \"ADDITIONAL_COL1\", \"DEFINITION_OWNERS\", \"ATI_ASSESSED_ORG_IDS\", \"ATI_ASSESSED_ORGS\", \"EVENT_ID\", \"NEXT_SCHEDULED_DATE\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_STATUS_TEMP\", \"FINAL_APPROVER\", \"PROVIDED_ON\", \"PROVIDED_BY\", \"OBJ_STATUS\", \"RISK_CATEGORY\", \"ASSESSED_ON\", \"CURRENT_PERDIOD_AS_TEXT\", \"RTF_ATTACH\", \"OBJECT_ACTION\", \"OBJECT_ID\", \"ATI_OBJECT_ID\", \"ACTION_COMMENTS\", \"ATI_COMMENTS\", \"ATTACH_FILES\", \"CRITICAL_DROPPING_THRESH\", \"CRITICAL_RISING_THRESH\", \"CURRENT_DATE_VALUE\", \"CURRENT_VALUE\", \"EXPECTED_VALUE\", \"NON_CRITICAL_DROPPING_THRESH\", \"NON_CRITICAL_RISING_THRESH\", \"PREVIOUS_PERIOD\", \"PREVIOUS_VALUE\", \"REASSIGN_DATA_OWNER\", \"DESCRIPTION\", \"INDICATOR_MEASUREMENT\", \"LEADING_TRAILING_INDICATOR\", \"MANUAL_ENTRY_AUTOMATED\", \"OBJECT_NAME\", \"RISING_DROPPING_INDICATOR\", \"ASSIGNED_ON\", \"DATA_ENTRY_APPROVER\", \"DATA_ENTRY_OWNER\", \"APPLIES_TO\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\", \"ATI_COMMENTS_HTM\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"APPROVE_ALL_MANUAL_KPI_KRI\", \"RISK_CATEGORIES_DE\", \"INDICATOR_MEASUREMENT_DE\") AS \n  SELECT ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Frequency\u0027, frequency ) \"FREQUENCY\",\n    \"ACCESS_RIGHTS\",\n    \"ADDITIONAL_COL1\",\n    \"DEFINITION_OWNERS\",\n    \"ATI_ASSESSED_ORG_IDS\" ,\n\t\"ATI_ASSESSED_ORGS\",\n    \"EVENT_ID\",\n    \"NEXT_SCHEDULED_DATE\",\n    \"OWN_APP_CONFIG_TEMP\",\n    \"OWN_APP_CONFIG\",\n    \"OBJ_STATUS_TEMP\",\n    \"FINAL_APPROVER\",\n    \"PROVIDED_ON\",\n    \"PROVIDED_BY\",\n    ms_grc_utilities.get_Status_name(dd_current_user_name,obj_status) \"OBJ_STATUS\",\n    \"RISK_CATEGORY\",\n    \"ASSESSED_ON\",\n    \"CURRENT_PERDIOD_AS_TEXT\",\n    \"RTF_ATTACH\",\n    \"OBJECT_ACTION\",\n    \"OBJECT_ID\",\n    (SELECT object_name\n    FROM ms_grc_risk\n    WHERE applies_to \u003d 2\n    AND object_id    \u003d ati_object_id\n    UNION\n    SELECT object_name\n    FROM ms_grc_compliance_obje\n    WHERE applies_to \u003d 4\n    AND object_id    \u003d ati_object_id\n    UNION\n    SELECT org_entity_name\n    FROM si_org_entities_t\n    WHERE applies_to  \u003d 1\n    AND org_entity_id \u003d ati_object_id\n    UNION\n    SELECT object_name\n    FROM ms_grc_core_object\n    WHERE applies_to \u003d 3\n    AND object_id    \u003d ati_object_id\n    ) \"ATI_OBJECT_ID\",\n    \"ACTION_COMMENTS\",\n    \"ATI_COMMENTS\",\n    \"ATTACH_FILES\",\n    \"CRITICAL_DROPPING_THRESH\",\n    \"CRITICAL_RISING_THRESH\",\n    \"CURRENT_DATE_VALUE\",\n    \"CURRENT_VALUE\",\n    \"EXPECTED_VALUE\",\n    \"NON_CRITICAL_DROPPING_THRESH\",\n    \"NON_CRITICAL_RISING_THRESH\",\n    \"PREVIOUS_PERIOD\",\n    \"PREVIOUS_VALUE\",\n    \"REASSIGN_DATA_OWNER\",\n    \"DESCRIPTION\",\n    ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Indicator Measurement\u0027, indicator_measurement ) \"INDICATOR_MEASUREMENT\",\n    ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Lead Trail Indicator\u0027, leading_trailing_indicator ) \"LEADING_TRAILING_INDICATOR\",\n    ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Manual Automated\u0027, manual_entry_automated ) \"MANUAL_ENTRY_AUTOMATED\",\n    \"OBJECT_NAME\",\n    ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Rise Drop Indicator\u0027, rising_dropping_indicator ) \"RISING_DROPPING_INDICATOR\",\n    \"ASSIGNED_ON\",\n    ms_apps_utilities.get_user_full_name (data_entry_approver) \"DATA_ENTRY_APPROVER\",\n    ms_apps_utilities.get_user_full_name (data_entry_owner) \"DATA_ENTRY_OWNER\",\n    ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Applies To Items\u0027, applies_to ) \"APPLIES_TO\",\n    \"DD_PROCESS_CODE\",\n    \"DD_CURRENT_STAGE\",\n    \"DD_ENTERPRISE_INFO\",\n    ms_apps_utilities.get_user_full_name(DD_EVENT_USER_NAME) \"DD_EVENT_USER_NAME\",\n    ms_apps_utilities.get_user_full_name (dd_current_user_name) \"DD_CURRENT_USER_NAME\",\n    \"DD_OBJECT_TYPE\",\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    \"CREATED_BY\",\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"DESCRIPTION_HTM\",\n    \"ATI_COMMENTS_HTM\",\n    ms_apps_utilities.get_display_values ( MS_GRC_UTILITIES.ACTIVE_ENT_ID, \u0027ORGENTITY\u0027, OWNER_ORGANIZATIONS, \u0027,\u0027 ) OWNER_ORGANIZATIONS,\n    ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Restrict Access To\u0027, restrict_access_to ) \"RESTRICT_ACCESS_TO\",\n    \"APPROVE_ALL_MANUAL_KPI_KRI\",\n    \"RISK_CATEGORIES_DE\",\n    \"INDICATOR_MEASUREMENT_DE\"\n    \n  FROM ms_grc_kpi_kri_data_entry_v\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_EXCEPTION_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_EXCEPTION_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_EXCEPTION_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"EXPLANATION\", \"EXPLANATION_HTM\", \"APPROVER\", \"VALID_FROM\", \"EXPIRES_ON\", \"SELECT_ITEMS\", \"EXCEPTION_USR\", \"REMINDER_DAYS\", \"EXCEPTION_FOR\", \"TYPE_EXCEPTION\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT OBJECT_ID,\n    OBJECT_NAME,\n    regexp_replace(EXPLANATION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS EXPLANATION,\n    EXPLANATION                                  AS EXPLANATION_HTM,\n    APPROVER,\n    VALID_FROM,\n    EXPIRES_ON,\n     (SELECT MS_CONCAT(SELECT_ITEMS)\n    FROM MS_GRC_EXCEPTION_ITM B\n    WHERE B.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS SELECT_ITEMS,\n    (SELECT MS_CONCAT(EXCEPTION_USR)\n    FROM MS_GRC_EXCEPTION_USR B\n    WHERE B.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS EXCEPTION_USR,\n    REMINDER_DAYS,\n    EXCEPTION_FOR,\n    TYPE_EXCEPTION,\n    DD_OBJECT_TYPE,\n    DD_PROCESS_INSTANCE_ID,\n    DD_INSTANCE_ID\n  FROM ms_grc_EXCEPTION a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REQUIREMENT_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REQUIREMENT_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_REQUIREMENT_V\" (\"ORB_REL_SOURCE_ID\", \"MRE_OBJECT_ID\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"STAKE_HOLDERS\", \"TYPE_OF_OWNER\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"NATURE_OF_RELATIONSHIP\", \"NATURE_OF_REL_PTOA\", \"CAUSE_CONSEQUENCE\", \"KEY_REQUIREMENT\", \"CONFIG_TYPE_OF_COMPL_VAL\", \"CONFIGURE_TFE_FIELDS\", \"VIOLATION_RISK_FACTOR\", \"VIOLATION_SEVERITY_LEVEL\", \"TIER\", \"TFE_YEAR\", \"TFE\", \"TYPE_OF_COMPLIANCE\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"CHECK_USER_AVAIL\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"OBJECT_ACTION\", \"OBJ_STATUS_TEMP\", \"FREQUENCY\", \"NEXT_SCHEDULED_DATE\", \"OBJ_STATUS\", \"START_AFTER\", \"DUE_BY\", \"OWN_APP_CONFIG\", \"OBJ_MODIFIED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_CREATED_ON\", \"OBJ_CREATED_BY\", \"OBJECT_PARENT\", \"OBJECT_LEVEL\", \"OWN_APP_CONFIG_TEMP\", \"MODEL_ID\", \"RESTRICT_ACCESS_TO\", \"ATTACH_FILES\", \"RTF_ATTACH\", \"MODEL_NAME\", \"MODEL_TYPE\", \"REQUIREMENT_TYPE\", \"REQUIREMENT_PENALTIES\", \"REQUIREMENT_IMPACT\", \"REQUIREMENT_CLASSIFICATION\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"OBJECT_NAME\", \"OBJECT_ID\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"DESCRIPTION\", \"ACTION_COMMENTS\", \"VALID_FROM\", \"VALID_UNTIL\", \"SECTION_NO\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\", \"REQUIREMENT_PENALTIES_HTM\") AS \n  select \"ORB_REL_SOURCE_ID\",\"MRE_OBJECT_ID\",\"TEMP_FIELD_OBJ_ID\",\"ORB_RPT_NAME\",\"STAKE_HOLDERS\",\"TYPE_OF_OWNER\",\"PTOA_INPUT\",\"ATO_ORB_TEMP\",\"ORB_KEY_RISK\",\"NATURE_OF_RELATIONSHIP\",\"NATURE_OF_REL_PTOA\",\"CAUSE_CONSEQUENCE\",\"KEY_REQUIREMENT\",\"CONFIG_TYPE_OF_COMPL_VAL\",\"CONFIGURE_TFE_FIELDS\",\"VIOLATION_RISK_FACTOR\",\"VIOLATION_SEVERITY_LEVEL\",\"TIER\",\"TFE_YEAR\",\"TFE\",\"TYPE_OF_COMPLIANCE\",\"ADDITIONAL_COLUMN5\",\"ADDITIONAL_COLUMN4\",\"ORB_FORM_URL\",\"ADDITIONAL_DETAILS_WITH_ID\",\"ATTRIBUTE_COLUMN_H2\",\"ATTRIBUTE_COLUMN_H3\",\"ADDITIONAL_COLUMN_H1\",\"CHECK_USER_AVAIL\",\"ADDITIONAL_COLUMN3\",\"ADDITIONAL_COLUMN2\",\"ADDITIONAL_COLUMN1\",\"ADDITIONAL_DETAILS\",\"APPLICABLE_TO\",\"ROW_NUM\",\"TGT_OBJ_TYPE\",\"SELF_REL_TYPE\",\"PARENT_REL_INST_ID\",\"REL_COMMENTS\",\"REL_VALID_UNTIL\",\"REL_VALID_FROM\",\"TGT_OBJ_ID\",\"TGT_OBJ_NAME\",\"REL_CONFIG_ID\",\"REL_INST_ID\",\"ASSOCIATED_WITH_TYPE\",\"CONFIGURED_OBJECT_TYPES\",\"REL_SOURCE_OBJECT_ID\",\"REL_SOURCE_ID\",\"OBJECT_ACTION\",\"OBJ_STATUS_TEMP\",\"FREQUENCY\",\"NEXT_SCHEDULED_DATE\",\"OBJ_STATUS\",\"START_AFTER\",\"DUE_BY\",\"OWN_APP_CONFIG\",\"OBJ_MODIFIED_ON\",\"OBJ_MODIFIED_BY\",\"OBJ_CREATED_ON\",\"OBJ_CREATED_BY\",\"OBJECT_PARENT\",\"OBJECT_LEVEL\",\"OWN_APP_CONFIG_TEMP\",\"MODEL_ID\",\"RESTRICT_ACCESS_TO\",\"ATTACH_FILES\",\"RTF_ATTACH\",\"MODEL_NAME\",\"MODEL_TYPE\",\"REQUIREMENT_TYPE\",\"REQUIREMENT_PENALTIES\",\"REQUIREMENT_IMPACT\",\"REQUIREMENT_CLASSIFICATION\",\"OWNER_ORGANIZATIONS\",\"OWNERS\",\"OBJECT_NAME\",\"OBJECT_ID\",\"LEVEL_2_APPROVER\",\"LEVEL_1_APPROVER\",\"DESCRIPTION\",\"ACTION_COMMENTS\",\"VALID_FROM\",\"VALID_UNTIL\",\"SECTION_NO\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\",\"DESCRIPTION_HTM\",\"REQUIREMENT_PENALTIES_HTM\" from SI_101002_T \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PRODUCT_DENORM_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PRODUCT_DENORM_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_PRODUCT_DENORM_RV\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"EDIT\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"PRODUCT_CATEGORY\", \"PRODUCT_TYPE\", \"SALES_CHANNEL\", \"PRODUCT_LOCATION\", \"OBJ_STATUS\", \"CREATED_ON\", \"CREATED_BY\", \"MODIFIED_ON\", \"MODIFIED_BY\", \"VALID_FROM\", \"VALID_UNTIL\", \"DESCRIPTION\", \"OWNER_ORG_IDS\", \"OWNER_ORG_NAMES\", \"OWNER_IDS\", \"OWNERS\", \"LEVEL_1_APPROVER\", \"LEVEL_2_APPROVER\", \"APPLIES_TO_ORGANIZATIONS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_ASSETS\", \"RELATED_CONTROLS\", \"RELATED_OBJECTIVES\", \"RELATED_PRODUCTS\", \"RELATED_PROCESSES\", \"RELATED_REFERENCES\", \"RELATED_RISKS\", \"LOCALE_ID\", \"LOCALE\", \"RESTRICT_ACCESS_TO\", \"DD_EDIT_FLAG\", \"DD_OBJECT_TYPE\", \"DD_INSTANCE_ID\", \"DD_PROCESS_INSTANCE_ID\", \"OVER_RIDE_ACTION\", \"DISPLAY_NAME\", \"EDIT_ACTION_PARAMETERS\", \"EXPIRED_FLAG\", \"ATTACHMENTS\") AS \n  SELECT prdSumm.object_id,\n    prdSumm.object_name,\n    (SELECT ms_grc_utilities.get_edit_url (prd.dd_object_type, prd.dd_process_instance_id, prd.dd_instance_id, NVL (prd.dd_edit_flag, \u0027Y\u0027), prd.object_name)\n    FROM MS_GRC_PRODUCT prd\n    WHERE prd.object_id \u003d prdSumm.object_id\n    ) AS EDIT,\n    mlsSumm.object_level,\n    prdSumm.object_parent,\n    mlsSumm.Product_Category_Agg,\n    mlsSumm.Product_Type_Agg,\n    mlsSumm.Sales_Channel_Agg,\n    mlsSumm.Locations_Agg,\n    mlsSumm.Obj_status,\n    prdSumm.created_date,\n    prdSumm.created_by,\n    prdSumm.updated_date,\n    prdSumm.updated_by,\n    prdSumm.valid_from,\n    prdSumm.valid_until,\n    prdSumm.description,\n    prdSumm.obj_own_org_id_agg,\n    prdSumm.obj_own_org_name_agg,\n    prdSumm.obj_owner_id_agg,\n    prdSumm.obj_owner_name_agg,\n    prdSumm.level_1_approver,\n    prdSumm.level_2_approver,\n    relSumm.Rel_Organization_names,\n    relSumm.Rel_Aoc_Obj_Name_Agg,\n    relSumm.Rel_Asset_Obj_Name_Agg,\n    relSumm.Rel_Ctrl_Obj_Name_Agg,\n    relSumm.Rel_Objective_Obj_Name_Agg,\n    relSumm.Rel_Product_name_Agg,\n    relSumm.Rel_Process_Obj_Name_Agg,\n    relSumm.Rel_Refrences_Obj_Name_Agg,\n    relSumm.Rel_Risk_Obj_Name_Agg,\n    mlsSumm.LOCALE_ID,\n    mlsSumm.LOCALE,\n    prdSumm.restrict_access_to,\n    prdSumm.dd_edit_flag,\n    prdSumm.dd_object_type,\n    prdSumm.dd_instance_id,\n    prdSumm.dd_process_instance_id,\n    prdSumm.over_ride_action,\n    SUBSTR(prdSumm.object_name,1,30) AS DISPLAY_NAME,\n    (SELECT \u0027proc\u003d\u0027\n      || TO_CHAR(prd.dd_process_instance_id)\n      ||\u0027|instid\u003d\u0027\n      ||TO_CHAR(prd.dd_instance_id)\n      ||\u0027|edit_flag\u003d\u0027\n      ||NVL(prd.dd_edit_flag,\u0027Y\u0027)\n    FROM MS_GRC_PRODUCT prd\n    WHERE prd.object_id \u003d prdSumm.object_id\n    )                                                                       AS EDIT_ACTION_PARAMETERS,\n    MS_GRC_UTILITIES.include_items(prdSumm.valid_from, prdSumm.valid_until) AS EXPIRED_FLAG,\n    prdSumm.attachments\n  FROM MS_GRC_PRODUCT_SUMMARY prdSumm,\n    MS_GRC_PRODUCT_MLS_SUMMARY mlsSumm,\n    MS_GRC_PRODUCT_REL_SUMMARY relSumm\n  WHERE prdSumm.object_id \u003d relSumm.object_id(+)\n  AND prdSumm.object_id   \u003d MLSSUMM.object_id(+)\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_CONTROL_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_CONTROL_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_CONTROL_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DESCRIPTION\", \"COMPLIANCE_TYPE\", \"COMPLIANCE_CATEGORY\", \"KEY_CONTROL\", \"APPLIES_TO_ORGANIZATIONS\", \"EDIT\", \"OWNER_ORG_IDS\", \"RESTRICT_ACCESS_TO\", \"OBJECT_LEVEL\", \"OBJ_STATUS\", \"OBJECT_PARENT\", \"FREQUENCY\", \"RELATED_PROCESSES\", \"RELATED_RISKS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_REQUIREMENTS\", \"RELATED_FRAMEWORK_REFERENCES\", \"RELATED_ASSERTIONS\", \"RELATED_OBJECTIVES\", \"RELATED_REFERENCES\", \"RELATED_FUNCTIONS\", \"CORE_OBJECT_TYPE\", \"RELATED_ASSETS\", \"RELATED_CORE_OBJECTS\", \"RELATED_ASSET_CLASSES\", \"DD_EDIT_FLAG\", \"DD_OBJECT_TYPE\", \"DD_INSTANCE_ID\", \"DD_PROCESS_INSTANCE_ID\", \"DISPLAY_NAME\", \"OBJECT_TYPE\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"EXPIRED_FLAG\", \"PURPOSE\", \"CONTROL_TYPE\", \"NATURE\", \"SOURCE_TYPE\", \"PRIORITY_TYPE\", \"SAMPLE_SIZE\") AS \n  SELECT a.object_name,\n    a.object_id,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_compliance_obje_own d,\n      si_users_t usr,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND usr.enterprise_id \u003d e.ENTERPRISE_ID\n    AND b.OBJECT_ID       \u003d D.OBJECT_ID\n    AND a.object_id       \u003d b.object_id\n    ) AS OWNERS,\n    valid_until,\n    valid_from,\n    DESCRIPTION,\n    null as COMPLIANCE_TYPE,\n    null as COMPLIANCE_CATEGORY,\n    key_compliance as Key_control,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      SI_ORG_ENTITIES base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.org_entity_id\n    )                                                                                                                                      AS APPLIES_TO_ORGANIZATIONS,\n    MS_GRC_UTILITIES.GET_EDIT_URL (A.DD_OBJECT_TYPE, A.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID, NVL (b.DD_EDIT_FLAG, \u0027Y\u0027), a.OBJECT_NAME) AS EDIT,\n    (SELECT ms_concat (owner_organizations)\n    FROM ms_grc_compliance_obje_ogn E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS OWNER_ORG_IDS,\n    a.restrict_access_to,\n    a.object_level,\n    a.obj_status,\n    object_parent AS OBJECT_PARENT,\n    a.frequency,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_Processes,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_RISKS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_AREAS_OF_COMPLIANCE,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_requirement base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_REQUIREMENTS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_model_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_MODEL_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Framework_References,\n    (SELECT MS_GRC_UTILITIES.Get_Relationships (A.OBJECT_ID, a.dd_object_type, \u0027ASSERTION\u0027)\n    FROM DUAL\n    ) AS RELATED_ASSERTIONS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_objectives base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_OBJECTIVES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_REFERENCES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_function base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_FUNCTIONS,\n    (SELECT MS_GRC_UTILITIES.Get_Relationships (A.OBJECT_ID, a.dd_object_type, \u0027MS_GRC_PROCESS\u0027,\u0027OBJECT_TYPE\u0027)\n    FROM DUAL\n    ) AS CORE_OBJECT_TYPE,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_ASSETS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type    \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type IN(\u0027MS_GRC_ASSET\u0027,\u0027MS_GRC_ASSET_CLASS\u0027,\u0027MS_GRC_PROCESS\u0027)\n    AND orb.src_obj_id    \u003d b.object_id\n    AND orb.tgt_obj_id    \u003d base.object_id\n    ) AS Related_core_objects,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    )                         AS RELATED_ASSET_CLASSES,\n    NVL (b.DD_EDIT_FLAG, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    a.dd_object_type,\n    a.dd_instance_id,\n    a.DD_PROCESS_INSTANCE_ID,\n    a.object_name AS display_name,\n    a.dd_object_type          AS Object_Type,\n    \u0027Y\u0027                       AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(a.dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(a.dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(a.dd_edit_flag,\u0027Y\u0027)                                  AS EDIT_ACTION_PARAMETERS,\n    ms_Grc_utilities.INCLUDE_items(a.VALID_FROM,a.VALID_UNTIL) AS EXPIRED_FLAG,\n     (select MS_CONCAT(Purpose)\n    from MS_GRC_CONTROL_PUR P\n    where P.OBJECT_ID \u003d A.OBJECT_ID\n    ) as Purpose, -- compliance category is changed to purpose.\n    (select MS_CONCAT(CONTROL_TYPE)\n        FROM MS_GRC_COMPLIANCE_OBJE_cty d\n        where b.OBJECT_ID       \u003d D.OBJECT_ID\n        and a.OBJECT_ID       \u003d B.OBJECT_ID\n        ) as CONTROL_TYPE,\n   (select MS_CONCAT(NATURE_TYPE)\n  from MS_GRC_CONTROL_NRE N\n    where N.OBJECT_ID \u003d a.OBJECT_ID\n    ) as Nature,\n    --Compliance type is changed to Nature.\n       a.SOURCE_TYPE,\n       a.PRIORITY_TYPE,\n       a.SAMPLE_SIZE\n  FROM MS_GRC_COMPLIANCE_OBJE A,\n    MS_GRC_CONTROL B\n  WHERE a.DD_OBJECT_TYPE \u003d \u0027MS_GRC_CONTROL\u0027\n  and a.OBJECT_ID        \u003d B.OBJECT_ID\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_AREA_OF_COMPLIANCE_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_AREA_OF_COMPLIANCE_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_AREA_OF_COMPLIANCE_DV\" (\"ORB_REL_SOURCE_ID\", \"REF_OBJECT_ID\", \"ATO_OBJECT_ID\", \"TYPE_OF_OWNER\", \"MRE_OBJECT_ID\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"NATURE_OF_RELATIONSHIP\", \"NATURE_OF_REL_PTOA\", \"CAUSE_CONSEQUENCE\", \"STAKE_HOLDERS\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"AOC_TYPE\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"ATO_ORGANIZATION\", \"ORGANIZATION_NAME\", \"OBJ_STATUS\", \"CHECK_USER_AVAIL\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"OBJ_STATUS_TEMP\", \"OWNER_ORGANIZATIONS\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"REGULATORY_BODY\", \"FUNCTION_NAME\", \"REF_ID\", \"FUNCTION_COMMENTS\", \"REG_BODY_COMMENTS\", \"OBJ_CREATED_ON\", \"OBJ_MODIFIED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_CREATED_BY\", \"REFERENCE_COMMENTS\", \"REFERENCE_NAME\", \"REFERENCE_TYPE\", \"MODEL_COMMENTS\", \"MODEL_SOURCE\", \"OWNERS\", \"MODEL_ID\", \"RTF_ATTACH\", \"MODEL_NAME\", \"MODEL_TYPE\", \"OWNERS_ATO\", \"ATO_COMMENTS\", \"VALID_UNTIL\", \"VALID_FROM\", \"RESTRICT_ACCESS_TO\", \"OBJECT_NAME\", \"OBJECT_ID\", \"OBJECT_ACTION\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"DESCRIPTION\", \"ATTACH_FILES\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  SELECT \"ORB_REL_SOURCE_ID\",\n\"REF_OBJECT_ID\",\n\"ATO_OBJECT_ID\",\n\"TYPE_OF_OWNER\",\n\"MRE_OBJECT_ID\",\n\"PTOA_INPUT\",\n\"ATO_ORB_TEMP\",\n\"ORB_KEY_RISK\",\n\"NATURE_OF_RELATIONSHIP\",\n\"NATURE_OF_REL_PTOA\",\n\"CAUSE_CONSEQUENCE\",\n\"STAKE_HOLDERS\",\n\"TEMP_FIELD_OBJ_ID\",\n\"ORB_RPT_NAME\",\nms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS_GRC_AOC_TYPES\u0027, aoc_type) \"AOC_TYPE\",\n\"ADDITIONAL_COLUMN5\",\n\"ADDITIONAL_COLUMN4\",\n\"ORB_FORM_URL\",\n\"ADDITIONAL_DETAILS_WITH_ID\",\n\"ATTRIBUTE_COLUMN_H2\",\n\"ATTRIBUTE_COLUMN_H3\",\n\"ADDITIONAL_COLUMN_H1\",\n\"ATO_ORGANIZATION\",\n\"ORGANIZATION_NAME\",\nms_grc_utilities.get_Status_name (dd_current_user_name, obj_status) \"OBJ_STATUS\",\n\"CHECK_USER_AVAIL\",\n\"ADDITIONAL_COLUMN3\",\n\"ADDITIONAL_COLUMN2\",\n\"ADDITIONAL_COLUMN1\",\n\"ADDITIONAL_DETAILS\",\n\"APPLICABLE_TO\",\n\"ROW_NUM\",\n\"TGT_OBJ_TYPE\",\n\"SELF_REL_TYPE\",\n\"PARENT_REL_INST_ID\",\n\"REL_COMMENTS\",\n\"REL_VALID_UNTIL\",\n\"REL_VALID_FROM\",\n\"TGT_OBJ_ID\",\n\"TGT_OBJ_NAME\",\n\"REL_CONFIG_ID\",\n\"REL_INST_ID\",\n\"ASSOCIATED_WITH_TYPE\",\n\"CONFIGURED_OBJECT_TYPES\",\n\"REL_SOURCE_OBJECT_ID\",\n\"REL_SOURCE_ID\",\n\"OBJ_STATUS_TEMP\",\n ms_apps_utilities.get_display_values ( MS_GRC_UTILITIES.active_ent_id, \u0027ORGENTITY\u0027, owner_organizations, \u0027,\u0027 ) \"OWNER_ORGANIZATIONS\",\n\"OWN_APP_CONFIG_TEMP\",\n\"OWN_APP_CONFIG\",\n\"REGULATORY_BODY\",\n\"FUNCTION_NAME\",\n\"REF_ID\",\n\"FUNCTION_COMMENTS\",\n\"REG_BODY_COMMENTS\",\n\"OBJ_CREATED_ON\",\n\"OBJ_MODIFIED_ON\",\n\"OBJ_MODIFIED_BY\",\n\"OBJ_CREATED_BY\",\n\"REFERENCE_COMMENTS\",\n\"REFERENCE_NAME\",\n\"REFERENCE_TYPE\",\n\"MODEL_COMMENTS\",\n\"MODEL_SOURCE\",\n MS_GRC_UTILITIES.get_users (owners) \"OWNERS\",\n\"MODEL_ID\",\n\"RTF_ATTACH\",\n\"MODEL_NAME\",\n\"MODEL_TYPE\",\n\"OWNERS_ATO\",\n\"ATO_COMMENTS\",\n\"VALID_UNTIL\",\n\"VALID_FROM\",\n ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Restrict Access To\u0027, restrict_access_to ) \"RESTRICT_ACCESS_TO\",\n\"OBJECT_NAME\",\n\"OBJECT_ID\",\nms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Action\u0027, object_action ) \"OBJECT_ACTION\",\nms_apps_utilities.get_user_full_name (level_2_approver) \"LEVEL_2_APPROVER\",\nms_apps_utilities.get_user_full_name (level_1_approver) \"LEVEL_1_APPROVER\",\n\"DESCRIPTION\",\n\"ATTACH_FILES\",\n\"ACTION_COMMENTS\",\n\"DD_PROCESS_CODE\",\n\"DD_CURRENT_STAGE\",\n\"DD_ENTERPRISE_INFO\",\nms_apps_utilities.get_user_full_name (dd_event_user_name) \"DD_EVENT_USER_NAME\",\nms_apps_utilities.get_user_full_name (dd_current_user_name) \"DD_CURRENT_USER_NAME\",\n\"DD_OBJECT_TYPE\",\n\"INSTANCE_REC_NUM\",\n\"INSTANCE_ID\",\n\"METRIC_ID\",\n\"METRIC_RUN_DATE\",\n\"METRIC_COMPLETED_DATE\",\n\"LATEST_FLAG\",\n\"CREATED_BY\",\n\"CREATION_DATE\",\n\"PROCESS_FLOW_STATUS\",\n\"PROCESS_INSTANCE_ID\",\n\"MULTIROW_GROUP_NAME\",\n\"ELECTRONICALLY_SIGNED\",\n\"MULTIROW_REGION_ID\",\n\"DESCRIPTION_HTM\"\n  FROM MS_GRC_AREA_OF_COMPLIANCE_V\n  WHERE DD_CURRENT_STAGE !\u003d \u0027NONE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_OBJ_SUMM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_OBJ_SUMM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_OBJ_SUMM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 21 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_QUESTION_PROCEDURE_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_QUESTION_PROCEDURE_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_QUESTION_PROCEDURE_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"QUESTION\", \"OBJECT_TYPE\", \"RESPONSE_TYPE\", \"MANUAL_AUTOMATIC\", \"OBJECTIVE\", \"QUESTION_PROCEDURE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"OWNER_ORG_IDS\", \"RESTRICT_ACCESS_TO\", \"STATUS\", \"APPLIES_TO_ORGANIZATIONS\", \"RELATED_STANDARD\", \"RELATED_ASSET\", \"RELATED_ASSET_CLASS\", \"RELATED_RISK\", \"RELATED_PROCESS\", \"RELATED_CONTROL\", \"RELATED_REQUIREMENT\", \"RELATED_REFERENCE\", \"RELATED_AREA_OF_COMPLIANCE\", \"EXPIRED_FLAG\", \"EDIT\", \"DISPLAY_NAME\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\") AS \n  SELECT a.object_name,\n    A.OBJECT_ID,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_question_proc_own d,\n      si_users_t usr ,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND USR.ENTERPRISE_ID \u003d E.ENTERPRISE_ID\n    AND A.OBJECT_ID       \u003d D.OBJECT_ID\n    ) AS OWNERS,\n    valid_until,\n    valid_from,\n    NULL           AS QUESTION,\n    dd_object_type AS OBJECT_TYPE,\n    response_type,\n    manual_automatic,\n    objective,\n    question \"QUESTION_PROCEDURE\",\n    dd_process_instance_id,\n    dd_instance_id,\n    NVL(dd_edit_flag, \u0027Y\u0027) AS dd_edit_flag,\n    (SELECT ms_concat(owner_organizations)\n    FROM ms_grc_question_proc_org e\n    WHERE e.object_id \u003d a.object_id\n    ) AS owner_org_ids,\n    restrict_access_to,\n    obj_status AS status,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      si_org_entities_t base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.org_entity_id\n    ) AS applies_to_organizations,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_standard,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_asset,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_ASSET_CLASS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_risk base\n    WHERE SRC_OBJ_TYPE   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS related_risk,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE SRC_OBJ_TYPE   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS related_process,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_compliance_obje base\n    WHERE SRC_OBJ_TYPE   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS related_control,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_requirement base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS RELATED_REQUIREMENT,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_reference base\n    WHERE SRC_OBJ_TYPE   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS RELATED_REFERENCE,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_area_of_complia base\n    WHERE SRC_OBJ_TYPE   \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    )                                                             AS RELATED_AREA_OF_COMPLIANCE,\n    MS_GRC_UTILITIES.INCLUDE_EXPIRED(A.VALID_FROM, A.VALID_UNTIL) AS EXPIRED_FLAG,\n    (SELECT ms_Grc_utilities.Get_Edit_Url(a.DD_OBJECT_TYPE, a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID, NVL(a.DD_EDIT_FLAG,\u0027Y\u0027), a.object_name)\n    FROM DUAL\n    )                            AS EDIT,\n    SUBSTR(a.object_name, 1, 30) AS Display_name,\n    \u0027Y\u0027                          AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(a.dd_process_instance_id)\n    || \u0027|instid\u003d\u0027\n    || TO_CHAR(a.dd_instance_id)\n    || \u0027|edit_flag\u003d\u0027\n    || NVL(a.dd_edit_flag, \u0027Y\u0027) AS EDIT_ACTION_PARAMETERS\n  FROM MS_GRC_QUESTION_PROC A\n  WHERE dd_object_type \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PRODUCT_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PRODUCT_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_PRODUCT_DV\" (\"STAKE_HOLDERS\", \"ORB_REL_SOURCE_ID\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"CAUSE_CONSEQUENCE\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"AUDIT_CONFIG_TEMP\", \"AUDIT_CONFIG\", \"OTHER_COMMENTS\", \"USER_CHECK\", \"RTF_ATTACH\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"ATTACHMENTS\", \"CHECK_USER_AVAIL\", \"OBJ_MODIFIED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_CREATED_ON\", \"OBJ_CREATED_BY\", \"OWNERS\", \"TYPE_OF_OWNER\", \"NATURE_OF_RELATIONSHIP\", \"ADDITIONAL_COLUMN5\", \"NATURE_OF_REL_PTOA\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"ACTION_COMMENTS\", \"OBJECT_ACTION\", \"OWNER_ORGANIZATIONS\", \"OBJ_STATUS_TEMP\", \"VALID_FROM\", \"VALID_UNTIL\", \"RESTRICT_ACCESS_TO\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"PRODUCT_LOCATION\", \"SALES_CHANNEL\", \"PRODUCT_TYPE\", \"PRODUCT_CATEGORY\", \"DESCRIPTION\", \"OBJECT_PARENT\", \"OBJECT_LEVEL\", \"OBJ_STATUS\", \"OBJECT_NAME\", \"OBJECT_ID\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  SELECT \"STAKE_HOLDERS\",\n    \"ORB_REL_SOURCE_ID\",\n    \"PTOA_INPUT\",\n    \"ATO_ORB_TEMP\",\n    \"ORB_KEY_RISK\",\n    \"CAUSE_CONSEQUENCE\",\n    \"TEMP_FIELD_OBJ_ID\",\n    \"ORB_RPT_NAME\",\n    \"AUDIT_CONFIG_TEMP\",\n    \"AUDIT_CONFIG\",\n    \"OTHER_COMMENTS\",\n    \"USER_CHECK\",\n    \"RTF_ATTACH\",\n    \"OWN_APP_CONFIG_TEMP\",\n    \"OWN_APP_CONFIG\",\n    \"ATTACHMENTS\",\n    \"CHECK_USER_AVAIL\",\n    \"OBJ_MODIFIED_ON\",\n    \"OBJ_MODIFIED_BY\",\n    \"OBJ_CREATED_ON\",\n    \"OBJ_CREATED_BY\",\n    MS_GRC_UTILITIES.GET_USERS (owners) owners,\n    \"TYPE_OF_OWNER\",\n    \"NATURE_OF_RELATIONSHIP\",\n    \"ADDITIONAL_COLUMN5\",\n    \"NATURE_OF_REL_PTOA\",\n    \"ADDITIONAL_COLUMN4\",\n    \"ORB_FORM_URL\",\n    \"ADDITIONAL_DETAILS_WITH_ID\",\n    \"ADDITIONAL_COLUMN3\",\n    \"ADDITIONAL_COLUMN2\",\n    \"ADDITIONAL_COLUMN1\",\n    \"ADDITIONAL_DETAILS\",\n    \"APPLICABLE_TO\",\n    \"ROW_NUM\",\n    \"TGT_OBJ_TYPE\",\n    \"SELF_REL_TYPE\",\n    \"PARENT_REL_INST_ID\",\n    \"REL_COMMENTS\",\n    \"REL_VALID_UNTIL\",\n    \"REL_VALID_FROM\",\n    \"TGT_OBJ_ID\",\n    \"TGT_OBJ_NAME\",\n    \"REL_CONFIG_ID\",\n    \"REL_INST_ID\",\n    \"ATTRIBUTE_COLUMN_H2\",\n    \"ATTRIBUTE_COLUMN_H3\",\n    \"ADDITIONAL_COLUMN_H1\",\n    \"ASSOCIATED_WITH_TYPE\",\n    \"CONFIGURED_OBJECT_TYPES\",\n    \"REL_SOURCE_OBJECT_ID\",\n    \"REL_SOURCE_ID\",\n    \"ACTION_COMMENTS\",\n    ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Action\u0027, object_action ) \"OBJECT_ACTION\",\n    MS_APPS_UTILITIES.GET_DISPLAY_VALUES ( MS_GRC_UTILITIES.ACTIVE_ENT_ID, \u0027ORGENTITY\u0027, OWNER_ORGANIZATIONS, \u0027,\u0027 ) \"OWNER_ORGANIZATIONS\",\n    \"OBJ_STATUS_TEMP\",\n    \"VALID_FROM\",\n    \"VALID_UNTIL\",\n     ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Restrict Access To\u0027, restrict_access_to ) \"RESTRICT_ACCESS_TO\",\n    ms_apps_utilities.get_user_full_name (level_2_approver) \"LEVEL_2_APPROVER\",\n    ms_apps_utilities.get_user_full_name (level_1_approver) \"LEVEL_1_APPROVER\",\n    MS_APPS_UTILITIES.GET_DISPLAY_VALUES ( MS_GRC_UTILITIES.ACTIVE_ENT_ID, \u0027MS GRC Product Locations\u0027, PRODUCT_LOCATION, \u0027,\u0027 ) \"PRODUCT_LOCATION\",\n    MS_APPS_UTILITIES.GET_DISPLAY_VALUES ( MS_GRC_UTILITIES.ACTIVE_ENT_ID, \u0027MS GRC Product Sales Channel\u0027, SALES_CHANNEL, \u0027,\u0027 ) \"SALES_CHANNEL\",\n    \"PRODUCT_TYPE\",\n    \"PRODUCT_CATEGORY\",\n    \"DESCRIPTION\",\n     MS_GRC_UTILITIES.GET_GRC_OBJECT_NAME (OBJECT_PARENT, \u0027MS_GRC_PRODUCT\u0027) OBJECT_PARENT,\n      ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS_GRC_LEVEL\u0027, object_level ) object_level,\n     ms_grc_utilities.get_status_name (dd_current_user_name,obj_status) \"OBJ_STATUS\",\n    \"OBJECT_NAME\",\n    \"OBJECT_ID\",\n    \"DD_PROCESS_CODE\",\n    \"DD_CURRENT_STAGE\",\n    \"DD_ENTERPRISE_INFO\",\n    ms_apps_utilities.get_user_full_name (dd_event_user_name) \"DD_EVENT_USER_NAME\",\n    ms_apps_utilities.get_user_full_name (dd_current_user_name) \"DD_CURRENT_USER_NAME\",\n    \"DD_OBJECT_TYPE\",\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    \"CREATED_BY\",\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"DESCRIPTION_HTM\"\nfrom MS_GRC_PRODUCT_V\nWHERE   DD_CURRENT_STAGE !\u003d \u0027NONE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_CONTROL_DENORM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_CONTROL_DENORM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_CONTROL_DENORM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 21 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REQ_DET_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REQ_DET_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_REQ_DET_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 21 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DBOARD_RISK_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DBOARD_RISK_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DBOARD_RISK_V\" (\"NAME\", \"OBJECT_ID\", \"OBJECT_NAME\", \"ATI_OBJECT_ID\", \"FREQUENCY\", \"EXPECTED_VALUE\", \"PREVIOUS_VALUE\", \"CURRENT_VALUE\", \"CURRENT_DATE_VALUE\", \"CHANGE\", \"LAST_3ASMNT\", \"LAST_6ASMNT\", \"LAST_12ASMNT\", \"APPLIES_TO\", \"DD_OBJECT_TYPE\", \"RESTRICT_ACCESS_TO\", \"OWNER_ORGANIZATIONS\", \"EVENT_ID\", \"RISK_CATEGORIES\", \"ASSESSED_ORGS\", \"METRIC_DEFN_URL\", \"CHANGE_NUMBER\") AS \n  SELECT b.object_name AS NAME,\n    a.object_id,\n    c.object_name AS object_name,\n    a.ati_object_id,\n    ms_apps_utilities.get_display_value (ms_grc_kpi_kri_utilities.get_ent_id (), \u0027MS GRC KI Frequency\u0027, frequency ) frequency,\n    a.expected_value,\n    a.previous_value,\n    a.current_value,\n    a.current_date_value,\n    ms_grc_kpi_kri_utilities.for_breached_rpt (current_value, previous_value )                AS CHANGE,\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 3 )  AS last_3asmnt,\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 6 )  AS last_6asmnt,\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 12 ) AS last_12asmnt,\n    applies_to,\n    b.dd_object_type,\n    b.restrict_access_to,\n    (SELECT ms_concat (owner_organizations)\n    FROM ms_grc_kpi_kri_defn_ogn d\n    WHERE a.object_id \u003d d.object_id\n    ) AS owner_organizations,\n    event_id,\n    (SELECT ms_concat(risk_category)\n    FROM MS_GRC_KPI_KRI_DEFN_RCG e\n    WHERE e.object_id \u003d b.object_id\n    )                                                                                                                                  AS RISK_CATEGORIES,\n    g.ati_assessed_orgs                                                                                                                AS ASSESSED_ORGS,\n    Ms_Grc_Utilities.Get_Edit_Url(b.DD_OBJECT_TYPE, b.dd_process_instance_id,b.dd_instance_id , NVL(b.dd_edit_flag,\u0027Y\u0027),b.object_name) AS Metric_defn_url,\n    ROUND((current_value - previous_value),2)                                                                                          AS Change_number\n  FROM ms_grc_kpi_kri_data_en a/*,\n    ms_grc_kpi_kri_defn b,\n    ms_grc_risk c,\n    ms_grc_kpi_kri_defn_aor g,\n    ms_grc_kpi_kri_defn_ati t*/\n  join ms_grc_kpi_kri_defn b on a.object_id \u003d b.object_id\n  join ms_grc_risk c on c.object_id \u003d a.ati_object_id\n  join ms_grc_kpi_kri_defn_ati t on t.object_id\u003d a.object_id and t.ati_obj_id \u003d a.ati_object_id\n  left join ms_grc_kpi_kri_defn_aor g on g.ati_object_id_pk \u003d t.ati_object_id_pk\n  WHERE current_date_value IN\n    (SELECT MAX (current_date_value)\n    FROM ms_grc_kpi_kri_data_en\n    WHERE object_id   \u003d a.object_id\n    AND ati_object_id \u003d a.ati_object_id\n    )\n  /*AND a.object_id \u003d b.object_id\n  AND c.object_id \u003d a.ati_object_id\n  and t.object_id\u003d g.object_id\n  and t.ati_object_id_pk\u003dg.ati_object_id_pk\n  and a.ati_object_id\u003dt.ati_obj_id\n  AND b.object_id \u003dg.object_id(+)*/\n  AND applies_to  \u003d \u00272\u0027\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DATA_EN_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DATA_EN_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DATA_EN_PUB_V\" (\"EVENT_ID\", \"CURRENT_DATE_VALUE\", \"ATI_OBJECT_ID\", \"OBJECT_ID\", \"CURRENT_VALUE\", \"PREVIOUS_VALUE\", \"PREVIOUS_PERIOD\", \"REASSIGN_DATA_OWNER\", \"CRITICAL_DROPPING_THRESH\", \"CRITICAL_RISING_THRESH\", \"NON_CRITICAL_DROPPING_THRESH\", \"NON_CRITICAL_RISING_THRESH\", \"EXPECTED_VALUE\", \"ASSESSED_ON\", \"PROVIDED_ON\", \"PROVIDED_BY\", \"DATA_ENTRY_OWNER\", \"DATA_ENTRY_APPROVER\", \"FINAL_APPROVER\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT event_id , \tcurrent_date_value , \tati_object_id , \n\tobject_id ,   current_value , \tprevious_value , \n\tprevious_period , \treassign_data_owner , \tapplies_to  \n\tcritical_dropping_thresh , \tcritical_rising_thresh , \tnon_critical_dropping_thresh , \n\tnon_critical_rising_thresh , \texpected_value ,   assessed_on , \n\tprovided_on , \tprovided_by ,  \tdata_entry_owner , \n\tdata_entry_approver , \tfinal_approver , \tdd_object_type , \n\tdd_process_instance_id , \tdd_instance_id   \n\tFROM  ms_grc_kpi_kri_data_en\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
