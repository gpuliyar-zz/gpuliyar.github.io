  "MS_GRC_PRODUCT_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PRODUCT_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_PRODUCT_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OBJECT_LEVEL\", \"OBJ_STATUS\", \"OBJECT_PARENT\", \"PRODUCT_CATEGORY\", \"PRODUCT_LOCATION\", \"SALES_CHANNEL\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"RESTRICT_ACCESS_TO\", \"DD_EDIT_FLAG\", \"DD_OBJECT_TYPE\", \"DD_INSTANCE_ID\", \"DD_PROCESS_INSTANCE_ID\", \"DISPLAY_NAME\", \"OBJECT_TYPE\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"EXPIRED_FLAG\", \"DESCRIPTION\", \"APPLIES_TO_ORGANIZATIONS\", \"RELATED_PROCESSES\", \"RELATED_RISKS\", \"RELATED_CONTROLS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_REQUIREMENTS\", \"RELATED_FINANCIAL_ACCOUNTS\", \"RELATED_FUNCTIONS\", \"RELATED_OBJECTIVES\", \"RELATED_FRAMEWORK_REFERENCES\", \"RELATED_REFERENCES\", \"RELATED_PRODUCTS\", \"EDIT\", \"OWNER_ORG_IDS\", \"PRODUCT_TYPE\", \"OBJ_CREATED_BY\", \"OBJ_CREATED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_MODIFIED_ON\", \"ATTACHMENTS\", \"LEVEL_1_APPROVER\", \"LEVEL_2_APPROVER\") AS \n  SELECT object_name,\n    object_id,\n    object_level,\n    obj_status,\n    object_parent ,\n    product_category,\n    (SELECT ms_concat(product_location)\n    FROM ms_grc_product_loc l\n    WHERE l.object_id \u003da.object_id\n    ) AS product_location,\n    (SELECT ms_concat(sales_channel)\n    FROM ms_grc_product_sch l\n    WHERE l.object_id\u003da.object_id\n    ) AS sales_channel,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_product_own d,\n      si_users_t usr,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND usr.enterprise_id \u003d e.ENTERPRISE_ID\n    AND d.object_id       \u003da.object_id\n    ) AS OWNERS,\n    valid_until,\n    valid_from,\n    restrict_access_to,\n    NVL (DD_EDIT_FLAG, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    dd_object_type,\n    dd_instance_id,\n    DD_PROCESS_INSTANCE_ID,\n   SUBSTR(a.object_name,1,30)AS display_name,\n    dd_object_type          AS Object_Type,\n    \u0027Y\u0027                     AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(dd_edit_flag,\u0027Y\u0027)                                AS EDIT_ACTION_PARAMETERS,\n    ms_Grc_utilities.INCLUDE_items(VALID_FROM,VALID_UNTIL) AS EXPIRED_FLAG,\n    DESCRIPTIOn,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      SI_ORG_ENTITIES base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n    AND orb.tgt_obj_id   \u003d base.org_entity_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS APPLIES_TO_ORGANIZATIONS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_Processes,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_Risks,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_Controls,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_areas_of_compliance,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_requirement base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_REQUIREMENTS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_financial_accou base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_FINANCIAL_ACCOUNTS\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_FINANCIAL_ACCOUNTS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_function base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_Functions,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_objectives base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_Objectives,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_model_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_MODEL_REFERENCE\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_FRAMEWORK_REFERENCES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    ) AS RELATED_REFERENCES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_product base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PRODUCT\u0027\n    AND orb.tgt_obj_id   \u003d base.object_id\n    AND orb.src_obj_id   \u003da.object_id\n    )                                                                                                                            AS RELATED_PRODUCTS,\n    MS_GRC_UTILITIES.GET_EDIT_URL (DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID, NVL (DD_EDIT_FLAG, \u0027Y\u0027), OBJECT_NAME) AS EDIT,\n    (SELECT ms_concat (owner_organizations)\n    FROM ms_grc_product_ogn E\n    WHERE E.OBJECT_ID \u003d a.OBJECT_ID\n    ) AS OWNER_ORG_IDS,\n    (SELECT MS_CONCAT(PRODUCT_TYPE)\n    FROM MS_GRC_PRODUCT_TYP T\n    WHERE T.OBJECT_ID\u003da.OBJECT_ID\n    )AS Product_type,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE OBJ_CREATED_BY \u003d usr.user_name\n    ) AS CREATED_BY,\n    OBJ_CREATED_ON,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE obj_modified_by \u003d usr.user_name\n    ) AS UPDATED_BY,\n    obj_modified_on,\n    attachments,\n    level_1_approver,\n    level_2_approver\n  from MS_GRC_PRODUCT a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_ASSERTION_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ASSERTION_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_ASSERTION_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DESCRIPTION\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"OWNER_ORG_IDS\", \"RESTRICT_ACCESS_TO\") AS \n  SELECT object_name, object_id,\n          ms_apps_utilities.get_user_full_name (owners),\n          ms_grc_utilities.prfn_return_owner_orgs\n                                                 (a.object_id,\n                                                  a.dd_object_type\n                                                 ) owner_orgs,\n          valid_until,\n          valid_from, description, dd_object_type,\n          dd_process_instance_id, dd_instance_id, NVL (dd_edit_flag, \u0027Y\u0027),\n          owner_organizations AS owner_org_ids, restrict_access_to\n     FROM ms_grc_assertion a \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_EXCEPTION_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_EXCEPTION_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_EXCEPTION_DV\" (\"ORB_REL_SOURCE_ID\", \"OWN_OBJECT_ID\", \"STAKE_HOLDERS\", \"OGN_OBJECT_ID\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"CAUSE_CONSEQUENCE\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"ATTACHMENTS\", \"VALID_UNTIL\", \"CHECK_USER_AVAIL\", \"SELECT_ITEMS\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"UPDATE_FLAG\", \"SOURCE_TYPE\", \"SOURCE_ID\", \"OBJECT_NAME\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"SELECT_BCKUP\", \"TYPE_OF_OWNER\", \"NATURE_OF_RELATIONSHIP\", \"ADDITIONAL_COLUMN5\", \"NATURE_OF_REL_PTOA\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"EXCEPTION_USR\", \"OBJ_STATUS_TEMP\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"ACTION_COMMENTS\", \"OBJECT_ACTION\", \"RTF_ATTACH\", \"OBJ_MODIFIED_BY\", \"OBJ_MODIFIED_ON\", \"OBJ_CREATED_BY\", \"OBJ_CREATED_ON\", \"APPROVER\", \"REMINDER_DAYS\", \"EXPIRES_ON\", \"VALID_FROM\", \"EXPLANATION\", \"OBJ_STATUS\", \"TYPE_EXCEPTION\", \"EXCEPTION_FOR\", \"OBJECT_ID\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"EXPLANATION_HTM\") AS \n  SELECT \"ORB_REL_SOURCE_ID\",\n  \"OWN_OBJECT_ID\",\n  \"STAKE_HOLDERS\",\n  \"OGN_OBJECT_ID\",\n  \"PTOA_INPUT\",\n  \"ATO_ORB_TEMP\",\n  \"ORB_KEY_RISK\",\n  \"CAUSE_CONSEQUENCE\",\n  \"TEMP_FIELD_OBJ_ID\",\n  \"ORB_RPT_NAME\",\n  \"ATTACHMENTS\",\n  \"VALID_UNTIL\",\n  \"CHECK_USER_AVAIL\",\n  ms_grc_utilities.EXCEPTION_SELECT_ITEMS_NAMES(SELECT_ITEMS || \u0027,\u0027,\n                                                                 EXCEPTION_FOR)  as   \"SELECT_ITEMS\",\n  \"OWNERS\",\n  \"OWNER_ORGANIZATIONS\",\n  \"UPDATE_FLAG\",\n  \"SOURCE_TYPE\",\n  \"SOURCE_ID\",\n  \"OBJECT_NAME\",\n  \"LEVEL_2_APPROVER\",\n  \"LEVEL_1_APPROVER\",\n  \"SELECT_BCKUP\",\n  \"TYPE_OF_OWNER\",\n  \"NATURE_OF_RELATIONSHIP\",\n  \"ADDITIONAL_COLUMN5\",\n  \"NATURE_OF_REL_PTOA\",\n  \"ADDITIONAL_COLUMN4\",\n  \"ORB_FORM_URL\",\n  \"ADDITIONAL_DETAILS_WITH_ID\",\n  \"ADDITIONAL_COLUMN3\",\n  \"ADDITIONAL_COLUMN2\",\n  \"ADDITIONAL_COLUMN1\",\n  \"ADDITIONAL_DETAILS\",\n  \"APPLICABLE_TO\",\n  \"ROW_NUM\",\n  \"TGT_OBJ_TYPE\",\n  \"SELF_REL_TYPE\",\n  \"PARENT_REL_INST_ID\",\n  \"REL_COMMENTS\",\n  \"REL_VALID_UNTIL\",\n  \"REL_VALID_FROM\",\n  \"TGT_OBJ_ID\",\n  \"TGT_OBJ_NAME\",\n  \"REL_CONFIG_ID\",\n  \"REL_INST_ID\",\n  \"ATTRIBUTE_COLUMN_H2\",\n  \"ATTRIBUTE_COLUMN_H3\",\n  \"ADDITIONAL_COLUMN_H1\",\n  \"ASSOCIATED_WITH_TYPE\",\n  \"CONFIGURED_OBJECT_TYPES\",\n  \"REL_SOURCE_OBJECT_ID\",\n  \"REL_SOURCE_ID\",\n  MS_APPS_UTILITIES.get_user_full_name(EXCEPTION_USR) \"EXCEPTION_USR\",\n  \"OBJ_STATUS_TEMP\",\n  \"OWN_APP_CONFIG_TEMP\",\n  \"OWN_APP_CONFIG\",\n  \"ACTION_COMMENTS\",\n  \"OBJECT_ACTION\",\n  \"RTF_ATTACH\",\n  \"OBJ_MODIFIED_BY\",\n  \"OBJ_MODIFIED_ON\",\n  \"OBJ_CREATED_BY\",\n  \"OBJ_CREATED_ON\",\n  ms_grc_utilities.get_users(Approver) \"APPROVER\",\n  \"REMINDER_DAYS\",\n  \"EXPIRES_ON\",\n  \"VALID_FROM\",\n  \"EXPLANATION\",\n  \"OBJ_STATUS\",\n  MS_APPS_UTILITIES.GET_DISPLAY_VALUES (\n               MS_GRC_UTILITIES.ACTIVE_ENT_ID,\n               \u0027MS_GRC_Type_Of_Exception\u0027,\n               TYPE_EXCEPTION,\n               \u0027,\u0027)TYPE_EXCEPTION,\nMS_APPS_UTILITIES.GET_DISPLAY_VALUES (\n               MS_GRC_UTILITIES.ACTIVE_ENT_ID,\n               \u0027MS_GRC_Exception_For\u0027,\n               EXCEPTION_FOR,\u0027,\u0027)\n            EXCEPTION_FOR,\n  \"OBJECT_ID\",\n  \"DD_PROCESS_CODE\",\n  \"DD_CURRENT_STAGE\",\n  \"DD_ENTERPRISE_INFO\",\n  \"DD_EVENT_USER_NAME\",\n  \"DD_CURRENT_USER_NAME\",\n  \"DD_OBJECT_TYPE\",\n  \"INSTANCE_REC_NUM\",\n  \"INSTANCE_ID\",\n  \"METRIC_ID\",\n  \"METRIC_RUN_DATE\",\n  \"METRIC_COMPLETED_DATE\",\n  \"LATEST_FLAG\",\n  \"CREATED_BY\",\n  \"CREATION_DATE\",\n  \"PROCESS_FLOW_STATUS\",\n  \"PROCESS_INSTANCE_ID\",\n  \"MULTIROW_GROUP_NAME\",\n  \"ELECTRONICALLY_SIGNED\",\n  \"MULTIROW_REGION_ID\",\n  \"EXPLANATION_HTM\" FROM ms_grc_exception_v\n\t\t\t\tWHERE   DD_CURRENT_STAGE !\u003d \u0027NONE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_AREA_OF_COMPLIANCE_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_AREA_OF_COMPLIANCE_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_AREA_OF_COMPLIANCE_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OBJ_STATUS\", \"DESCRIPTION\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DD_OBJECT_TYPE\", \"ATO_ORGANIZATIONS\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"DISPLAY_NAME\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"RESTRICT_ACCESS_TO\", \"OBJECT_NAME_URL\", \"RELATED_FRAMEWORK_REFERENCES\", \"RELATED_RISKS\", \"RELATED_PROCESS\", \"RELATED_CONTROL\", \"RELATED_REQUIREMENT\", \"RELATED_REGULATORY_BODY\", \"RELATED_FUNCTION\", \"RELATED_REFERENCE\", \"REL_FRAME_REFERENCE_TYPE\", \"RELATED_STANDARDS\", \"OWNER_ORG_IDS\", \"EXPIRED_FLAG\", \"AOC_TYPE\") AS \n  SELECT\n    a.object_name,\n    a.object_id,\n    a.obj_status,\n    a.description,\n      ( SELECT ms_concat(usr.first_name\n    || \u0027 \u0027\n    || usr.last_name) AS owners\n  FROM ms_grc_Area_of_COmplia_own d,\n    si_users_t usr  , si_ent e\n  WHERE usr.user_name \u003d d.owners\n  and  usr.enterprise_id \u003d e.ENTERPRISE_ID\nAND a.OBJECT_ID     \u003d D.OBJECT_ID ) AS OWNERS,\n      valid_until,\n    valid_from,\n    dd_object_type,\n       (\n SELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    si_org_entities_t base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.org_entity_id\n  ) AS ATO_ORGANIZATIONS,\n    dd_process_instance_id,\n    dd_instance_id,\n    NVL (dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    SUBSTR (object_name, 1, 30) as display_name,\n    \u0027Y\u0027 AS OVER_RIDE_ACTION,\n     \u0027proc\u003d\u0027|| TO_CHAR(dd_process_instance_id)||\u0027|instid\u003d\u0027||TO_CHAR(dd_instance_id)||\u0027|edit_flag\u003d\u0027||NVL(dd_edit_flag,\u0027Y\u0027)  AS EDIT_ACTION_PARAMETERS,\n    restrict_access_to,\n    ms_grc_utilities.get_edit_url (dd_object_type, dd_process_instance_id,dd_instance_id,NVL(DD_EDIT_FLAG,\u0027Y\u0027),\n\ta.OBJECT_NAME) AS object_name_url,\n         (\n  SELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_model_reference base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_MODEL_REFERENCE\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS Related_framework_references,\n      (\nSELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_risk base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS Related_risks,\n      (\nSELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_core_object base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS related_process,\n        (\n  SELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_compliance_obje base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS related_control,\n        (\n  SELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    MS_GRC_REQUIREMENT base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS related_requirement,\n      (\n SELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    MS_GRC_REGULATORY_BODY base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_REGULATORY_BODY\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS related_regulatory_body,\n        (\n   SELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_function base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_FUNCTION\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS related_function,\n        (\n   SELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    MS_GRC_REFERENCE base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS related_reference,\n        (\n  SELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_model_reference base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_MODEL_REFERENCE\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS Rel_Frame_Reference_Type,\n     (\n  SELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_area_of_complia base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS Related_Standards,\n    (\n      SELECT\n        MS_CONCAT (OWNER_ORGANIZATIONS)\n      FROM\n        MS_GRC_Area_of_complia_ORG E\n      WHERE\n        E.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS owner_org_ids,\n\tms_Grc_utilities.INCLUDE_eXPIRED(VALID_FROM,VALID_UNTIL) AS EXPIRED_FLAG,\n    (\n      SELECT\n        MS_CONCAT (t.AOC_TYPE)\n      FROM\n       MS_GRC_AREA_OF_COMPLIA_TYP t\n      WHERE\n        t.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS AOC_TYPE\n  FROM\n    ms_grc_AREA_OF_COMPLIA a \n  WHERE\n    dd_object_type\u003d\u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_CTRL_SUMM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_CTRL_SUMM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_CTRL_SUMM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 21 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_ORG_PROFILE_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ORG_PROFILE_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_ORG_PROFILE_RV\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"ORG_LEVEL\", \"ORG_PARENT\", \"DESCRIPTION\", \"VALID_FROM\", \"VALID_UNTIL\", \"BASEL_II_ENTITY\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"DD_OBJECT_TYPE\", \"RESTRICT_ACCESS_TO\", \"OWNERS\", \"RELATED_OBJECTIVES\", \"RELATED_CONTROLS\", \"RELATED_PROCESSES\", \"RELATED_RISKS\", \"RELATED_REFERENCES\", \"STATUS\", \"OBJECT_NAME_URL\", \"OWNER_ORG_IDS\", \"EXPIRED_FLAG\", \"DISPLAY_NAME\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\") AS \n  SELECT a.object_id,\n    a.object_name,\n    a.org_level,\n    ms_grc_utilities.prfn_return_owner_orgs (a.object_id, a.dd_object_type, a.org_parent) org_parent,\n    DESCRIPTION,\n    valid_from,\n    VALID_UNTIL,\n    BASEL_II_ENTITY,\n    a.dd_process_instance_id,\n    a.dd_instance_id,\n    NVL (a.dd_edit_flag, \u0027Y\u0027) dd_edit_flag,\n    a.dd_object_type as dd_object_type,\n    restrict_access_to,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_org_profile_Own d,\n      si_users_t usr ,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND USR.ENTERPRISE_ID \u003d E.ENTERPRISE_ID\n    AND A.OBJECT_ID       \u003d D.OBJECT_ID\n    ) AS OWNERS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_objectives base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ORG_PROFILE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_OBJECTIVES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ORG_PROFILE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_CONTROLS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ORG_PROFILE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_PROCESSES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ORG_PROFILE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_RISKS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ORG_PROFILE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    )                                                                                                                            AS RELATED_REFERENCES,\n    OBJ_STATUS                                                                                                                   AS STATUS,\n    ms_grc_utilities.get_edit_url (dd_object_type, dd_process_instance_id, dd_instance_id, NVL(DD_EDIT_FLAG,\u0027Y\u0027), a.OBJECT_NAME) AS object_name_url,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM ms_grc_org_profile_ORG E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    )                                                        AS owner_org_ids,\n    MS_GRC_UTILITIES.INCLUDE_EXPIRED(VALID_FROM,VALID_UNTIL) AS EXPIRED_FLAG,\n    SUBSTR(object_name, 1, 30) as display_name,\n             \u0027Y\u0027 AS OVER_RIDE_ACTION,\n             \n             \u0027proc\u003d\u0027 || TO_CHAR(dd_process_instance_id) || \u0027|instid\u003d\u0027 ||\n             TO_CHAR(dd_instance_id) || \u0027|edit_flag\u003d\u0027 ||\n             NVL(dd_edit_flag, \u0027Y\u0027) AS EDIT_ACTION_PARAMETERS\n  FROM ms_grc_org_profile a\n  WHERE a.dd_object_type \u003d \u0027MS_GRC_ORG_PROFILE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_DATA_UPLOAD_FORM_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_DATA_UPLOAD_FORM_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_DATA_UPLOAD_FORM_V\" (\"TRIGGER_WORKFLOW_TMP\", \"TRIGGER_WORKFLOW\", \"WORKFLOW_CODE\", \"GRC_PROFILE\", \"ATTACHMENT\", \"SYSTEM_ENTITY\", \"FILE_TO_UPLOAD\", \"DATA_FORM\", \"MODULE\", \"UPLOAD_TYPE\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\") AS \n  select \"TRIGGER_WORKFLOW_TMP\",\"TRIGGER_WORKFLOW\",\"WORKFLOW_CODE\",\"GRC_PROFILE\",\"ATTACHMENT\",\"SYSTEM_ENTITY\",\"FILE_TO_UPLOAD\",\"DATA_FORM\",\"MODULE\",\"UPLOAD_TYPE\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MUL