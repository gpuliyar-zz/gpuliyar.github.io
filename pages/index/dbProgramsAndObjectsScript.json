\"VALID_UNTIL\", \"VALID_FROM\", \"TYPE_OBJ\", \"RESTRICT_ACCESS_TO\", \"OWNERS\", \"OBJECT_NAME\", \"OBJECT_ACTION\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"DESCRIPTION\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  SELECT ORB_REL_SOURCE_ID,\n  OBJECTIVE_LEVEL,\n  PARENT_OBJECTIVE_NAME,\n  KEY_OBJECTIVE,\n  OBJECTIVE_TYPE,\n  COSO_TYPE,\n  STAKE_HOLDERS,\n  TYPE_OF_OWNER,\n  PTOA_INPUT,\n  ATO_ORB_TEMP,\n  ORB_KEY_RISK,\n  NATURE_OF_RELATIONSHIP,\n  NATURE_OF_REL_PTOA,\n  CAUSE_CONSEQUENCE,\n  TEMP_FIELD_OBJ_ID,\n  ORB_RPT_NAME,\n  ADDITIONAL_COLUMN5,\n  ADDITIONAL_COLUMN4,\n  ORB_FORM_URL,\n  ADDITIONAL_DETAILS_WITH_ID,\n  ATTRIBUTE_COLUMN_H2,\n  ATTRIBUTE_COLUMN_H3,\n  ADDITIONAL_COLUMN_H1,\n  ADDITIONAL_COLUMN3,\n  ADDITIONAL_COLUMN2,\n  ADDITIONAL_COLUMN1,\n  CHECK_USER_AVAIL,\n  ADDITIONAL_DETAILS,\n  APPLICABLE_TO,\n  ROW_NUM,\n  TGT_OBJ_TYPE,\n  SELF_REL_TYPE,\n  PARENT_REL_INST_ID,\n  REL_COMMENTS,\n  REL_VALID_UNTIL,\n  REL_VALID_FROM,\n  TGT_OBJ_ID,\n  TGT_OBJ_NAME,\n  REL_CONFIG_ID,\n  REL_INST_ID,\n  ASSOCIATED_WITH_TYPE,\n  CONFIGURED_OBJECT_TYPES,\n  REL_SOURCE_OBJECT_ID,\n  REL_SOURCE_ID,\n  OBJ_STATUS_TEMP,\n  ATTACHMENTS,\n  OWN_APP_CONFIG_TEMP,\n  OWN_APP_CONFIG,\n  MS_GRC_UTILITIES.GET_STATUS_NAME (DD_CURRENT_USER_NAME,\n                                              obj_status) AS OBJ_STATUS,\n  OBJ_MODIFIED_ON,\n  OBJ_MODIFIED_BY,\n  OBJ_CREATED_ON,\n  OBJ_CREATED_BY,\n  RTF_ATTACH,\n  MS_APPS_UTILITIES.GET_DISPLAY_VALUES (\n               MS_GRC_UTILITIES.ACTIVE_ENT_ID,\n               \u0027ORGENTITY\u0027,\n               OWNER_ORGANIZATIONS,\n               \u0027,\u0027\n            ) as \n               OWNER_ORGANIZATIONS,\n  OBJECT_ID,\n  VALID_UNTIL,\n  VALID_FROM,\n  ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS _GRC_OBJECTIVE_TYPE\u0027,\n               TYPE_OBJ\n            ) AS TYPE_OBJ,\n  ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Restrict Access To\u0027,\n               RESTRICT_ACCESS_TO\n            ) as RESTRICT_ACCESS_TO,\n  MS_GRC_UTILITIES.GET_USERS (OWNERS) AS OWNERS,\n  OBJECT_NAME,\n     ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Action\u0027,\n               object_action\n            )\n               OBJECT_ACTION,\n            ms_apps_utilities.get_user_full_name (LEVEL_2_APPROVER)\n               level_2_approver,\n            MS_APPS_UTILITIES.GET_USER_FULL_NAME (LEVEL_1_APPROVER)\n               level_1_approver,\n  DESCRIPTION,\n  ACTION_COMMENTS,\n  DD_PROCESS_CODE,\n  DD_CURRENT_STAGE,\n  DD_ENTERPRISE_INFO,\n  MS_APPS_UTILITIES.GET_USER_FULL_NAME (DD_EVENT_USER_NAME)\n               \"DD_EVENT_USER_NAME\",\n            MS_APPS_UTILITIES.GET_USER_FULL_NAME (DD_CURRENT_USER_NAME)\n               \"DD_CURRENT_USER_NAME\",\n  DD_OBJECT_TYPE,\n  INSTANCE_REC_NUM,\n  INSTANCE_ID,\n  METRIC_ID,\n  METRIC_RUN_DATE,\n  METRIC_COMPLETED_DATE,\n  LATEST_FLAG,\n  CREATED_BY,\n  CREATION_DATE,\n  PROCESS_FLOW_STATUS,\n  PROCESS_INSTANCE_ID,\n  MULTIROW_GROUP_NAME,\n  ELECTRONICALLY_SIGNED,\n  MULTIROW_REGION_ID,\n  DESCRIPTION_HTM\nFROM   MS_GRC_OBJECTIVES_V \n    where   DD_CURRENT_STAGE !\u003d \u0027NONE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
