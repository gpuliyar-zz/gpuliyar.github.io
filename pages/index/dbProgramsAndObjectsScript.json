\",\n    \"ATTACHMENTS\",\n    ms_grc_utilities.get_status_name\n                                          (dd_current_user_name,\n                                           obj_status\n                                          ) \"OBJ_STATUS\",\n    \"OBJ_CREATED_BY\",\n    \"VALID_UNTIL\",\n    \"VALID_FROM\",\n    \"RTF_ATTACH\",\n    ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Restrict Access To\u0027, restrict_access_to ) restrict_access_to,\n    ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Reference URL Type\u0027, reference_url_type ) reference_url_type,\n    \"REFERENCE_URL\",\n    ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Reference Type\u0027, reference_type ) reference_type,\n    \"REFERENCE_DOCUMENT\",\n    ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Reference Category\u0027, reference_category ) REFERENCE_CATEGORY,\n    MS_APPS_UTILITIES.GET_DISPLAY_VALUES (MS_GRC_UTILITIES.ACTIVE_ENT_ID, \u0027ORGENTITY\u0027,OWNER_ORGANIZATIONS,\u0027,\u0027 ) OWNER_ORGANIZATIONS,\n    MS_GRC_UTILITIES.get_users (owners ) owners,\n    \"OBJECT_NAME\",\n    \"OBJECT_ID\",\n     ms_apps_utilities.get_display_value (MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC Action\u0027, object_action ) object_action,\n    ms_apps_utilities.get_user_full_name (LEVEL_2_APPROVER) level_2_approver,\n    ms_apps_utilities.get_user_full_name (level_1_approver) LEVEL_1_APPROVER,\n    \"DESCRIPTION\",\n    \"ACTION_COMMENTS\",\n    \"DD_PROCESS_CODE\",\n    \"DD_CURRENT_STAGE\",\n    \"DD_ENTERPRISE_INFO\",\n    MS_APPS_UTILITIES.GET_USER_FULL_NAME (DD_EVENT_USER_NAME) DD_EVENT_USER_NAME,\n    MS_APPS_UTILITIES.GET_USER_FULL_NAME (dd_current_user_name) dd_current_user_name,\n    \"DD_OBJECT_TYPE\",\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    \"CREATED_BY\",\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"DESCRIPTION_HTM\"\n    FROM\nMS_GRC_REFERENCE_V\n  WHERE   DD_CURRENT_STAGE !\u003d \u0027NONE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_AOC_ORB_REPORT_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_AOC_ORB_REPORT_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_AOC_ORB_REPORT_V\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DD_OBJECT_TYPE\", \"ATO_ORGANIZATIONS\", \"OWNER_ORG_IDS\", \"RESTRICT_ACCESS_TO\", \"AOC_TYPE\") AS \n  SELECT a.object_name,\n    a.object_id,\n    (SELECT ms_concat(ms_apps_utilities.get_display_value ( (ms_grc_utilities.active_ent_id), \u0027ORGENTITY\u0027, owner_organizations ))\n    FROM ms_grc_Area_of_complia_org d\n    WHERE a.object_id \u003d d.object_id\n    ) AS owner_organizations,\n    (SELECT ms_concat ( ms_apps_utilities.get_user_full_name (owners) )\n    FROM ms_grc_Area_of_COmplia_own e\n    WHERE a.object_id \u003d e.object_id\n    ) AS OWNERS,\n    valid_until,\n    valid_from,\n    dd_object_type,\n    ms_grc_utilities.get_relationships (object_id, dd_object_type, \u0027MS_GRC_ORGANIZATION\u0027) AS ato_organizations,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM MS_GRC_Area_of_complia_ORG E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS owner_org_ids,\n    restrict_access_to,\n     (SELECT ms_concat(ms_apps_utilities.get_display_value ( (ms_grc_utilities.active_ent_id), \u0027MS_GRC_AOC_TYPES\u0027, aoc_type ))\n    FROM ms_grc_Area_of_complia_typ d\n    WHERE a.object_id \u003d d.object_id\n    ) AS aoc_type\n  FROM ms_grc_AREA_OF_COMPLIA a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_MODEL_REFERENCE_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_MODEL_REFERENCE_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_MODEL_REFERENCE_V\" (\"ORB_REL_SOURCE_ID\", \"REF_OBJECT_ID\", \"STAKE_HOLDERS\", \"MRE_OBJECT_ID\", \"TYPE_OF_OWNER\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"NATURE_OF_RELATIONSHIP\", \"NATURE_OF_REL_PTOA\", \"CAUSE_CONSEQUENCE\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"CHECK_USER_AVAIL\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"OBJ_SOURCE\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"OBJ_STATUS_TEMP\", \"REF_COMMENTS\", \"MRE_SOURCE\", \"MRE_COMMENTS\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_CREATED_BY\", \"OBJ_CREATED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_MODIFIED_ON\", \"OBJ_STATUS\", \"REFERENCE_ID\", \"MODEL_ID\", \"MODEL_TYPE\", \"MODEL_NAME\", \"RTF_ATTACH\", \"REFERENCE_TYPE\", \"REFERENCE_NAME\", \"VALID_UNTIL\", \"VALID_FROM\", \"RESTRICT_ACCESS_TO\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"OBJ_TYPE\", \"OBJECT_NAME\", \"OBJECT_ID\", \"OBJECT_ACTION\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"DESCRIPTION\", \"ATTACHMENTS\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  select \"ORB_REL_SOURCE_ID\",\"REF_OBJECT_ID\",\"STAKE_HOLDERS\",\"MRE_OBJECT_ID\",\"TYPE_OF_OWNER\",\"PTOA_INPUT\",\"ATO_ORB_TEMP\",\"ORB_KEY_RISK\",\"NATURE_OF_RELATIONSHIP\",\"NATURE_OF_REL_PTOA\",\"CAUSE_CONSEQUENCE\",\"TEMP_FIELD_OBJ_ID\",\"ORB_RPT_NAME\",\"ADDITIONAL_COLUMN5\",\"ADDITIONAL_COLUMN4\",\"ORB_FORM_URL\",\"ADDITIONAL_DETAILS_WITH_ID\",\"ATTRIBUTE_COLUMN_H2\",\"ATTRIBUTE_COLUMN_H3\",\"ADDITIONAL_COLUMN_H1\",\"CHECK_USER_AVAIL\",\"ADDITIONAL_COLUMN3\",\"ADDITIONAL_COLUMN2\",\"ADDITIONAL_COLUMN1\",\"ADDITIONAL_DETAILS\",\"APPLICABLE_TO\",\"OBJ_SOURCE\",\"ROW_NUM\",\"TGT_OBJ_TYPE\",\"SELF_REL_TYPE\",\"PARENT_REL_INST_ID\",\"REL_COMMENTS\",\"REL_VALID_UNTIL\",\"REL_VALID_FROM\",\"TGT_OBJ_ID\",\"TGT_OBJ_NAME\",\"REL_CONFIG_ID\",\"REL_INST_ID\",\"ASSOCIATED_WITH_TYPE\",\"CONFIGURED_OBJECT_TYPES\",\"REL_SOURCE_OBJECT_ID\",\"REL_SOURCE_ID\",\"OBJ_STATUS_TEMP\",\"REF_COMMENTS\",\"MRE_SOURCE\",\"MRE_COMMENTS\",\"OWN_APP_CONFIG_TEMP\",\"OWN_APP_CONFIG\",\"OBJ_CREATED_BY\",\"OBJ_CREATED_ON\",\"OBJ_MODIFIED_BY\",\"OBJ_MODIFIED_ON\",\"OBJ_STATUS\",\"REFERENCE_ID\",\"MODEL_ID\",\"MODEL_TYPE\",\"MODEL_NAME\",\"RTF_ATTACH\",\"REFERENCE_TYPE\",\"REFERENCE_NAME\",\"VALID_UNTIL\",\"VALID_FROM\",\"RESTRICT_ACCESS_TO\",\"OWNER_ORGANIZATIONS\",\"OWNERS\",\"OBJ_TYPE\",\"OBJECT_NAME\",\"OBJECT_ID\",\"OBJECT_ACTION\",\"LEVEL_2_APPROVER\",\"LEVEL_1_APPROVER\",\"DESCRIPTION\",\"ATTACHMENTS\",\"ACTION_COMMENTS\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\",\"DESCRIPTION_HTM\" from SI_100991_T \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PROCESS_DENORM_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PROCESS_DENORM_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_PROCESS_DENORM_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"KEY_PROCESS\", \"LOCALE_ID\", \"LOCALE\", \"DESCRIPTION\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"TYPE\", \"CATEGORY\", \"OBJECT_LEVEL\", \"CONFIDENTIALITY\", \"INTEGRITY\", \"PROCESS_AVAILABILITY\", \"BUSINESS_CRITICALITY\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"OWNER_ORG_IDS\", \"OWNER_ORG_NAME\", \"RESTRICT_ACCESS_TO\", \"PARENT\", \"STATUS\", \"RELATED_AOC_ID\", \"RELATED_AOC_NAME\", \"RELATED_ASSET_CLASS_ID\", \"RELATED_ASSET_CLASS_NAME\", \"RELATED_ASSET_ID\", \"RELATED_ASSET_NAME\", \"RELATED_CONTROL_ID\", \"RELATED_CONTROL_NAME\", \"RELATED_EVIDENCE_ID\", \"RELATED_EVIDENCE_NAME\", \"RELATED_EXCEPTION_ID\", \"RELATED_EXCEPTION_NAME\", \"RELATED_FIN_ACCT_ID\", \"RELATED_FIN_ACCT_NAME\", \"RELATED_FUNCTION_ID\", \"RELATED_FUNCTION_NAME\", \"RELATED_KPIKRI_ID\", \"RELATED_KPIKRI_NAME\", \"RELATED_FRAMEWORK_REF_ID\", \"RELATED_FRAMEWORK_REF_NAME\", \"RELATED_OBJECTIVE_ID\", \"RELATED_OBJECTIVE_NAME\", \"RELATED_APPLIES_TO_ORG_ID\", \"RELATED_APPLIES_TO_ORG_NAME\", \"RELATED_ORG_PROFILE_ID\", \"RELATED_ORG_PROFILE_NAME\", \"RELATED_PROCESS_ID\", \"RELATED_PROCESS_NAME\", \"RELATED_PRODUCT_ID\", \"RELATED_PRODUCT_NAME\", \"RELATED_QUES_PROC_ID\", \"RELATED_QUES_PROC_NAME\", \"RELATED_REFERENCE_ID\", \"RELATED_REFERENCE_NAME\", \"RELATED_REG_BODY_ID\", \"RELATED_REG_BODY_NAME\", \"RELATED_REQUIREMENT_ID\", \"RELATED_REQUIREMENT_NAME\", \"RELATED_RISK_ID\", \"RELATED_RISK_NAME\", \"RELATED_STANDARD_ID\", \"RELATED_STANDARD_NAME\", \"CORE_OBJECT_TYPE\", \"DISPLAY_NAME\", \"DD_OBJECT_TYPE\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"EXPIRED_FLAG\", \"EDIT\", \"ATTACHMENTS\", \"CREATED_ON\", \"CREATED_BY\", \"UPDATED_ON\", \"UPDATED_BY\", \"LEVEL_1_APPROVER\", \"LEVEL_2_APPROVER\", \"OBJECT_NAME_URL\") AS \n  SELECT ProcSumm.Proc_Obj_Name AS OBJECT_NAME,\n    ProcSumm.Proc_Obj_Id        AS OBJECT_ID,\n    INITCAP(ProcSumm.KEY_PROCESS),\n    ProcMlsSumm.LOCALE_ID,\n    ProcMlsSumm.LOCALE,\n    ProcSumm.description AS DESCRIPTION,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_core_object_own d,\n      si_users_t usr ,\n      si_ent e\n    WHERE usr.user_name      \u003d d.owners\n    AND usr.enterprise_id    \u003d e.enterprise_id\n    AND procsumm.proc_obj_id \u003d d.object_id\n    )                    AS OWNERS,\n    procsumm.valid_until AS VALID_UNTIL,\n    procsumm.valid_from  AS VALID_FROM,\n    --procsumm.process_TYPE               AS TYPE,\n    --procsumm.process_category           AS CATEGORY,\n    ProcMlsSumm.PROCESS_TYPE         AS TYPE,\n    ProcMlsSumm.PROCESS_CATEGORY     AS CATEGORY,\n    ProcMlsSumm.object_level         AS OBJECT_LEVEL,\n    procsumm.confidentiality         AS CONFIDENTIALITY,\n    procsumm.integrity               AS INTEGRITY,\n    procsumm.availability_process    AS PROCESS_AVAILABILITY,\n    procsumm.business_criticality    AS BUSINESS_CRITICALITY,\n    procsumm.dd_process_instance_id  AS DD_PROCESS_INSTANCE_ID,\n    procsumm.dd_instance_id          AS DD_INSTANCE_ID,\n    NVL (procsumm.dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    procsumm.Obj_Own_Org_Id_Agg      AS OWNER_ORG_IDS,\n    procsumm.Obj_Own_Org_Name_Agg    AS OWNER_ORG_IDS,\n    procsumm.restrict_access_to      AS RESTRICT_ACCESS_TO,\n    (SELECT object_name\n    FROM ms_grc_core_object co\n    WHERE co.object_id    \u003d procsumm.proc_obj_parent\n    AND co.dd_object_type \u003d \u0027MS_GRC_PROCESS\u0027\n    )                                    AS PARENT,\n    ProcMlsSumm.OBJECT_STATUS            AS STATUS,\n    RelSumm.REL_AOC_OBJ_ID_AGG           AS Related_Aoc_Id,\n    RelSumm.REL_AOC_OBJ_NAME_AGG         AS Related_Aoc_Name,\n    RelSumm.REL_ASSET_CLASS_OBJ_ID_AGG   AS Related_Asset_Class_Id,\n    RelSumm.REL_ASSET_CLASS_OBJ_NAME_AGG AS Related_Asset_Class_Name,\n    RelSumm.REL_ASSET_OBJ_ID_AGG         AS Related_Asset_Id,\n    RelSumm.REL_ASSET_OBJ_NAME_AGG       AS Related_Asset_Name,\n    RelSumm.REL_CTRL_OBJ_ID_AGG          AS Related_Control_Id,\n    RelSumm.REL_CTRL_OBJ_NAME_AGG        AS Related_Control_Name,\n    RelSumm.REL_EVIDENCE_OBJ_ID_AGG      AS Related_Evidence_Id,\n    RelSumm.REL_EVIDENCE_OBJ_NAME_AGG    AS Related_Evidence_Name,\n    RelSumm.REL_EXCEPTION_OBJ_ID_AGG     AS Related_Exception_Id,\n    RelSumm.REL_EXCEPTION_OBJ_NAME_AGG   AS Related_Exception_Name,\n    RelSumm.REL_FIN_ACT_OBJ_ID_AGG       AS Related_Fin_Acct_Id,\n    RelSumm.REL_FIN_ACT_OBJ_NAME_AGG     AS Related_Fin_Acct_Name,\n    RelSumm.REL_FUNCTION_OBJ_ID_AGG      AS Related_Function_Id,\n    RelSumm.REL_FUNCTION_OBJ_NAME_AGG    AS Related_Function_Name,\n    RelSumm.REL_KPIKRI_DEF_OBJ_ID_AGG    AS Related_KpiKri_Id,\n    RelSumm.REL_KPIKRI_DEF_OBJ_NAME_AGG  AS Related_KpiKri_Name,\n    RelSumm.REL_MOD_REF_OBJ_ID_AGG       AS Related_Framework_Ref_Id,\n    RelSumm.REL_MOD_REF_OBJ_NAME_AGG     AS Related_Framework_Ref_Name,\n    RelSumm.REL_OBJECTIVE_OBJ_ID_AGG     AS Related_Objective_Id,\n    RelSumm.REL_OBJECTIVE_OBJ_NAME_AGG   AS Related_Objective_Name,\n    RelSumm.REL_ORGANIZATION_IDS         AS Related_Applies_To_Org_Id,\n    RelSumm.REL_ORGANIZATION_NAMES       AS Related_Applies_To_Org_Name,\n    RelSumm.REL_ORG_PROFILE_OBJ_ID_AGG   AS Related_Org_profile_Id,\n    RelSumm.REL_ORG_PROFILE_OBJ_NAME_AGG AS Related_Org_profile_Name,\n    RelSumm.REL_PROCESS_OBJ_ID_AGG       AS Related_Process_Id,\n    RelSumm.REL_PROCESS_OBJ_NAME_AGG     AS Related_Process_Name,\n    RelSumm.REL_PRODUCT_ID_AGG           AS Related_Product_Id,\n    RelSumm.REL_PRODUCT_NAME_AGG         AS Related_Product_Name,\n    RelSumm.REL_QUE_PROC_OBJ_ID_AGG      AS Related_Ques_Proc_Id,\n    RelSumm.REL_QUE_PROC_OBJ_NAME_AGG    AS Related_Ques_Proc_Name,\n    RelSumm.REL_REFRENCES_OBJ_ID_AGG     AS Related_Reference_Id,\n    RelSumm.REL_REFRENCES_OBJ_NAME_AGG   AS Related_Reference_Name,\n    RelSumm.REL_REG_BODY_OBJ_ID_AGG      AS Related_Reg_Body_Id,\n    RelSumm.REL_REG_BODY_OBJ_NAME_AGG    AS Related_Reg_Body_Name,\n    RelSumm.REL_REQUIREMENT_OBJ_ID_AGG   AS Related_Requirement_Id,\n    RelSumm.REL_REQUIREMENT_OBJ_NAME_AGG AS Related_Requirement_Name,\n    RelSumm.REL_RISK_OBJ_ID_AGG          AS Related_Risk_Id,\n    RelSumm.REL_RISK_OBJ_NAME_AGG        AS Related_Risk_Name,\n    RelSumm.REL_STANDARD_OBJ_ID_AGG      AS Related_Standard_Id,\n    RelSumm.REL_STANDARD_OBJ_NAME_AGG    AS Related_Standard_Name,\n    procsumm.core_object_type            AS CORE_OBJECT_TYPE,\n    SUBSTR(ProcSumm.Proc_Obj_Name,1,30)  AS DISPLAY_NAME,\n    procsumm.dd_object_type              AS DD_OBJECT_TYPE,\n    \u0027Y\u0027                                  AS OVER_RIDE_ACTION,\n    (SELECT \u0027proc\u003d\u0027\n      || TO_CHAR(co.dd_process_instance_id)\n      ||\u0027|instid\u003d\u0027\n      ||TO_CHAR(co.dd_instance_id)\n      ||\u0027|edit_flag\u003d\u0027\n      ||NVL(proc1.dd_edit_flag,\u0027Y\u0027)\n    FROM ms_grc_core_object co,\n      ms_grc_process proc1\n    WHERE co.object_id    \u003d procsumm.PROC_OBJ_ID\n    AND co.dd_object_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND co.object_id      \u003d proc1.object_id\n    )                        AS EDIT_ACTION_PARAMETERS,\n    procsumm.expired_flag    AS EXPIRED_FLAG,\n    procsumm.object_name_url AS EDIT,\n    procsumm.attachments     AS ATTACHMENTS,\n    procsumm.CREATED_DATE,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE procsumm.CREATED_BY \u003d usr.user_name\n    ) AS CREATED_BY,\n    procsumm.UPDATED_DATE,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE procsumm.UPDATED_BY \u003d usr.user_name\n    ) AS UPDATED_BY,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE ProcSumm.level_1_approver \u003d usr.user_name\n    ) AS level_1_approver,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE ProcSumm.level_2_approver \u003d usr.user_name\n    ) AS level_2_approver,\n    (SELECT ms_grc_utilities.get_edit_url (co.dd_object_type, co.dd_process_instance_id, co.dd_instance_id, NVL(proc1.DD_EDIT_FLAG,\u0027Y\u0027), co.OBJECT_NAME)\n    FROM ms_grc_core_object co,\n      ms_grc_process proc1\n    WHERE co.object_id    \u003d procsumm.PROC_OBJ_ID\n    AND co.dd_object_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND co.object_id      \u003d proc1.object_id\n    ) AS object_name_url\n    /*ms_grc_utilities.get_edit_url (ProcSumm.dd_object_type,\n    ProcSumm.dd_process_instance_id,\n    ProcSumm.dd_instance_id,\n    NVL(ProcSumm.DD_EDIT_FLAG,\u0027Y\u0027),\n    ProcSumm.PROC_OBJ_NAME) AS object_name_url*/\n  FROM MS_GRC_PROCESS_SUMMARY ProcSumm,\n    MS_GRC_PROCESS_REL_SUMMARY RelSumm,\n    MS_GRC_PROCESS_MLS_SUMMARY ProcMlsSumm\n  WHERE ProcSumm.Proc_Obj_Id \u003d RelSumm.Proc_Obj_Id(+)\n  AND PROCSUMM.PROC_OBJ_ID   \u003d PROCMLSSUMM.OBJECT_ID(+)\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-foot