  "MS_GRC_LOG_ID_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_LOG_ID_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_LOG_ID_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 3950 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_COMMON_BROWSER_ROWTYPE": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_COMMON_BROWSER_ROWTYPE\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"SURVEYSP3DEV\".\"MS_GRC_COMMON_BROWSER_ROWTYPE\" AS OBJECT (  DISPLAY_NAME        \tVARCHAR2(4000) ,OBJECT_TYPE         \tVARCHAR2(4000) ,OBJECT_ID           \tVARCHAR2(4000) ,OBJECT_LEVEL        \tVARCHAR2(4000) ,REL_CONFIG_ID       \tVARCHAR2(4000) ,OVER_RIDE_ACTION    \tVARCHAR2(4000) ,EDIT_ACTION_PARAMETERS  VARCHAR2(4000)  )\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_CONTROL_OWNERSHIP_RPT": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_CONTROL_OWNERSHIP_RPT\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_CONTROL_OWNERSHIP_RPT\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"LEGACY_ID\", \"OWNER_ORGS\", \"COMPLIANCE_CATEGORY\", \"KEY_CONTROL\", \"FREQUENCY\", \"OWNERS\", \"ATO_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"DD_OBJECT_TYPE\", \"DUMMY1\", \"DUMMY2\", \"DUMMY3\", \"DUMMY4\", \"DUMMY5\", \"OWNER_ORG_IDS\", \"ATO_ORG_IDS\", \"OWNER_IDS\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"DD_EDIT_FLAG\", \"DD_INSTANCE_ID\", \"DD_PROCESS_INSTANCE_ID\") AS \n  SELECT   a.object_id,\n            a.object_name,\n            a.legacy_id,\n            (SELECT   ms_concat(ms_apps_utilities.get_display_values (\n                                   ms_grc_utilities.active_ent_id,\n                                   \u0027ORGENTITY\u0027,\n                                   d.owner_organizations,\n                                   \u0027,\u0027\n                                ))\n               FROM   ms_grc_compliance_obje_ogn d\n              WHERE   a.object_id \u003d d.object_id)\n               owner_orgs,\n--            ms_apps_utilities.get_display_value (\n--               ms_grc_utilities.active_ent_id,\n--               \u0027MS_GRC_COMPLIANCE_CATEGORY\u0027,\n--               compliance_category\n--            )  COMPLIANCE_CATEGORY,\n--  ms_apps_utilities.get_display_values ( MS_GRC_UTILITIES.active_ent_id, \u0027MS_GRC_Nature_of_Relationship\u0027,\n--(SELECT MS_CONCAT (E.PURPOSE)\n--FROM MS_GRC_COMPLIANCE_OBJE_PUR e\n--where a.OBJECT_ID \u003d E.OBJECT_ID\n--), \u0027,\u0027 ) AS COMPLIANCE_CATEGORY,  \n(SELECT MS_CONCAT (E.PURPOSE)\nFROM MS_GRC_COMPLIANCE_OBJE_PUR e\nwhere a.OBJECT_ID \u003d E.OBJECT_ID\n) AS COMPLIANCE_CATEGORY,  \n--ms_apps_utilities.get_display_value (\n--               ms_grc_utilities.active_ent_id,\n--               \u0027MS GRC Yes No\u0027,\n--               key_compliance\n--            )\n              key_compliance as  key_control,\n--            ms_apps_utilities.get_display_value (\n--               ms_grc_utilities.active_ent_id,\n--               \u0027MS_GRC_Frequency\u0027,\n--               a.frequency\n--            )\n             a.frequency  as  frequency,\n            ms_apps_utilities.get_user_full_name (b.owners) AS owners,\n            (SELECT   ms_grc_utilities.get_relationships (\n                         a.object_id,\n                         a.dd_object_type,\n                         \u0027MS_GRC_ORGANIZATION\u0027\n                      )\n               FROM   DUAL)\n               AS ato_organizations,\n            restrict_access_to,\n            a.dd_object_type,\n            a.dd_process_instance_id AS dummy1,\n            a.dd_object_type AS dummy2,\n            a.dd_instance_id AS dummy3,\n            NVL (a.dd_edit_flag, \u0027Y\u0027) AS dummy4,\n            ms_grc_utilities.get_edit_url (a.dd_object_type,\n                                           a.dd_process_instance_id,\n                                           a.dd_instance_id,\n                                           NVL (a.dd_edit_flag, \u0027Y\u0027),\n                                           a.object_name)\n               AS dummy5,\n            (SELECT   ms_concat (d.owner_organizations)\n               FROM   ms_grc_compliance_obje_ogn d\n              WHERE   a.object_id \u003d d.object_id)\n               AS owner_org_ids,\n            (SELECT   ms_grc_utilities.get_relationships (\n                         a.object_id,\n                         \u0027MS_GRC_CONTROL\u0027,\n                         \u0027MS_GRC_ORGANIZATION\u0027,\n                         \u0027OBJECT_ID\u0027\n                      )\n               FROM   DUAL)\n               ato_org_ids,\n            (SELECT   ms_concat (owners)\n               FROM   ms_grc_compliance_obje_own\n              WHERE   object_id \u003d a.object_id)\n               owner_ids,\n            \u0027Y\u0027 AS over_ride_action,\n               \u0027proc\u003d\u0027\n            || TO_CHAR (a.dd_process_instance_id)\n            || \u0027|instid\u003d\u0027\n            || TO_CHAR (a.dd_instance_id)\n            || \u0027|edit_flag\u003d\u0027\n            || NVL (c.dd_edit_flag, \u0027Y\u0027)\n               AS edit_action_parameters,\n            NVL (c.dd_Edit_flag, \u0027Y\u0027),\n            a.dd_instance_id,\n            a.dd_process_instance_id\n     FROM   ms_grc_compliance_obje a,\n            ms_grc_compliance_obje_own b,\n            ms_Grc_control c\n    where   B.OBJECT_ID \u003d a.OBJECT_ID and B.OBJECT_ID \u003d C.OBJECT_ID\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_MODEL_REFERENCE_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_MODEL_REFERENCE_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_MODEL_REFERENCE_DV\" (\"ORB_REL_SOURCE_ID\", \"REF_OBJECT_ID\", \"STAKE_HOLDERS\", \"MRE_OBJECT_ID\", \"TYPE_OF_OWNER\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"NATURE_OF_RELATIONSHIP\", \"NATURE_OF_REL_PTOA\", \"CAUSE_CONSEQUENCE\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"CHECK_USER_AVAIL\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"OBJ_SOURCE\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"OBJ_STATUS_TEMP\", \"REF_COMMENTS\", \"MRE_SOURCE\", \"MRE_COMMENTS\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_CREATED_BY\", \"OBJ_CREATED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_MODIFIED_ON\", \"OBJ_STATUS\", \"REFERENCE_ID\", \"MODEL_ID\", \"MODEL_TYPE\", \"MODEL_NAME\", \"RTF_ATTACH\", \"REFERENCE_TYPE\", \"REFERENCE_NAME\", \"VALID_UNTIL\", \"VALID_FROM\", \"RESTRICT_ACCESS_TO\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"OBJ_TYPE\", \"OBJECT_NAME\", \"OBJECT_ID\", \"OBJECT_ACTION\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"DESCRIPTION\", \"ATTACHMENTS\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  SELECT \"ORB_REL_SOURCE_ID\",\n    \"REF_OBJECT_ID\",\n    \"STAKE_HOLDERS\",\n    \"MRE_OBJECT_ID\",\n    \"TYPE_OF_OWNER\",\n    \"PTOA_INPUT\",\n    \"ATO_ORB_TEMP\",\n    \"ORB_KEY_RISK\",\n    \"NATURE_OF_RELATIONSHIP\",\n    \"NATURE_OF_REL_PTOA\",\n    \"CAUSE_CONSEQUENCE\",\n    \"TEMP_FIELD_OBJ_ID\",\n    \"ORB_RPT_NAME\",\n    \"ADDITIONAL_COLUMN5\",\n    \"ADDITIONAL_COLUMN4\",\n    \"ORB_FORM_URL\",\n    \"ADDITIONAL_DETAILS_WITH_ID\",\n    \"ATTRIBUTE_COLUMN_H2\",\n    \"ATTRIBUTE_COLUMN_H3\",\n    \"ADDITIONAL_COLUMN_H1\",\n    \"CHECK_USER_AVAIL\",\n    \"ADDITIONAL_COLUMN3\",\n    \"ADDITIONAL_COLUMN2\",\n    \"ADDITIONAL_COLUMN1\",\n    \"ADDITIONAL_DETAILS\",\n    \"APPLICABLE_TO\",\n    \"OBJ_SOURCE\",\n    \"ROW_NUM\",\n    \"TGT_OBJ_TYPE\",\n    \"SELF_REL_TYPE\",\n    \"PARENT_REL_INST_ID\",\n    \"REL_COMMENTS\",\n    \"REL_VALID_UNTIL\",\n    \"REL_VALID_FROM\",\n    \"TGT_OBJ_ID\",\n    \"TGT_OBJ_NAME\",\n    \"REL_CONFIG_ID\",\n    \"REL_INST_ID\",\n    \"ASSOCIATED_WITH_TYPE\",\n    \"CONFIGURED_OBJECT_TYPES\",\n    \"REL_SOURCE_OBJECT_ID\",\n    \"REL_SOURCE_ID\",\n    \"OBJ_STATUS_TEMP\",\n    \"REF_COMMENTS\",\n    \"MRE_SOURCE\",\n    \"MRE_COMMENTS\",\n    \"OWN_APP_CONFIG_TEMP\",\n    \"OWN_APP_CONFIG\",\n    \"OBJ_CREATED_BY\",\n    \"OBJ_CREATED_ON\",\n    \"OBJ_MODIFIED_BY\",\n    \"OBJ_MODIFIED_ON\",\n   ms_grc_utilities.get_status_name (dd_current_user_name,\n                                              obj_status)\n               \"OBJ_STATUS\",\n    \"REFERENCE_ID\",\n    \"MODEL_ID\",\n    \"MODEL_TYPE\",\n    \"MODEL_NAME\",\n    \"RTF_ATTACH\",\n    \"REFERENCE_TYPE\",\n    \"REFERENCE_NAME\",\n    \"VALID_UNTIL\",\n    \"VALID_FROM\",\n              ms_apps_utilities.get_display_value (100000,\n                                                 \u0027MS GRC Restrict Access To\u0027,\n                                                 restrict_access_to)\n               restrict_access_to,\n            ms_apps_utilities.get_display_values (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027ORGENTITY\u0027,\n               owner_organizations,\n               \u0027,\u0027\n            )\n               \"OWNER_ORGANIZATIONS\",\n            MS_GRC_UTILITIES.get_users (owners) \"OWNERS\",\n            \"OBJ_TYPE\",\n            \"OBJECT_NAME\",\n            \"OBJECT_ID\",\n            ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Action\u0027,\n               object_action\n            )\n               \"OBJECT_ACTION\",\n            ms_apps_utilities.get_user_full_name (level_2_approver)\n               level_2_approver,\n            ms_apps_utilities.get_user_full_name (level_1_approver)\n               level_1_approver,\n            \"DESCRIPTION\",\n            \"ATTACHMENTS\",\n            \"ACTION_COMMENTS\",\n            \"DD_PROCESS_CODE\",\n            \"DD_CURRENT_STAGE\",\n            \"DD_ENTERPRISE_INFO\",\n            ms_apps_utilities.get_user_full_name (dd_event_user_name)\n               \"DD_EVENT_USER_NAME\",\n            ms_apps_utilities.get_user_full_name (dd_current_user_name)\n               \"DD_CURRENT_USER_NAME\",\n    \"DD_OBJECT_TYPE\",\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    \"CREATED_BY\",\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"DESCRIPTION_HTM\"\n  from MS_GRC_MODEL_REFERENCE_V\n  WHERE   DD_CURRENT_STAGE !\u003d \u0027NONE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_COMPLIANCE_OBJE_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_COMPLIANCE_OBJE_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_COMPLIANCE_OBJE_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"LEGACY_ID\", \"COMPLIANCE_CATEGORY\", \"COMPLIANCE_TYPE\", \"KEY_COMPLIANCE\", \"FREQUENCY\", \"FREQUENCY_DATE\", \"START_AFTER\", \"DUE_BY\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM MS_GRC_COMPLIANCE_OBJE_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM MS_GRC_COMPLIANCE_OBJE_OGN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \nRESTRICT_ACCESS_TO, obj_STATUS,\nVALID_FROM, VALID_UNTIL, OBJECT_LEVEL,\nOBJECT_PARENT, LEGACY_ID, compliance_category, COMPLIANCE_TYPE, KEY_COMPLIANCE, \nFREQUENCY, FREQUENCY_DATE, START_AFTER, DUE_BY, NEXT_SCHEDULED_DATE\nRTF_aTTACH, DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID\nFROM ms_grc_compliance_obje a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PROC_MLS_SUMM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PROC_MLS_SUMM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_PROC_MLS_SUMM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_OBJC_MLS_SUMM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_OBJC_MLS_SUMM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_OBJC_MLS_SUMM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 21 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_FUNCTION_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_FUNCTION_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_FUNCTION_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \na.OBJECT_ID, a.OBJECT_NAME, regexp_replace(a.DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\na.DESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_Grc_function_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM ms_Grc_function_ORG B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \na.RESTRICT_ACCESS_TO, a.obj_STATUS,\na.VALID_FROM, a.VALID_UNTIL,   \na.RTF_aTTACH, a.DD_OBJECT_TYPE, a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID\nFROM ms_Grc_function a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DEFN_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DEFN_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DEFN_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"KPI_KRI_FLAG\", \"INDICATOR_MEASUREMENT\", \"MANUAL_ENTRY_AUTOMATED\", \"FREQUENCY\", \"DUE_BY\", \"APPROVE_ALL_MANUAL_KPI_KRI\", \"RISING_DROPPING_INDICATOR\", \"LEADING_TRAILING_INDICATOR\", \"CRITICAL_RISING\", \"NON_CRITICAL_RISING\", \"EXPECTED_VALUE\", \"NON_CRITICAL_DROPPING\", \"CRITICAL_DROPPING\", \"ASSEMENT_FOR\", \"NEXT_SCHEDULED_DATE\", \"OBJ_CURRENCY\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_Grc_kpi_kri_defn_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM ms_Grc_kpi_kri_defn_Ogn B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \nrestrict_access_to, obj_status,\nvalid_from, valid_until,   \nrtf_attach, dd_object_type, dd_process_instance_id, dd_instance_id,\na.kpi_kri_flag, indicator_measurement, manual_entry_automated, frequency, due_by, approve_all_manual_kpi_kri,\nrising_dropping_indicator, leading_trailing_indicator, critical_rising, non_critical_rising, expected_value, \nnon_critical_dropping, critical_dropping , assement_for, next_scheduled_date, obj_currency\nFROM ms_Grc_kpi_kri_defn a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_AREA_OF_COMPLIA_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_AREA_OF_COMPLIA_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_AREA_OF_COMPLIA_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"AOC_TYPE\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_grc_area_of_complia_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM ms_grc_area_of_complia_Org B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \n(SELECT MS_CONCAT(AOC_TYPE) FROM MS_GRC_AREA_OF_COMPLIA_TYP K WHERE K.OBJECT_ID \u003d A.OBJECT_ID ) AS AOC_TYPE,\nRESTRICT_ACCESS_TO, obj_STATUS,\nVALID_FROM, VALID_UNTIL,   \nRTF_aTTACH, DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID\nFROM ms_grc_area_of_complia a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DBOARD_CTRL_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DBOARD_CTRL_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DBOARD_CTRL_V\" (\"NAME\", \"OBJECT_ID\", \"OBJECT_NAME\", \"ATI_OBJECT_ID\", \"FREQUENCY\", \"EXPECTED_VALUE\", \"PREVIOUS_VALUE\", \"CURRENT_VALUE\", \"CURRENT_DATE_VALUE\", \"CHANGE\", \"LAST_3ASMNT\", \"LAST_6ASMNT\", \"LAST_12ASMNT\", \"APPLIES_TO\", \"DD_OBJECT_TYPE\", \"RESTRICT_ACCESS_TO\", \"OWNER_ORGANIZATIONS\", \"EVENT_ID\", \"RISK_CATEGORIES\", \"ASSESSED_ORGS\", \"METRIC_DEFN_URL\", \"CHANGE_NUMBER\") AS \n  SELECT b.object_name AS NAME,\n    a.object_id,\n    c.object_name AS object_name,\n    a.ati_object_id,\n    ms_apps_utilities.get_display_value (ms_grc_kpi_kri_utilities.get_ent_id (), \u0027MS GRC KI Frequency\u0027, b.frequency ) frequency,\n    a.expected_value,\n    a.previous_value,\n    a.current_value,\n    a.current_date_value,\n    ms_grc_kpi_kri_utilities.for_breached_rpt (current_value, previous_value )                AS CHANGE,\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 3 )  AS last_3asmnt,\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 6 )  AS last_6asmnt,\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 12 ) AS last_12asmnt,\n    applies_to,\n    b.dd_object_type,\n    b.restrict_access_to,\n    (SELECT ms_concat (owner_organizations)\n    FROM ms_grc_kpi_kri_defn_ogn d\n    WHERE a.object_id \u003d d.object_id\n    ) AS owner_organizations,\n    event_id,\n    (SELECT ms_concat(risk_category)\n    FROM MS_GRC_KPI_KRI_DEFN_RCG e\n    WHERE e.object_id \u003d b.object_id\n    )                                                                                                                                  AS RISK_CATEGORIES,\n    g.ati_assessed_orgs                                                                                                                AS ASSESSED_ORGS,\n    Ms_Grc_Utilities.Get_Edit_Url(b.DD_OBJECT_TYPE, b.dd_process_instance_id,b.dd_instance_id , NVL(b.dd_edit_flag,\u0027Y\u0027),b.object_name) AS Metric_defn_url,\n    ROUND((current_value - Previous_value),2)                                                                                          AS change_number\n  FROM ms_grc_kpi_kri_data_en a\n  join ms_grc_kpi_kri_defn b on a.object_id \u003d b.object_id\n  join ms_grc_compliance_obje c on c.object_id \u003d a.ati_object_id\n  join ms_grc_kpi_kri_defn_ati t on t.object_id\u003d a.object_id and t.ati_obj_id \u003d a.ati_object_id\n  left join ms_grc_kpi_kri_defn_aor g on g.ati_object_id_pk \u003d t.ati_object_id_pk\n  WHERE current_date_value IN\n    (SELECT MAX (current_date_value)\n    FROM ms_grc_kpi_kri_data_en\n    WHERE object_id   \u003d a.object_id\n    AND ati_object_id \u003d a.ati_object_id\n    )\n /* AND a.object_id \u003d b.object_id\n  AND c.object_id \u003d a.ati_object_id\n  and t.object_id\u003d g.object_id\n  and t.ati_object_id_pk\u003dg.ati_object_id_pk\n  and a.ati_object_id\u003dt.ati_obj_id\n  AND b.object_id \u003d g.object_id(+)*/\n  AND applies_to  \u003d \u00274\u0027\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_OBJECTIVES_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_OBJECTIVES_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_OBJECTIVES_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_Grc_objectives_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM ms_Grc_objectives_Org B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \nRESTRICT_ACCESS_TO, obj_STATUS,\nVALID_FROM, VALID_UNTIL,   \nRTF_aTTACH, DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID\nFROM ms_Grc_objectives a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_ORG_ACTIVITY_TAB_TYPE": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ORG_ACTIVITY_TAB_TYPE\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"SURVEYSP3DEV\".\"MS_GRC_ORG_ACTIVITY_TAB_TYPE\" AS TABLE OF MS_GRC_ORG_ACTIVITY_ROWTYPE \n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_RISK_DENORM_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_RISK_DENORM_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_RISK_DENORM_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"LOCALE_ID\", \"LOCALE\", \"OWNERS\", \"KEY_RISK\", \"LEVEL_1_APPROVER\", \"LEVEL_2_APPROVER\", \"VALID_UNTIL\", \"VALID_FROM\", \"DESCRIPTION\", \"RISK_CATEGORIES\", \"TYPE_RISK\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"RESTRICT_ACCESS_TO\", \"OVER_RIDE_ACTION\", \"OBJECT_TYPE\", \"EDIT_ACTION_PARAMETERS\", \"OBJECT_LEVEL\", \"DISPLAY_NAME\", \"OBJECT_NAME_URL\", \"RELATED_FRAMEWORK_REF_IDS\", \"RELATED_FRAMEWORK_REFERENCES\", \"RELATED_RISKS_IDS\", \"RELATED_RISKS\", \"RELATED_PROCESS_IDS\", \"RELATED_PROCESS\", \"RELATED_ASSETS_IDS\", \"RELATED_ASSETS\", \"RELATED_ASSET_CLASSES_IDS\", \"RELATED_ASSET_CLASSES\", \"RELATED_CONTROL_IDS\", \"RELATED_CONTROL\", \"RELATED_REQUIREMENT_IDS\", \"RELATED_REQUIREMENT\", \"RELATED_REG_BODY_IDS\", \"RELATED_REGULATORY_BODY\", \"RELATED_FUNCTION_IDS\", \"RELATED_FUNCTION\", \"RELATED_REFERENCE_IDS\", \"RELATED_REFERENCE\", \"APPLIES_TO_ORG_IDS\", \"APPLIES_TO_ORGANIZATIONS\", \"RELATED_AOC_IDS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_FIN_ACCT_IDS\", \"RELATED_FINANCIAL_ACCOUNTS\", \"RELATED_OBJECTIVES_IDS\", \"RELATED_OBJECTIVES\", \"RELATED_EVIDENCE_IDS\", \"RELATED_EVIDENCES\", \"RELATED_PRODUCT_IDS\", \"RELATED_PRODUCTS\", \"RELATED_STANDARD_IDS\", \"RELATED_STANDARDS\", \"OWNER_ORG_IDS\", \"OWNER_ORG_NAMES\", \"OBJ_STATUS\", \"OBJECT_PARENT\", \"EXPIRED_FLAG\", \"DD_OBJECT_TYPE\", \"CREATED_ON\", \"CREATED_BY\", \"UPDATED_ON\", \"UPDATED_BY\") AS \n  SELECT\n    (SELECT rsk.object_name\n    FROM MS_GRC_RISK rsk\n    WHERE rsk.object_id \u003d rsk_summ.object_id\n    ) AS Object_Name,\n    rsk_summ.object_id,\n    mls_Summ.locale_id,\n    mls_summ.locale,\n    rsk_summ.Obj_Owner_Name_Agg AS OWNERS,\n    INITCAP(mls_Summ.key_risk),\n    rsk_summ.level_1_approver,\n    rsk_summ.level_2_approver,\n    rsk_summ.valid_until,\n    rsk_summ.valid_from,\n    rsk_summ.description,\n    mls_Summ.CAT_NAME_AGG AS RISK_CATEGORIES,\n    mls_Summ.TYPE_RISK,\n    rsk_summ.dd_process_instance_id,\n    rsk_summ.dd_instance_id,\n    NVL (rsk_summ.dd_edit_flag, \u0027Y\u0027) AS dd_edit_flag,\n    rsk_summ.restrict_access_to,\n    \u0027Y\u0027                     AS OVER_RIDE_ACTION,\n    rsk_summ.DD_OBJECT_TYPE AS OBJECT_TYPE,\n    (SELECT (\u0027proc\u003d\u0027\n      || TO_CHAR (rsk.dd_process_instance_id)\n      || \u0027|instid\u003d\u0027\n      || TO_CHAR (rsk.dd_instance_id)\n      || \u0027|edit_flag\u003d\u0027\n      || NVL(rsk.dd_edit_flag, \u0027Y\u0027))\n    FROM MS_GRC_RISK rsk\n    WHERE rsk.object_id \u003d rsk_summ.object_id\n    ) AS EDIT_ACTION_PARAMETERS,\n    mls_Summ.object_level,\n    (SELECT SUBSTR (rsk.object_name,1, 30)\n    FROM MS_GRC_RISK rsk\n    WHERE rsk.object_id \u003d rsk_summ.object_id\n    ) AS Display_Name,\n    (SELECT ms_grc_utilities.get_edit_url (rsk_summ.dd_object_type, rsk_summ.dd_process_instance_id, rsk_summ.dd_instance_id, NVL (dd_edit_flag, \u0027Y\u0027), rsk_summ.object_name)\n    FROM MS_GRC_RISK rsk\n    WHERE rsk.object_id \u003d rsk_summ.object_id\n    )                                     AS object_name_url,\n    rel_summ.Rel_Mod_Ref_Obj_Id_Agg       AS Related_framework_ref_ids,\n    rel_summ.Rel_Mod_Ref_Obj_Name_Agg     AS Related_framework_references,\n    rel_summ.Rel_Risk_Obj_Id_Agg          AS Related_risks_ids,\n    rel_summ.Rel_Risk_Obj_Name_Agg        AS Related_risks,\n    rel_summ.Rel_Process_Obj_Id_Agg       AS related_process_ids,\n    rel_summ.Rel_Process_Obj_Name_Agg     AS related_process,\n    rel_summ.Rel_Asset_Obj_Id_Agg         AS related_asset_ids,\n    rel_summ.Rel_Asset_Obj_Name_Agg       AS related_asset,\n    rel_summ.Rel_Asset_Class_Obj_Id_Agg   AS related_asset_class_ids,\n    rel_summ.Rel_Asset_Class_Obj_Name_Agg AS related_asset_class,\n    rel_summ.Rel_Ctrl_Obj_Id_Agg          AS related_control_ids,\n    rel_summ.Rel_Ctrl_Obj_Name_Agg        AS related_control,\n    rel_summ.Rel_Requirement_Obj_Id_Agg   AS related_requirement_ids,\n    rel_summ.Rel_Requirement_Obj_Name_Agg AS related_requirement,\n    rel_summ.Rel_Reg_Body_Obj_Id_Agg      AS related_regulatory_body_ids,\n    rel_summ.Rel_Reg_Body_Obj_Name_Agg    AS related_regulatory_body,\n    rel_summ.Rel_Function_Obj_Id_Agg      AS related_function_ids,\n    rel_summ.Rel_Function_Obj_Name_Agg    AS related_function,\n    rel_summ.Rel_Refrences_Obj_Id_Agg     AS related_reference_ids,\n    rel_summ.Rel_Refrences_Obj_Name_Agg   AS related_reference,\n    rel_summ.Rel_Organization_ids         AS applies_to_organizations_ids,\n    rel_summ.Rel_Organization_names       AS applies_to_organizations,\n    rel_summ.Rel_Aoc_Obj_Id_Agg related_aoc_ids,\n    rel_summ.Rel_Aoc_Obj_Name_Agg related_areas_of_compliance,\n    rel_summ.Rel_Fin_Act_Obj_Id_Agg   AS related_fin_acct_ids,\n    rel_summ.Rel_Fin_Act_Obj_Name_Agg AS related_financial_accounts,\n    Rel_Objective_Obj_Id_Agg          AS related_objectives_ids,\n    Rel_Objective_Obj_Name_Agg        AS related_objectives,\n    REL_EVIDENCE_OBJ_ID_AGG           AS related_evidence_ids,\n    REL_EVIDENCE_OBJ_NAME_AGG         AS related_evidences,\n    Rel_Product_id_Agg                AS Rel_Product_ids,\n    Rel_Product_name_Agg              AS Rel_Product_names,\n    Rel_Standard_Obj_Id_Agg           AS Rel_Standard_ids,\n    Rel_Standard_Obj_Name_Agg         AS Rel_Standard_names,\n    rsk_summ.Obj_Own_Org_Id_Agg       AS owner_org_ids,\n    rsk_summ.Obj_Own_Org_Name_Agg     AS owner_organizations,\n    mls_Summ.obj_status               AS OBJ_STATUS,\n    rsk_summ.OBJECT_PARENT,\n    EXPIRED_FLAG,\n    DD_OBJECT_TYPE,\n    rsk_summ.CREATED_DATE,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE rsk_summ.CREATED_BY \u003d usr.user_name\n    ) AS CREATED_BY,\n    rsk_summ.UPDATED_DATE,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE rsk_summ.UPDATED_BY \u003d usr.user_name\n    ) AS UPDATED_BY\n  FROM MS_GRC_RISK_SUMMARY rsk_summ,\n    MS_GRC_RISK_REL_SUMMARY rel_summ,\n    MS_GRC_RISK_MLS_SUMMARY mls_Summ\n  WHERE rel_summ.object_id(+) \u003d rsk_summ.object_id\n  AND mls_Summ.object_id(+)   \u003d rsk_summ.object_id\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PROCESS_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PROCESS_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_PROCESS_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"REFERENCE_ID\", \"LEGACY_ID\", \"CORE_CATEGORY\", \"CORE_TYPE\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM MS_GRC_CORE_OBJECT_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM MS_GRC_CORE_OBJECT_OGN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \nRESTRICT_ACCESS_TO, obj_STATUS,\nVALID_FROM, VALID_UNTIL, OBJECT_LEVEL,\nOBJECT_PARENT, reference_id, LEGACY_ID, core_category, COre_TYPE,  \nRTF_aTTACH, DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID\nFROM ms_grc_core_object a\nWHERE dd_object_type \u003d \u0027MS_GRC_PROCESS\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PROC_REL_SUMM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PROC_REL_SUMM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_PROC_REL_SUMM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_ORG_PROFILE_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ORG_PROFILE_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_ORG_PROFILE_DV\" (\"ORB_REL_SOURCE_ID\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"STAKE_HOLDERS\", \"TYPE_OF_OWNER\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"NATURE_OF_RELATIONSHIP\", \"NATURE_OF_REL_PTOA\", \"CAUSE_CONSEQUENCE\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"CHECK_USER_AVAIL\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"OTHER_COMMENTS\", \"ACTION_COMMENTS\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"OBJ_STATUS_TEMP\", \"OBJ_MODIFIED_ON\", \"OBJ_MODIFIED_BY\", \"AD_ATTACHMENTS\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_CREATED_BY\", \"OBJ_CREATED_ON\", \"OBJ_STATUS\", \"USER_CHECK\", \"RESTRICT_ACCESS_TO\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"ATTACHMENTS\", \"BASEL_II_ENTITY\", \"DESCRIPTION\", \"OBJECT_ACTION\", \"OBJECT_ID\", \"OBJECT_NAME\", \"ORG_DESCRIPTION\", \"ORG_LEVEL\", \"ORG_NAME\", \"ORG_PARENT\", \"ORG_PROFILE_ID\", \"RTF_ATTACH\", \"VALID_FROM\", \"VALID_UNTIL\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  SELECT \"ORB_REL_SOURCE_ID\",\n    \"TEMP_FIELD_OBJ_ID\",\n    \"ORB_RPT_NAME\",\n    \"STAKE_HOLDERS\",\n    \"TYPE_OF_OWNER\",\n    \"PTOA_INPUT\",\n    \"ATO_ORB_TEMP\",\n    \"ORB_KEY_RISK\",\n    \"NATURE_OF_RELATIONSHIP\",\n    \"NATURE_OF_REL_PTOA\",\n    \"CAUSE_CONSEQUENCE\",\n    \"ADDITIONAL_COLUMN5\",\n    \"ADDITIONAL_COLUMN4\",\n    \"ORB_FORM_URL\",\n    \"ADDITIONAL_DETAILS_WITH_ID\",\n    \"ATTRIBUTE_COLUMN_H2\",\n    \"ATTRIBUTE_COLUMN_H3\",\n    \"ADDITIONAL_COLUMN_H1\",\n    \"CHECK_USER_AVAIL\",\n    \"ADDITIONAL_COLUMN3\",\n    \"ADDITIONAL_COLUMN2\",\n    \"ADDITIONAL_COLUMN1\",\n    \"ADDITIONAL_DETAILS\",\n    \"APPLICABLE_TO\",\n    \"OTHER_COMMENTS\",\n    \"ACTION_COMMENTS\",\n    \"ROW_NUM\",\n    \"TGT_OBJ_TYPE\",\n    \"SELF_REL_TYPE\",\n    \"PARENT_REL_INST_ID\",\n    \"REL_COMMENTS\",\n    \"REL_VALID_UNTIL\",\n    \"REL_VALID_FROM\",\n    \"TGT_OBJ_ID\",\n    \"TGT_OBJ_NAME\",\n    \"REL_CONFIG_ID\",\n    \"REL_INST_ID\",\n    \"ASSOCIATED_WITH_TYPE\",\n    \"CONFIGURED_OBJECT_TYPES\",\n    \"REL_SOURCE_OBJECT_ID\",\n    \"REL_SOURCE_ID\",\n    \"OBJ_STATUS_TEMP\",\n    \"OBJ_MODIFIED_ON\",\n    \"OBJ_MODIFIED_BY\",\n    \"AD_ATTACHMENTS\",\n    \"OWN_APP_CONFIG_TEMP\",\n    \"OWN_APP_CONFIG\",\n    \"OBJ_CREATED_BY\",\n    \"OBJ_CREATED_ON\",\n    ms_grc_utilities.get_status_name (dd_current_user_name,\n                                              obj_status)\n               \"OBJ_STATUS\",\n            \"USER_CHECK\",\n            ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Restrict Access To\u0027,\n               restrict_access_to\n            )\n               RESTRICT_ACCESS_TO,\n            ms_apps_utilities.get_user_full_name (level_2_approver)\n               level_2_approver,\n            ms_apps_utilities.get_user_full_name (level_1_approver)\n               LEVEL_1_APPROVER,\n            MS_GRC_UTILITIES.GET_USERS (OWNERS) owners,\n            MS_APPS_UTILITIES.GET_DISPLAY_VALUES (\n               MS_GRC_UTILITIES.ACTIVE_ENT_ID,\n               \u0027ORGENTITY\u0027,\n               OWNER_ORGANIZATIONS,\n               \u0027,\u0027\n            )\n               owner_organizations,\n            \"ATTACHMENTS\",\n            ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027Basel II - Basel Business Lines\u0027,\n               basel_ii_entity\n            )\n               AS basel_ii_entity,\n            \"DESCRIPTION\",\n            ms_apps_utilities.get_display_value (\n               MS_GRC_UTILITIES.active_ent_id,\n               \u0027MS GRC Action\u0027,\n               object_action\n            )\n               object_action,\n            \"OBJECT_ID\",\n            \"OBJECT_NAME\",\n            \"ORG_DESCRIPTION\",\n            \"ORG_LEVEL\",\n            \"ORG_NAME\",\n            (SELECT   org_entity_name\n               FROM   si_org_entities\n              WHERE   org_entity_id \u003d org_parent)\n               AS org_parent,\n            \"ORG_PROFILE_ID\",\n            \"RTF_ATTACH\",\n            \"VALID_FROM\",\n            \"VALID_UNTIL\",\n            \"DD_PROCESS_CODE\",\n            \"DD_CURRENT_STAGE\",\n            \"DD_ENTERPRISE_INFO\",\n            ms_apps_utilities.get_user_full_name (dd_event_user_name)\n               \"DD_EVENT_USER_NAME\",\n            MS_APPS_UTILITIES.GET_USER_FULL_NAME (DD_CURRENT_USER_NAME)\n               \"DD_CURRENT_USER_NAME\",\n    \"DD_OBJECT_TYPE\",\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    \"CREATED_BY\",\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"DESCRIPTION_HTM\" from\nMS_GRC_ORG_PROFILE_V\n  WHERE   DD_CURRENT_STAGE !\u003d \u0027NONE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PRODUCT_SUM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PRODUCT_SUM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_PRODUCT_SUM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_EXCEPTION_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_EXCEPTION_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_EXCEPTION_RV\" (\"EXCEPTION_ID\", \"EXCEPTION_FOR\", \"TYPE_EXCEPTION\", \"VALID_FROM\", \"EXPIRES_ON\", \"STATUS\", \"ITEMS\", \"EXPLANATION\", \"EXCEPTION_APPROVER\", \"APPLIES_TO_ORGANIZATIONS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_ASSET_CLASSES\", \"RELATED_ASSETS\", \"RELATED_CONTROLS\", \"RELATED_OBJECTIVES\", \"RELATED_PROCESSES\", \"RELATED_REFERENCES\", \"RELATED_REG_BODIES\", \"RELATED_REQUIREMENTS\", \"RELATED_RISKS\", \"RELATED_STANDARDS\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"OBJECT_TYPE\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"EDIT\", \"EXPIRED_FLAG\") AS \n  SELECT a.object_id Exception_ID,--a.exception_for exception_for_stored_val,\n  exception_for,\n  type_exception,\n  a.valid_from valid_from,\n  a.Valid_Until Expires_On,\n  a.obj_status Status,\n    (SELECT ms_concat(itm.select_items)\n    FROM Ms_Grc_Exception_itm itm\n    WHERE itm.object_id \u003d a.object_id) AS items,\n    --            b.Select_Items Items,\n    a.explanation Explanation,\n    a.approver as Exception_Approver,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     si_org_entities_t base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.org_entity_id\n    ) AS Applies_to_Organizations,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_area_of_complia base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS RELATED_AREAS_OF_COMPLIANCE,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_core_object base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS RELATED_ASSET_CLASSES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_core_object base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_ASSETS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_compliance_obje base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS RELATED_CONTROLS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_objectives base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_OBJECTIVES\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS RELATED_OBJECTIVES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_core_object base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS RELATED_PROCESSES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_reference base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS RELATED_REFERENCES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_regulatory_body base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_REGULATORY_BODY\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id)\n     AS RELATED_REG_BODIES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_requirement base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id)\n     AS RELATED_REQUIREMENTS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_risk base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS RELATED_RISKS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_area_of_complia base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_EXCEPTION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS RELATED_STANDARDS ,\n    A.DD_OBJECT_TYPE,\n    A.DD_PROCESS_INSTANCE_ID,\n    A.DD_INSTANCE_ID,\n    A.DD_EDIT_FLAG,\n    a.dd_object_type as Object_type,\n    \u0027Y\u0027 as OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027|| TO_CHAR(a.dd_process_instance_id)||\u0027|instid\u003d\u0027||TO_CHAR(a.dd_instance_id)||\u0027|edit_flag\u003d\u0027||NVL(a.dd_edit_flag,\u0027Y\u0027) as EDIT_ACTION_PARAMETERS,\n    ms_grc_utilities.get_edit_url(a.dd_object_type, a.dd_process_instance_id, a.dd_instance_id, nvl(a.dd_edit_flag,\u0027Y\u0027),a.object_id) as EDIT,\n     ms_Grc_utilities.INCLUDE_eXPIRED(a.VALID_FROM,a.VALID_UNTIL) AS EXPIRED_FLAG\n  FROM MS_GRC_EXCEPTION a\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_FUNCTION_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_FUNCTION_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_FUNCTION_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DESCRIPTION\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"RESTRICT_ACCESS_TO\", \"STATUS\", \"DISPLAY_NAME\", \"RELATED_PROCESSES\", \"RELATED_RISKS\", \"RELATED_CONTROLS\", \"RELATED_REFERENCES\", \"RELATED_AREA_OF_COMPLIANCES\", \"RELATED_REQUIREMENTS\", \"RELATED_STANDARDS\", \"EXPIRED_FLAG\", \"OVER_RIDE_ACTION\", \"EDIT\", \"EDIT_ACTION_PARAMETERS\", \"OWNER_ORG_IDS\") AS \n  SELECT object_name,\n    object_id,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_function_own d,\n      si_users_t usr,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND usr.enterprise_id \u003d e.ENTERPRISE_ID\n    AND a.OBJECT_ID       \u003d D.OBJECT_ID\n    ) AS OWNERS,\n    valid_until,\n    valid_from,\n    description,\n    dd_object_type,\n    dd_process_instance_id,\n    dd_instance_id,\n    NVL (dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    -- owner_organizations AS owner_org_ids,\n    restrict_access_to,\n    obj_status AS STATUS,\n    SUBSTR(object_name,1,30) display_name,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Processes,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Risks,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Controls,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_references,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_area_of_compliances,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_requirement base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Requirements,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    )                                                            AS Related_Standards,\n    ms_Grc_utilities.INCLUDE_eXPIRED(a.VALID_FROM,a.VALID_UNTIL) AS EXPIRED_FLAG,\n    \u0027Y\u0027                                                          AS OVER_RIDE_ACTION,\n    (SELECT ms_Grc_utilities.Get_Edit_Url(DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID, NVL(DD_EDIT_FLAG,\u0027Y\u0027),object_name)\n    FROM dual\n    ) AS EDIT,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(dd_edit_flag,\u0027Y\u0027) AS EDIT_ACTION_PARAMETERS,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM MS_GRC_FUNCTION_ORG E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS owner_org_ids\n  FROM MS_GRC_FUNCTION a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
