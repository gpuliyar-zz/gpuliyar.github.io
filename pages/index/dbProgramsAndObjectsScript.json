", \"LIST_OF_RESPONSE\", \"OBJECTIVE\", \"VALID_UNTIL\", \"APPLIES_TO_CONTROLS\", \"QUESTION\", \"RESTRICT_ACCESS_TO\", \"RESPONSE_TYPE\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"OBJECT_NAME\", \"OBJECT_ID\", \"OBJECT_ACTION\", \"MANUAL_AUTOMATIC\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"INFOLET_SOURCE\", \"CATEGORY_QUEST\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"QUESTION_HTM\", \"OBJECTIVE_HTM\") AS \n  select \"ORB_REL_SOURCE_ID\",\"TEMP_FIELD_OBJ_ID\",\"ORB_RPT_NAME\",\"STAKE_HOLDERS\",\"TYPE_OF_OWNER\",\"PTOA_INPUT\",\"ATO_ORB_TEMP\",\"ORB_KEY_RISK\",\"NATURE_OF_RELATIONSHIP\",\"NATURE_OF_REL_PTOA\",\"CAUSE_CONSEQUENCE\",\"ADDITIONAL_COLUMN5\",\"ADDITIONAL_COLUMN4\",\"ORB_FORM_URL\",\"ADDITIONAL_DETAILS_WITH_ID\",\"ATTRIBUTE_COLUMN_H2\",\"ATTRIBUTE_COLUMN_H3\",\"ADDITIONAL_COLUMN_H1\",\"ADDITIONAL_COLUMN3\",\"ADDITIONAL_COLUMN2\",\"ADDITIONAL_COLUMN1\",\"ADDITIONAL_DETAILS\",\"APPLICABLE_TO\",\"CHECK_USER_AVAIL\",\"ATTACHMENTS\",\"ROW_NUM\",\"TGT_OBJ_TYPE\",\"SELF_REL_TYPE\",\"PARENT_REL_INST_ID\",\"REL_COMMENTS\",\"REL_VALID_UNTIL\",\"REL_VALID_FROM\",\"TGT_OBJ_ID\",\"TGT_OBJ_NAME\",\"REL_CONFIG_ID\",\"REL_INST_ID\",\"ASSOCIATED_WITH_TYPE\",\"CONFIGURED_OBJECT_TYPES\",\"REL_SOURCE_OBJECT_ID\",\"REL_SOURCE_ID\",\"TYPE_QUEST\",\"OBJ_STATUS_TEMP\",\"OWN_APP_CONFIG_TEMP\",\"OWN_APP_CONFIG\",\"CURRENCY\",\"OBJ_STATUS\",\"OBJ_MODIFIED_ON\",\"OBJ_MODIFIED_BY\",\"OBJ_CREATED_ON\",\"OBJ_CREATED_BY\",\"APPLIES_TO_AOC\",\"VALID_FROM\",\"APPLIES_TO_CORE_OBJECTS\",\"RTF_ATTACH\",\"APPLIES_TO_REQUIREMENTS\",\"LIST_OF_RESPONSE\",\"OBJECTIVE\",\"VALID_UNTIL\",\"APPLIES_TO_CONTROLS\",\"QUESTION\",\"RESTRICT_ACCESS_TO\",\"RESPONSE_TYPE\",\"OWNER_ORGANIZATIONS\",\"OWNERS\",\"OBJECT_NAME\",\"OBJECT_ID\",\"OBJECT_ACTION\",\"MANUAL_AUTOMATIC\",\"LEVEL_2_APPROVER\",\"LEVEL_1_APPROVER\",\"INFOLET_SOURCE\",\"CATEGORY_QUEST\",\"ACTION_COMMENTS\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\",\"QUESTION_HTM\",\"OBJECTIVE_HTM\" from SI_100999_T \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REGULATORY_BODY_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REGULATORY_BODY_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_REGULATORY_BODY_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DESCRIPTION\", \"DD_OBJECT_TYPE\", \"OBJ_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"OBJECT_NAME_URL\", \"OWNER_ORG_IDS\", \"REL_FRAMEWORK_REF\", \"RELATED_REFERENCES\", \"RELATED_STANDARDS\", \"RELATED_AREA_OF_COMPLIANCE\", \"EXPIRED_FLAG\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\") AS \n  SELECT object_name,\n    object_id,\n     (SELECT ms_concat(usr.first_name\n    || \u0027 \u0027\n    || usr.last_name) AS owners\n   FROM ms_grc_regulatory_body_own d,\n    si_users_t usr, si_ent e\n   WHERE usr.user_name \u003d d.owners\n    and  usr.enterprise_id \u003d e.ENTERPRISE_ID\n   AND a.OBJECT_ID     \u003d D.OBJECT_ID\n    ) AS OWNERS,\n    valid_until,\n    valid_from,\n    description,\n    dd_object_type,\n    dd_object_type AS OBJ_TYPE,\n    dd_process_instance_id,\n    dd_instance_id,\n    NVL (dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    --owner_organizations AS owner_org_ids,\n    restrict_access_to,\n    obj_status,\n    ms_grc_utilities.get_edit_url (dd_object_type, dd_process_instance_id, dd_instance_id, NVL(dd_edit_flag, \u0027Y\u0027), a.OBJECT_NAME) AS object_name_url,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM ms_grc_regulatory_body_ORG E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    )AS owner_org_ids,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     Ms_Grc_Model_Reference base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_REGULATORY_BODY\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_MODEL_REFERENCE\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS Rel_Framework_Ref,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_reference base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_REGULATORY_BODY\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS related_references,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_area_of_complia base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_REGULATORY_BODY\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS Related_Standards,\n     (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_area_of_complia base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_REGULATORY_BODY\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS related_area_of_compliance,\n    --ms_grc_utilities.get_Model_Reference_type(object_id,dd_object_type,\u0027MS_GRC_MODEL_REFERENCE\u0027 ) AS Rel_Frame_Reference_Type,\n    ms_Grc_utilities.INCLUDE_eXPIRED(A.VALID_FROM,A.VALID_UNTIL) AS EXPIRED_FLAG,\n       \u0027Y\u0027 AS OVER_RIDE_ACTION,\n       \u0027proc\u003d\u0027|| TO_CHAR(dd_process_instance_id)||\u0027|instid\u003d\u0027||TO_CHAR(dd_instance_id)||\u0027|edit_flag\u003d\u0027||NVL(dd_edit_flag,\u0027Y\u0027)  AS EDIT_ACTION_PARAMETERS\n    FROM ms_grc_regulatory_body a\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REQ_MLS_SUMM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REQ_MLS_SUMM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_REQ_MLS_SUMM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 21 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_ASSET_CLASS_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ASSET_CLASS_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_ASSET_CLASS_RV\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"OWNERS\", \"STATUS\", \"DESCRIPTION\", \"VALID_FROM\", \"VALID_UNTIL\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"ASSET_CLASS_TYPE\", \"CPE_OR_EQUIVALENT_DEFINITION\", \"RELATED_RISKS\", \"RELATED_FRAMEWORK_REFERENCES\", \"RELATED_REFERENCES\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_CONTROLS\", \"RESTRICT_ACCESS_TO\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"OWNER_ORG_IDS\", \"RELATED_ASSETS\", \"RELATED_QUEST_PROC\", \"MODEL_TYPE\", \"EXPIRED_FLAG\", \"EDIT\", \"DISPLAY_NAME\", \"OBJECT_TYPE\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\") AS \n  SELECT b.object_id,\n    b.object_name,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_core_object_own d,\n      si_users_t usr,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND usr.enterprise_id \u003d e.ENTERPRISE_ID\n    AND b.OBJECT_ID       \u003d D.OBJECT_ID\n    )            AS OWNERS,\n    b.obj_status AS STATUS,\n    b.description,\n    b.valid_from,\n    b.valid_until,\n    b.object_level     AS OBJECT_LEVEL,\n    b.object_parent    AS OBJECT_PARENT,\n    a.asset_class_type AS asset_class_type,\n    a.cpe_or_equivalent_definition,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_risks,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_model_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_MODEL_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_framework_references,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_references,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Areas_of_Compliance,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Controls,\n    b.restrict_access_to,\n    a.dd_object_type,\n    a.dd_process_instance_id,\n    a.dd_instance_id,\n    NVL(a.dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    (SELECT MS_CONCAT(OWNER_ORGANIZATIONS)\n    FROM MS_GRC_ASSET_CLASS_OGN E\n    WHERE E.OBJECT_ID \u003d B.OBJECT_ID\n    ) AS owner_org_ids,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.src_obj_id   \u003d b.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_ASSETS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_question_proc base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    )                                                            AS RELATED_QUEST_PROC,\n    NULL                                                         AS model_type,\n    ms_Grc_utilities.INCLUDE_eXPIRED(B.VALID_FROM,B.VALID_UNTIL) AS EXPIRED_FLAG,\n    (SELECT ms_Grc_utilities.Get_Edit_Url(b.DD_OBJECT_TYPE,a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID,NVL(a.DD_EDIT_FLAG,\u0027Y\u0027),b.object_name)\n    FROM dual\n    )                AS EDIT,\n    b.object_name    AS Display_name,\n    a.dd_object_type AS Object_Type,\n    \u0027Y\u0027              AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(a.dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(a.dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(a.dd_edit_flag,\u0027Y\u0027) AS EDIT_ACTION_PARAMETERS\n  FROM ms_grc_asset_class a,\n    ms_grc_core_object b\n  WHERE a.object_id    \u003d b.object_id\n  AND a.DD_OBJECT_TYPE \u003d \u0027MS_GRC_ASSET_CLASS\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_FINANCIAL_ACCOUNTS_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_FINANCIAL_ACCOUNTS_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_FINANCIAL_ACCOUNTS_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"STATUS\", \"DESCRIPTION\", \"OWNERS\", \"ACCOUNT_BALANCE\", \"ACCOUNT_BALANCE_AS_OF\", \"SOX_MATERIAL\", \"VALID_UNTIL\", \"VALID_FROM\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"RESTRICT_ACCESS_TO\", \"RELATED_PROCESSES\", \"RELATED_RISKS\", \"RELATED_REFERENCES\", \"RELATED_ORG_PROFILE\", \"ACCOUNT_TYPE\", \"ACCOUNT_CATEGORY\", \"EXPIRED_FLAG\", \"OWNER_ORG_IDS\", \"EDIT\", \"DISPLAY_NAME\", \"DD_OBJECT_TYPE\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\") AS \n  SELECT object_name,\n    object_id,\n    Obj_Status AS Status,\n    DESCRIPTION ,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_financial_accou_own d,\n      si_users_t usr,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND usr.enterprise_id \u003d e.ENTERPRISE_ID\n    AND a.OBJECT_ID       \u003d D.OBJECT_ID\n    ) AS OWNERS,\n    account_balance,\n    acc_balance_as_of AS account_balance_as_of,\n    INITCAP (sox_material) AS sox_material,\n    valid_until,\n    valid_from,\n    dd_process_instance_id,\n    dd_instance_id,\n    NVL (dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    --owner_organizations AS owner_org_ids,\n    restrict_access_to,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FINANCIAL_ACCOUNTS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_Processes,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FINANCIAL_ACCOUNTS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_risks,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FINANCIAL_ACCOUNTS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS related_references,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_org_profile base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_FINANCIAL_ACCOUNTS\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORG_PROFILE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    )                                                            AS related_org_profile,\n    obj_type                                                     AS Account_type,\n    obj_category                                                 AS Account_category,\n    ms_Grc_utilities.INCLUDE_eXPIRED(a.VALID_FROM,a.VALID_UNTIL) AS EXPIRED_FLAG,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM MS_GRC_FINANCIAL_ACCOU_ORG E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS owner_org_ids,\n    (SELECT ms_Grc_utilities.Get_Edit_Url(a.DD_OBJECT_TYPE,a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID,NVL (a.DD_EDIT_FLAG, \u0027Y\u0027),a.object_name)\n    FROM dual\n    )                AS EDIT,\n    a.object_name    AS Display_name,\n    a.dd_object_type AS dd_object_type,\n    \u0027Y\u0027              AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(a.dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(a.dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(a.dd_edit_flag,\u0027Y\u0027) AS EDIT_ACTION_PARAMETERS\n  FROM MS_GRC_FINANCIAL_ACCOU a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_RISK_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_RISK_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_RISK_V\" (\"ORB_REL_SOURCE_ID\", \"SYS_OBJECT_ID\", \"REF_OBJECT_ID\", \"AOC_OBJECT_ID\", \"OBJ_OBJECT_ID\", \"MRE_OBJECT_ID\", \"ORG_OBJECT_ID\", \"PRI_OBJECT_PARENT\", \"OBJECT_PARENT\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"STAKE_HOLDERS\", \"TYPE_OF_OWNER\", \"PTOA_INPUT\", \"NATURE_OF_RELATIONSHIP\", \"NATURE_OF_REL_PTOA\", \"CAUSE_CONSEQUENCE\", \"KEY_RISK\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"REQUIREMENTS\", \"CATEGORY_RISK\", \"AREAS_OF_IMPACT\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"CHECK_USER_AVAIL\", \"OBJECT_ACTION\", \"OBJ_STATUS\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"OBJ_MODIFIED_ON\", \"OBJ_CREATED_ON\", \"OBJ_STATUS_TEMP\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_MODIFIED_BY\", \"OBJ_CREATED_BY\", \"APPLIES_TO_ID\", \"AOC_ID\", \"REF_ID\", \"OTHER_ID\", \"MODEL_ID\", \"RELATIONSHIP_TYPE\", \"RTF_ATTACH\", \"D_RELATIONSHIP\", \"D_OTHER_NAME\", \"D_SYS_COMMENTS\", \"D_SYS_NAME\", \"REFERENCE_TYPE\", \"REFERENCE_NAME\", \"ATO_OWNERS\", \"ATO_ORGANIZATION\", \"ATO_COMMENTS\", \"RELATIONSHIP_DATE\", \"RELATIONSHIP_COMMENTS\", \"MODEL_TYPE\", \"MODEL_NAME\", \"AREA_OF_COMPLIANCE\", \"AOC_COMMENTS\", \"VALID_UNTIL\", \"VALID_FROM\", \"TYPE_RISK\", \"RESTRICT_ACCESS_TO\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"OBJECT_NAME\", \"OBJECT_LEVEL\", \"OBJECT_ID\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"DESCRIPTION\", \"ATTACHMENTS\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  select \"ORB_REL_SOURCE_ID\",\"SYS_OBJECT_ID\",\"REF_OBJECT_ID\",\"AOC_OBJECT_ID\",\"OBJ_OBJECT_ID\",\"MRE_OBJECT_ID\",\"ORG_OBJECT_ID\",\"PRI_OBJECT_PARENT\",\"OBJECT_PARENT\",\"TEMP_FIELD_OBJ_ID\",\"ORB_RPT_NAME\",\"ATO_ORB_TEMP\",\"ORB_KEY_RISK\",\"STAKE_HOLDERS\",\"TYPE_OF_OWNER\",\"PTOA_INPUT\",\"NATURE_OF_RELATIONSHIP\",\"NATURE_OF_REL_PTOA\",\"CAUSE_CONSEQUENCE\",\"KEY_RISK\",\"ADDITIONAL_COLUMN5\",\"ADDITIONAL_COLUMN4\",\"ORB_FORM_URL\",\"ADDITIONAL_DETAILS_WITH_ID\",\"ATTRIBUTE_COLUMN_H2\",\"ATTRIBUTE_COLUMN_H3\",\"ADDITIONAL_COLUMN_H1\",\"REQUIREMENTS\",\"CATEGORY_RISK\",\"AREAS_OF_IMPACT\",\"ADDITIONAL_COLUMN3\",\"ADDITIONAL_COLUMN2\",\"ADDITIONAL_COLUMN1\",\"CHECK_USER_AVAIL\",\"OBJECT_ACTION\",\"OBJ_STATUS\",\"ADDITIONAL_DETAILS\",\"APPLICABLE_TO\",\"ROW_NUM\",\"TGT_OBJ_TYPE\",\"SELF_REL_TYPE\",\"PARENT_REL_INST_ID\",\"REL_COMMENTS\",\"REL_VALID_UNTIL\",\"REL_VALID_FROM\",\"TGT_OBJ_ID\",\"TGT_OBJ_NAME\",\"REL_CONFIG_ID\",\"REL_INST_ID\",\"ASSOCIATED_WITH_TYPE\",\"CONFIGURED_OBJECT_TYPES\",\"REL_SOURCE_OBJECT_ID\",\"REL_SOURCE_ID\",\"OBJ_MODIFIED_ON\",\"OBJ_CREATED_ON\",\"OBJ_STATUS_TEMP\",\"OWN_APP_CONFIG_TEMP\",\"OWN_APP_CONFIG\",\"OBJ_MODIFIED_BY\",\"OBJ_CREATED_BY\",\"APPLIES_TO_ID\",\"AOC_ID\",\"REF_ID\",\"OTHER_ID\",\"MODEL_ID\",\"RELATIONSHIP_TYPE\",\"RTF_ATTACH\",\"D_RELATIONSHIP\",\"D_OTHER_NAME\",\"D_SYS_COMMENTS\",\"D_SYS_NAME\",\"REFERENCE_TYPE\",\"REFERENCE_NAME\",\"ATO_OWNERS\",\"ATO_ORGANIZATION\",\"ATO_COMMENTS\",\"RELATIONSHIP_DATE\",\"RELATIONSHIP_COMMENTS\",\"MODEL_TYPE\",\"MODEL_NAME\",\"AREA_OF_COMPLIANCE\",\"AOC_COMMENTS\",\"VALID_UNTIL\",\"VALID_FROM\",\"TYPE_RISK\",\"RESTRICT_ACCESS_TO\",\"OWNER_ORGANIZATIONS\",\"OWNERS\",\"OBJECT_NAME\",\"OBJECT_LEVEL\",\"OBJECT_ID\",\"LEVEL_2_APPROVER\",\"LEVEL_1_APPROVER\",\"DESCRIPTION\",\"ATTACHMENTS\",\"ACTION_COMMENTS\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\",\"DESCRIPTION_HTM\" from SI_101003_T \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\