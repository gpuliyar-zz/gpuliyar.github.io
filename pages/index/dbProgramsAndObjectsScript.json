  "MS_GRC_QUESTION_PROC_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_QUESTION_PROC_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_QUESTION_PROC_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"QUESTION\", \"QUESTION_HTM\", \"OBJECTIVE\", \"OBJECTIVE_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"CATEGORIES\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"MANUAL_AUTOMATIC\", \"RESPONSE_TYPE\", \"LIST_OF_RESPONSE\", \"CURRENCY\", \"TYPE_QUEST\") AS \n  SELECT \na.OBJECT_ID, a.OBJECT_NAME, regexp_replace(a.question, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS question,\na.question as question_HTM,\nregexp_replace(a.objective, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS objective,\na.objective as objective_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_Grc_question_proc_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM ms_Grc_question_proc_ORG B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \n(SELECT MS_CONCAT(CATEGORY_QUEST) FROM ms_Grc_question_proc_CAT B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS categories, \na.RESTRICT_ACCESS_TO, a.obj_STATUS,\na.VALID_FROM, a.VALID_UNTIL,   \na.RTF_aTTACH, a.DD_OBJECT_TYPE, a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID,\na.manual_automatic, response_type, list_of_response, currency, type_quest \nFROM ms_Grc_question_proc a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PRODUCT_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PRODUCT_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_PRODUCT_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"PRODUCT_TYPE\", \"PRODUCT_LOCATION\", \"SALES_CHANNEL\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"PRODUCT_CATEGORY\", \"OBJECT_LEVEL\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_grc_product_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM MS_GRC_PRODUCT_OGN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \n(SELECT MS_CONCAT(PRODUCT_TYPE) FROM ms_grc_product_typ K WHERE K.OBJECT_ID \u003d A.OBJECT_ID ) AS PRODUCT_TYPE,\n(SELECT MS_CONCAT(PRODUCT_LOCATION) FROM ms_grc_product_loc p WHERE p.OBJECT_ID \u003d A.OBJECT_ID ) AS PRODUCT_LOCATION,\n(SELECT MS_CONCAT(SALES_CHANNEL) FROM ms_grc_product_sch q WHERE q.OBJECT_ID \u003d A.OBJECT_ID ) AS SALES_CHANNEL,\nRESTRICT_ACCESS_TO, obj_STATUS,PRODUCT_CATEGORY,OBJECT_LEVEL,OBJECT_PA