  "MS_GRC_MODEL_REFERENCE_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_MODEL_REFERENCE_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_MODEL_REFERENCE_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \na.OBJECT_ID, a.OBJECT_NAME, regexp_replace(a.DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\na.DESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_Grc_model_reference_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM ms_Grc_model_reference_ORG B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \na.RESTRICT_ACCESS_TO, a.obj_STATUS,\na.VALID_FROM, a.VALID_UNTIL,   \na.RTF_aTTACH, a.DD_OBJECT_TYPE, a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID\nFROM ms_Grc_model_reference a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_OBJECTIVES_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_OBJECTIVES_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_OBJECTIVES_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"STATUS\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DD_OBJECT_TYPE\", \"OBJECTIVE_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"RESTRICT_ACCESS_TO\", \"RELATED_PROCESSES\", \"RELATED_RISKS\", \"RELATED_CONTROLS\", \"RELATED_ORG_PROFILES\", \"RELATED_REFERENCES\", \"OWNER_ORG_IDS\", \"EXPIRED_FLAG\", \"DISPLAY_NAME\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"DESCRIPTION\", \"EDIT\", \"OBJECTIVE_TYPE_NEW\", \"PARENT_OBJECTIVE_ID\", \"COSO_TYPE\", \"OBJECT_LEVEL\") AS \n  SELECT A.OBJECT_NAME,\n    a.object_id,\n    obj_status AS status,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_objectives_own d,\n      si_users_t usr,\n      si_ent e\n    WHERE usr.user_name  \u003d d.owners\n    AND a.object_id      \u003d D.OBJECT_ID\n    AND usr.enterprise_id\u003de.enterprise_id\n    ) AS OWNERS,\n    valid_until,\n    VALID_FROM,\n    DD_OBJECT_TYPE,\n    TYPE_OBJ AS objective_type,\n    dd_process_instance_id,\n    dd_instance_id,\n    NVL (dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    RESTRICT_ACCESS_TO,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Processes,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Risks,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Controls,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_org_profile base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORG_PROFILE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Org_Profiles,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_OBJECTIVES\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_References,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM MS_GRC_OBJECTIVES_ORG E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    )                                                        AS OWNER_ORG_IDS,\n    MS_GRC_UTILITIES.INCLUDE_EXPIRED(VALID_FROM,VALID_UNTIL) AS EXPIRED_FLAG,\n    SUBSTR(a.object_name,1,30)                               AS Display_name,\n    \u0027Y\u0027                                                      AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(a.dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(a.dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(a.dd_edit_flag,\u0027Y\u0027) AS EDIT_ACTION_PARAMETERS,\n    DESCRIPTION,\n    (SELECT ms_Grc_utilities.Get_Edit_Url(a.DD_OBJECT_TYPE, a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID, NVL(a.DD_EDIT_FLAG,\u0027Y\u0027), a.object_name)\n    from DUAL\n    ) as EDIT,OBJECTIVE_TYPE as OBJECTIVE_TYPE_NEW,\n     PARENT_OBJECTIVE_NAME as PARENT_OBJECTIVE_ID,\n        (select MS_CONCAT (Ms_Apps_Utilities.Get_Display_Value((Ms_Grc_Utilities.Active_Ent_Id), \u0027MS_GRC_COSO_Type\u0027, COSO_TYPE))\n    from MS_GRC_OBJECTIVES_COS F\n    where F.OBJECT_ID \u003d a.OBJECT_ID\n    ) as COSO_TYPE,\n     \u0027Level \u0027||objective_level as Object_level\n  FROM MS_GRC_OBJECTIVES A\n  where DD_OBJECT_TYPE \u003d \u0027MS_GRC_OBJECTIVES\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_EVIDENCE_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_EVIDENCE_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_EVIDENCE_RV\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"OWNERS\", \"DESCRIPTION\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RELATED_AOC\", \"RELATED_REQUIRMENTS\", \"RELATED_REFERENCES\", \"RELATED_STANDARDS\", \"APPLIES_TO_ORGANIZATIONS\", \"EDIT_ACTION_PARAMETERS\", \"RESTRICT_ACCESS_TO\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"OWNER_ORG_IDS\", \"OBJECT_NAME_URL\", \"EXPIRED_FLAG\", \"OVER_RIDE_ACTION\", \"DISPLAY_NAME\") AS \n  SELECT a.object_id,\n    a.object_name,\n          (SELECT ms_concat(usr.first_name\n    || \u0027 \u0027\n    || usr.last_name) AS owners\n  FROM ms_grc_evidence_own d,\n    si_users_t usr  , si_ent e \n  WHERE usr.user_name \u003d d.owners\n  and  usr.enterprise_id \u003d e.ENTERPRISE_ID\nAND a.OBJECT_ID     \u003d D.OBJECT_ID) as owners,\n      a.Description as description,\n      a.OBJ_STATUS,\n    a.valid_from as valid_from,\n    a.valid_until as valid_until,\n       ( \nSELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_area_of_complia base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_EVIDENCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS Related_AOC,\n     ( \nSELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    MS_GRC_REQUIREMENT base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_EVIDENCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS Related_requirments,\n    ( \nSELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    MS_GRC_REFERENCE base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_EVIDENCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS related_references,\n        ( \nSELECT ms_concat(orb.tgt_obj_id)\n  FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_area_of_complia base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_EVIDENCE\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.object_id\n  ) AS related_standards,\n ( SELECT  ms_concat(b.org_entity_name)  FROM ms_grc_orb_relations_pub_v orb ,si_org_entities_t b\n where src_obj_type \u003d\u0027MS_GRC_EVIDENCE\u0027\nand orb.tgt_obj_type\u003d \u0027MS_GRC_ORGANIZATION\u0027\nand orb.src_obj_id\u003d a.object_id\nand orb.tgt_obj_id \u003d b.org_entity_id) as Applies_To_Organizations,\n /*  ms_grc_utilities.get_relationships_New (object_id,\n                                                dd_object_type,\n                                                \u0027MS_GRC_ORGANIZATION\u0027)\n               AS Applies_To_Organizations,*/\n     \u0027proc\u003d\u0027\n    || TO_CHAR(a.dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(a.dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(a.dd_edit_flag,\u0027Y\u0027) AS EDIT_ACTION_PARAMETERS,\n    a.restrict_access_to,\n    a.dd_object_type,\n    a.dd_process_instance_id,\n    a.dd_instance_id,\n    NVL (a.dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM MS_GRC_evidence_Org E\n    WHERE E.OBJECT_ID \u003d a.OBJECT_ID\n    )                                                                                                                                      AS owner_org_ids,\n    ms_grc_utilities.get_edit_url (a.dd_object_type, a.dd_process_instance_id, a.dd_instance_id, NVL (a.DD_EDIT_FLAG, \u0027Y\u0027), a.OBJECT_NAME) AS object_name_url,\n\tms_Grc_utilities.INCLUDE_eXPIRED(VALID_FROM,VALID_UNTIL) AS EXPIRED_FLAG,\n  \u0027Y\u0027 AS OVER_RIDE_ACTION,\n    object_name as display_name\n  FROM ms_grc_evidence a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_OBJECTIVE_DENORM_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_OBJECTIVE_DENORM_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_OBJECTIVE_DENORM_RV\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"LOCALE_ID\", \"LOCALE\", \"OBJECT_NAME_URL\", \"KEY_OBJECTIVE\", \"OBJECTIVE_TYPE\", \"COSO_TYPE\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"OBJ_STATUS\", \"CREATED_ON\", \"CREATED_BY\", \"UPDATED_ON\", \"UPDATED_BY\", \"VALID_FROM\", \"VALID_UNTIL\", \"DESCRIPTION\", \"OWNER_ORG_IDS\", \"OWNER_ORG_NAMES\", \"OWNER_IDS\", \"OWNER_NAMES\", \"LEVEL_1_APPROVER\", \"LEVEL_2_APPROVER\", \"APPLIES_TO_ORGANIZATIONS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_ASSETS\", \"RELATED_ASSET_CLASSES\", \"RELATED_CONTROLS\", \"RELATED_FINANCIAL_ACCTS\", \"RELATED_FUNCTIONS\", \"RELATED_OBJECTIVES\", \"RELATED_PROCESSES\", \"RELATED_PRODUCTS\", \"RELATED_REFERENCES\", \"RELATED_REQUIREMENTS\", \"RELATED_RISKS\", \"RESTRICT_ACCESS_TO\", \"DD_EDIT_FLAG\", \"DD_OBJECT_TYPE\", \"DD_INSTANCE_ID\", \"DD_PROCESS_INSTANCE_ID\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"EXPIRED_FLAG\", \"ATTACHMENTS\") AS \n  SELECT objSumm.object_id,\n    objSumm.object_Name,\n    mlsSumm.LOCALE_ID,\n    mlsSumm.LOCALE,\n    (SELECT MS_GRC_UTILITIES.GET_EDIT_URL (objc1.DD_OBJECT_TYPE, objc1.DD_PROCESS_INSTANCE_ID, objc1.DD_INSTANCE_ID, NVL (objc1.DD_EDIT_FLAG, \u0027Y\u0027), objc1.OBJECT_NAME)\n    FROM ms_grc_objectives objc1\n    WHERE objc1.object_id \u003d objSumm.object_id\n    ) AS OBJECT_NAME_URL,\n    INITCAP(mlsSumm.KEY_OBJECTIVE),\n    mlsSumm.OBJECTIVE_TYPE,\n    mlsSumm.COSO_TYPE,\n    mlsSumm.object_level,\n    (SELECT object_name\n    FROM MS_GRC_OBJECTIVES objc\n    WHERE objc.object_id \u003d objSumm.parent\n    ) AS OBJECT_PARENT,\n    mlsSumm.Object_Status,\n    objSumm.Created_Date,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE objSumm.Created_By \u003d usr.user_name\n    ) AS Created_By,\n    objSumm.Updated_Date,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE objSumm.Updated_By \u003d usr.user_name\n    ) AS Updated_By,\n    objSumm.valid_from,\n    objSumm.valid_until,\n    objSumm.description,\n    objSumm.Obj_Own_Org_Id_Agg   AS OWNER_ORG_IDS,\n    objSumm.Obj_Own_Org_Name_Agg AS OWNER_ORG_NAMES,\n    objSumm.Obj_Owner_Id_Agg     AS OWNER_IDS,\n    objSumm.Obj_Owner_Name_Agg   AS OWNER_NAMES,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE objSumm.level_1_approver \u003d usr.user_name\n    ) AS level_1_approver,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE objSumm.level_2_approver \u003d usr.user_name\n    ) AS level_2_approver,\n    relSumm.REL_ORGANIZATION_NAMES,\n    relSumm.REL_AOC_OBJ_NAME_AGG,\n    relSumm.REL_ASSET_OBJ_NAME_AGG,\n    relSumm.REL_ASSET_CLASS_OBJ_NAME_AGG,\n    relSumm.REL_CTRL_OBJ_NAME_AGG,\n    relSumm.REL_FIN_ACT_OBJ_NAME_AGG,\n    relSumm.REL_FUNCTION_OBJ_NAME_AGG,\n    relSumm.REL_OBJECTIVE_OBJ_NAME_AGG,\n    relSumm.REL_PROCESS_OBJ_NAME_AGG,\n    relSumm.REL_PRODUCT_NAME_AGG,\n    relSumm.REL_REFRENCES_OBJ_NAME_AGG,\n    relSumm.REL_REQUIREMENT_OBJ_NAME_AGG,\n    relSumm.REL_RISK_OBJ_NAME_AGG,\n    objSumm.restrict_access_to,\n    objSumm.dd_edit_flag,\n    objSumm.dd_object_type,\n    objSumm.dd_instance_id,\n    objSumm.dd_process_instance_id,\n    objSumm.over_ride_action,\n    (SELECT \u0027proc\u003d\u0027\n      || TO_CHAR(objc1.dd_process_instance_id)\n      ||\u0027|instid\u003d\u0027\n      ||TO_CHAR(objc1.dd_instance_id)\n      ||\u0027|edit_flag\u003d\u0027\n      ||NVL(objc1.dd_edit_flag,\u0027Y\u0027)\n    FROM ms_grc_objectives objc1\n    WHERE objc1.object_id \u003d objSumm.object_id\n    ) AS EDIT_ACTION_PARAMETERS,\n    objSumm.expired_flag,\n    objSumm.ATTACHMENTS\n  FROM MS_GRC_OBJECTIVE_SUMMARY objSumm,\n    MS_GRC_OBJECTIVE_MLS_SUMMARY mlsSumm,\n    MS_GRC_OBJECTIVE_REL_SUMMARY relSumm\n  WHERE objSumm.object_id \u003d relSumm.object_id(+)\n  AND objSumm.object_id   \u003d mlsSumm.object_id(+)\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DEFN_ATI_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DEFN_ATI_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DEFN_ATI_PUB_V\" (\"OBJECT_ID\", \"ATI_OBJ_ID\", \"ATI_OWNER\", \"ATI_APPROVER\", \"CRITICAL_RISING\", \"NON_CRITICAL_RISING\", \"CRITICAL_DROPPING\", \"NON_CRITICAL_DROPPING\", \"EXPECTED_VALUE\", \"MANUAL_ENTRY_AUTOMATED\") AS \n  select \t OBJECT_ID , ati_obj_id,ati_owner, ati_approver, critical_rising_orgs as critical_rising, non_critical_rising_orgs as non_critical_rising,\ncritical_dropping_orgs as critical_dropping, non_critical_droping_orgs as non_critical_dropping, ati_expected_value as expected_value,\nati_manual_entry_automated as manual_entry_automated\nfrom MS_GRC_KPI_KRI_DEFN_ATI\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_STANDARD_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_STANDARD_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_STANDARD_RV\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"STANDARD_TYPE\", \"OWNERS\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RELATED_AOC\", \"RELATED_REQUIRMENTS\", \"RELATED_REFERENCES\", \"RELATED_REGULATORY_BODIES\", \"RELATED_FUNCTION\", \"RELATED_QUESTION_PROCEDURE\", \"RELATED_ORGANIZATIONS\", \"APPLIES_TO_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"OVER_RIDE_ACTION\", \"DISPLAY_NAME\", \"EDIT_ACTION_PARAMETERS\", \"OWNER_ORG_IDS\", \"OBJECT_NAME_URL\", \"EXPIRED_FLAG\", \"DESCRIPTION\") AS \n  SELECT a.object_id,\n       a.object_name,\n       /* (SELECT displayed_value\n         FROM z lovvalue\n         WHERE lov_name   \u003d \u0027MS_GRC_LEVEL\u0027\n         AND stored_value \u003d object_level\n         ) AS object_level,*/\n       object_level,\n       (SELECT a.object_name\n          FROM ms_grc_area_of_complia a\n         WHERE a.object_id \u003d b.object_parent\n           AND a.dd_object_type \u003d \u0027MS_GRC_STANDARD\u0027) AS OBJECT_PARENT,\n       b.standard_type,\n       (SELECT ms_concat(usr.first_name || \u0027 \u0027 || usr.last_name) AS owners\n          FROM ms_grc_standard_own d, si_users_t usr, si_ent e\n         WHERE usr.user_name \u003d d.owners\n           AND d.object_id \u003d a.object_id\n           and e.enterprise_id \u003d usr.enterprise_id) AS OWNERS,\n       a.OBJ_STATUS,\n       a.valid_from,\n       a.valid_until,\n       (SELECT ms_concat(orb.tgt_obj_id)\n          FROM ms_grc_orb_relations_pub_v orb, ms_grc_area_of_complia base\n         WHERE src_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n           AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n           AND orb.src_obj_id \u003d a.object_id\n           AND orb.tgt_obj_id \u003d base.object_id) AS Related_AOC,\n       (SELECT ms_concat(orb.tgt_obj_id)\n          FROM ms_grc_orb_relations_pub_v orb, MS_GRC_REQUIREMENT base\n         WHERE src_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n           AND orb.tgt_obj_type \u003d \u0027MS_GRC_REQUIREMENT\u0027\n           AND orb.src_obj_id \u003d a.object_id\n           AND orb.tgt_obj_id \u003d base.object_id) AS Related_requirments,\n       (SELECT ms_concat(orb.tgt_obj_id)\n          FROM ms_grc_orb_relations_pub_v orb, MS_GRC_REFERENCE base\n         WHERE src_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n           AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n           AND orb.src_obj_id \u003d a.object_id\n           AND orb.tgt_obj_id \u003d base.object_id) AS related_references,\n       (SELECT ms_concat(orb.tgt_obj_id)\n          FROM ms_grc_orb_relations_pub_v orb, MS_GRC_REGULATORY_BODY base\n         WHERE src_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n           AND orb.tgt_obj_type \u003d \u0027MS_GRC_REGULATORY_BODY\u0027\n           AND orb.src_obj_id \u003d a.object_id\n           AND orb.tgt_obj_id \u003d base.object_id) AS Related_Regulatory_Bodies,\n       (SELECT ms_concat(orb.tgt_obj_id)\n          FROM ms_grc_orb_relations_pub_v orb, ms_grc_function base\n         WHERE src_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n           AND orb.tgt_obj_type \u003d \u0027MS_GRC_FUNCTION\u0027\n           AND orb.src_obj_id \u003d a.object_id\n           AND orb.tgt_obj_id \u003d base.object_id) AS related_function,\n       (SELECT ms_concat(orb.tgt_obj_id)\n          FROM ms_grc_orb_relations_pub_v orb, ms_grc_question_proc base\n         WHERE src_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n           AND orb.tgt_obj_type \u003d \u0027MS_GRC_QUESTION_PROCEDURE\u0027\n           AND orb.src_obj_id \u003d a.object_id\n           AND orb.tgt_obj_id \u003d base.object_id) AS related_question_procedure,\n       null as   related_organizations,\n\n\n                         (\n SELECT ms_concat(orb.tgt_obj_id )\n  FROM ms_grc_orb_relations_pub_v orb,\n    si_org_entities_t base\n  WHERE src_obj_type   \u003d \u0027MS_GRC_STANDARD\u0027\n  AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n  AND orb.src_obj_id   \u003d a.object_id\n  AND orb.tgt_obj_id   \u003d base.org_entity_id\n  ) AS APPLIES_TO_ORGANIZATIONS,\n        a.restrict_access_to,\n       a.dd_object_type,\n       a.dd_process_instance_id,\n       a.dd_instance_id,\n       NVL(b.dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n       \u0027Y\u0027 AS OVER_RIDE_ACTION,\n       SUBSTR (object_name, 1, 30) AS display_name ,\n      (\u0027proc\u003d\u0027|| TO_CHAR(a.dd_process_instance_id)||\u0027|instid\u003d\u0027||TO_CHAR(a.dd_instance_id)||\u0027|edit_flag\u003d\u0027||NVL(b.DD_EDIT_FLAG,\u0027Y\u0027))  AS EDIT_ACTION_PARAMETERS,\n       (SELECT MS_CONCAT(OWNER_ORGANIZATIONS)\n          FROM MS_GRC_STANDARD_org E\n         WHERE E.OBJECT_ID \u003d a.OBJECT_ID\n           AND e.object_id \u003d b.object_id) AS owner_org_ids,\n       ms_grc_utilities.get_edit_url(a.dd_object_type,\n                                     a.dd_process_instance_id,\n                                     a.dd_instance_id,\n                                     NVL(b.DD_EDIT_FLAG, \u0027Y\u0027),\n                                     a.OBJECT_NAME) AS object_name_url,\n\n       CASE\n      WHEN VALID_UNTIL IS NULL\n      THEN \u00271\u0027\n      WHEN TRUNC (VALID_UNTIL) \u003e\u003d TRUNC (SYSDATE)\n      THEN \u00271\u0027\n      ELSE \u00272\u0027\n    END\n        AS EXPIRED_FLAG,\n       DESCRIPTION\n\n  FROM ms_grc_area_of_complia a, ms_grc_standard b\n WHERE a.object_id \u003d b.object_id\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_PROCESS_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_PROCESS_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_PROCESS_V\" (\"ORB_REL_SOURCE_ID\", \"MRE_OBJECT_ID\", \"RIS_OBJECT_ID\", \"SYS_OBJECT_ID\", \"CON_OBJECT_ID\", \"OBJ_OBJECT_ID\", \"REF_OBJECT_ID\", \"AOC_OBJECT_ID\", \"ORG_OBJECT_ID\", \"ORB_KEY_RISK\", \"KEY_PROCESS\", \"STAKE_HOLDERS\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"CAUSE_CONSEQUENCE\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"NATURE_OF_REL_PTOA\", \"TYPE_OF_OWNER\", \"NATURE_OF_RELATIONSHIP\", \"PARAMETER_TYPE\", \"PARAMETER_NAME\", \"CONFIGURE_BCM_CIA_VAL\", \"RECOVERY_TIME_ACTUAL\", \"MAXIMUM_TOLERABLE_DOWNTIME\", \"WORK_RECOVERY_TIME\", \"RECOVERY_POINT_OBJECTIVE\", \"RECOVERY_TIME_OBJECTIVE\", \"DURATION_RTA\", \"DURATION_MTD\", \"DURATION_WRT\", \"DURATION_RPO\", \"DURATION_RTO\", \"AVAILABILITY_PROCESS\", \"INTEGRITY\", \"CONFIDENTIALITY\", \"BUSINESS_CRITICALITY\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"OBJECT_LEVEL\", \"APPLICABLE_TO\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"ADDITIONAL_DETAILS\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"CHECK_USER_AVAIL\", \"OBJ_STATUS_TEMP\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_STATUS\", \"OBJ_MODIFIED_BY\", \"OBJ_MODIFIED_ON\", \"OBJ_CREATED_BY\", \"OBJ_CREATED_ON\", \"ORG_ID\", \"AOC_ID\", \"CONTROL_ID\", \"REF_ID\", \"OTHER_ID\", \"RISK_ID\", \"MODEL_ID\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"MODEL_NAME\", \"MODEL_TYPE\", \"RISK\", \"RISK_COMMENTS\", \"SYS_COMMENTS\", \"SYS_NAME\", \"CONTROL\", \"CONTROL_COMMENTS\", \"OTH_NAME\", \"RELATIONSHIP_COMMENTS\", \"RELATIONSHIP_DATE\", \"RELATIONSHIP\", \"REFERENCE_TYPE\", \"REFERENCE_NAME\", \"REQUIREMENT\", \"AOC_COMMENTS\", \"AREA_OF_COMPLIANCE\", \"ATO_OWNER\", \"ATO_COMMENTS\", \"ATO_ORGANIZATION\", \"CORE_CATEGORY\", \"LEGACY_ID\", \"FUNCTION_NAME\", \"LEVEL_1_APPROVER\", \"LEVEL_2_APPROVER\", \"RESTRICT_ACCESS_TO\", \"OBJECT_ACTION\", \"RTF_ATTACH\", \"OBJECT_ID\", \"OBJECT_NAME\", \"OBJECT_PARENT\", \"DESCRIPTION\", \"VALID_FROM\", \"VALID_UNTIL\", \"REFERENCE_ID\", \"CORE_TYPE\", \"ATTACHMENTS\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  select \"ORB_REL_SOURCE_ID\",\"MRE_OBJECT_ID\",\"RIS_OBJECT_ID\",\"SYS_OBJECT_ID\",\"CON_OBJECT_ID\",\"OBJ_OBJECT_ID\",\"REF_OBJECT_ID\",\"AOC_OBJECT_ID\",\"ORG_OBJECT_ID\",\"ORB_KEY_RISK\",\"KEY_PROCESS\",\"STAKE_HOLDERS\",\"PTOA_INPUT\",\"ATO_ORB_TEMP\",\"CAUSE_CONSEQUENCE\",\"TEMP_FIELD_OBJ_ID\",\"ORB_RPT_NAME\",\"NATURE_OF_REL_PTOA\",\"TYPE_OF_OWNER\",\"NATURE_OF_RELATIONSHIP\",\"PARAMETER_TYPE\",\"PARAMETER_NAME\",\"CONFIGURE_BCM_CIA_VAL\",\"RECOVERY_TIME_ACTUAL\",\"MAXIMUM_TOLERABLE_DOWNTIME\",\"WORK_RECOVERY_TIME\",\"RECOVERY_POINT_OBJECTIVE\",\"RECOVERY_TIME_OBJECTIVE\",\"DURATION_RTA\",\"DURATION_MTD\",\"DURATION_WRT\",\"DURATION_RPO\",\"DURATION_RTO\",\"AVAILABILITY_PROCESS\",\"INTEGRITY\",\"CONFIDENTIALITY\",\"BUSINESS_CRITICALITY\",\"ADDITIONAL_COLUMN5\",\"ADDITIONAL_COLUMN4\",\"ORB_FORM_URL\",\"ADDITIONAL_DETAILS_WITH_ID\",\"ATTRIBUTE_COLUMN_H2\",\"ATTRIBUTE_COLUMN_H3\",\"ADDITIONAL_COLUMN_H1\",\"ADDITIONAL_COLUMN3\",\"ADDITIONAL_COLUMN2\",\"ADDITIONAL_COLUMN1\",\"OBJECT_LEVEL\",\"APPLICABLE_TO\",\"ASSOCIATED_WITH_TYPE\",\"CONFIGURED_OBJECT_TYPES\",\"REL_SOURCE_OBJECT_ID\",\"REL_SOURCE_ID\",\"ADDITIONAL_DETAILS\",\"ROW_NUM\",\"TGT_OBJ_TYPE\",\"SELF_REL_TYPE\",\"PARENT_REL_INST_ID\",\"REL_COMMENTS\",\"REL_VALID_UNTIL\",\"REL_VALID_FROM\",\"TGT_OBJ_ID\",\"TGT_OBJ_NAME\",\"REL_CONFIG_ID\",\"REL_INST_ID\",\"CHECK_USER_AVAIL\",\"OBJ_STATUS_TEMP\",\"OWN_APP_CONFIG_TEMP\",\"OWN_APP_CONFIG\",\"OBJ_STATUS\",\"OBJ_MODIFIED_BY\",\"OBJ_MODIFIED_ON\",\"OBJ_CREATED_BY\",\"OBJ_CREATED_ON\",\"ORG_ID\",\"AOC_ID\",\"CONTROL_ID\",\"REF_ID\",\"OTHER_ID\",\"RISK_ID\",\"MODEL_ID\",\"OWNERS\",\"OWNER_ORGANIZATIONS\",\"MODEL_NAME\",\"MODEL_TYPE\",\"RISK\",\"RISK_COMMENTS\",\"SYS_COMMENTS\",\"SYS_NAME\",\"CONTROL\",\"CONTROL_COMMENTS\",\"OTH_NAME\",\"RELATIONSHIP_COMMENTS\",\"RELATIONSHIP_DATE\",\"RELATIONSHIP\",\"REFERENCE_TYPE\",\"REFERENCE_NAME\",\"REQUIREMENT\",\"AOC_COMMENTS\",\"AREA_OF_COMPLIANCE\",\"ATO_OWNER\",\"ATO_COMMENTS\",\"ATO_ORGANIZATION\",\"CORE_CATEGORY\",\"LEGACY_ID\",\"FUNCTION_NAME\",\"LEVEL_1_APPROVER\",\"LEVEL_2_APPROVER\",\"RESTRICT_ACCESS_TO\",\"OBJECT_ACTION\",\"RTF_ATTACH\",\"OBJECT_ID\",\"OBJECT_NAME\",\"OBJECT_PARENT\",\"DESCRIPTION\",\"VALID_FROM\",\"VALID_UNTIL\",\"REFERENCE_ID\",\"CORE_TYPE\",\"ATTACHMENTS\",\"ACTION_COMMENTS\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\",\"DESCRIPTION_HTM\" from SI_100997_T \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_CONTROL_DENORM_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_CONTROL_DENORM_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_CONTROL_DENORM_RV\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"LOCALE_ID\", \"LOCALE\", \"OBJECT_NAME_URL\", \"KEY_CONTROL\", \"CONTROL_PURPOSE\", \"CONTROL_TYPE\", \"CONTROL_NATURE\", \"CONTROL_SOURCE\", \"CONTROL_PRIORITY\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"OBJ_STATUS\", \"CONTROL_FREQUENCY\", \"CREATED_ON\", \"CREATED_BY\", \"UPDATED_ON\", \"UPDATED_BY\", \"VALID_FROM\", \"VALID_UNTIL\", \"DESCRIPTION\", \"OWNER_ORG_IDS\", \"OWNER_ORG_NAMES\", \"OWNER_IDS\", \"OWNER_NAMES\", \"LEVEL_1_APPROVER\", \"LEVEL_2_APPROVER\", \"APPLIES_TO_ORGANIZATIONS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_ASSERTIONS\", \"RELATED_ASSETS\", \"RELATED_ASSET_CLASSES\", \"RELATED_CONTROLS\", \"RELATED_EVIDENCES\", \"RELATED_FRAMEWORK_REFERENCES\", \"RELATED_FUNCTIONS\", \"RELATED_OBJECTIVES\", \"RELATED_PROCESSES\", \"RELATED_PRODUCTS\", \"REALTED_QUES_PROCS\", \"RELATED_REFERENCES\", \"RELATED_REQUIREMENTS\", \"RELATED_RISKS\", \"RESTRICT_ACCESS_TO\", \"DD_EDIT_FLAG\", \"DD_OBJECT_TYPE\", \"DD_INSTANCE_ID\", \"DD_PROCESS_INSTANCE_ID\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"EXPIRED_FLAG\", \"SAMPLE_SIZE\") AS \n  SELECT contSumm.object_id,\n    contSumm.object_Name,\n    mlsSumm.LOCALE_ID,\n    mlsSumm.LOCALE,\n    (SELECT MS_GRC_UTILITIES.GET_EDIT_URL (comp.DD_OBJECT_TYPE, comp.DD_PROCESS_INSTANCE_ID, comp.DD_INSTANCE_ID, NVL (ctrl1.DD_EDIT_FLAG, \u0027Y\u0027), comp.OBJECT_NAME)\n    FROM MS_GRC_COMPLIANCE_OBJE comp,\n         MS_GRC_CONTROL ctrl1\n    WHERE comp.object_id    \u003d contSumm.object_id\n    AND comp.object_id      \u003d ctrl1.object_id\n    AND comp.dd_object_type \u003d \u0027MS_GRC_CONTROL\u0027\n    ) AS OBJECT_NAME_URL,\n    mlsSumm.Key_Control,\n    mlsSumm.control_purpose,\n    mlsSumm.control_type,\n    mlsSumm.control_nature,\n    mlsSumm.control_source,\n    mlsSumm.control_priority,\n    mlsSumm.object_level,\n    (SELECT object_name\n    FROM MS_GRC_COMPLIANCE_OBJE comp\n    WHERE comp.object_id    \u003d contSumm.parent\n    AND comp.dd_object_type \u003d \u0027MS_GRC_CONTROL\u0027\n    ) AS OBJECT_PARENT,\n    mlsSumm.Object_Status,\n    mlsSumm.control_frequency,\n    contSumm.Created_Date,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE contSumm.Created_By \u003d usr.user_name\n    ) AS created_by,\n    contSumm.Updated_Date,\n    (SELECT usr.first_name\n      || \u0027 \u0027\n      || usr.last_name\n    FROM si_users_t usr\n    WHERE contSumm.Updated_By \u003d usr.user_name\n    ) AS Updated_By,\n    contSumm.valid_from,\n    contSumm.valid_until,\n    contSumm.description,\n    contSumm.Obj_Own_Org_Id_Agg   AS OWNER_ORG_IDS,\n    contSumm.Obj_Own_Org_Name_Agg AS OWNER_ORG_NAMES,\n    contSumm.Obj_Owner_Id_Agg     AS OWNER_IDS,\n    contSumm.Obj_Owner_Name_Agg   AS OWNER_NAMES,\n    contSumm.level_1_approver,\n    contSumm.level_2_approver,\n    relSumm.REL_ORGANIZATION_NAMES,\n    relSumm.REL_AOC_OBJ_NAME_AGG,\n    mlsSumm.assertions,\n    relSumm.REL_ASSET_OBJ_NAME_AGG,\n    relSumm.REL_ASSET_CLASS_OBJ_NAME_AGG,\n    relSumm.REL_CTRL_OBJ_NAME_AGG,\n    relSumm.REL_EVIDENCE_OBJ_NAME_AGG,\n    relSumm.REL_MOD_REF_OBJ_NAME_AGG,\n    relSumm.REL_FUNCTION_OBJ_NAME_AGG,\n    relSumm.REL_OBJECTIVE_OBJ_NAME_AGG,\n    relSumm.REL_PROCESS_OBJ_NAME_AGG,\n    relSumm.REL_PRODUCT_NAME_AGG,\n    relSumm.REL_QUE_PROC_OBJ_NAME_AGG,\n    relSumm.REL_REFRENCES_OBJ_NAME_AGG,\n    relSumm.REL_REQUIREMENT_OBJ_NAME_AGG,\n    relSumm.REL_RISK_OBJ_NAME_AGG,\n    contSumm.restrict_access_to,\n    contSumm.dd_edit_flag,\n    contSumm.dd_object_type,\n    contSumm.dd_instance_id,\n    contSumm.dd_process_instance_id,\n    contSumm.over_ride_action,\n    (SELECT \u0027proc\u003d\u0027\n      || TO_CHAR(comp.dd_process_instance_id)\n      ||\u0027|instid\u003d\u0027\n      ||TO_CHAR(comp.dd_instance_id)\n      ||\u0027|edit_flag\u003d\u0027\n      ||NVL(ctrl1.dd_edit_flag,\u0027Y\u0027)\n    FROM MS_GRC_COMPLIANCE_OBJE comp,\n         MS_GRC_CONTROL ctrl1\n    WHERE comp.object_id    \u003d contSumm.object_id\n    AND comp.object_id      \u003d ctrl1.object_id\n    AND comp.dd_object_type \u003d \u0027MS_GRC_CONTROL\u0027\n    ) AS EDIT_ACTION_PARAMETERS,\n    contSumm.expired_flag,\n    contSumm.sample_size\n  FROM MS_GRC_CONTROL_SUMMARY contSumm,\n    MS_GRC_CONTROL_MLS_SUMMARY mlsSumm,\n    MS_GRC_CONTROL_REL_SUMMARY relSumm\n  WHERE contSumm.object_id \u003d relSumm.object_id(+)\n  and CONTSUMM.OBJECT_ID   \u003d MLSSUMM.OBJECT_ID(+)\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REGULATORY_BODY_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REGULATORY_BODY_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_REGULATORY_BODY_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_Grc_regulatory_body_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM ms_Grc_regulatory_body_Org B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \nRESTRICT_ACCESS_TO, obj_STATUS,\nVALID_FROM, VALID_UNTIL,   \nRTF_aTTACH, DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID\nFROM ms_Grc_regulatory_body a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REFERENCE_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REFERENCE_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_REFERENCE_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"REFERENCE_TYPE\", \"DESCRIPTION\", \"VALID_FROM\", \"VALID_UNTIL\", \"REFERENCE_DOCUMENT\", \"REFERENCE_URL\", \"OWNERS\", \"REFERENCE_URL_TYPE\", \"EDIT\", \"DOC_URL\", \"DOWNLOAD\", \"FILE_URL\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"RESTRICT_ACCESS_TO\", \"STATUS\", \"RELATED_PROCESSES\", \"RELATED_RISKS\", \"RELATED_CONTROLS\", \"RELATED_FUNCTIONS\", \"REFERENCE_CATEGORY\", \"OWNER_ORG_IDS\", \"RELATED_ASSETS\", \"RELATED_ASSET_CLASSES\", \"EXPIRED_FLAG\", \"DISPLAY_NAME\", \"DD_OBJECT_TYPE\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\") AS \n  SELECT a.object_name,\n    a.object_id,\n    reference_type,\n    DESCRIPTION,\n    valid_from,\n    VALID_UNTIL,\n    reference_document,\n    reference_url,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_reference_Own d,\n      si_users_t usr ,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND USR.ENTERPRISE_ID \u003d E.ENTERPRISE_ID\n    AND A.OBJECT_ID       \u003d D.OBJECT_ID\n    ) AS OWNERS,\n    reference_URL_type,\n    (SELECT ms_Grc_utilities.Get_Edit_Url(DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID, NVL(DD_EDIT_FLAG,\u0027Y\u0027),object_name)\n    FROM dual\n    ) AS EDIT,\n    \u0027\u003ca href\u003d\"http\u0027\n    ||chr(58)\n    ||\u0027//\u0027\n    ||REFERENCE_URL\n    ||\u0027\"target\u003d\"_blank\"\u003e\u0027\n    ||REFERENCE_URL\n    ||\u0027\u003c/a\u003e\u0027 AS doc_url,\n    \u0027\"\u0027\n    ||REFERENCE_DOCUMENT\n    ||\u0027\",8\u0027                                                      AS Download,\n    SUBSTR(REFERENCE_DOCUMENT,0,instr(REFERENCE_DOCUMENT,\u0027#\u0027)-1) AS File_URL,\n    dd_process_instance_id,\n    dd_instance_id,\n    NVL (dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    restrict_access_to,\n    a.obj_status AS Status,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Processes,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_risks,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Controls,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_function base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS Related_Functions,\n    REFERENCE_CATEGORY,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM MS_GRC_REFERENCE_ORG E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS owner_org_ids,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_ASSETS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ASSET_CLASS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    )                                                        AS RELATED_ASSET_CLASSES,\n    ms_Grc_utilities.INCLUDE_eXPIRED(VALID_FROM,VALID_UNTIL) AS EXPIRED_FLAG,\n    SUBSTR(object_name,1,30) display_name,\n    dd_object_type AS DD_OBJECT_TYPE,\n    \u0027Y\u0027            AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(dd_edit_flag,\u0027Y\u0027) AS EDIT_ACTION_PARAMETERS\n  FROM ms_grc_reference a\n  WHERE dd_object_type \u003d \u0027MS_GRC_REFERENCE\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DATA_ENTRY_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DATA_ENTRY_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DATA_ENTRY_V\" (\"ACCESS_RIGHTS\", \"ADDITIONAL_COL1\", \"DEFINITION_OWNERS\", \"ATI_ASSESSED_ORG_IDS\", \"ATI_ASSESSED_ORGS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"APPROVE_ALL_MANUAL_KPI_KRI\", \"FREQUENCY\", \"EVENT_ID\", \"NEXT_SCHEDULED_DATE\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_STATUS_TEMP\", \"FINAL_APPROVER\", \"PROVIDED_ON\", \"PROVIDED_BY\", \"OBJ_STATUS\", \"RISK_CATEGORY\", \"ASSESSED_ON\", \"CURRENT_PERDIOD_AS_TEXT\", \"RTF_ATTACH\", \"OBJECT_ACTION\", \"OBJECT_ID\", \"ATI_OBJECT_ID\", \"ACTION_COMMENTS\", \"ATI_COMMENTS\", \"ATTACH_FILES\", \"CRITICAL_DROPPING_THRESH\", \"CRITICAL_RISING_THRESH\", \"CURRENT_DATE_VALUE\", \"CURRENT_VALUE\", \"EXPECTED_VALUE\", \"NON_CRITICAL_DROPPING_THRESH\", \"NON_CRITICAL_RISING_THRESH\", \"PREVIOUS_PERIOD\", \"PREVIOUS_VALUE\", \"REASSIGN_DATA_OWNER\", \"DESCRIPTION\", \"INDICATOR_MEASUREMENT\", \"LEADING_TRAILING_INDICATOR\", \"MANUAL_ENTRY_AUTOMATED\", \"OBJECT_NAME\", \"RISING_DROPPING_INDICATOR\", \"ASSIGNED_ON\", \"DATA_ENTRY_APPROVER\", \"DATA_ENTRY_OWNER\", \"APPLIES_TO\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURREN