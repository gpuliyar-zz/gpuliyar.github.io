  "MS_GRC_EMAILS_AND_ASSI_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_EMAILS_AND_ASSI_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_EMAILS_AND_ASSI_V\" (\"ASSESSMENT_LINK\", \"KPI_KRI\", \"ADDITIONAL_COMMENTS\", \"CRITICA_RISING_DROPING\", \"LAST_COMMENT\", \"ASSESSMENT_TYPE\", \"INDICATOR_MEASUREMENT\", \"OBJECT_STATUS\", \"ASSESSMENT\", \"CC_RECIPIENTS\", \"DUE_BY\", \"PREVIOUS_VALUE\", \"CURRENT_VALUE\", \"OWNERS\", \"FREQUENCY\", \"OWNER_ORGANIZATIONS\", \"RECIPIENTS\", \"MAIL_TYPE\", \"L2_APPROVER\", \"L1_APPROVER\", \"VALID_TO\", \"VALID_FROM\", \"OBJECT_NAME\", \"OBJECT_ID\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\") AS \n  select \"ASSESSMENT_LINK\",\"KPI_KRI\",\"ADDITIONAL_COMMENTS\",\"CRITICA_RISING_DROPING\",\"LAST_COMMENT\",\"ASSESSMENT_TYPE\",\"INDICATOR_MEASUREMENT\",\"OBJECT_STATUS\",\"ASSESSMENT\",\"CC_RECIPIENTS\",\"DUE_BY\",\"PREVIOUS_VALUE\",\"CURRENT_VALUE\",\"OWNERS\",\"FREQUENCY\",\"OWNER_ORGANIZATIONS\",\"RECIPIENTS\",\"MAIL_TYPE\",\"L2_APPROVER\",\"L1_APPROVER\",\"VALID_TO\",\"VALID_FROM\",\"OBJECT_NAME\",\"OBJECT_ID\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\" from SI_100984_T \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REQ_SUMM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REQ_SUMM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_REQ_SUMM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 21 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REQUIREMENT_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REQUIREMENT_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_REQUIREMENT_RV\" (\"OBJECT_NAME\", \"OBJECT_ID\", \"OWNERS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DD_OBJECT_TYPE\", \"DESCRIPTION\", \"REQUIREMENT_TYPE\", \"CLASSIFICATION\", \"IMPACT\", \"PENALTIES\", \"KEY_REQUIREMENT\", \"SECTION_NO\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"RESTRICT_ACCESS_TO\", \"STATUS\", \"OBJECT_PARENT\", \"OBJECT_LEVEL\", \"FREQUENCY\", \"OBJECT_NAME_URL\", \"OWNER_ORG_IDS\", \"APPLIES_TO_ORGANIZATIONS\", \"RELATED_STANDARDS\", \"RELATED_FUNCTIONS\", \"RELATED_PROCESSES\", \"RELATED_CONTROLS\", \"RELATED_FRAMEWORK_REFERENCES\", \"RELATED_RISKS\", \"RELATED_AREAS_OF_COMPLIANCE\", \"RELATED_REFERENCES\", \"REL_FRAME_REFERENCE_TYPE\", \"DISPLAY_NAME\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"EXPIRED_FLAG\") AS \n  SELECT a.object_name,\n    a.object_id,\n    (SELECT ms_concat(usr.first_name\n      || \u0027 \u0027\n      || usr.last_name) AS owners\n    FROM ms_grc_requirement_own d,\n      si_users_t usr ,\n      si_ent e\n    WHERE usr.user_name   \u003d d.owners\n    AND USR.ENTERPRISE_ID \u003d E.ENTERPRISE_ID\n    AND a.OBJECT_ID       \u003d D.OBJECT_ID\n    ) AS OWNERS,\n    valid_until,\n    valid_from,\n    DD_OBJECT_TYPE,\n    DESCRIPTION,\n    requirement_type           AS REQUIREMENT_TYPE,\n    requirement_classification AS CLASSIFICATION,\n    requirement_impact         AS impact,\n    requirement_penalties penalties,\n     NVL(key_requirement, \u0027no\u0027) as key_requirement,\n    section_no,\n    dd_process_instance_id,\n    dd_instance_id,\n    NVL (dd_edit_flag, \u0027Y\u0027) AS DD_EDIT_FLAG,\n    restrict_access_to,\n    OBJ_STATUS      AS status,\n    a.object_parent AS OBJECT_PARENT,\n    OBJECT_LEVEL,\n    FREQUENCY,\n    ms_grc_utilities.get_edit_url (dd_object_type, dd_process_instance_id, dd_instance_id, NVL(DD_EDIT_FLAG,\u0027Y\u0027), a.OBJECT_NAME) AS object_name_url,\n    (SELECT MS_CONCAT (OWNER_ORGANIZATIONS)\n    FROM ms_grc_requirement_ORG E\n    WHERE E.OBJECT_ID \u003d A.OBJECT_ID\n    ) AS owner_org_ids,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      SI_ORG_ENTITIES_T base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.org_entity_id\n    ) AS Applies_to_Organizations,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_STANDARD\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS RELATED_STANDARDS ,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_function base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_FUNCTION\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS RELATED_FUNCTIONS ,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_core_object base\n    WHERE SRC_OBJ_TYPE   \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS Related_Processes ,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_compliance_obje base\n    WHERE SRC_OBJ_TYPE   \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS Related_Controls ,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n      ms_grc_model_reference base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_MODEL_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS RELATED_FRAMEWORK_REFERENCES,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_risk base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS RELATED_RISKS,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_area_of_complia base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    ) AS RELATED_AREAS_OF_COMPLIANCE,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM MS_GRC_ORB_RELATIONS_PUB_V ORB,\n      ms_grc_reference base\n    WHERE SRC_OBJ_TYPE   \u003d \u0027MS_GRC_REQUIREMENT\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_REFERENCE\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND ORB.TGT_OBJ_ID   \u003d BASE.OBJECT_ID\n    )                                                                                             AS RELATED_REFERENCES,\n    MS_GRC_UTILITIES.GET_MODEL_REFERENCE_TYPE(OBJECT_ID,DD_OBJECT_TYPE,\u0027MS_GRC_MODEL_REFERENCE\u0027 ) as REL_FRAME_REFERENCE_TYPE,\n    OBJECT_NAME as DISPLAY_NAME,\n    \u0027Y\u0027 AS OVER_RIDE_ACTION,\n    \u0027proc\u003d\u0027\n    || TO_CHAR(a.dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||TO_CHAR(a.dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||NVL(A.DD_EDIT_FLAG,\u0027Y\u0027)                                AS EDIT_ACTION_PARAMETERS,\n    MS_GRC_UTILITIES.INCLUDE_EXPIRED(VALID_FROM,VALID_UNTIL) AS EXPIRED_FLAG\n  FROM MS_GRC_REQUIREMENT A\n  where DD_OBJECT_TYPE \u003d \u0027MS_GRC_REQUIREMENT\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REFERENCE_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REFERENCE_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_REFERENCE_V\" (\"CHECK_USER_AVAIL\", \"OBJ_STATUS_TEMP\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_MODIFIED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_CREATED_ON\", \"ATTACHMENTS\", \"OBJ_STATUS\", \"OBJ_CREATED_BY\", \"VALID_UNTIL\", \"VALID_FROM\", \"RTF_ATTACH\", \"RESTRICT_ACCESS_TO\", \"REFERENCE_URL_TYPE\", \"REFERENCE_URL\", \"REFERENCE_TYPE\", \"REFERENCE_DOCUMENT\", \"REFERENCE_CATEGORY\", \"OWNER_ORGANIZATIONS\", \"OWNERS\", \"OBJECT_NAME\", \"OBJECT_ACTION\", \"OBJECT_ID\", \"LEVEL_2_APPROVER\", \"LEVEL_1_APPROVER\", \"DESCRIPTION\", \"ACTION_COMMENTS\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  select \"CHECK_USER_AVAIL\",\"OBJ_STATUS_TEMP\",\"OWN_APP_CONFIG_TEMP\",\"OWN_APP_CONFIG\",\"OBJ_MODIFIED_ON\",\"OBJ_MODIFIED_BY\",\"OBJ_CREATED_ON\",\"ATTACHMENTS\",\"OBJ_STATUS\",\"OBJ_CREATED_BY\",\"VALID_UNTIL\",\"VALID_FROM\",\"RTF_ATTACH\",\"RESTRICT_ACCESS_TO\",\"REFERENCE_URL_TYPE\",\"REFERENCE_URL\",\"REFERENCE_TYPE\",\"REFERENCE_DOCUMENT\",\"REFERENCE_CATEGORY\",\"OWNER_ORGANIZATIONS\",\"OWNERS\",\"OBJECT_NAME\",\"OBJECT_ACTION\",\"OBJECT_ID\",\"LEVEL_2_APPROVER\",\"LEVEL_1_APPROVER\",\"DESCRIPTION\",\"ACTION_COMMENTS\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\",\"DESCRIPTION_HTM\" from SI_101000_T \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_KPI_KRI_DEFN_RV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_KPI_KRI_DEFN_RV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_KPI_KRI_DEFN_RV\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"OWNERS\", \"OWNER_ORG_IDS\", \"VALID_UNTIL\", \"VALID_FROM\", \"OBJ_STATUS\", \"OBJECT_URL\", \"MANUAL_ENTRY_AUTOMATED\", \"INDICATOR_MEASUREMENT\", \"ASSEMENT_TYPE\", \"FREQUENCY\", \"DESCRIPTION\", \"DD_OBJECT_TYPE\", \"DISPLAY_NAME\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_EDIT_FLAG\", \"RESTRICT_ACCESS_TO\", \"ASSEMENT_FOR\", \"OBJ_MODIFIED_ON\", \"OBJ_MODIFIED_BY\", \"KPI_KRI_FLAG\", \"RISK_CATEGORIES\", \"ATI_OWNER\", \"RELEATED_CONTROLS\", \"RELEATED_CORE_OBJECTS\", \"RELEATED_ORGANIZATIONS\", \"RELEATED_RISKS\", \"OVER_RIDE_ACTION\", \"EDIT_ACTION_PARAMETERS\", \"EXPIRED_FLAG\") AS \n  SELECT  object_id,\n        trim(object_name) object_name,\n        ( SELECT ms_concat(usr.first_name|| \u0027 \u0027|| usr.last_name) AS owners\n                FROM ms_grc_kpi_kri_defn_own d,si_users_t usr  , si_ent e\n                WHERE usr.user_name \u003d d.owners\n                 AND  usr.enterprise_id \u003d e.ENTERPRISE_ID\n                 AND a.OBJECT_ID \u003d D.OBJECT_ID) AS OWNERS,\n       (SELECT ms_concat(owner_organizations)\n               FROM ms_grc_kpi_kri_defn_ogn d\n               WHERE a.object_id \u003d d.object_id) AS owner_org_ids,\n       valid_until,\n       valid_from,\n       obj_status,\n       ms_grc_utilities.get_edit_url (dd_object_type, dd_process_instance_id, dd_instance_id, NVL (dd_edit_flag, \u0027Y\u0027), a.OBJECT_NAME) AS OBJECT_URL,\n       manual_entry_automated,/*ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Manual Automated\u0027, manual_entry_automated )*/\n       indicator_measurement,/*ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Indicator Measurement\u0027, indicator_measurement )*/\n       assement_for as assement_type ,/* ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Applies To Items\u0027, assement_for )*/\n       frequency,/*ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Frequency\u0027, frequency )*/\n       description,\n       dd_object_type,\n       SUBSTR (trim(object_name), 1, 30) display_name,\n       dd_process_instance_id,\n       dd_instance_id,\n       NVL (dd_edit_flag, \u0027Y\u0027) AS dd_edit_flag,\n       restrict_access_to,\n       (SELECT ms_concat(ms_grc_kpi_kri_utilities.get_kpi_kri_ati_name ( a.assement_for, k.ati_obj_id ))\n               FROM ms_grc_kpi_kri_defn_ati k\n               WHERE a.object_id \u003d k.object_id) AS ASSEMENT_FOR,\n       TO_CHAR(OBJ_MODIFIED_ON,\u0027MM/DD/YYYY\u0027) as OBJ_MODIFIED_ON,\n       OBJ_MODIFIED_by,\n       KPI_KRI_FLAG,/*ms_apps_utilities.get_display_value ( MS_GRC_UTILITIES.active_ent_id, \u0027MS GRC KI Type\u0027, KPI_KRI_FLAG )*/\n    (SELECT ms_concat(risk_category)--ms_concat(ms_apps_utilities.get_display_values ( ms_grc_utilities.active_ent_id, \u0027MS GRC Risk Category\u0027, risk_category, \u0027,\u0027 ))\n    FROM MS_GRC_KPI_KRI_DEFN_RCG e\n    WHERE e.object_id \u003d a.object_id\n    ) AS RISK_CATEGORIES,\n    ( SELECT ms_concat(usr.first_name|| \u0027 \u0027|| usr.last_name)\n                FROM MS_GRC_KPI_KRI_DEFN_ATI s,si_users_t usr  , si_ent e\n                WHERE usr.user_name \u003d s.ati_owner\n                 AND  usr.enterprise_id \u003d e.ENTERPRISE_ID\n                 AND a.OBJECT_ID \u003d s.OBJECT_ID) as ATI_OWNER,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_compliance_obje base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_KPI_KRI_DEFINITION\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_CONTROL\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n    AND orb.tgt_obj_id   \u003d base.object_id\n    ) AS releated_controls,\n    (SELECT ms_concat(orb.tgt_obj_id)\n    FROM ms_grc_orb_relations_pub_v orb,\n    ms_grc_core_object base\n    WHERE src_obj_type   \u003d \u0027MS_GRC_KPI_KRI_DEFINITION\u0027\n    AND orb.tgt_obj_type \u003d \u0027MS_GRC_PROCESS\u0027\n    AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) AS releated_core_objects,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     si_org_entities_t base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_KPI_KRI_DEFINITION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_ORGANIZATION\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.org_entity_id\n    ) AS releated_organizations,\n    (SELECT ms_concat(orb.tgt_obj_id)\n     FROM ms_grc_orb_relations_pub_v orb,\n     ms_grc_risk base\n     WHERE src_obj_type   \u003d \u0027MS_GRC_KPI_KRI_DEFINITION\u0027\n     AND orb.tgt_obj_type \u003d \u0027MS_GRC_RISK\u0027\n     AND orb.src_obj_id   \u003d a.object_id\n     AND orb.tgt_obj_id   \u003d base.object_id\n     ) releated_risks,\n     \u0027Y\u0027 AS OVER_RIDE_ACTION,\n     \u0027proc\u003d\u0027|| TO_CHAR(dd_process_instance_id)||\u0027|instid\u003d\u0027||TO_CHAR(dd_instance_id)||\u0027|edit_flag\u003d\u0027||NVL(dd_edit_flag,\u0027Y\u0027)  AS EDIT_ACTION_PARAMETERS,\n     ms_Grc_utilities.INCLUDE_eXPIRED(a.VALID_FROM,a.VALID_UNTIL) AS EXPIRED_FLAG\n\n  FROM ms_grc_kpi_kri_defn a\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_ORG_ACTIVITY_ROWTYPE": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ORG_ACTIVITY_ROWTYPE\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"SURVEYSP3DEV\".\"MS_GRC_ORG_ACTIVITY_ROWTYPE\" AS OBJECT (   ORG_ENTITY_ID       \tVARCHAR2(4000) ,ACTIVITY_SHORT_NAME  \tVARCHAR2(4000) ,ORG_ENTITY_NAME   varchar2(4000)  )\n\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_OBJC_REL_SUMM_SEQ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_OBJC_REL_SUMM_SEQ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"SURVEYSP3DEV\".\"MS_GRC_OBJC_REL_SUMM_SEQ\"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_GRC_REGULATORY_BODY_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REGULATORY_BODY_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_REGULATORY_BODY_V\" (\"ORB_REL_SOURCE_ID\", \"MRE_OBJECT_ID\", \"TEMP_FIELD_OBJ_ID\", \"ORB_RPT_NAME\", \"STAKE_HOLDERS\", \"TYPE_OF_OWNER\", \"PTOA_INPUT\", \"ATO_ORB_TEMP\", \"ORB_KEY_RISK\", \"NATURE_OF_RELATIONSHIP\", \"NATURE_OF_REL_PTOA\", \"CAUSE_CONSEQUENCE\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"CHECK_USER_AVAIL\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"MODEL_TYPE\", \"OBJ_STATUS_TEMP\", \"OBJECT_ACTION\", \"OWN_APP_CONFIG_TEMP\", \"OWN_APP_CONFIG\", \"OBJ_STATUS\", \"OBJ_MODIFIED_ON\", \"OBJ_MODIFIED_BY\", \"OBJ_CREATED_ON\", \"OBJ_CREATED_BY\", \"MRE_MODEL_TYPE\", \"MODEL_ID\", \"MODEL_NAME\", \"VALID_UNTIL\", \"VALID_FROM\", \"RTF_ATTACH\", \"RESTRICT