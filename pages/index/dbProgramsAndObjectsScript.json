  "MS_GRC_ASSET_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_ASSET_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_ASSET_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"BUSINESS_CRITICALITY\", \"SERIAL_NUMBER\", \"PHYSICAL_LOCATION\", \"HOST_NAME\", \"IP_ADDRESS\") AS \n  SELECT \na.OBJECT_ID, a.OBJECT_NAME, regexp_replace(a.DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\na.DESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_Grc_core_object_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM ms_Grc_core_object_Ogn B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \na.RESTRICT_ACCESS_TO, a.obj_STATUS,\na.VALID_FROM, a.VALID_UNTIL,   \na.RTF_aTTACH, a.DD_OBJECT_TYPE, a.DD_PROCESS_INSTANCE_ID, a.DD_INSTANCE_ID,\nc.business_criticality, c.serial_number, c.physical_location, c.host_name, c.ip_address\nFROM ms_Grc_asset c , ms_grc_core_object a\nWHERE c.object_id \u003d a.object_id\nand a.dd_object_type \u003d \u0027MS_GRC_ASSET\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
