  "MS_GRC_REFERENCE_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_GRC_REFERENCE_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"SURVEYSP3DEV\".\"MS_GRC_REFERENCE_PUB_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"REFERENCE_CATEGORY\", \"REFERENCE_TYPE\", \"REFERENCE_URL_TYPE\", \"REFERENCE_DOCUMENT\", \"REFERENCE_URL\") AS \n  SELECT \nOBJECT_ID, OBJECT_NAME, regexp_replace(DESCRIPTION, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS DESCRIPTION,\nDESCRIPTION as DESCRIPTION_HTM,\n(SELECT MS_CONCAT(OWNERS) FROM ms_Grc_reference_OWN B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNERS, \n(SELECT MS_CONCAT(OWNER_ORGANIZATIONS) FROM ms_Grc_reference_Org B WHERE B.OBJECT_ID \u003d A.OBJECT_ID ) AS OWNER_ORGANIZATIONS, \nRESTRICT_ACCESS_TO, obj_STATUS,\nVALID_FROM, VALID_UNTIL,   \nRTF_aTTACH, DD_OBJECT_TYPE, DD_PROCESS_INSTANCE_ID, DD_INSTANCE_ID\n, a.REFERENCE_CATEGORY, a.REFERENCE_TYPE, a.REFERENCE_URL_TYPE, a.REFERENCE_DOCUMENT, a.REFERENCE_URL\nFROM ms_Grc_reference a\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003