<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[SELECT distinct w.action_code,
  w.action_name,
  V.STATUS_CODE
FROM MS_RGA_STATUS V,
  MS_RGA_ACTIONS W,
  ms_rga_status_action_mapping x,
  SI_USERS_T Y,
  si_locales z
where y.user_name = :1
and w.locale_id   =
  (SELECT locale_id FROM si_locales WHERE locale_string=y.locale
  )
and w.action_code =x.actions
and V.STATUS_CODE =X.STATUS
and X.STATUS=:2]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML><![CDATA[<?xml version="1.0"?><METRIC_DEPLOYMENT><METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" ><RESULTMAP ></RESULTMAP></METRIC></METRIC_DEPLOYMENT>]]></XML>
<COMMENTS/>
</SQL_INFOLET>
