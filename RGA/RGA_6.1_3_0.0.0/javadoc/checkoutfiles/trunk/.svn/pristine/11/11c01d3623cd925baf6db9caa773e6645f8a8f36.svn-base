<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>SELECT   a.modified_by, a.pid, a.subscription_id, a.channel_id, a.filter_keywords,
         a.notify_by_email, a.user_id, a.status, a.created_on, a.created_by,
         a.modified_on
    FROM ms_rga_subscription_request a,si_users_t t,MS_RGA_SUBSCRPTN_REQ_V v1    where a.user_id = t.user_id
    and t.user_name=v1.CURRENT_USER_NAME
    and v1.CURRENT_USER_NAME = :1 
ORDER BY subscription_id</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
<COMMENTS/>
</SQL_INFOLET>
