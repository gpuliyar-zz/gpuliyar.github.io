<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[SELECT distinct a.CHANNEL_ID CHANNEL_ID, (case
         when length(channel_name) > 100 then
          substr(channel_name, 1, 100) || '...'
         when length(channel_name) <= 100 then
          channel_name
       END ) CHANNEL_NAME
FROM MS_RGA_CHANNEL_MASTER A WHERE exists (SELECT CHANNEL_NAME FROM MS_RGA_SUBS_CHANNEL_CHN WHERE channel_name=A.CHANNEL_ID and CHN_CREATED_BY=:1)
ORDER BY CHANNEL_ID]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML><![CDATA[<?xml version="1.0"?><METRIC_DEPLOYMENT><METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" ><RESULTMAP ></RESULTMAP></METRIC></METRIC_DEPLOYMENT>]]></XML>
<COMMENTS/>
</SQL_INFOLET>
</METRIC_TYPE>
