<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[select distinct  dbms_lob.substr(comment_history,3998,1) comment_history,
  (case  
  when LENGTH(OBJ_CHNG_HSTRY) >=3999 then dbms_lob.substr(obj_chng_hstry,3995,1)||'...' 
  when LENGTH(OBJ_CHNG_HSTRY) < 3999 then dbms_lob.substr(obj_chng_hstry,3998,1)
  end) object_change_history,dd_current_user_name,to_char(creation_date,'MON/DD/YYYY HH:MI:SS') date_time from ms_rga_alert_object_relatio_v where alert_id=:1 order by date_time desc]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
