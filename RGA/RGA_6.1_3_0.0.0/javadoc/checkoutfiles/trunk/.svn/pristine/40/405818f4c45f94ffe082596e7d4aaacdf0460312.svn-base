<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>select distinct (SELECT MS_APPS_UTILITIES.get_user_id(user_name) FROM dual) AS user_id,(SELECT MS_APPS_UTILITIES.get_user_full_name(user_name) FROM dual) AS user_full_name, user_name,org_entity_id,activity_short_name,null as Column1,null as Column2,null as Column3
FROM MS_RGA_FLOW_DOWN_ORG_V where activity_short_name='MS GRC View Manage Alerts'</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
