<PLATFORM_DETAILS>
<PLATFORM_VERSION>6.1</PLATFORM_VERSION>
<BUILD_VERSION>4.1.1.0</BUILD_VERSION>
</PLATFORM_DETAILS>
<VIEW_NAME>MS_RGA_CHANNEL_MASTER</VIEW_NAME>
<OBJECT_ID>MS_RGA_CHANNEL_MASTER</OBJECT_ID>
<VIEW_TEXT>("CHANNEL_ID","CHANNEL_NAME","CHANNEL_TYPE","INFOLET_NAME","CHANNEL_DESCRIPTION","ACTIVE","CREATED_ON","CREATED_BY","MODIFIED_ON","MODIFIED_BY","PID","SRC_PK") AS select a.object_id channel_id,
       a.channel_name,
       a.channel_type,
       '' infolet_name,
       '' channel_description,
       a.channel_status active,
       a.DD_CREATED_ON created_on,
       a.DD_CREATED_BY created_by,
       a.DD_MODIFIED_ON modified_on,
       a.DD_MODIFIED_BY modified_by,
       a.DD_PROCESS_INSTANCE_ID pid,
       b.Src_Pk as SRC_PK
  from MS_RGA_CHANNEL_DTLS a,
       MS_RGA_CHANNEL_DTLS_SRC b
  Where a.object_id=b.object_id
</VIEW_TEXT>
</VIEWS_SPEC>
