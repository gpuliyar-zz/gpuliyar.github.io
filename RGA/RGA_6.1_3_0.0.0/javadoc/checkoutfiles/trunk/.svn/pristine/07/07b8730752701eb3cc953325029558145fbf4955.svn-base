<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[SELECT STORED_VALUE, DISPLAYED_VALUE FROM MS_QS_LIST_OF_VALUES LOV,SI_USERS_T USERS,SI_LOCALES LOCALE
WHERE LOV.LOV_ID IN (SELECT LOV_ID FROM MS_QS_LOV_NAMES WHERE LOV_NAME = 'MS_RGA_Issue_Status')
AND LOV.STORED_VALUE <> '3' AND LOV.LOCALE_ID = LOCALE.LOCALE_ID AND LOCALE.LOCALE_STRING = USERS.LOCALE
AND USERS.USER_NAME = :1
ORDER BY STORED_VALUE]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML><![CDATA[<?xml version="1.0"?><METRIC_DEPLOYMENT><METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" ><RESULTMAP ></RESULTMAP></METRIC></METRIC_DEPLOYMENT>]]></XML>
<COMMENTS/>
</SQL_INFOLET>
