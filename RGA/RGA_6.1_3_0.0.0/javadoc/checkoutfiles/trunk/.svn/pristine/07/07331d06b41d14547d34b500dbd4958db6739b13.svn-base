<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>SELECT DISTINCT fdo.user_id,
                fdo.user_name,
                ms_grc_utilities.get_user_full_name(fdo.user_id) USER_FULL_NAME,
                fdo.activity_id,
                fdo.activity_short_name,
                fdo.activity_name
  FROM ms_qms_flow_down_org_v fdo
 WHERE INSTR(','||:1||',', ','||FDO.ORG_ENTITY_ID||',') &gt; 0 AND 
 Activity_Name = 'RGA - Approve Alert'</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
