package com.metricstream.systemi.rga.grcobject;

import java.util.List;

/**
 * GRCFMandatoryFields is an POJO class which holds mandatory fields 
 * required by GRCF Object.
 * @author munavar.basha
 *
 */
public class GRCFMandatoryFields {
	
	private List<String> ownerOrgName;
	private List<String> ownerName;
	private boolean isApprovalRequired;
	private String restricted;
	private String frequency;
	
	
	/**
	 * @return the ownerOrgName
	 */
	public List<String> getOwnerOrgName() {
		return this.ownerOrgName;
	}
	
	/**
	 * @param ownerOrgName the ownerOrgName to set
	 */
	public void setOwnerOrgName(List<String> ownerOrgName) {
		this.ownerOrgName = ownerOrgName;
	}
	
	/**
	 * @return the ownerName
	 */
	public List<String> getOwnerName() {
		return this.ownerName;
	}
	
	/**
	 * @param ownerName the ownerName to set
	 */
	public void setOwnerName(List<String> ownerName) {
		this.ownerName = ownerName;
	}
	
	
	/**
	 * @return the isApprovalRequired
	 */
	public boolean isApprovalRequired() {
		return this.isApprovalRequired;
	}
	
	
	/**
	 * @param isApprovalRequired the isApprovalRequired to set
	 */
	public void setApprovalRequired(boolean isApprovalRequired) {
		this.isApprovalRequired = isApprovalRequired;
	}
	
	/**
	 * @return the frequency
	 */
	public String getFrequency() {
		return this.frequency;
	}
	

	/**
	 * @param frequency the frequency to set
	 */
	public void setFrequency(String frequency) {
		this.frequency = frequency;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "GRCFMandatoryFields [ownerOrgName=" + this.ownerOrgName
				+ ", ownerName=" + this.ownerName + ", isApprovalRequired="
				+ this.isApprovalRequired + ", isRestricted="
				+ this.restricted + ", frequency=" + this.frequency + "]";
	}

	/**
	 * @return the restricted
	 */
	public String getRestricted() {
		return restricted;
	}

	/**
	 * @param restricted the restricted to set
	 */
	public void setRestricted(String restricted) {
		this.restricted = restricted;
	}

}
