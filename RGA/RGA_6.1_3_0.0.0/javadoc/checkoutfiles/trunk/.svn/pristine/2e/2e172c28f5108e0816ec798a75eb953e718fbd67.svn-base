<INFOLET_TYPE>SQL-Based</INFOLET_TYPE>
<INFOLET_NAME>MS RGA Get All Channel Name</INFOLET_NAME>
<OBJECT_ID>101171</OBJECT_ID>
<OUTPUT_TYPE>Graph and Report</OUTPUT_TYPE>
<CATEGORY>Assets</CATEGORY>
<DESCRIPTION>This infolet is used to retrieve the Channel Name</DESCRIPTION>
<START_DATE>01-JUN-2010</START_DATE>
<END_DATE/>
<HISTORY>Y</HISTORY>
<PARAMETERS>N</PARAMETERS>
<METRIC_RUN_MODE>On Request</METRIC_RUN_MODE>
<ARCHIVE_PERIOD_INTERVAL>1</ARCHIVE_PERIOD_INTERVAL>
<ARCHIVE_PERIOD_INTERVAL_TYPE>Years</ARCHIVE_PERIOD_INTERVAL_TYPE>
<APPLICATION_NAME>MetricStream</APPLICATION_NAME>
<METRIC_VALIDATED_FLAG>Y</METRIC_VALIDATED_FLAG>
<METRIC_READY_FLAG>Y</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>SELECT object_id channel_id, channel_name, NULL AS custom_field1, NULL AS custom_field2,
       NULL AS custom_field3
  FROM MS_RGA_CHANNEL_DTLS  a
 Where CHANNEL_STATUS = '1'
 and a.channel_type in ('1','3')
  order by channel_name</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
<COMMENTS/>
</SQL_INFOLET>
</METRIC_TYPE>
