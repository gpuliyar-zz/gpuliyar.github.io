<PLATFORM_DETAILS>
<PLATFORM_VERSION>6.1</PLATFORM_VERSION>
<BUILD_VERSION>1.0.0.0</BUILD_VERSION>
</PLATFORM_DETAILS>
<STORED_PROCEDURE_NAME>MS_RGA_MESSAGE_LOG_CLEAN_UP</STORED_PROCEDURE_NAME>
<OBJECT_ID>MS_RGA_MESSAGE_LOG_CLEAN_UP</OBJECT_ID>
<OBJECT_TYPE>PROCEDURE</OBJECT_TYPE>
<TEXT>procedure MS_RGA_MESSAGE_LOG_CLEAN_UP(ERROR_CODE OUT VARCHAR2,
                                                        ERROR_DESC OUT VARCHAR2) is

begin
--This procedure will clean up the log entries made in MS_APPS_MESSAGE_LOG Table which are releated to RGA Module
  delete from ms_apps_message_log
   where module_name = 'RGA'
     and trunc(creation_date) &lt; trunc(sysdate) - 30;

  DBMS_OUTPUT.put_line('SUCCESS');
exception
  when others then
  
    ERROR_CODE := SQLCODE;
    ERROR_DESC := SQLERRM;
    DBMS_OUTPUT.put_line('FAILURE');
  
end;
 </TEXT>
</PROCEDURE_SPEC>
