<INFOLET_NAME>MS_RGA_HIGH_LOW</INFOLET_NAME>
<OBJECT_ID><![CDATA[101585]]></OBJECT_ID>
<OUTPUT_TYPE>Ordinary Graph Infolet</OUTPUT_TYPE>
<CATEGORY>Assets</CATEGORY>
<DESCRIPTION>Infolet to bring the Lov Values High or Low in “Likelihood” and "OverallImpact" fields of  Manage Alerts form</DESCRIPTION>
<START_DATE>10-FEB-2015</START_DATE>
<END_DATE/>
<HISTORY>M</HISTORY>
<PARAMETERS>Y</PARAMETERS>
<METRIC_RUN_MODE>On Request</METRIC_RUN_MODE>
<ARCHIVE_PERIOD_INTERVAL>1</ARCHIVE_PERIOD_INTERVAL>
<ARCHIVE_PERIOD_INTERVAL_TYPE>Days</ARCHIVE_PERIOD_INTERVAL_TYPE>
<APPLICATION_NAME>MetricStream</APPLICATION_NAME>
<METRIC_VALIDATED_FLAG>Y</METRIC_VALIDATED_FLAG>
<METRIC_READY_FLAG>Y</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[SELECT x.displayed_value,x.stored_value,x.lov_name,x.locale_id
FROM Ms_Qs_Lov_Org_Values_v x,Si_Users_t y
WHERE x.LOV_NAME = 'MS RGA High Low' and y.user_name = :1
AND x.locale_id    =
  (select locale_id from si_locales where locale_string=y.locale
  )
ORDER BY display_order,displayed_value]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML><![CDATA[<?xml version="1.0"?><METRIC_DEPLOYMENT><METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" ><RESULTMAP ></RESULTMAP></METRIC></METRIC_DEPLOYMENT>]]></XML>
<COMMENTS/>
</SQL_INFOLET>
