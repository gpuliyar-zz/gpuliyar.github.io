<VIEW_NAME>MS_RGA_CHANNEL_DETAILS</VIEW_NAME>
<OBJECT_ID>MS_RGA_CHANNEL_DETAILS</OBJECT_ID>
<VIEW_TEXT>("CHANNEL_ID","SOURCE_ID","SOURCE_TYPE","SERVER_ADD_URL","SCR_USER_NAME","SCR_PASSWORD","EMAIL_FROM","KEYWORDS","SOURCE_DESCRIPTION","ACTIVE","CREATED_ON","CREATED_BY","MODIFIED_ON","MODIFIED_BY","SERVER_PARAMETERS","STRUCT_CONT_HAND_INFOLET","SAVE_ATTACHMENTS","QUEUE_NAME","QUEUE_URL") AS select
