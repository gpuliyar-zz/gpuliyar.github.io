<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>Select channel_id,
       channel_name,
       response_id,
       response_date,
       source_id,
       title_subject,
       subs_response_id,
       subscription_id,
       user_id,
       feed_data_id,
       attachment,
       Body,
       actions,
       custumfield1,custumfield2,custumfield3,custumfield4,custumfield5        
From   Ms_Rga_View_Alerts_v a
Where  a.User_Id = Ms_Apps_Utilities.Get_User_Id(:1)</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
