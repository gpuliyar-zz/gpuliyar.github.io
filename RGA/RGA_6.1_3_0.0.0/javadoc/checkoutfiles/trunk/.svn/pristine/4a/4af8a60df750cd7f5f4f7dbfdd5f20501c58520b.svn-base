<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[SELECT DISTINCT fdo.user_id
, fdo.user_name
, ms_apps_utilities.get_user_full_name(fdo.user_id) USER_FULL_NAME
, NULL "fdo.role_id"
, NULL "fdo.role_name"
, NULL "fdo.activity_id"
, NULL "fdo.activity_short_name"
, fdo.activity_name
FROM MS_QMS_FLOW_DOWN_ORG_V fdo
WHERE fdo.org_entity_id = :1
AND trim(Activity_Name) = 'ISM - Issue Owner']]></METRIC_EXPRESSION>
