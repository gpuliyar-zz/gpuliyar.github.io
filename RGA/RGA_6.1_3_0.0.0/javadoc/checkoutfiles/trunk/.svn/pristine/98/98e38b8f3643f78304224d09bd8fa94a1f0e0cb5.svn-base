<CATEGORY>Assets</CATEGORY>
<DESCRIPTION>MSNPI Infolet to list all the organizations along with their locations, hierarchies, org levels and parent org name</DESCRIPTION>
<START_DATE>27-SEP-2010</START_DATE>
<END_DATE/>
<HISTORY>V</HISTORY>
<PARAMETERS>N</PARAMETERS>
<METRIC_RUN_MODE>On Request</METRIC_RUN_MODE>
<ARCHIVE_PERIOD_INTERVAL>1</ARCHIVE_PERIOD_INTERVAL>
<ARCHIVE_PERIOD_INTERVAL_TYPE>Months</ARCHIVE_PERIOD_INTERVAL_TYPE>
<APPLICATION_NAME>MetricStream</APPLICATION_NAME>
<METRIC_VALIDATED_FLAG>Y</METRIC_VALIDATED_FLAG>
<METRIC_READY_FLAG>Y</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>SELECT org.org_entity_id   AS org_entity_id
, org.parent_org_entity_id    AS parent_org_entity_id
, org.org_entity_name          AS org_entity_name
, porg.org_entity_name       AS parent_org_entity_name
, org.description                   AS org_description
, loc.location_name            AS org_location
, ohl.org_level                       AS org_level
, ohl.org_hierarchy_name          AS org_hierarchy_name
FROM si_org_entities org
, si_org_locations loc
, si_org_hierarchy_levels ohl
, si_org_entities porg
WHERE org.location_id = loc.location_id (+)
AND SYSDATE BETWEEN org.start_date AND Nvl(org.end_date, SYSDATE)
AND org.org_hierarchy_id = ohl.org_hierarchy_id
AND org.parent_org_entity_id = porg.org_entity_id (+)
and ORG.ORG_ENTITY_id &lt;&gt; 1
Order By org.org_entity_name </METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
<COMMENTS/>
</SQL_INFOLET>
</METRIC_TYPE>
