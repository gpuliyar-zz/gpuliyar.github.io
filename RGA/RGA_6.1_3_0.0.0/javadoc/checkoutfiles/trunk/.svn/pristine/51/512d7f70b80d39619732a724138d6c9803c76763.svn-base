				    	    	MS_APPS_MDF_ERRORS_PKG.INSERT_LOG(LV_log_no.APP_LOG_NO,iop_error_seq,SYSDATE,'D','[ ' || GC_package_name || '.' || LV_PROGRAM_NAME ||  '] ' || TY_RGA_MANAGE_ALERTS_ACT(j).DUE_DATE, ip_pid);
				    	    	MS_APPS_MDF_ERRORS_PKG.INSERT_LOG(LV_log_no.APP_LOG_NO,iop_error_seq,SYSDATE,'D','[ ' || GC_package_name || '.' || LV_PROGRAM_NAME ||  '] ' || TY_RGA_MANAGE_ALERTS_ACT(j).SEND_REMINDER_BEFORE_DUE_DATE, ip_pid);
				    	    	MS_APPS_MDF_ERRORS_PKG.INSERT_LOG(LV_log_no.APP_LOG_NO,iop_error_seq,SYSDATE,'D','[ ' || GC_package_name || '.' || LV_PROGRAM_NAME ||  '] ' || TY_RGA_MANAGE_ALERTS_ACT(j).ACTION_ID, ip_pid);
