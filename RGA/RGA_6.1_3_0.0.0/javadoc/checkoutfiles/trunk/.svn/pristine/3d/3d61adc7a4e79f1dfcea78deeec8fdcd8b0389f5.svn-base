package com.metricstream.systemi.rga.domain;

import java.util.Date;

/**
 * Message ALert Pojo
 * Created by munavar.basha on 7/8/2014.
 */
public class Alert {

    /** The sender. */
    private String sender;
    
    /** The recipient. */
    private String recipient;
    
    /** The subject. */
    private String subject;
    
    /** The body. */
    private String body;
    
    /** The attachment. */
    private String attachment;
    
    /** The msg id. */
    private String msgId;
    
    /** The feed sent date. */
    private Date feedSentDate;
    
    /** The channel. */
    private Channel channel;
    
    /** The instance rec number. */
    private long instanceRecNumber;
    
    /** The instance id. */
    private long instanceId;
    
    /** The metric id. */
    private long metricId;
    
    /** The metric run date. */
    private Date metricRunDate;
    
    /** The metric completed date. */
    private Date metricCompletedDate;
    
    /** The created by. */
    private String createdBy;

    /**
     * Instantiates a new alert.
     *
     * @param sender the sender
     * @param recipient the recipient
     * @param subject the subject
     * @param body the body
     * @param attachment the attachment
     * @param msgId the msg id
     * @param feedSentDate the feed sent date
     * @param channel the channel
     */
    public Alert(String sender, String recipient, String subject, String body, String attachment, String msgId, Date feedSentDate, Channel channel)
    {
        this.sender = sender;
        this.recipient = recipient;
        this.subject = subject;
        this.body = body;
        this.attachment = attachment;
        this.msgId = msgId;
        this.feedSentDate = feedSentDate;
        this.channel = channel;
    }

    /**
     * Gets the sender.
     *
     * @return the sender
     */
    public String getSender()
    {
        return this.sender;
    }

    /**
     * Sets the sender.
     *
     * @param sender the new sender
     */
    public void setSender(String sender)
    {
        this.sender = sender;
    }

    /**
     * Gets the recipient.
     *
     * @return the recipient
     */
    public String getRecipient()
    {
        return this.recipient;
    }

    /**
     * Sets the recipient.
     *
     * @param recipient the new recipient
     */
    public void setRecipient(String recipient)
    {
        this.recipient = recipient;
    }

    /**
     * Gets the subject.
     *
     * @return the subject
     */
    public String getSubject()
    {
        return this.subject;
    }

    /**
     * Sets the subject.
     *
     * @param subject the new subject
     */
    public void setSubject(String subject)
    {
        this.subject = subject;
    }

    /**
     * Gets the body.
     *
     * @return the body
     */
    public String getBody()
    {
        return this.body;
    }

    /**
     * Sets the body.
     *
     * @param body the new body
     */
    public void setBody(String body)
    {
        this.body = body;
    }

    /**
     * Gets the attachment.
     *
     * @return the attachment
     */
    public String getAttachment()
    {
        return this.attachment;
    }

    /**
     * Sets the attachment.
     *
     * @param attachment the new attachment
     */
    public void setAttachment(String attachment)
    {
        this.attachment = attachment;
    }

    /**
     * Gets the msg id.
     *
     * @return the msg id
     */
    public String getMsgId()
    {
        return this.msgId;
    }

    /**
     * Sets the msg id.
     *
     * @param msgId the new msg id
     */
    public void setMsgId(String msgId)
    {
        this.msgId = msgId;
    }

    /**
     * Gets the channel.
     *
     * @return the channel
     */
    public Channel getChannel()
    {
        return this.channel;
    }

    /**
     * Sets the channel.
     *
     * @param channel the new channel
     */
    public void setChannel(Channel channel)
    {
        this.channel = channel;
    }

    /**
     * Gets the instance rec number.
     *
     * @return the instance rec number
     */
    long getInstanceRecNumber()
    {
        return this.instanceRecNumber;
    }

    /**
     * Sets the instance rec number.
     *
     * @param instanceRecNumber the new instance rec number
     */
    void setInstanceRecNumber(long instanceRecNumber)
    {
        this.instanceRecNumber = instanceRecNumber;
    }

    /**
     * Gets the instance id.
     *
     * @return the instance id
     */
    long getInstanceId()
    {
        return this.instanceId;
    }

    /**
     * Sets the instance id.
     *
     * @param instanceId the new instance id
     */
    void setInstanceId(long instanceId)
    {
        this.instanceId = instanceId;
    }

    /**
     * Gets the metric id.
     *
     * @return the metric id
     */
    long getMetricId()
    {
        return this.metricId;
    }

    /**
     * Sets the metric id.
     *
     * @param metricId the new metric id
     */
    void setMetricId(long metricId)
    {
        this.metricId = metricId;
    }

    /**
     * Gets the metric run date.
     *
     * @return the metric run date
     */
    Date getMetricRunDate()
    {
        return this.metricRunDate;
    }

    /**
     * Sets the metric run date.
     *
     * @param metricRunDate the new metric run date
     */
    void setMetricRunDate(Date metricRunDate)
    {
        this.metricRunDate = metricRunDate;
    }

    /**
     * Gets the metric completed date.
     *
     * @return the metric completed date
     */
    Date getMetricCompletedDate()
    {
        return this.metricCompletedDate;
    }

    /**
     * Sets the metric completed date.
     *
     * @param metricCompletedDate the new metric completed date
     */
    void setMetricCompletedDate(Date metricCompletedDate)
    {
        this.metricCompletedDate = metricCompletedDate;
    }

    /**
     * Gets the created by.
     *
     * @return the created by
     */
    String getCreatedBy()
    {
        return this.createdBy;
    }

    /**
     * Sets the created by.
     *
     * @param createdBy the new created by
     */
    void setCreatedBy(String createdBy)
    {
        this.createdBy = createdBy;
    }

    /**
     * Gets the feed sent date.
     *
     * @return the feed sent date
     */
    public Date getFeedSentDate()
    {
        return this.feedSentDate;
    }

    /**
     * Sets the feed sent date.
     *
     * @param feedSentDate the new feed sent date
     */
    public void setFeedSentDate(Date feedSentDate)
    {
        this.feedSentDate = feedSentDate;
    }
}
