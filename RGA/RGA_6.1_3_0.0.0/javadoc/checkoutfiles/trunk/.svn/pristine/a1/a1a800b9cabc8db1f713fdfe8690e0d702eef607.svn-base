<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[SELECT feed_data_id,BODY,subject, Attachments, actions 
FROM 
(SELECT feed_data_id,BODY,SUBJECT as subject,
 Attachment Attachments,
 NULL  AS ACTIONS
  FROM MS_RGA_AGGREGATOR
  WHERE feed_data_id=:1
)]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
