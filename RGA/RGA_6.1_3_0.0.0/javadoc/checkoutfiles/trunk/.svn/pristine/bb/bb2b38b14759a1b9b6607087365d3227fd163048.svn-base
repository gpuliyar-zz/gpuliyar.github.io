<INFOLET_FIELDS>
<COLUMN_NAME>         DECODE(A.STATUS,'O','ACTIVE','C','INACTIVE')</COLUMN_NAME>
<COLUMN_SEQUENCE>4</COLUMN_SEQUENCE>
<RESULT_COLUMN_NAME>SUBSCRIPTION_STATUS</RESULT_COLUMN_NAME>
<COLUMN_TYPE>Varchar2</COLUMN_TYPE>
<USER_COLUMN_NAME>Subscription Status</USER_COLUMN_NAME>
