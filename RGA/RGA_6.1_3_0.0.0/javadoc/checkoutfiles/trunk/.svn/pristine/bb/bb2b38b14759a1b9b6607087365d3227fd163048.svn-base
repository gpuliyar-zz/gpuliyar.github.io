<COLUMN_NAME>          DECODE(A.NOTIFY_BY_EMAIL,0,'NO',1,'YES')</COLUMN_NAME>
<COLUMN_SEQUENCE>3</COLUMN_SEQUENCE>
<RESULT_COLUMN_NAME>NOTIFY_BY_EMAIL</RESULT_COLUMN_NAME>
<COLUMN_TYPE>Varchar2</COLUMN_TYPE>
<USER_COLUMN_NAME>Notify By Email</USER_COLUMN_NAME>
<AXIS_NUM>Y2</AXIS_NUM>
