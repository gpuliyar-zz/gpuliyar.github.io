<PLATFORM_DETAILS>
<PLATFORM_VERSION>6.0</PLATFORM_VERSION>
<BUILD_VERSION>4.11.0</BUILD_VERSION>
</PLATFORM_DETAILS>
<INFOLET_TYPE>SQL-Based</INFOLET_TYPE>
<INFOLET_NAME>MS_RGA_Subscriptions_Report_Infolet</INFOLET_NAME>
<OBJECT_ID>100372</OBJECT_ID>
<OUTPUT_TYPE>Ordinary Data Report Infolet</OUTPUT_TYPE>
<CATEGORY>Assets</CATEGORY>
<DESCRIPTION>MS_RGA_Subscriptions_Report_Infolet</DESCRIPTION>
<START_DATE>01-JUN-2010</START_DATE>
<END_DATE/>
<HISTORY>Y</HISTORY>
<PARAMETERS>N</PARAMETERS>
<METRIC_RUN_MODE>On Request</METRIC_RUN_MODE>
<ARCHIVE_PERIOD_INTERVAL>1</ARCHIVE_PERIOD_INTERVAL>
<ARCHIVE_PERIOD_INTERVAL_TYPE>Years</ARCHIVE_PERIOD_INTERVAL_TYPE>
<APPLICATION_NAME>MetricStream</APPLICATION_NAME>
<METRIC_VALIDATED_FLAG>Y</METRIC_VALIDATED_FLAG>
<METRIC_READY_FLAG>N</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>Select b.Channel_Name, 
       a.Filter_Keywords, 
       Decode(a.Notify_By_Email,0,'No',1,'Yes'),
       Decode(a.Status,'O','Active','C','Inactive'),
       Decode(b.active,'1','Active','0','Inactive'),
       Ms_Apps_Utilities.get_Edit_Url(Ms_Apps_Utilities.get_Enterprise_Id('MetricStream'),a.PID) Edit_Url
From   Ms_Rga_Subscription_Request a, 
       Ms_Rga_Channel_Master b
Where  a.Channel_Id = b.Channel_Id
And    a.Status     = 'O'</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
<COMMENTS/>
</SQL_INFOLET>
</METRIC_TYPE>
<INFOLET_FIELDS>
<COLUMN_NAME> B.CHANNEL_NAME</COLUMN_NAME>
<COLUMN_SEQUENCE>1</COLUMN_SEQUENCE>
<RESULT_COLUMN_NAME>CHANNEL_NAME</RESULT_COLUMN_NAME>
<COLUMN_TYPE>Varchar2</COLUMN_TYPE>
<USER_COLUMN_NAME>Channel Name</USER_COLUMN_NAME>
<AXIS_NUM>Y2</AXIS_NUM>
<MASK_COLUMN_FLAG>N</MASK_COLUMN_FLAG>
<LOCALE_COLUMN_FLAG>N</LOCALE_COLUMN_FLAG>
<INCREMENTAL_KEY/>
<XML/>
<COMMENTS/>
</INFOLET_FIELDS>
<INFOLET_FIELDS>
<COLUMN_NAME>          A.FILTER_KEYWORDS</COLUMN_NAME>
<COLUMN_SEQUENCE>2</COLUMN_SEQUENCE>
<RESULT_COLUMN_NAME>FILTER_KEYWORDS</RESULT_COLUMN_NAME>
<COLUMN_TYPE>Varchar2</COLUMN_TYPE>
<USER_COLUMN_NAME>Filter Keywords</USER_COLUMN_NAME>
<AXIS_NUM>Y2</AXIS_NUM>
<MASK_COLUMN_FLAG>N</MASK_COLUMN_FLAG>
<LOCALE_COLUMN_FLAG>N</LOCALE_COLUMN_FLAG>
<INCREMENTAL_KEY/>
<XML/>
