<METRIC_READY_FLAG>Y</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[SELECT REVIEWED, FOLLOW_UP, RESPONSE_DATE, CHANNEL_NAME, TITLE_SUBJECT, BODY, BODY_LINK, ATTACHMENT, ACTIONS, SREVIEWED, SFOLLOWUP, CHANNEL_ID, RESPONSE_ID, SUBS_RESPONSE_ID, SUBSCRIPTION_ID, FEED_DATA_ID   
  FROM
   (
  (
    SELECT V.RESPONSE_DATE, V.CHANNEL_NAME, V.TITLE_SUBJECT, V.BODY, V.BODY_LINK, V.ATTACHMENT, V.ACTIONS, V.CHANNEL_ID, V.RESPONSE_ID, V.SUBS_RESPONSE_ID, V.SUBSCRIPTION_ID, V.FEED_DATA_ID,
          DECODE(NVL( I.REVIEWED_STATUS,0), 0, V.REVIEW_UNCHECKED,
                                            1, V.REVIEW_CHECKED) REVIEWED,
          DECODE(NVL(I.FOLLOWUP_STATUS,0),  0, V.FOLLOW_UNCHECKED,
                                            1, V.FOLLOW_CHECKED) FOLLOW_UP,
	NVL(I.REVIEWED_STATUS,0) SREVIEWED,
	NVL(I.FOLLOWUP_STATUS,0) SFOLLOWUP
           FROM(
    SELECT
    CH_SB_FD.RESPONSE_DATE AS RESPONSE_DATE,
    SUBSTR(CH_SB_FD.CHANNEL_NAME, 1, 100) ||(CASE WHEN LENGTH(CH_SB_FD.Channel_NAME) > 100  THEN  '...'   END ) as Channel_Name,
    CH_SB_FD.TITLE_SUBJECT,
    NULL BODY,
    '<a target="_self" href="javascript'||chr(58)||'callreportdetails1(''Pushinfolet?id=' ||
	content_form.metric_id || CHR(38) || 'x_v' || content_form.column_sequence || '=' || CH_SB_FD.FEED_DATA_ID || CHR(38) ||
	'flag=1' || CHR(38) || 'emd=1' || CHR(38) || 'wrapper=no'')">' ||
	'<img border="0" src="/si_image/Mail-Icon.gif" title="Content">' || '</a>' AS BODY_LINK,
    NULL AS Attachment,
    '<a target="_self" href=#'||CH_SUB_DTLS.CHN_CREATED_BY||'#'||CH_SB_FD.FEED_DATA_ID||'#'||TO_CHAR(CH_SB_FD.RESPONSE_DATE,'mm/dd/yyyy')||'#'||CH_SB_FD.CHANNEL_ID||'><img border="0" src="/si_image/MS_RGA/Actions.png" title="Alert Action"></a>' AS ACTIONS,
     CH_SB_FD.CHANNEL_ID,
    CH_SB_FD.RESPONSE_ID,
    NULL  SUBS_RESPONSE_ID,
    Ch_sub_dtls.SUBSCRIPTION_ID,
    CH_SB_FD.FEED_DATA_ID,
    CH_SUB_DTLS.CHN_CREATED_BY
,
    CH_SB_FD.REVIEW_UNCHECKED,
    CH_SB_FD.FOLLOW_UNCHECKED,
    CH_SB_FD.REVIEW_CHECKED,
    CH_SB_FD.FOLLOW_CHECKED
    FROM
    MS_RGA_VIEW_ALERTS_FILTERED_V CH_SB_FD,
    MS_RGA_SUBS_CHANNEL_CHn Ch_sub_dtls
,
   ( select metric_id,column_sequence from si_metric_columns 
	where metric_id = (select metric_id from si_metrics_t where metric_name = 'MS_RGA_EMAIL_CONTENT')
	AND RESULT_COLUMN_NAME = 'FEED_ID'
	) content_form
	WHERE CH_SB_FD.CHANNEL_ID   =    Ch_sub_dtls.CHANNEL_NAME
  and ( Ch_sub_dtls.FILTER_KEYWORDS IS NULL OR MS_RGA_UTILITIES.FN_MATCH_KEYWORD(CH_SB_FD.BODY,CH_SB_FD.TITLE_SUBJECT,Ch_sub_dtls.FILTER_KEYWORDS)='TRUE')
   and ( (CH_SUB_DTLS.STATUS                  =    '1'	)
        OR
 CH_SB_FD.FEED_DATA_ID IN (
                          SELECT FEED_DATA_ID from MS_RGA_NOTIFY_USERS where DD_CREATED_BY=:1
                          UNION ALL
                          SELECT TO_NUMBER(FEED_DATA_ID) from MS_RGA_LOG_ISSUES_FND WHERE ISSUE_CREATED_BY = :1
                          UNION ALL
                          SELECT TO_NUMBER(ALERT_ID) from MS_RGA_ALERT_OBJECT_RELATIO_V WHERE DD_CURRENT_USER_NAME= :1
                          UNION ALL
						  SELECT FEED_DATA_ID  FROM ms_rga_feed_user_info WHERE USER_NAME = :1 AND (REVIEWED_STATUS=1 OR FOLLOWUP_STATUS =1)
                         )
        )
    AND CH_SUB_DTLS.CHN_CREATED_BY		    = 	 :1
 ) V, ms_rga_feed_user_info I
 WHERE V.FEED_DATA_ID=I.FEED_DATA_ID (+)
   AND V.CHN_CREATED_BY=I.USER_NAME  (+)
   )) order by response_date desc]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML><![CDATA[<?xml version="1.0"?><METRIC_DEPLOYMENT><METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" ><RESULTMAP ></RESULTMAP></METRIC></METRIC_DEPLOYMENT>]]></XML>
<COMMENTS/>
</SQL_INFOLET>
