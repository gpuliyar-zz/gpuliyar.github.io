<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>select cnlmaster.CHANNEL_ID,cnlmaster.CHANNEL_NAME,cnlmaster.CHANNEL_TYPE,cnlmaster.INFOLET_ID,cnlmaster.NOTIFY_USERS,cnlmaster.CHANNEL_DESCRIPTION,cnlmaster.PID,details.SOURCE_SEQ_ID,details.SOURCE_TYPE,details.SERVER_ADD_URL,details.SCR_USER_NAME,details.SCR_PASSWORD,details.EMAIL_FROM,details.KEYWORDS,details.SOURCE_DESCRIPTION,
( select'&lt;a target="_self" href="javascript'|| CHR (58)|| 'showApi(''Pushinfolet'||CHR(63)||'id='|| (SELECT get_infolet_id
('MS ISM Monitor Issue') FROM DUAL)|| CHR (38)|| 'proc='|| cnlmaster.pid
|| CHR (38)|| 'flag=1'|| CHR (38)|| 'emd=1'|| CHR (38)|| 'instid=0'|| CHR (38) || 'x_edit_mode=Yes'|| CHR (38)|| 'wrapper=no'')"&gt;'||cnlmaster.CHANNEL_ID||'&lt;/a&gt;'
from dual ) as COL1,NULL AS COL2 from MS_RGA_CHANNEL_MASTER cnlmaster,MS_RGA_CHANNEL_DETAILS details where cnlmaster.CHANNEL_ID=details.CHANNEL_ID</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
<COMMENTS/>
</SQL_INFOLET>
