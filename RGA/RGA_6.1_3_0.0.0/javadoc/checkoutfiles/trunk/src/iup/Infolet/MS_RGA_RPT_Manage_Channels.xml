<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>Select channel_id,
      -- channel_name,
       Ms_APPS_Utilities.Get_Edit_Url(Ms_Apps_Utilities.Get_Infolet_Id('MS_RGA_CHANNEL_DTLS'),
                                    cust_col4,HTF.escape_sc(channel_name)) channel_name,
      --ms_rga_utilities.build_form_url ('MS RGA CREATE CHANNEL', cust_col4,'Y', channel_name ) channel_name,
       channel_type_disp,
       source_id,
       source_status,
       source_type_disp,
       infolet_name,
       notify_users,
       channel_active_disp,
       edit_url,
       Channel_Description,
       ms_rga_utilities.get_rss_url(Server_Add_Url) as Server_Add_Url,
        Scr_User_Name,
       Email_From,
       Keywords,
       Source_Description,
       Server_Parameters,
       cust_col1,
       cust_col2,
       cust_col3,
       cust_col4,
       cust_col5 
From   Ms_Rga_Manage_Channel_V</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
<COMMENTS/>
</SQL_INFOLET>
