<METRIC_READY_FLAG>Y</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>SELECT distinct (case
         when length(channel_name) &gt; 100 then
          substr(channel_name, 1, 100) || '...'
         when length(channel_name) &lt;= 100 then
          channel_name
       end ) channel_name
FROM ms_rga_channel_master a
ORDER BY upper(channel_name)</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
<COMMENTS/>
</SQL_INFOLET>
</METRIC_TYPE>
