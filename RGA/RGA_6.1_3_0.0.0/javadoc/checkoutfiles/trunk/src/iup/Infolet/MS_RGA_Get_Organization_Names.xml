<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>SELECT DISTINCT b.org_entity_id, b.org_entity_name,a.org_hierarchy_id,
b.user_name,NULL AS dummy1,NULL AS dummy2,NULL AS dummy3
FROM SI_ORG_ENTITIES_T a, MS_RGA_FLOW_DOWN_ORG_V b
WHERE a.org_entity_id = b.org_entity_id
ORDER BY b.org_entity_name</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
