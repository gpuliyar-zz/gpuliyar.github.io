<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>select decode(STATUS, 'PASS', NULL,to_char(REASON)) REASON,STATUS,
       MISSED,REJECTD,
       ACCEPTD,UPDATES,
       ELAPSE,END,
       STAR,PREV,
       SERVER,TYPE,
       THREAD
       from MS_RGA_LOG_SUMMARY_V
       where STATUS &lt;&gt; 'Status'
       and MISSED &lt;&gt; 'Missed'</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
