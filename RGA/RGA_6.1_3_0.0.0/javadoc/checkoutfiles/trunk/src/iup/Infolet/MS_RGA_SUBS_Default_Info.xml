<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[Select null as Pid,
         a.Subscription_Id     Subscription_Id,
         a.Channel_Id          Channel_Id,
         a.Channel_Name         Channel_Name,
         a.Filter_Keywords     Filter_Keywords,
         a.Notify_By_Email     Notify_By_Email,
         null as          User_Id,
        a.Status Subscription_Status,
         null as       Created_On,
         a.CHN_CREATED_BY          Created_By,
         null as        Modified_On,
         null as  Modified_By,
         a.CUSTOM_FIELD10 CUSTOM_FIELD10
  From   MS_RGA_SUBS_CHANNEL_cHN a 
  where a.CHN_CREATED_BY = :1
  order by a.Subscription_Id]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML><![CDATA[<?xml version="1.0"?><METRIC_DEPLOYMENT><METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" ><RESULTMAP ></RESULTMAP></METRIC></METRIC_DEPLOYMENT>]]></XML>
<COMMENTS/>
