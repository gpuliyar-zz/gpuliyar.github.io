<INFOLET_NAME>MS RGA Get Username</INFOLET_NAME>
<OBJECT_ID>100377</OBJECT_ID>
<OUTPUT_TYPE>Ordinary Data Report Infolet</OUTPUT_TYPE>
<CATEGORY>Assets</CATEGORY>
<DESCRIPTION>To get all the system users</DESCRIPTION>
<START_DATE>02-JUN-2010</START_DATE>
<END_DATE/>
<HISTORY>Y</HISTORY>
<PARAMETERS>N</PARAMETERS>
<METRIC_RUN_MODE>On Request</METRIC_RUN_MODE>
<ARCHIVE_PERIOD_INTERVAL>1</ARCHIVE_PERIOD_INTERVAL>
<ARCHIVE_PERIOD_INTERVAL_TYPE>Years</ARCHIVE_PERIOD_INTERVAL_TYPE>
<APPLICATION_NAME>MetricStream</APPLICATION_NAME>
<METRIC_VALIDATED_FLAG>Y</METRIC_VALIDATED_FLAG>
<METRIC_READY_FLAG>Y</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>SELECT   user_name,
         first_name,
         middle_initial,
         last_name,
         email_address,
         phone_number,
         start_date,
         end_date,
         user_id,
         first_name || ' ' || last_name AS full_name
    FROM SI_USERS_T
   WHERE user_name NOT IN ('SYSTEMI', 'pfadmin') and (end_date is null or end_date&gt;sysdate) 
ORDER BY user_name</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
<COMMENTS/>
</SQL_INFOLET>
</METRIC_TYPE>
