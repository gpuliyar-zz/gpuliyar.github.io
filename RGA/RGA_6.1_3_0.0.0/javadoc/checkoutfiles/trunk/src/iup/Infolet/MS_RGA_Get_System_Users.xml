<INFOLET_NAME>MS RGA Get System Users</INFOLET_NAME>
<OBJECT_ID><![CDATA[101327]]></OBJECT_ID>
<OUTPUT_TYPE>Ordinary Graph Infolet</OUTPUT_TYPE>
<CATEGORY>Assets</CATEGORY>
<DESCRIPTION>This infolet is used to fetch all the Users associated with SYSTEM</DESCRIPTION>
<START_DATE>31-OCT-2012</START_DATE>
<END_DATE/>
<HISTORY>M</HISTORY>
<PARAMETERS>N</PARAMETERS>
<METRIC_RUN_MODE>On Request</METRIC_RUN_MODE>
<ARCHIVE_PERIOD_INTERVAL>1</ARCHIVE_PERIOD_INTERVAL>
<ARCHIVE_PERIOD_INTERVAL_TYPE>Days</ARCHIVE_PERIOD_INTERVAL_TYPE>
<APPLICATION_NAME>MetricStream</APPLICATION_NAME>
<METRIC_VALIDATED_FLAG>Y</METRIC_VALIDATED_FLAG>
<METRIC_READY_FLAG>Y</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[SELECT   user_name as "User Name",
         first_name,
         middle_initial,
         last_name,
         email_address,
         phone_number,
         start_date,
         end_date,
         user_id,
         first_name || ' ' || last_name AS "Full Name"
    FROM SI_USERS_T
   WHERE user_name NOT IN ('SYSTEMI', 'pfadmin') and (end_date is null or end_date>sysdate) 
ORDER BY user_name]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML><![CDATA[<?xml version="1.0"?><METRIC_DEPLOYMENT><METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" ><RESULTMAP ></RESULTMAP></METRIC></METRIC_DEPLOYMENT>]]></XML>
<COMMENTS/>
</SQL_INFOLET>
</METRIC_TYPE>
