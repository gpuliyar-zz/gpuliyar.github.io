<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>Select Channel_Id, 
       Channel_Name, 
       Channel_Description,  
       Source_Id, 
       Server_Add_url, 
       Scr_User_name, 
       Scr_Password, 
       Email_From, 
       keywords,
       source_description,
       source_active,
       response_date,
       body,
       title_subject,
       attachments,
       created_on,
       modified_on,
       processed_flag
From   Ms_Rga_Strctrd_Cntnt_Feeds_V
Where  Rownum &lt;= Nvl(:1, 10)
And    Channel_Name = Nvl(:2, Channel_Name)</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
