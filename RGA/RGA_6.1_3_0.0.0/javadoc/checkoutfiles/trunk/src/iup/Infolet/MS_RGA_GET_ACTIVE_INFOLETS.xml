<PLATFORM_DETAILS>
<PLATFORM_VERSION>6.1</PLATFORM_VERSION>
<BUILD_VERSION>1.0.0.0</BUILD_VERSION>
</PLATFORM_DETAILS>
<INFOLET_TYPE>SQL-Based</INFOLET_TYPE>
<INFOLET_NAME>MS_RGA_GET_ACTIVE_INFOLETS</INFOLET_NAME>
<OBJECT_ID>101152</OBJECT_ID>
<OUTPUT_TYPE>Ordinary Graph Infolet</OUTPUT_TYPE>
<CATEGORY>Assets</CATEGORY>
<DESCRIPTION/>
<START_DATE>04-FEB-2013</START_DATE>
<END_DATE/>
<HISTORY>Y</HISTORY>
<PARAMETERS>N</PARAMETERS>
<METRIC_RUN_MODE>On Request</METRIC_RUN_MODE>
<ARCHIVE_PERIOD_INTERVAL>1</ARCHIVE_PERIOD_INTERVAL>
<ARCHIVE_PERIOD_INTERVAL_TYPE>Days</ARCHIVE_PERIOD_INTERVAL_TYPE>
<APPLICATION_NAME>MetricStream</APPLICATION_NAME>
<METRIC_VALIDATED_FLAG>Y</METRIC_VALIDATED_FLAG>
<METRIC_READY_FLAG>Y</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION>select a.metric_name "Infolet Name",
       a.DESCRIPTION   "Description",
       a.METRIC_ID     "Infolet Id"
  from si_metrics a, si_ent b,si_metric_group_details c,si_metric_groups d
 where a.metric_ready_flag = 'Y'
   and a.start_date &lt; sysdate
   and (a.end_date &gt; sysdate or a.end_date is null)
   and a.enterprise_id = b.enterprise_id
   and a.METRIC_ID=c.metric_id
   and c.metric_group_id=d.metric_group_id
   and d.metric_group_name='MS_RGA_SCI_INFOLET_GROUP'</METRIC_EXPRESSION>
<INCREMENTAL_DATA>N</INCREMENTAL_DATA>
<XML>&lt;?xml version="1.0"?&gt;&lt;METRIC_DEPLOYMENT&gt;&lt;METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" &gt;&lt;RESULTMAP &gt;&lt;/RESULTMAP&gt;&lt;/METRIC&gt;&lt;/METRIC_DEPLOYMENT&gt;</XML>
<COMMENTS/>
</SQL_INFOLET>
</METRIC_TYPE>
<INFOLET_FIELDS>
<COLUMN_NAME>A.METRIC_ID"INFOLETID"</COLUMN_NAME>
<COLUMN_SEQUENCE>3</COLUMN_SEQUENCE>
<RESULT_COLUMN_NAME>INFOLET_ID</RESULT_COLUMN_NAME>
<COLUMN_TYPE>Varchar2</COLUMN_TYPE>
<USER_COLUMN_NAME>Infolet ID</USER_COLUMN_NAME>
<AXIS_NUM>Z</AXIS_NUM>
<MASK_COLUMN_FLAG>N</MASK_COLUMN_FLAG>
<LOCALE_COLUMN_FLAG>N</LOCALE_COLUMN_FLAG>
<INCREMENTAL_KEY/>
<XML/>
<COMMENTS/>
</INFOLET_FIELDS>
<INFOLET_FIELDS>
<COLUMN_NAME>A.METRIC_NAME"INFOLETNAME"</COLUMN_NAME>
<COLUMN_SEQUENCE>1</COLUMN_SEQUENCE>
<RESULT_COLUMN_NAME>INFOLET_NAME</RESULT_COLUMN_NAME>
<COLUMN_TYPE>Varchar2</COLUMN_TYPE>
<USER_COLUMN_NAME>Infolet Name</USER_COLUMN_NAME>
<AXIS_NUM>Z</AXIS_NUM>
<MASK_COLUMN_FLAG>N</MASK_COLUMN_FLAG>
<LOCALE_COLUMN_FLAG>N</LOCALE_COLUMN_FLAG>
<INCREMENTAL_KEY/>
<XML/>
<COMMENTS/>
