<METRIC_VALIDATED_FLAG>Y</METRIC_VALIDATED_FLAG>
<METRIC_READY_FLAG>Y</METRIC_READY_FLAG>
<METRIC_TYPE>
<!-- SQL BASED INFOLET -->
<SQL_INFOLET>
<METRIC_EXPRESSION><![CDATA[SELECT  A.stored_value, 
         A.displayed_value
    FROM ms_qs_lov_org_values_v A,SI_USERS_T B,SI_LOCALES C
   WHERE NVL (A.end_date, SYSDATE) >= SYSDATE 
   AND A.LOCALE_ID = C.LOCALE_ID
   AND C.LOCALE_STRING=B.LOCALE
   AND A.lov_name='ms_rga_email_channel_type'
   AND B.USER_NAME = :1
   ORDER BY A.stored_value]]></METRIC_EXPRESSION>
<INCREMENTAL_DATA/>
<XML><![CDATA[<?xml version="1.0"?><METRIC_DEPLOYMENT><METRIC       PROCESS="0"  INSTANCE="0"  NOTIFY="YES"   RETURN="RESULTSET" ><RESULTMAP ></RESULTMAP></METRIC></METRIC_DEPLOYMENT>]]></XML>
<COMMENTS/>
</SQL_INFOLET>
</METRIC_TYPE>
