<OBJECT_ID><![CDATA[101890]]></OBJECT_ID>
<OUTPUT_TYPE>Report Infolet</OUTPUT_TYPE>
<CATEGORY>SYSTEMI</CATEGORY>
<DESCRIPTION>This infolet is used to select the dropdown values for EMAIL type i.e. IMAP or POP3</DESCRIPTION>
<START_DATE>13-APR-2015</START_DATE>
<END_DATE/>
<HISTORY>M</HISTORY>
<PARAMETERS>Y</PARAMETERS>
<METRIC_RUN_MODE>On Request</METRIC_RUN_MODE>
<ARCHIVE_PERIOD_INTERVAL>1</ARCHIVE_PERIOD_INTERVAL>
<ARCHIVE_PERIOD_INTERVAL_TYPE>Months</ARCHIVE_PERIOD_INTERVAL_TYPE>
