<VIEW_TEXT>(USER_ID,ORG_ENTITY_ID,ROLE_ID) AS SELECT   b.user_id, a.org_entity_id, a.role_id
       FROM si_org_roles a, si_user_org_roles b
      WHERE a.org_role_id = b.org_role_id
   ORDER BY user_id, org_entity_id, role_id </VIEW_TEXT>
