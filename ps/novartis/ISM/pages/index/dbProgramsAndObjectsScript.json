  "MS_ISM_DETAILED_ISSUE_LIST_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_DETAILED_ISSUE_LIST_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_DETAILED_ISSUE_LIST_V\" (\"ISSUE_TITLE\", \"ISSUE_STATUS\", \"ISSUE_STATUS_STORED\", \"ISSUE_OWNER\", \"ISSUE_RATING\", \"PRIORITY\", \"SOURCE_TYPE\", \"ISSUE_DUE_DATE\", \"ISSUE_INITIATOR\", \"FIRST_IDENTIFIED_ON\", \"ISSUE_ID\", \"ISSUE_DETAILS\", \"NO_OF_ACTIONS\", \"ISSUE_OWNER_ORG\", \"ISSUE_APPROVERORG\", \"OWNERORG\", \"APPROVERORG\", \"PROGRESS_STATUS\", \"PID\", \"INSTANCEID\", \"CORE_OBJECTS\", \"AREA_OF_COMPLIANCE\", \"CONTROLS\", \"RISKS\", \"ORG\", \"STANDARD\", \"ASSET\", \"ASSET_CLASS\", \"FINANCIAL_ACCOUNT\", \"FUNCTIONOBJ\", \"FRAMEWORK_REFERENCE\", \"OBJECTIVE\", \"PROCESS\", \"REFERENCE\", \"REGULATORY_BOD\", \"REQUIREMENT\", \"QUESTION_PROCEDURE\", \"EXCEPTIONOBJ\", \"SUPPLIER\", \"AUDITABLE_ENTITY\", \"ISSUE_APPROVER\", \"ISSUE_RISK_IMPACT\", \"ISSUE_RISK_LIKLIHOOD\", \"ISSUE_SOURCE\", \"ISSUE_RISK_LOSS_GAIN\", \"ISSUE_RISK_LOSS_GAIN_AMT\", \"EXCEPTION_TYPE\", \"REACHED_MANAGE\", \"ADHOC_SRC_DETAILS\", \"NO_OF_EXTENSIONS\", \"REPEAT_ISSUE\", \"REOPEN_ISSUE\", \"ISSUE_TYPE\", \"RELATED_ISSUES\", \"DD_CREATED_ON\", \"ISSUE_SOURCE_STORED\") AS \n  SELECT\n    (SELECT \u0027\u003ca href\u003d\"javascript\u0027\n      || CHR (58)\n      || \u0027try{ callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027\n      --|| ms_apps_utilities.get_infolet_id (\u0027MS_ISM_ISSUE\u0027)\n      || ms_ism_utilities.get_ism_infolet_id (\u0027ISSUE\u0027)\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027proc\u003d\u0027\n      || a.dd_process_instance_id\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027flag\u003d1\u0027\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027emd\u003d1\u0027\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027x_edit_mode\u003dYes\u0027\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027wrapper\u003dno\u0027\u0027);}catch(e){}\"\u003e\u0027\n      || REPLACE(REPLACE(a.issue_title,\u0027\u003c\u0027, \u0027\u0026\u0027||\u0027lt;\u0027), \u0027\u003e\u0027, \u0027\u0026\u0027||\u0027gt;\u0027) \n      || \u0027\u003c/a\u003e\u0027\n    FROM DUAL\n    )                                                                                                      AS issue_title,\n    ms_ism_helper.get_status_display_name(a.issue_status)                                                  AS issue_status,\n    a.issue_status                                                                                         AS issue_status_stored,\n    ms_apps_utilities.get_user_full_name (a.issue_owner)                                                   AS issue_owner,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, a.issue_risk_rating, 1, NULL, NULL) AS issue_rating,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027, a.issue_risk_priority, 1, NULL, NULL)  AS priority,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Source Type\u0027, a.issue_source_type, 1, NULL, NULL) AS source_type,\n    a.issue_due_date                                                                                       AS issue_due_date,\n    ms_apps_utilities.get_user_full_name (a.dd_created_by)                                                 AS issue_initiator,\n    a.issue_identified_on                                                                                  AS first_identified_on,\n    a.issue_id                                                                                             AS issue_id,\n    REGEXP_REPLACE (a.issue_details, \u0027\u003c[^\u003e]+\u003e\u0027, NULL)                                                      AS issue_details,\n    (SELECT COUNT (1) FROM ms_ism_action WHERE issue_id \u003d a.issue_id\n    ) AS no_of_actions,\n    (SELECT org_entity_name\n    FROM si_org_entities_t\n    WHERE org_entity_id \u003d a.issue_ownerorg\n    ) AS issue_owner_org,\n    (SELECT org_entity_name\n    FROM si_org_entities_t\n    WHERE org_entity_id \u003d a.issue_approverorg\n    ) AS issue_approverorg,\n    a.issue_ownerorg,\n    a.issue_approverorg,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Update Status\u0027, a.progress_status, 1, NULL, NULL)                        AS progress_status,\n    a.dd_process_instance_id                                                                                                      AS pid,\n    a.dd_instance_id                                                                                                              AS instanceid,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Core Object(s)\u0027,a.issue_id)                                                         AS Core_Objects,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Area(s) of Compliance\u0027,a.issue_id)                                                  AS Area_of_Compliance,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Control(s)\u0027,a.issue_id)                                                             AS Controls,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Risk(s)\u0027,a.issue_id)                                                                AS Risks,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Organization(s)\u0027,a.issue_id)                                                        AS Org,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Standard(s)\u0027,a.issue_id)                                                            AS Standard,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Asset(s)\u0027,a.issue_id)                                                               AS Asset,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Asset Class(es)\u0027,a.issue_id)                                                        AS Asset_Class,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Financial Account(s)\u0027,a.issue_id)                                                   AS Financial_Account,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Function(s)\u0027,a.issue_id)                                                            AS FunctionObj,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Framework Reference(s)\u0027,a.issue_id)                                                 AS Framework_Reference,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Objective(s)\u0027,a.issue_id)                                                           AS Objective,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Process(s)\u0027,a.issue_id)                                                             AS Process,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Reference(s)\u0027,a.issue_id)                                                           AS REFERENCE,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Regulatory Bod(ies)\u0027,a.issue_id)                                                    AS Regulatory_Bod,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Requirement(s)\u0027,a.issue_id)                                                         AS Requirement,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Question(s)/Procedure(s)\u0027,a.issue_id)                                               AS Question_Procedure,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Exception(s)\u0027,a.issue_id)                                                           AS ExceptionObj,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Supplier(s)\u0027,a.issue_id)                                                            AS Supplier,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Auditable Entity(s)\u0027,a.issue_id)                                                    AS Auditable_Entity,\n    ms_apps_utilities.get_user_full_name (a.ISSUE_APPROVER)                                                                       AS ISSUE_APPROVER,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Impact\u0027, a.issue_risk_impact, 1, NULL, NULL)                             AS issue_risk_impact,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Likelihood\u0027, a.issue_risk_liklihood, 1, NULL, NULL)                      AS issue_risk_liklihood,\n    DECODE (a.issue_source_type, \u00270\u0027, a.adhoc_source, MS_ISM_HELPER.get_Issue_Source_Title (a.issue_source, a.issue_source_type)) AS ISSUE_SOURCE,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Loss Gain\u0027, a.issue_risk_loss_gain, 1, NULL, NULL)                       AS issue_risk_loss_gain,\n    issue_risk_loss_gain_amt,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Classification Type\u0027, a.TYPE_OF_EXECUTION, 1, NULL, NULL) AS EXCEPTION_TYPE,\n    a.reached_manage                                                                                               AS reached_manage,\n    REGEXP_REPLACE (a.ADHOC_SRC_DETAILS, \u0027\u003c[^\u003e]+\u003e\u0027, NULL)                                                          AS ADHOC_SRC_DETAILS,\n    a.NO_OF_EXTENSIONS                                                                                             AS NO_OF_EXTENSIONS,\n    ms_apps_utilities.get_display_value (100000, \u0027MS_ISM_YES_NO\u0027, a.REPEAT_ISSUE, 1, NULL, NULL)                   AS REPEAT_ISSUE,\n    a.REOPEN_ISSUE                                                                                                 AS REOPEN_ISSUE,\n    ms_ism_helper.get_comma_sep_value_for_mlov (\u0027ISSUE_TYPE\u0027,\u0027MS_ISM_ISSUE_IST\u0027,a.issue_id,\u0027MS ISM Issue Type\u0027)    AS ISSUE_TYPE,\n    ms_ism_helper.get_comma_sep_related_issues(a.issue_id)                                                      AS RELATED_ISSUES,\n    dd_created_on                                                                                               As DD_CREATED_ON,\n    ISSUE_SOURCE                                                                                                As ISSUE_SOURCE_STORED \n  FROM ms_ism_issue a\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_PIS_LOG_ID_S": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_PIS_LOG_ID_S\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"NOVT_DEV\".\"MS_ISM_PIS_LOG_ID_S\"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 100520 NOCACHE  NOORDER  NOCYCLE \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_CIF_RESPONSE": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_CIF_RESPONSE\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_ISM_CIF_RESPONSE\" AS OBJECT\n   (code VARCHAR2 (10), issue_id VARCHAR2 (100),request_id VARCHAR2(255), errors varchar2_array)  ; \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_COMMENT_ID_S": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_COMMENT_ID_S\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"NOVT_DEV\".\"MS_ISM_COMMENT_ID_S\"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 100038 NOCACHE  NOORDER  NOCYCLE \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_REOPEN_ISSUE_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_REOPEN_ISSUE_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_REOPEN_ISSUE_V\" (\"ISSUE_LINK\", \"ISSUE_TITLE\", \"ISSUE_OWNER\", \"ISSUE_APPROVER\", \"SOURCE_TYPE\", \"ISSUE_RATING\", \"ISSUE_DUE_DATE\", \"ISSUE_CLOSED_ON\", \"ISSUE_ID\", \"PID\", \"INSTANCEID\", \"ACTIONS\", \"REOPEN_COUNT\", \"NO_OF_ACTIONS\", \"ISSUE_OWNERORG_DISP\", \"ISSUE_OWNERORG\", \"ISSUE_APPRORG_DISP\", \"ISSUE_APPRORG\") AS \n  SELECT\n    (SELECT \u0027\u003ca target\u003d\"_self\" href\u003d\"javascript\u0027\n      || CHR (58)\n      || \u0027callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027\n      --|| ms_apps_utilities.get_infolet_id (\u0027MS_ISM_ISSUE\u0027)\n      || ms_ism_utilities.get_ism_infolet_id (\u0027ISSUE\u0027)\n      || CHR (38)\n      || \u0027proc\u003d\u0027\n      || a.dd_process_instance_id\n      || CHR (38)\n      || \u0027flag\u003d21\u0027\n      || CHR (38)\n      || \u0027emd\u003d11\u0027\n      || CHR (38)\n      || \u0027x_edit_mode\u003dYes\u0027\n      || CHR (38)\n      || \u0027wrapper\u003dno\u0027\u0027)\"\u003e\u0027\n      || a.issue_title\n      || \u0027\u003c/a\u003e\u0027\n    FROM DUAL\n    )                                                                                                      AS issue_link,\n    a.issue_title                                                                                          AS issue_title,\n    ms_apps_utilities.get_user_full_name (a.issue_owner)                                                   AS issue_owner,\n    ms_apps_utilities.get_user_full_name (a.issue_approver)                                                AS issue_approver,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Source Type\u0027, a.issue_source_type, 1, NULL, NULL) AS source_type,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, a.issue_risk_rating, 1, NULL, NULL) AS issue_rating,\n    a.issue_due_date                                                                                       AS issue_due_date,\n    a.issue_closed_on                                                                                      AS issue_closed_on,\n    a.issue_id                                                                                             AS issue_id,\n    a.dd_process_instance_id                                                                               AS pid,\n    a.dd_instance_id                                                                                       AS instanceid,\n    NULL                                                                                                   AS actions,\n    NVL(a.reopen_issue,0)                                                                                  AS reopen_count,\n    (SELECT COUNT (1) FROM ms_ism_action WHERE issue_id \u003d a.issue_id\n    ) AS no_of_actions,\n    (SELECT org_entity_name\n    FROM si_org_entities_t\n    WHERE org_entity_id \u003d a.issue_ownerorg\n    )                AS issue_ownerorg_disp,\n    a.issue_ownerorg AS issue_ownerorg,\n    (SELECT org_entity_name\n    FROM si_org_entities_t\n    WHERE org_entity_id \u003d a.issue_approverorg\n    )                   AS issue_approrg_disp,\n    a.issue_approverorg AS issue_approrg\n  FROM ms_ism_issue a\n  WHERE a.issue_status \u003d \u002710\u0027\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ISSUE_IAC": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_IAC\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ISSUE_IAC\" (\"IAC_COMMENTS\", \"IAC_TITLE\", \"IAC_CONTROL\", \"ISSUE_ID\", \"DD_STATUS_FLAG\") AS \n  SELECT rel_comments iac_comments,\n    tgt_obj_id iac_title,\n    tgt_obj_name iac_control,\n    rel_source_id issue_id,\n    \u0027Y\u0027 dd_status_flag\n  FROM ms_ism_orb_relation_orb\n  WHERE tgt_obj_type \u003d \u0027Controls\u0027\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ISSUE_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ISSUE_DV\" (\"IAP_ACTION_ID\", \"ORB_REL_SOURCE_ID\", \"IAP_ISSUE_ID\", \"FIELD_VALIDATION\", \"EDITABILITY_JSON\", \"ISSUE_AUTO_CLOSED\", \"UPLOADED_ISSUE\", \"POPULATE_APPROVER_COMMENTS\", \"JSON_APPROVER_COMMENTS\", \"ACT_RESULTS\", \"ACT_WORK_DONE\", \"DUMMY_APPROVER_ORG\", \"DUMMY_APPROVER\", \"CONFIG_NOTE\", \"SAME_APPROVER_FLAG\", \"FINAL_ISS_APPROVERORG\", \"FINAL_ISS_APPROVER\", \"ACT_PLAN_APPROVERORG\", \"ACTION_PLAN_APPROVER\", \"PRIVATE_PUBLIC\", \"UPDATE_ISSUE\", \"REOPEN_ISSUE\", \"REPEAT_ISSUE\", \"ISSUE_CLOSED_FROM\", \"ADDITIONAL_COLUMN5\", \"ADDITIONAL_COLUMN4\", \"ORB_FORM_URL\", \"ADDITIONAL_DETAILS_WITH_ID\", \"ADDITIONAL_COLUMN3\", \"ADDITIONAL_COLUMN2\", \"ADDITIONAL_COLUMN1\", \"ADDITIONAL_DETAILS\", \"APPLICABLE_TO\", \"ROW_NUM\", \"TGT_OBJ_TYPE\", \"SELF_REL_TYPE\", \"PARENT_REL_INST_ID\", \"REL_COMMENTS\", \"REL_VALID_UNTIL\", \"REL_VALID_FROM\", \"TGT_OBJ_ID\", \"TGT_OBJ_NAME\", \"ISSUE_ID\", \"REL_CONFIG_ID\", \"REL_INST_ID\", \"ATTRIBUTE_COLUMN_H2\", \"ATTRIBUTE_COLUMN_H3\", \"ADDITIONAL_COLUMN_H1\", \"ASSOCIATED_WITH_TYPE\", \"CONFIGURED_OBJECT_TYPES\", \"REL_SOURCE_OBJECT_ID\", \"REL_SOURCE_ID\", \"ID_GEN\", \"ISSUE_LEGACY_ID\", \"ADHOC_SRC_DETAILS\", \"ADHOC_SOURCE\", \"C_TEXT_5\", \"C_TEXT_4\", \"C_TEXT_3\", \"C_TEXT_2\", \"C_TEXT_1\", \"C_RTF_2\", \"C_RTF_1\", \"C_DATE_5\", \"C_DATE_4\", \"C_DATE_3\", \"C_DATE_2\", \"C_DATE_1\", \"DUMMY_ISSUE_OWNER\", \"ISSUE_CLOSED_ON\", \"ISSUE_RELATED_ISSUES\", \"CONFIG_FLAG\", \"IAP_SAVE\", \"REACHED_MANAGE\", \"OTHER_IMPACT\", \"ACTION_ID\", \"DELEGATED\", \"ISSUE_LOGIN\", \"DUMMY_RELATED_ISSUES\", \"CUSTOM_FIELD3\", \"ACTION_STATE\", \"ISSUE_DELEGATEE\", \"REMOVE_DELEGATION\", \"ISSUE_ACTION_ID\", \"REQ_DUE_DATE_EXTN\", \"CUSTOM_FIELD2\", \"CUSTOM_FIELD1\", \"RTF_ATTACH\", \"F_LAST_MODIFIED_ON\", \"F_LAST_MODIFIED_BY\", \"F_CREATED_ON\", \"F_CREATED_BY\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"ACTION_METRIC_ID\", \"NO_OF_EXTENSIONS\", \"HIDDEN_APPROVER_ORG\", \"ISSUE_RESOLUTION_SUMMARY\", \"FINAL_ACTION_APPROVAL\", \"ISSUE_ACTION\", \"ORIG_ISSUE_DUE_DT\", \"CONFIG_FLAG_DUMMY\", \"ASSIGNEE\", \"ISSUE_TYPE\", \"PROGRESS_STATUS\", \"REASSIGNED_ISSUE_OWNER\", \"TYPE_OF_EXECUTION\", \"ACTION_EXIST\", \"ISSUE_INITIATOR\", \"ACTIVITIES\", \"ASSIGN_TO\", \"PREVIOUS_STAGE\", \"ACTION_APPROVER1\", \"ACTION_APPROVER2\", \"ACTION_CATEGORY\", \"ACTION_DESCRIPTION\", \"ACTION_DUE_DATE\", \"ACTION_OWNER\", \"ACTION_PRIORITY\", \"ACTION_RELATED_TO\", \"ACTION_START_DATE\", \"ACTION_TITLE\", \"ACTION_TYPE\", \"ISSUE_APPROVER\", \"ISSUE_APPROVERORG\", \"ISSUE_ATTACH_FILES\", \"ISSUE_COMMENTS\", \"ISSUE_DETAILS\", \"ISSUE_DUE_DATE\", \"ISSUE_IDENTIFIED_ON\", \"ISSUE_OWNER\", \"ISSUE_OWNERORG\", \"ISSUE_REVIEWER\", \"ISSUE_RISK_IMPACT\", \"ISSUE_RISK_LIKLIHOOD\", \"ISSUE_RISK_LOSS_GAIN\", \"ISSUE_RISK_LOSS_GAIN_AMT\", \"ISSUE_RISK_PRIORITY\", \"ISSUE_RISK_RATING\", \"ISSUE_SOURCE\", \"ISSUE_SOURCE_REF\", \"ISSUE_SOURCE_TYPE\", \"ISSUE_STATUS\", \"ISSUE_TITLE\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"ISSUE_DETAILS_HTM\", \"ACTION_DESCRIPTION_HTM\", \"ISSUE_RESOLUTION_SUMMARY_HTM\", \"C_RTF_1_HTM\", \"C_RTF_2_HTM\", \"ADHOC_SRC_DETAILS_HTM\", \"ACT_WORK_DONE_HTM\", \"ACT_RESULTS_HTM\", \"CONFIG_FLAG_SETUP\", \"NO_OF_ACTION\") AS \n  SELECT \"IAP_ACTION_ID\",\n    \"ORB_REL_SOURCE_ID\",\n    \"IAP_ISSUE_ID\",\n    \"FIELD_VALIDATION\",\n    \"EDITABILITY_JSON\",\n    \"ISSUE_AUTO_CLOSED\",\n    \"UPLOADED_ISSUE\",\n    \"POPULATE_APPROVER_COMMENTS\",\n    \"JSON_APPROVER_COMMENTS\",\n    \"ACT_RESULTS\",\n    \"ACT_WORK_DONE\",\n    ms_ism_helper.get_comma_sep_display_value(DUMMY_APPROVER_ORG,\u0027ORG\u0027) AS DUMMY_APPROVER_ORG,\n    ms_apps_utilities.get_user_full_name (DUMMY_APPROVER) AS DUMMY_APPROVER,\n    \"CONFIG_NOTE\",\n    \"SAME_APPROVER_FLAG\",\n    ms_ism_helper.get_comma_sep_display_value(FINAL_ISS_APPROVERORG,\u0027ORG\u0027) AS FINAL_ISS_APPROVERORG,\n    ms_apps_utilities.get_user_full_name (FINAL_ISS_APPROVER) AS FINAL_ISS_APPROVER,\n    ms_ism_helper.get_comma_sep_display_value(ACT_PLAN_APPROVERORG,\u0027ORG\u0027) AS ACT_PLAN_APPROVERORG,\n    ms_apps_utilities.get_user_full_name (ACTION_PLAN_APPROVER) AS ACTION_PLAN_APPROVER,\n    \"PRIVATE_PUBLIC\",\n    \"UPDATE_ISSUE\",\n    \"REOPEN_ISSUE\",\n    \"REPEAT_ISSUE\",\n    \"ISSUE_CLOSED_FROM\",\n    \"ADDITIONAL_COLUMN5\",\n    \"ADDITIONAL_COLUMN4\",\n    \"ORB_FORM_URL\",\n    \"ADDITIONAL_DETAILS_WITH_ID\",\n    \"ADDITIONAL_COLUMN3\",\n    \"ADDITIONAL_COLUMN2\",\n    \"ADDITIONAL_COLUMN1\",\n    \"ADDITIONAL_DETAILS\",\n    \"APPLICABLE_TO\",\n    \"ROW_NUM\",\n    \"TGT_OBJ_TYPE\",\n    \"SELF_REL_TYPE\",\n    \"PARENT_REL_INST_ID\",\n    \"REL_COMMENTS\",\n    \"REL_VALID_UNTIL\",\n    \"REL_VALID_FROM\",\n    \"TGT_OBJ_ID\",\n    \"TGT_OBJ_NAME\",\t\n    \"ISSUE_ID\",\n    \"REL_CONFIG_ID\",\n    \"REL_INST_ID\",\n    \"ATTRIBUTE_COLUMN_H2\",\n    \"ATTRIBUTE_COLUMN_H3\",\n    \"ADDITIONAL_COLUMN_H1\",\n    \"ASSOCIATED_WITH_TYPE\",\n    \"CONFIGURED_OBJECT_TYPES\",\n    \"REL_SOURCE_OBJECT_ID\",\n    \"REL_SOURCE_ID\",\n    \"ID_GEN\",\n    \"ISSUE_LEGACY_ID\",\n    \"ADHOC_SRC_DETAILS\",\n    \"ADHOC_SOURCE\",\n    \"C_TEXT_5\",\n    \"C_TEXT_4\",\n    \"C_TEXT_3\",\n    \"C_TEXT_2\",\n    \"C_TEXT_1\",\n    \"C_RTF_2\",\n    \"C_RTF_1\",\n    \"C_DATE_5\",\n    \"C_DATE_4\",\n    \"C_DATE_3\",\n    \"C_DATE_2\",\n    \"C_DATE_1\",\n     ms_apps_utilities.get_user_full_name (DUMMY_ISSUE_OWNER) AS DUMMY_ISSUE_OWNER,\n    \"ISSUE_CLOSED_ON\",\n    ms_ism_helper.get_comma_sep_issue_title (ISSUE_RELATED_ISSUES, \u0027REL\u0027) AS ISSUE_RELATED_ISSUES,\n    \"CONFIG_FLAG\",\n    \"IAP_SAVE\",\n    \"REACHED_MANAGE\",\n    \"OTHER_IMPACT\",\n    \"ACTION_ID\",\n    \"DELEGATED\",\n    \"ISSUE_LOGIN\",\n    \"DUMMY_RELATED_ISSUES\",\n    \"CUSTOM_FIELD3\",\n    \"ACTION_STATE\",\n    (ms_apps_utilities.get_user_full_name (Issue_Delegatee)) AS ISSUE_DELEGATEE,\n    \"REMOVE_DELEGATION\",\n    \"ISSUE_ACTION_ID\",\n    \"REQ_DUE_DATE_EXTN\",\n    ms_apps_utilities.get_user_full_name (CUSTOM_FIELD2) AS CUSTOM_FIELD2,\n    \"CUSTOM_FIELD1\",\n    \"RTF_ATTACH\",\n    \"F_LAST_MODIFIED_ON\",\n    (ms_apps_utilities.get_user_full_name (f_last_modified_by)) AS F_LAST_MODIFIED_BY,\n    \"F_CREATED_ON\",\n    (ms_apps_utilities.get_user_full_name (f_created_by))                   AS F_CREATED_BY,\n    ms_ism_helper. get_comma_sep_display_value (owner_organizations, \u0027ORG\u0027) AS OWNER_ORGANIZATIONS,\n    \"RESTRICT_ACCESS_TO\",\n    \"ACTION_METRIC_ID\",\n    \"NO_OF_EXTENSIONS\",\n    \"HIDDEN_APPROVER_ORG\",\n    \"ISSUE_RESOLUTION_SUMMARY\",\n    ms_apps_utilities. get_display_value (100000, \u0027MS ISM Final Action Approval\u0027, final_action_approval, 1, NULL, NULL) AS FINAL_ACTION_APPROVAL,\n    (SELECT action_name FROM ms_ism_actions WHERE action_id \u003d issue_action\n    ) AS ISSUE_ACTION,\n    \"ORIG_ISSUE_DUE_DT\",\n    \"CONFIG_FLAG_DUMMY\",\n    (ms_apps_utilities.get_user_full_name (assignee))                                                            AS ASSIGNEE,\n    ms_ism_helper.get_comma_sep_value_for_mlov (\u0027ISSUE_TYPE\u0027,\u0027MS_ISM_ISSUE_IST\u0027,issue_id,\u0027MS ISM Issue Type\u0027)                AS ISSUE_TYPE,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Update Status\u0027, progress_status, 1, NULL, NULL)         AS PROGRESS_STATUS,\n    (ms_apps_utilities.get_user_full_name (reassigned_issue_owner))                                              AS REASSIGNED_ISSUE_OWNER,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Classification Type\u0027, TYPE_OF_EXECUTION, 1, NULL, NULL) AS TYPE_OF_EXECUTION,\n    \"ACTION_EXIST\",\n    (ms_apps_utilities.get_user_full_name (issue_initiator)) AS ISSUE_INITIATOR,\n    \"ACTIVITIES\",\n    (ms_ism_helper.get_comma_sep_display_value (assign_to, \u0027ASN\u0027)) AS ASSIGN_TO,\n    (SELECT stage_name FROM ms_ism_stages WHERE stage_id \u003d previous_stage\n    )                                                         AS PREVIOUS_STAGE,\n    (ms_apps_utilities.get_user_full_name (action_approver1)) AS ACTION_APPROVER1,\n    (ms_apps_utilities.get_user_full_name (action_approver2)) AS ACTION_APPROVER2,\n    \"ACTION_CATEGORY\",\n    \"ACTION_DESCRIPTION\",\n    \"ACTION_DUE_DATE\",\n    (ms_apps_utilities.get_user_full_name (action_owner))                                           AS ACTION_OWNER,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027, action_priority, 1, NULL, NULL) AS ACTION_PRIORITY,\n    \"ACTION_RELATED_TO\",\n    \"ACTION_START_DATE\",\n    \"ACTION_TITLE\",\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Action Item Type\u0027, action_type, 1, NULL, NULL) AS ACTION_TYPE,\n    (ms_apps_utilities.get_user_full_name (issue_approver))                                             AS ISSUE_APPROVER,\n    (SELECT ORG_ENTITY_NAME\n    FROM SI_ORG_ENTITIES_T\n    WHERE ORG_ENTITY_ID \u003d issue_approverorg\n    ) AS ISSUE_APPROVERORG,\n    \"ISSUE_ATTACH_FILES\",\n    \"ISSUE_COMMENTS\",\n    \"ISSUE_DETAILS\",\n    \"ISSUE_DUE_DATE\",\n    \"ISSUE_IDENTIFIED_ON\",\n    (ms_apps_utilities.get_user_full_name (issue_owner)) AS ISSUE_OWNER,\n    (SELECT ORG_ENTITY_NAME\n    FROM SI_ORG_ENTITIES_T\n    WHERE ORG_ENTITY_ID \u003d issue_ownerorg\n    )                                                                                                      AS ISSUE_OWNERORG,\n    (ms_apps_utilities.get_user_full_name (issue_reviewer))                                                AS ISSUE_REVIEWER,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Impact\u0027, ISSUE_RISK_IMPACT, 1, NULL, NULL)        AS ISSUE_RISK_IMPACT,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Likelihood\u0027, ISSUE_RISK_LIKLIHOOD, 1, NULL, NULL) AS ISSUE_RISK_LIKLIHOOD,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Loss Gain\u0027, ISSUE_RISK_LOSS_GAIN, 1, NULL, NULL)  AS ISSUE_RISK_LOSS_GAIN,\n    \"ISSUE_RISK_LOSS_GAIN_AMT\",\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027, issue_risk_priority, 1, NULL, NULL) AS ISSUE_RISK_PRIORITY,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, issue_risk_rating, 1, NULL, NULL)   AS ISSUE_RISK_RATING,\n    \"ISSUE_SOURCE\",\n    \"ISSUE_SOURCE_REF\",\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Source Type\u0027, issue_source_type, 1, NULL, NULL) AS ISSUE_SOURCE_TYPE,\n    ms_ism_helper.get_status_display_name(issue_status)AS ISSUE_STATUS,\n    \"ISSUE_TITLE\",\n    \"DD_PROCESS_CODE\",\n    (SELECT stage_name FROM ms_ism_stages WHERE stage_id \u003d dd_current_stage\n    ) AS DD_CURRENT_STAGE,\n    \"DD_ENTERPRISE_INFO\",\n    (ms_apps_utilities.get_user_full_name (DD_EVENT_USER_NAME))   AS DD_EVENT_USER_NAME,\n    (ms_apps_utilities.get_user_full_name (DD_CURRENT_USER_NAME)) AS DD_CURRENT_USER_NAME,\n    (SUBSTR (TRIM (dd_object_type), 4, 3))                        AS DD_OBJECT_TYPE,\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    A.CREATED_BY AS CREATED_BY,\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"ISSUE_DETAILS_HTM\",\n    \"ACTION_DESCRIPTION_HTM\",\n    \"ISSUE_RESOLUTION_SUMMARY_HTM\",\n    \"C_RTF_1_HTM\",\n    \"C_RTF_2_HTM\",\n    \"ADHOC_SRC_DETAILS_HTM\",\n    \"ACT_WORK_DONE_HTM\",\n    \"ACT_RESULTS_HTM\",\n\t\"CONFIG_FLAG_SETUP\",\n\t\"NO_OF_ACTION\"\n  FROM MS_ISM_ISSUE_V A\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ACTION_DATE_CHANGE_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ACTION_DATE_CHANGE_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ACTION_DATE_CHANGE_DV\" (\"DUMMY1\", \"RTF_ATTACH\", \"STATUS\", \"ISSUE_OWNER\", \"ISSUE_DUE_DATE\", \"OWNED_BY_ORG\", \"ISSUE_APPROVERORG\", \"ORG_ACTION_DUE_DATE\", \"ISSUE_ID\", \"ACTION_ID\", \"ACTION_TITLE\", \"ISSUE_TITLE\", \"PREVIOUS_STAGE\", \"REQ_DUE_DATE\", \"ACTION_APPROVER\", \"ACTION_APPROVER2\", \"ACTION_OWNER\", \"ACTION_PRIORITY\", \"ACTION_SOURCE\", \"ACTION_START_DATE\", \"ACT_ACTION\", \"ACT_COMMENTS\", \"ACT_DUE_DATE\", \"ASSIGN_TO\", \"NO_OF_CHANGES\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"ACTION_DESCRIPTIOON\", \"ACTION_DESCRIPTIOON_HTM\") AS \n  SELECT \"DUMMY1\",\n    \"RTF_ATTACH\",\n    \"STATUS\",\n    (ms_apps_utilities.get_user_full_name (issue_owner)) AS ISSUE_OWNER,\n    \"ISSUE_DUE_DATE\",\n    ms_apps_utilities.get_enterprise_name (OWNED_BY_ORG)      AS OWNED_BY_ORG,\n    ms_apps_utilities.get_enterprise_name (ISSUE_APPROVERORG) AS ISSUE_APPROVERORG,\n    \"ORG_ACTION_DUE_DATE\",\n    \"ISSUE_ID\",\n    \"ACTION_ID\",\n    \"ACTION_TITLE\",\n    \"ISSUE_TITLE\",\n    (SELECT stage_name FROM ms_ism_stages WHERE stage_id \u003d previous_stage\n    ) AS PREVIOUS_STAGE,\n    \"REQ_DUE_DATE\",\n    (ms_apps_utilities.get_user_full_name (action_approver))                                           AS ACTION_APPROVER,\n    (ms_apps_utilities.get_user_full_name (action_approver2))                                          AS ACTION_APPROVER2,\n    \"ACTION_DESCRIPTIOON\",\n    (ms_apps_utilities.get_user_full_name (action_owner))                                              AS ACTION_OWNER,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027, action_priority, 1, NULL, NULL)    AS ACTION_PRIORITY,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Action Source\u0027, ACTION_SOURCE, 1, NULL, NULL) AS ACTION_SOURCE,\n    \"ACTION_START_DATE\",\n    (SELECT action_name FROM ms_ism_actions WHERE action_id \u003d ACT_ACTION\n    ) AS ACT_ACTION,\n    \"ACT_COMMENTS\",\n    \"ACT_DUE_DATE\",\n    (ms_apps_utilities.get_user_full_name (assign_to)) AS ASSIGN_TO,\n    \"NO_OF_CHANGES\",\n    \"DD_PROCESS_CODE\",\n    (SELECT stage_name FROM ms_ism_stages WHERE stage_id \u003d dd_current_stage\n    ) AS DD_CURRENT_STAGE,\n    \"DD_ENTERPRISE_INFO\",\n    (ms_apps_utilities.get_user_full_name (dd_event_user_name))   AS DD_EVENT_USER_NAME,\n    (ms_apps_utilities.get_user_full_name (dd_current_user_name)) AS DD_CURRENT_USER_NAME,\n    (SUBSTR (TRIM (dd_object_type), 4, 3))                        AS DD_OBJECT_TYPE,\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    (ms_apps_utilities.get_user_full_name (created_by)) AS CREATED_BY,\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"ACTION_DESCRIPTIOON_HTM\"\n  FROM MS_ISM_ACTION_DATE_CHANGE_V\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_INTG_ACTION_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_INTG_ACTION_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_INTG_ACTION_PUB_V\" (\"ACTION_ID\", \"ISSUE_ID\", \"ACTION_TITLE\", \"ACTION_STATUS\", \"ACTION_DUE_DATE\", \"CREATED_BY\", \"CREATED_ON\", \"LAST_MODIFIED_BY\", \"LAST_MODIFIED_ON\", \"ACTION_START_DATE\", \"ACTION_CLOSED_ON\", \"OBJECT_TYPE\", \"ACTION_OWNER\", \"ACTION_DELEGATEE\", \"ACTION_APPROVER1\", \"ACTION_APPROVER2\", \"NO_OF_REOPEN\", \"PRIORITY\", \"ACTION_TYPE\", \"PERCENTAGE_COMPLETED\", \"ACTION_DETAILS\", \"WORK_DONE\", \"RESULTS\") AS \n  Select A.Action_Id  As Action_Id,\n A.Issue_Id As Issue_Id,\n    a.action_title  AS action_title,\n    Ms_Ism_Helper.Get_Status_Display_Name(A.Action_Status) As Action_Status,\n    A.action_Due_Date As action_Due_Date,\n    A.Dd_Created_By As Created_By,\n    A.Dd_Created_On As Created_On,\n    A.Dd_Modified_By as Last_Modified_By,\n    A.Dd_Modified_On As Last_Modified_On,\n    A.Action_Start_Date As Action_Start_Date,\n    A.action_Closed_On As action_Closed_On,\n    A.Dd_Object_Type As Object_Type,\n    A.Action_Owner As Action_Owner,\n    A.Action_Delegatee As Action_Delegatee,\n    (Select Decode(Final_Action_Approval, \u00274\u0027,A.Issue_Owner, A.Action_Approver1)\n    From Ms_Ism_Issue Where Issue_Id \u003d A.Issue_Id)As Action_Approver1,\n    A.Action_Approver2 As Action_Approver2,\n    A.Reopen_Action As Reopen_Count,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027, a.action_priority, 1, NULL, NULL)  AS priority,\n    Ms_Apps_Utilities.Get_Display_Value (100000, \u0027MS ISM Action Item Type\u0027,\n      (Select Action_Type From Ms_Ism_Issue_Iap Where Issue_Action_Id\u003dA.Action_Id), 1, Null, Null) As Action_Type,\n    A.Remove_Delegation      As Percentage_Completed,\n    Regexp_Replace (A.action_details, \u0027\u003c[^\u003e]+\u003e\u0027, Null)      As Action_Details,\n    Regexp_Replace (A.Action_Work_Done, \u0027\u003c[^\u003e]+\u003e\u0027, Null)      As Work_Done,\n    REGEXP_REPLACE (a.ACTION_RESULTS, \u0027\u003c[^\u003e]+\u003e\u0027, NULL)        AS Results\n  From Ms_Ism_action A\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ISSUE_ATC": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_ATC\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ISSUE_ATC\" (\"ATC_COMMENTS\", \"ATC_TITLE\", \"ATC_COMPLIANCE\", \"ISSUE_ID\", \"DD_STATUS_FLAG\") AS \n  SELECT rel_comments atc_comments,\n    tgt_obj_id atc_title,\n    tgt_obj_name atc_compliance,\n    rel_source_id issue_id,\n    \u0027Y\u0027 dd_status_flag\n  From Ms_Ism_Orb_Relation_Orb\n  WHERE tgt_obj_type \u003d \u0027Areas of Compliance\u0027\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ISSUE_TITLE": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_TITLE\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_ISM_ISSUE_TITLE\" as object\n(  issue_ownerorg varchar2(4000),\n    issue_approverorg varchar2(4000),\n    issue_id  varchar2(255),\n    issue_title varchar2(1000))   \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_006_ACT_LIST_REPORT_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_006_ACT_LIST_REPORT_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_006_ACT_LIST_REPORT_V\" (\"ACTION_TITLE\", \"ACTION_STATUS\", \"ACTION_OWNER\", \"PRIORITY\", \"ACTION_DUE_DATE\", \"ACTION_COMPLETED_ON\", \"ISSUE_TITLE\", \"ISSUE_OWNER\", \"PERCENTAGE_COMPLETED\", \"PID\", \"INSTANCEID\", \"OWNER_ORGANIZATION\", \"ISSUE_ID\", \"APPROVER_ORG\") AS \n  SELECT\n    (SELECT \u0027\u003ca target\u003d\"_self\" href\u003d\"javascript\u0027\n      || CHR (58)\n      || \u0027callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027\n      --|| ms_apps_utilities.get_infolet_id (\u0027MS_ISM_ACTION\u0027)\n      ||ms_ism_utilities.get_ism_infolet_id(\u0027ACTION\u0027)\n      || CHR (38) ||\u0027amp;\u0027\n      || \u0027proc\u003d\u0027\n      || a.dd_process_instance_id\n      || CHR (38) ||\u0027amp;\u0027\n      || \u0027flag\u003d1\u0027\n      || CHR (38) ||\u0027amp;\u0027\n      || \u0027emd\u003d1\u0027\n      || CHR (38) ||\u0027amp;\u0027\n      || \u0027x_edit_mode\u003dYes\u0027\n      || CHR (38) ||\u0027amp;\u0027\n      || \u0027wrapper\u003dno\u0027\u0027)\"\u003e\u0027\n      || REPLACE(REPLACE(a.action_title,\u0027\u003c\u0027, \u0027\u0026\u0027||\u0027lt;\u0027), \u0027\u003e\u0027, \u0027\u0026\u0027||\u0027gt;\u0027) \n      || \u0027\u003c/a\u003e\u0027\n    FROM DUAL\n    )                                                                                                    AS action_title,\n    ms_ism_helper.get_status_display_name(a.action_status)                                               AS action_status,\n    ms_apps_utilities.get_user_full_name (a.action_owner)                                                AS action_owner,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, a.action_priority, 1, NULL, NULL) AS priority,\n    a.action_due_date                                                                                    AS action_due_date,\n    NULL                                                                                                 AS action_completed_on,\n    a.issue_title                                                                                        AS issue_title,\n    ms_apps_utilities.get_user_full_name (a.issue_owner)                                                 AS issue_owner,\n    --a.action_per_complte AS percentage_completed,\n    a.remove_delegation      AS percentage_completed, --remove delegation has been used to capture percentage complete value as it will get nullified on chnage of actions\n    a.dd_process_instance_id AS pid,\n    a.dd_instance_id         AS instanceid,\n    b.ISSUE_OWNERORG         AS owner_organization,\n    a.issue_id               AS issue_id,\n    b.ISSUE_APPROVERORG      AS approver_org\n    --start of bug - 39313 , c.IAR_TITLE as RISK End of Bug - 39313\n  FROM ms_ism_action a,\n    ms_ism_issue b              -- start of bug - 39313, MS_ISM_ISSUE_IAR c End of Bug - 39313\n  WHERE a.issue_id      \u003d b.issue_id -- start of bug - 39313 AND b.issue_id \u003d c.issue_id End of Bug - 39313\n  AND b.issue_status NOT IN (\u00271\u0027, \u0027Issue Reviewed\u0027, \u00275\u0027, \u00277\u0027, \u00276\u0027)\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ACTION_ID_S": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ACTION_ID_S\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"NOVT_DEV\".\"MS_ISM_ACTION_ID_S\"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 100000 NOCACHE  NOORDER  NOCYCLE \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_RESPONSE": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_RESPONSE\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_ISM_RESPONSE\" AS OBJECT\n   (code VARCHAR2 (10), issue_id VARCHAR2 (100), errors varchar2_array)   \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ISSUE_ICO": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_ICO\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ISSUE_ICO\" (\"ICO_COMMENTS\", \"ICO_TITLE\", \"ICO_CORE\", \"ISSUE_ID\", \"DD_STATUS_FLAG\") AS \n  SELECT rel_comments ico_comments,\n    tgt_obj_id ico_title,\n    tgt_obj_name ico_core,\n    rel_source_id issue_id,\n    \u0027Y\u0027 dd_status_flag\n  FROM ms_ism_orb_relation_orb\n  WHERE tgt_obj_type \u003d \u0027Core Objects\u0027\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ISSUES_DASHBOARD_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUES_DASHBOARD_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ISSUES_DASHBOARD_V\" (\"ISSUE_ID\", \"ISSUE_TITLE\", \"ISSUE_STATUS\", \"ISSUE_STATUS_ACT\", \"ISSUE_RISK_RATING_STR\", \"ISSUE_RISK_RATING\", \"ISSUE_RISK_PRIORITY_STR\", \"ISSUE_RISK_PRIORITY\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"DD_OBJECT_TYPE\", \"ISSUE_OWNERORG\", \"ISSUE_APPROVERORG\") AS \n  SELECT a.issue_id,\n    a.issue_title ,\n    DECODE (a.issue_status ,\u002711\u0027, \u0027Open\u0027 ,\u002710\u0027, \u0027Closed\u0027 ,\u00277\u0027, \u0027Cancelled\u0027 ,\u00274\u0027, \u0027Open\u0027 ,\u00276\u0027, \u0027Open\u0027 ,\u002717\u0027, \u0027Open\u0027 ,\u00275\u0027, \u0027Open\u0027 ,\u002714\u0027, \u0027Open\u0027 ,\u002721\u0027,\u0027Open\u0027,\u0027Open\u0027 ) issue_status ,\n    ms_ism_helper.get_status_display_name(a.issue_status)                                                AS issue_status_act ,\n    a.issue_risk_rating                                                                                  AS issue_risk_rating_str ,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, issue_risk_rating, 1, NULL, NULL) AS issue_risk_rating ,\n    a.issue_risk_priority                                                                                AS issue_risk_priority_str ,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027, issue_risk_priority, 1, NULL, NULL)  AS issue_risk_priority,\n    owner_organizations ,\n    restrict_access_to,\n    dd_object_type,\n    a.ISSUE_OWNERORG,\n    a.ISSUE_APPROVERORG\n  FROM ms_ism_issue a\n  WHERE\n    issue_status NOT IN (\u00271\u0027,\u00275\u0027,\u00276\u0027)\n    --Start of bug 40919\n    --AND EXISTS (SELECT previous_stage\n    --  FROM ms_ism_issue_v\n    --  WHERE issue_id \u003d a.issue_id AND dd_current_stage \u003d 4)\n    --AND a.issue_action NOT IN (\u00271\u0027,\u00273\u0027)\n  AND a.reached_manage \u003d \u0027Y\u0027\n    --End of bug 40919;;\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_RISK_OBJ": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_RISK_OBJ\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_ISM_RISK_OBJ\" AS OBJECT (\n  Object_Id VARCHAR2(255),\n  object_name VARCHAR2(255)\n); \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_INTEGRATION_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_INTEGRATION_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_INTEGRATION_PUB_V\" (\"ISSUE_ID\", \"ISSUE_TITLE\", \"ISSUE_STATUS\", \"ISSUE_DUE_DATE\", \"ISSUE_SOURCE_TYPE\", \"ISSUE_SOURCE_REF\", \"CREATED_ON\", \"ISSUE_CLOSED_ON\", \"OBJECT_TYPE\", \"RELATED_RISKS\", \"RELATED_AREA_OF_COMP\", \"RELATED_CONTROLS\", \"RELATED_CORE_PROCESSES\", \"RELATED_ORGS\") AS \n  SELECT a.issue_id                                                                                                                                    AS issue_id,\n    a.issue_title                                                                                                                                      AS issue_title,\n    ms_ism_helper.get_status_display_name(a.issue_status)                                                                                              AS issue_status,\n    a.issue_due_date                                                                                                                                   AS issue_due_date,\n    DECODE (a.issue_source_type, 1, \u0027Audit\u0027, 2, \u0027Test Execution\u0027, 3, \u0027Risk Assessment\u0027, 4, \u0027Surveys\u0027, 5, \u0027Internal Loss Event\u0027, 6, \u0027GRC Intelligence\u0027) AS issue_source_type,\n    a.issue_source_ref                                                                                                                                 AS issue_source_ref,\n    a.dd_created_on                                                                                                                                    AS created_on,\n    a.issue_closed_on                                                                                                                                  AS issue_closed_on,\n    a.dd_object_type                                                                                                                                   AS object_type\n /*   (SELECT XMLAGG (XMLELEMENT (\"elem\", r.iar_title\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_issue_iar r\n    WHERE r.issue_id \u003d a.issue_id\n    AND iar_title   IS NOT NULL\n    ) AS related_risks,\n    (SELECT XMLAGG (XMLELEMENT (\"elem\", r.atc_title\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_issue_atc r\n    WHERE r.issue_id \u003d a.issue_id\n    AND atc_title   IS NOT NULL\n    ) AS related_area_of_comp,\n    (SELECT XMLAGG (XMLELEMENT (\"elem\", r.iac_title\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_issue_iac r\n    WHERE r.issue_id \u003d a.issue_id\n    AND iac_title   IS NOT NULL\n    ) AS related_controls,\n    (SELECT XMLAGG (XMLELEMENT (\"elem\", r.ico_title\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_issue_ico r\n    WHERE r.issue_id \u003d a.issue_id\n    AND ico_title   IS NOT NULL\n    ) AS related_core_processes,\n    (SELECT XMLAGG (XMLELEMENT (\"elem\", r.iao_title\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_issue_iao r\n    WHERE r.issue_id \u003d a.issue_id\n    AND iao_title   IS NOT NULL\n    ) AS related_orgs*/\n     ,(SELECT XMLAGG (XMLELEMENT (\"elem\", r.tgt_obj_name\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_orb_relation_orb r\n    WHERE r.rel_source_id \u003d a.issue_id\n    AND r.tgt_obj_type    \u003d \u0027Risk(s)\u0027\n    ) AS related_risks,\n    (SELECT XMLAGG (XMLELEMENT (\"elem\", r.tgt_obj_name\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_orb_relation_orb r\n    WHERE r.rel_source_id \u003d a.issue_id\n    AND r.tgt_obj_type    \u003d \u0027Area(s) of Compliance\u0027\n    ) AS related_area_of_comp,\n    (SELECT XMLAGG (XMLELEMENT (\"elem\", r.tgt_obj_name\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_orb_relation_orb r\n    WHERE r.rel_source_id \u003d a.issue_id\n    AND r.tgt_obj_type    \u003d \u0027Control(s)\u0027\n    ) AS related_controls,\n    (SELECT XMLAGG (XMLELEMENT (\"elem\", r.tgt_obj_name\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_orb_relation_orb r\n    WHERE r.rel_source_id \u003d a.issue_id\n    AND r.tgt_obj_type    \u003d \u0027Core Object(s)\u0027\n    ) AS related_core_processes,\n    (SELECT XMLAGG (XMLELEMENT (\"elem\", r.tgt_obj_name\n      || \u0027,\u0027)). EXTRACT (\u0027/elem/text()\u0027).getclobval ()\n    FROM ms_ism_orb_relation_orb r\n    WHERE r.rel_source_id \u003d a.issue_id\n    AND r.tgt_obj_type    \u003d \u0027Organization(s)\u0027\n    ) AS related_orgs\n  FROM ms_ism_issue a\n  WHERE NVL (issue_source_type, \u00270\u0027) \u003c\u003e 0\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ISSUE_IAO": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_IAO\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ISSUE_IAO\" (\"IAO_COMMENTS\", \"IAO_TITLE\", \"IAO_ORG\", \"ISSUE_ID\", \"DD_STATUS_FLAG\") AS \n  SELECT rel_comments iao_comments,\n    tgt_obj_id iao_title,\n    tgt_obj_name iao_org,\n    rel_source_id issue_id,\n    \u0027Y\u0027 dd_status_flag\n  FROM ms_ism_orb_relation_orb\n  WHERE tgt_obj_type \u003d \u0027Organizations\u0027\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ACTION_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ACTION_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ACTION_DV\" (\"DOC_ACTION_ID\", \"FIELD_VALIDATION\", \"ISSUE_STATUS\", \"EDITABILITY_JSON\", \"UPDATE_ACTION\", \"ACTION_CLOSED_ON\", \"REOPEN_ACTION\", \"ACTION_LEGACY_ID\", \"C_TEXT_5\", \"C_TEXT_4\", \"C_TEXT_3\", \"C_TEXT_2\", \"C_TEXT_1\", \"C_RTF_2\", \"C_RTF_1\", \"C_DATE_5\", \"C_DATE_4\", \"C_DATE_3\", \"C_DATE_2\", \"C_DATE_1\", \"DELEGATED\", \"ACTION_LOGIN\", \"INDEPENDENT_ACT\", \"RESTRICT_ACCESS_TO\", \"OWNER_ORGANIZATIONS\", \"CUSTOM_FIELD2\", \"CUSTOM_FIELD1\", \"RTF_ATTACH\", \"ISSUE_PID\", \"ISSUE_METRIC_ID\", \"FINAL_ACTION_APPROVED\", \"ORIG_ACTION_DUE_DT\", \"CONFIG_FLAG\", \"CONFIG_FLAG_DUMMY\", \"ASSIGNEE\", \"ASSIGN_TO\", \"DOC_ID\", \"PREVIOUS_STAGE\", \"ISSUE_ID\", \"NO_OF_EXTENSIONS\", \"DOC_ATTACHMENTS\", \"ACTION_COMMENTS\", \"ACTION_PER_COMPLTE\", \"ACTION_REVIEWER\", \"ACT_ACTION\", \"REQUESTED_DUE_DATE\", \"REMOVE_DELEGATION\", \"ACTION_DELEGATEE\", \"ACTION_STATUS\", \"DELEGATEE_COMMENTS\", \"DOCUMENT_NAME\", \"FILE_DESCRIPTION\", \"ISSUE_APPROVER_ORG\", \"ISSUE_OWNER_ORG\", \"ISSUE_TITLE\", \"ISSUE_SOURCE_TYPE\", \"ISSUE_SOURCE\", \"ISSUE_OWNER\", \"ISSUE_DUE_BY\", \"ISSUE_DETAILS\", \"ACTION_WORK_DONE\", \"ACTION_TITLE\", \"ACTION_START_DATE\", \"ACTION_SOURCE_TYPE\", \"ACTION_SOURCE\", \"ACTION_RESULTS\", \"ACTION_PRIORITY\", \"ACTION_OWNER\", \"ACTION_ID\", \"ACTION_DUE_DATE\", \"ACTION_DETAILS\", \"ACTION_ATTACHMENT\", \"ACTION_APPROVER2\", \"ACTION_APPROVER1\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"ACTION_DETAILS_HTM\", \"ACTION_RESULTS_HTM\", \"ACTION_WORK_DONE_HTM\", \"ISSUE_DETAILS_HTM\", \"C_RTF_1_HTM\", \"C_RTF_2_HTM\", \"ACTION_CATEGORY\", \"CONFIG_FLAG_SETUP\") AS \n  SELECT \"DOC_ACTION_ID\",\n  \"FIELD_VALIDATION\",\n    \"ISSUE_STATUS\",\n    \"EDITABILITY_JSON\",\n    \"UPDATE_ACTION\",\n    \"ACTION_CLOSED_ON\",\n    \"REOPEN_ACTION\",\n    \"ACTION_LEGACY_ID\",\n    \"C_TEXT_5\",\n    \"C_TEXT_4\",\n    \"C_TEXT_3\",\n    \"C_TEXT_2\",\n    \"C_TEXT_1\",\n    \"C_RTF_2\",\n    \"C_RTF_1\",\n    \"C_DATE_5\",\n    \"C_DATE_4\",\n    \"C_DATE_3\",\n    \"C_DATE_2\",\n    \"C_DATE_1\",\n    \"DELEGATED\",\n    \"ACTION_LOGIN\",\n    \"INDEPENDENT_ACT\",\n    \"RESTRICT_ACCESS_TO\",\n    ms_apps_utilities.get_enterprise_name (OWNER_ORGANIZATIONS) AS OWNER_ORGANIZATIONS,\n    (ms_apps_utilities.get_user_full_name (CUSTOM_FIELD2))      AS CUSTOM_FIELD2,\n    (ms_apps_utilities.get_user_full_name (CUSTOM_FIELD1))      AS CUSTOM_FIELD1,\n    \"RTF_ATTACH\",\n    \"ISSUE_PID\",\n    \"ISSUE_METRIC_ID\",\n    ms_apps_utilities. get_display_value (100000, \u0027MS ISM Final Action Approval\u0027, FINAL_ACTION_APPROVED, 1, NULL, NULL) AS FINAL_ACTION_APPROVED,\n    \"ORIG_ACTION_DUE_DT\",\n    \"CONFIG_FLAG\",\n    \"CONFIG_FLAG_DUMMY\",\n    (ms_apps_utilities.get_user_full_name (assignee))  AS ASSIGNEE,\n    (ms_apps_utilities.get_user_full_name (assign_to)) AS ASSIGN_TO,\n    \"DOC_ID\",\n    (SELECT stage_name FROM ms_ism_stages WHERE stage_id \u003d previous_stage\n    ) AS PREVIOUS_STAGE,\n    \"ISSUE_ID\",\n    \"NO_OF_EXTENSIONS\",\n    \"DOC_ATTACHMENTS\",\n    \"ACTION_COMMENTS\",\n    \"ACTION_PER_COMPLTE\",\n    (ms_apps_utilities.get_user_full_name (action_reviewer)) AS ACTION_REVIEWER,\n    (SELECT action_name FROM ms_ism_actions WHERE action_id \u003d ACT_ACTION\n    ) AS ACT_ACTION,\n    \"REQUESTED_DUE_DATE\",\n    \"REMOVE_DELEGATION\",\n    (ms_apps_utilities.get_user_full_name (action_delegatee)) AS ACTION_DELEGATEE,\n    \"ACTION_STATUS\",\n    \"DELEGATEE_COMMENTS\",\n    \"DOCUMENT_NAME\",\n    \"FILE_DESCRIPTION\",\n    ms_apps_utilities.get_enterprise_name (ISSUE_APPROVER_ORG) AS ISSUE_APPROVER_ORG,\n    ms_apps_utilities.get_enterprise_name (ISSUE_OWNER_ORG)    AS ISSUE_OWNER_ORG,\n    \"ISSUE_TITLE\",\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Source Type\u0027, ISSUE_SOURCE_TYPE, 1, NULL, NULL) AS ISSUE_SOURCE_TYPE,\n    \"ISSUE_SOURCE\",\n    (ms_apps_utilities.get_user_full_name (issue_owner)) AS ISSUE_OWNER,\n    \"ISSUE_DUE_BY\",\n    \"ISSUE_DETAILS\",\n    \"ACTION_WORK_DONE\",\n    \"ACTION_TITLE\",\n    \"ACTION_START_DATE\",\n    \"ACTION_SOURCE_TYPE\",\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Action Source\u0027, ACTION_SOURCE, 1, NULL, NULL) AS ACTION_SOURCE,\n    \"ACTION_RESULTS\",\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, action_priority, 1, NULL, NULL) AS ACTION_PRIORITY,\n    (ms_apps_utilities.get_user_full_name (action_owner))                                              AS ACTION_OWNER,\n    \"ACTION_ID\",\n    \"ACTION_DUE_DATE\",\n    \"ACTION_DETAILS\",\n    \"ACTION_ATTACHMENT\",\n    (ms_apps_utilities.get_user_full_name (action_approver2)) AS ACTION_APPROVER2,\n    (ms_apps_utilities.get_user_full_name (action_approver1)) AS ACTION_APPROVER1,\n    \"DD_PROCESS_CODE\",\n    (SELECT stage_name FROM ms_ism_stages WHERE stage_id \u003d dd_current_stage\n    ) AS DD_CURRENT_STAGE,\n    \"DD_ENTERPRISE_INFO\",\n    (ms_apps_utilities.get_user_full_name (DD_EVENT_USER_NAME))   AS DD_EVENT_USER_NAME,\n    (ms_apps_utilities.get_user_full_name (DD_CURRENT_USER_NAME)) AS DD_CURRENT_USER_NAME,\n    (SUBSTR (TRIM (dd_object_type), 4, 3))                        AS DD_OBJECT_TYPE,\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    \"CREATED_BY\",\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"ACTION_DETAILS_HTM\",\n    \"ACTION_RESULTS_HTM\",\n    \"ACTION_WORK_DONE_HTM\",\n    \"ISSUE_DETAILS_HTM\",\n    \"C_RTF_1_HTM\",\n    \"C_RTF_2_HTM\",\n\t\"ACTION_CATEGORY\",\n    \"CONFIG_FLAG_SETUP\"\n  FROM MS_ISM_ACTION_V A\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
