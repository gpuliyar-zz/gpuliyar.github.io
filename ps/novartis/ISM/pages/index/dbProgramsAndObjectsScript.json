dbProgramsAndObjectsScriptData = {
  "MS_ISM_001_ISS_LIST_REP_NEW_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_001_ISS_LIST_REP_NEW_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_001_ISS_LIST_REP_NEW_V\" (\"ISSUE_TITLE\", \"ISSUE_STATUS\", \"ISSUE_OWNER\", \"ISSUE_RATING\", \"PRIORITY\", \"SOURCE_TYPE\", \"ISSUE_DUE_DATE\", \"ISSUE_INITIATOR\", \"FIRST_IDENTIFIED_ON\", \"ISSUE_ID\", \"ISSUE_DETAILS\", \"NO_OF_ACTIONS\", \"ISSUE_OWNER_ORG\", \"ISSUE_APPROVERORG\", \"OWNERORG\", \"APPROVERORG\", \"PROGRESS_STATUS\", \"PID\", \"INSTANCEID\", \"ORG\", \"AREA_OF_COMPLIANCE\", \"CONTROLS\", \"RISKS\", \"CORE_OBJECTS\") AS \n  SELECT (SELECT    \u0027\u003ca target\u003d\"_self\" href\u003d\"javascript\u0027\n                  || CHR (58)\n                  || \u0027callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027\n                  || ms_apps_utilities.get_infolet_id (\u0027MS_ISM_ISSUE\u0027)\n                  || CHR (38)\n                  || \u0027proc\u003d\u0027\n                  || c.process_instance_id\n                  || CHR (38)\n                  || \u0027flag\u003d1\u0027\n                  || CHR (38)\n                  || \u0027emd\u003d1\u0027\n                  || CHR (38)\n                  || \u0027x_edit_mode\u003dYes\u0027\n                  || CHR (38)\n                  || \u0027wrapper\u003dno\u0027\u0027)\"\u003e\u0027\n                  || a.issue_title\n                  || \u0027\u003c/a\u003e\u0027\n             FROM DUAL)\n             AS issue_title,\n          a.issue_status AS issue_status,\n          ms_apps_utilities.get_user_full_name (a.issue_owner) AS issue_owner,\n          ms_apps_utilities.get_display_value (100000,\n                                               \u0027MS ISM Risk Rating\u0027,\n                                               a.issue_risk_rating,\n                                               1,\n                                               NULL,\n                                               NULL)\n             AS issue_rating,\n          ms_apps_utilities.get_display_value (100000,\n                                               \u0027MS ISM Priority\u0027,\n                                               a.issue_risk_priority,\n                                               1,\n                                               NULL,\n                                               NULL)\n             AS priority,\n          ms_apps_utilities.get_display_value (100000,\n                                               \u0027MS ISM Source Type\u0027,\n                                               a.issue_source_type,\n                                               1,\n                                               NULL,\n                                               NULL)\n             AS source_type,\n          a.issue_due_date AS issue_due_date,\n          ms_apps_utilities.get_user_full_name (a.dd_created_by)\n             AS issue_initiator,\n          a.issue_identified_on AS first_identified_on,\n          a.issue_id AS issue_id,\n          REGEXP_REPLACE (a.issue_details, \u0027\u003c[^\u003e]+\u003e\u0027, NULL) AS issue_details,\n          (SELECT COUNT (1)\n             FROM ms_ism_action\n            WHERE issue_id \u003d a.issue_id)\n             AS no_of_actions,\n          (SELECT org_entity_name\n             FROM si_org_entities_t\n            WHERE org_entity_id \u003d a.issue_ownerorg)\n             AS issue_owner_org,\n          (SELECT org_entity_name\n             FROM si_org_entities_t\n            WHERE org_entity_id \u003d a.issue_approverorg)\n             AS issue_approverorg,\n          a.issue_ownerorg,\n          a.issue_approverorg,\n          ms_apps_utilities.get_display_value (100000,\n                                               \u0027MS ISM Update Status\u0027,\n                                               a.progress_status,\n                                               1,\n                                               NULL,\n                                               NULL)\n             AS progress_status,\n          a.dd_process_instance_id AS pid,\n          a.dd_instance_id AS instanceid,\n          ms_ism_helper.get_comma_seprated_value (\u0027IAO_TITLE\u0027,\n                                                  \u0027MS_ISM_ISSUE_IAO\u0027,\n                                                  a.issue_id,\n                                                  \u0027IAO\u0027)\n             AS org,\n          ms_ism_helper.get_comma_seprated_value (\u0027ATC_TITLE\u0027,\n                                                  \u0027MS_ISM_ISSUE_ATC\u0027,\n                                                  a.issue_id,\n                                                  \u0027ATC\u0027)\n             AS area_of_compliance,\n          ms_ism_helper.get_comma_seprated_value (\u0027IAC_TITLE\u0027,\n                                                  \u0027MS_ISM_ISSUE_IAC\u0027,\n                                                  a.issue_id,\n                                                  \u0027IAC\u0027)\n             AS controls,\n          ms_ism_helper.get_comma_seprated_value (\u0027IAR_TITLE\u0027,\n                                                  \u0027MS_ISM_ISSUE_IAR\u0027,\n                                                  a.issue_id,\n                                                  \u0027IAR\u0027)\n             AS risks,\n          ms_ism_helper.get_comma_seprated_value (\u0027ICO_TITLE\u0027,\n                                                  \u0027MS_ISM_ISSUE_ICO\u0027,\n                                                  a.issue_id,\n                                                  \u0027ICO\u0027)\n             AS core_objects\n     FROM ms_ism_issue a, ms_ism_issue_v c\n    WHERE     a.issue_id \u003d c.issue_id\n       AND c.instance_id \u003d a.dd_instance_id\n          AND c.instance_rec_num \u003d 1\n          AND a.issue_status NOT IN\n                 (\u0027New\u0027,\n                  \u0027Issue Reviewed\u0027,\n                  \u0027Pending Review\u0027,\n                  \u0027Pending Approval\u0027)\n          -- start of fixing of bug - 40918\n          AND EXISTS\n                 (SELECT previous_stage\n                    FROM ms_ism_issue_v\n                   WHERE issue_id \u003d a.issue_id AND dd_current_stage \u003d 4)\n          AND a.issue_action NOT IN (\u00271\u0027, \u00273\u0027)\n-- end of fixing of bug - 40918;;\n \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_001_ISSUE_LIST_REPORT_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_001_ISSUE_LIST_REPORT_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_001_ISSUE_LIST_REPORT_V\" (\"ISSUE_TITLE\", \"ISSUE_STATUS\", \"ISSUE_OWNER\", \"ISSUE_RATING\", \"PRIORITY\", \"SOURCE_TYPE\", \"ISSUE_DUE_DATE\", \"ISSUE_INITIATOR\", \"FIRST_IDENTIFIED_ON\", \"ISSUE_ID\", \"ISSUE_DETAILS\", \"NO_OF_ACTIONS\", \"ISSUE_OWNER_ORG\", \"ISSUE_APPROVERORG\", \"OWNERORG\", \"APPROVERORG\", \"PROGRESS_STATUS\", \"PID\", \"INSTANCEID\", \"ORG\", \"AREA_OF_COMPLIANCE\", \"CONTROLS\", \"RISKS\", \"CORE_OBJECTS\", \"EXCEPTION_TYPE\") AS \n  SELECT\n    (SELECT \u0027\u003ca target\u003d\"_self\" href\u003d\"javascript\u0027\n      || CHR (58)\n      || \u0027callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027\n      --|| ms_apps_utilities.get_infolet_id (\u0027MS_ISM_ISSUE\u0027)\n      || ms_ism_utilities.get_ism_infolet_id (\u0027ISSUE\u0027)\n      || CHR (38)\n      || \u0027proc\u003d\u0027\n      || a.dd_process_instance_id\n      || CHR (38)\n      || \u0027flag\u003d1\u0027\n      || CHR (38)\n      || \u0027emd\u003d1\u0027\n      || CHR (38)\n      || \u0027x_edit_mode\u003dYes\u0027\n      || CHR (38)\n      || \u0027wrapper\u003dno\u0027\u0027)\"\u003e\u0027\n      || a.issue_title\n      || \u0027\u003c/a\u003e\u0027\n    FROM DUAL\n    )                                                                                                      AS issue_title,\n    a.issue_status                                                                                         AS issue_status,\n    ms_apps_utilities.get_user_full_name (a.issue_owner)                                                   AS issue_owner,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, a.issue_risk_rating, 1, NULL, NULL) AS issue_rating,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027, a.issue_risk_priority, 1, NULL, NULL)  AS priority,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Source Type\u0027, a.issue_source_type, 1, NULL, NULL) AS source_type,\n    a.issue_due_date                                                                                       AS issue_due_date,\n    ms_apps_utilities.get_user_full_name (a.dd_created_by)                                                 AS issue_initiator,\n    a.issue_identified_on                                                                                  AS first_identified_on,\n    a.issue_id                                                                                             AS issue_id,\n    REGEXP_REPLACE (a.issue_details, \u0027\u003c[^\u003e]+\u003e\u0027, NULL)                                                      AS issue_details,\n    (SELECT COUNT (1) FROM ms_ism_action WHERE issue_id \u003d a.issue_id\n    ) AS no_of_actions,\n    (SELECT org_entity_name\n    FROM si_org_entities_t\n    WHERE org_entity_id \u003d a.issue_ownerorg\n    ) AS issue_owner_org,\n    (SELECT org_entity_name\n    FROM si_org_entities_t\n    WHERE org_entity_id \u003d a.issue_approverorg\n    ) AS issue_approverorg,\n    a.issue_ownerorg,\n    a.issue_approverorg,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Update Status\u0027, a.progress_status, 1, NULL, NULL) AS progress_status,\n    a.dd_process_instance_id                                                                               AS pid,\n    a.dd_instance_id                                                                                       AS instanceid,\n    /*ms_ism_helper.get_comma_seprated_value (\u0027IAO_TITLE\u0027, \u0027MS_ISM_ISSUE_IAO\u0027, a.issue_id, \u0027IAO\u0027)            AS org,\n    ms_ism_helper.get_comma_seprated_value (\u0027ATC_TITLE\u0027, \u0027MS_ISM_ISSUE_ATC\u0027, a.issue_id, \u0027ATC\u0027)            AS area_of_compliance,\n    ms_ism_helper.get_comma_seprated_value (\u0027IAC_TITLE\u0027, \u0027MS_ISM_ISSUE_IAC\u0027, a.issue_id, \u0027IAC\u0027)            AS controls,\n    ms_ism_helper.get_comma_seprated_value (\u0027IAR_TITLE\u0027, \u0027MS_ISM_ISSUE_IAR\u0027, a.issue_id, \u0027IAR\u0027)            AS risks,\n    ms_ism_helper.get_comma_seprated_value (\u0027ICO_TITLE\u0027, \u0027MS_ISM_ISSUE_ICO\u0027, a.issue_id, \u0027ICO\u0027)            AS core_objects*/\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Organization(s)\u0027,a.issue_id)  AS org,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Area(s) of Compliance\u0027, a.issue_id) AS area_of_compliance,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Control(s)\u0027, a.issue_id) AS controls,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Risk(s)\u0027, a.issue_id) AS risks,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Core Object(s)\u0027, a.issue_id) AS core_objects,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Classification Type\u0027, a.TYPE_OF_EXECUTION, 1, NULL, NULL) AS EXCEPTION_TYPE\n  FROM ms_ism_issue a\n    --     , ms_ism_issue_v c\n    --    WHERE     a.issue_id \u003d c.issue_id\n    --          AND c.instance_id \u003d (SELECT MAX (instance_id)\n    --                                 FROM ms_ism_issue_v\n    --                                WHERE issue_id \u003d a.issue_id)\n    --          AND c.instance_rec_num \u003d 1\n    --          AND\n  WHERE a.issue_status NOT IN (\u0027New\u0027, \u0027Issue Reviewed\u0027, \u0027Pending Review\u0027, \u0027Pending Approval\u0027) -- start of fixing of bug - 40918\n    --          AND EXISTS\n    --                 (SELECT previous_stage\n    --                    FROM ms_ism_issue_v\n    --                   WHERE issue_id \u003d a.issue_id AND dd_current_stage \u003d 4)\n  AND a.reached_manage \u003d \u0027Y\u0027\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ISSUE_DATE_CHANGE_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_DATE_CHANGE_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ISSUE_DATE_CHANGE_DV\" (\"DUMMY1\", \"RTF_ATTACH\", \"STATUS\", \"PREVIOUS_STAGE\", \"ASSIGN_TO\", \"REQ_DUE_DATE\", \"ORG_ISSUE_DUE_DATE\", \"ACT_COMMENTS\", \"ACT_ACTION\", \"NO_OF_CHANGES\", \"OWNED_BY_ORG\", \"ISSUE_TITLE\", \"ISSUE_ID\", \"ISSUE_DESCRIPTION\", \"ISSUE_SOURCE_TYPE\", \"ISSUE_DUE_DATE\", \"ISSUE_IDENTIFIED_ON\", \"ISSUE_PRIORITY\", \"ISSUE_SOURCE\", \"ISSUE_OWNER\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"ISSUE_DESCRIPTION_HTM\") AS \n  SELECT \"DUMMY1\",\n    \"RTF_ATTACH\",\n    \"STATUS\",\n    (SELECT stage_name FROM ms_ism_stages WHERE stage_id \u003d previous_stage\n    )                                                  AS PREVIOUS_STAGE,\n    (ms_apps_utilities.get_user_full_name (assign_to)) AS ASSIGN_TO,\n    \"REQ_DUE_DATE\",\n    \"ORG_ISSUE_DUE_DATE\",\n    \"ACT_COMMENTS\",\n    (SELECT action_name FROM ms_ism_actions WHERE action_id \u003d ACT_ACTION\n    ) AS ACT_ACTION,\n    \"NO_OF_CHANGES\",\n    ms_apps_utilities.get_enterprise_name (OWNED_BY_ORG) AS OWNED_BY_ORG,\n    \"ISSUE_TITLE\",\n    \"ISSUE_ID\",\n    \"ISSUE_DESCRIPTION\",\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Source Type\u0027, issue_source_type, 1, NULL, NULL) AS ISSUE_SOURCE_TYPE,\n    \"ISSUE_DUE_DATE\",\n    \"ISSUE_IDENTIFIED_ON\",\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, issue_priority, 1, NULL, NULL) AS ISSUE_PRIORITY,\n    \"ISSUE_SOURCE\",\n    (ms_apps_utilities.get_user_full_name (issue_owner)) AS ISSUE_OWNER,\n    \"DD_PROCESS_CODE\",\n    (SELECT stage_name FROM ms_ism_stages WHERE stage_id \u003d dd_current_stage\n    ) AS DD_CURRENT_STAGE,\n    \"DD_ENTERPRISE_INFO\",\n    (ms_apps_utilities.get_user_full_name (dd_event_user_name))   AS DD_EVENT_USER_NAME,\n    (ms_apps_utilities.get_user_full_name (dd_current_user_name)) AS DD_CURRENT_USER_NAME,\n    (SUBSTR (TRIM (dd_object_type), 4, 3))                        AS DD_OBJECT_TYPE,\n    \"INSTANCE_REC_NUM\",\n    \"INSTANCE_ID\",\n    \"METRIC_ID\",\n    \"METRIC_RUN_DATE\",\n    \"METRIC_COMPLETED_DATE\",\n    \"LATEST_FLAG\",\n    (ms_apps_utilities.get_user_full_name (created_by)) AS CREATED_BY,\n    \"CREATION_DATE\",\n    \"PROCESS_FLOW_STATUS\",\n    \"PROCESS_INSTANCE_ID\",\n    \"MULTIROW_GROUP_NAME\",\n    \"ELECTRONICALLY_SIGNED\",\n    \"MULTIROW_REGION_ID\",\n    \"ISSUE_DESCRIPTION_HTM\"\n  FROM MS_ISM_ISSUE_DATE_CHANGE_V\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_MAM_USR_ROW": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_MAM_USR_ROW\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_ISM_MAM_USR_ROW\" AS OBJECT (\n  OUT_USER_ID NUMBER,\n  OUT_USER_NAME VARCHAR2(255),\n  OUT_USER_FULLNAME VARCHAR2(4000),\n  OUT_ROLES VARCHAR2(100),\n  OUT_ORGS VARCHAR2(255)\n); \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_LOV_VALUES": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_LOV_VALUES\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_LOV_VALUES\" (\"STORED_VALUE\", \"DISPLAYED_VALUE\", \"LOV_NAME\", \"LOCALE_ID\", \"ENTERPRISE_ID\", \"LOV_ID\", \"LOV_VALUE_ID\", \"ORG_ENTITY_ID\", \"ORG_ENTITY_NAME\", \"START_DATE\", \"END_DATE\") AS \n  Select Stored_Value,\n       Displayed_Value,\n       Lov_Name,\n       Locale_Id,\n       Enterprise_Id,\n       Lov_Id,\n       Lov_Value_Id,\n       Org_Entity_Id,\n       Org_Entity_Name,\n       Start_Date,\n       End_Date\nFrom   Ms_Qs_Lov_Org_Values_v\norder by Lov_Name,TO_NUMBER(Stored_Value) DESC \n \n \n \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_STAGE_ACTION_MAPPING_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_STAGE_ACTION_MAPPING_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_STAGE_ACTION_MAPPING_V\" (\"ACTION_ID\", \"ACTION_NAME\", \"LOCALE_ID\", \"ROLE_ACTIVITY\", \"CURRENT_STAGE\", \"PREVIOUS_STAGE\", \"ACTIVE\", \"CREATED_BY\", \"CREATION_DATE\", \"LAST_UPDATE_DATE\", \"LAST_UPDATED_BY\", \"STAGE_TYPE\", \"STATUS\") AS \n  SELECT a.action_id, b.action_name, b.locale_id, a.role_activity,\n          a.current_stage, a.previous_stage, a.active, a.created_by,\n          a.creation_date, a.last_update_date, a.last_updated_by,\n          a.stage_type, a.status\n     FROM ms_ism_stage_action_mapping a, ms_ism_actions_tl_v b\n    WHERE a.action_id \u003d b.action_id\n      AND ms_ism_helper.config_parameters_actfilter (a.action_id) \u003d 1 \n \n \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_STATUS_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_STATUS_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_STATUS_V\" (\"CREATED_BY\", \"CREATION_DATE\", \"LAST_UPDATED_BY\", \"LAST_UPDATE_DATE\", \"STATUS_TYPE\", \"STATUS_ID\", \"STATUS_NAME\", \"ACTIVE\", \"LOCALE_ID\") AS \n  SELECT t.created_by,\n    t.creation_date,\n    t.last_updated_by,\n    t.last_update_date,\n    t.status_type,\n    t.status_id,\n    NVL(tl.status_name,t.status_name),\n    t.active,\n    t.locale_id\n  FROM ms_ism_status t,\n    ms_ism_status_tl tl\n  WHERE t.status_id   \u003d tl.status_id(+)\n  AND tl.locale_id(+) \u003d si_db_locale_sv.getvalue\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
