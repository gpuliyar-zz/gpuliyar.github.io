  "MS_ISM_ISSUE_AGEING_RPT_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_AGEING_RPT_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ISSUE_AGEING_RPT_V\" (\"ORG_ID\", \"ORG_NAME\", \"ISSUE_SOURCE_TYPE\", \"OVER_DUE\", \"DUE_THIS_MONTH\", \"OVERDUE_PAST_3_MONTH\", \"DUE_BEYOND_3_MONTH\", \"DUE_NEXT_X_MONTH\", \"DUE_BEYOND_X_MONTH\", \"TOTAL\", \"OWNER_ORG\", \"COL1\", \"COL2\", \"COL3\") AS \n  select ISSUE_OWNER_ORG as Org_id, Org_name as Org_name,\n  null as ISSUE_SOURCE_TYPE,\n  NVL(SUM(OVERDUE_THIS_MONTH),0)    as OVER_DUE,\n  NVL(SUM(DUE_THIS_MONTH),0)        as DUE_THIS_MONTH,\n  NVL(SUM(OD_PAST_THREE_MONTH),0)   as OVERDUE_PAST_3_MONTH,\n  NVL(SUM(OD_BEYOND_THREE_MONTH),0) as DUE_BEYOND_3_MONTH,\n  NVL(SUM(DUE_WITHIN_X_MONTH),0)    as DUE_NEXT_X_MONTH,\n  NVL(SUM(DUE_BEYOND_X_MONTH),0)    as DUE_BEYOND_X_MONTH,\n  SUM((nvl(OVERDUE_THIS_MONTH,0)+nvl(DUE_THIS_MONTH,0)+nvl(OD_PAST_THREE_MONTH,0)+nvl(OD_BEYOND_THREE_MONTH,0)+nvl(DUE_WITHIN_X_MONTH,0)+nvl(DUE_BEYOND_X_MONTH,0)))\n  ||\u0027$\u003d\u003d$\u0027\n  ||ms_apps_utilities.get_parameter_value (1, \u0027MS_ISM_Configuration\u0027, \u0027Enable_Ageing_Due_In_Months\u0027)\nAS total,\n  null AS Owner_Org,\n  (select count(issue_id) from MS_ISM_ISSUE where ISSUE_OWNERORG\u003da.ISSUE_OWNER_ORG) AS Col1,\n  2          AS Col2,\n  3          AS COL3\nFROM(\nSELECT ISSUE_OWNER_ORG, (select org_entity_name from SI_ORG_ENTITIES_T where org_entity_id in (ISSUE_OWNER_ORG)) as Org_name,\n    DECODE(OVERDUE_THIS_MONTH,\u0027Y\u0027,COUNT(OVERDUE_THIS_MONTH))       AS OVERDUE_THIS_MONTH,\n    DECODE(DUE_THIS_MONTH,\u0027Y\u0027,COUNT(DUE_THIS_MONTH))               AS DUE_THIS_MONTH,\n    DECODE(OD_PAST_THREE_MONTH,\u0027Y\u0027,COUNT(OD_PAST_THREE_MONTH))     AS OD_PAST_THREE_MONTH,\n    DECODE(OD_BEYOND_THREE_MONTH,\u0027Y\u0027,COUNT(OD_BEYOND_THREE_MONTH)) AS OD_BEYOND_THREE_MONTH,\n    DECODE(DUE_WITHIN_X_MONTH,\u0027Y\u0027,COUNT(DUE_WITHIN_X_MONTH))       AS DUE_WITHIN_X_MONTH,\n    DECODE(DUE_BEYOND_X_MONTH,\u0027Y\u0027,COUNT(DUE_BEYOND_X_MONTH))       AS DUE_BEYOND_X_MONTH\n  FROM MS_ISM_ISSUE_RPT_TBL\nGROUP BY ISSUE_OWNER_ORG,\n    OVERDUE_THIS_MONTH,\n    DUE_THIS_MONTH,\n    OD_PAST_THREE_MONTH,\n    OD_BEYOND_THREE_MONTH,\n    DUE_WITHIN_X_MONTH,\n    DUE_BEYOND_X_MONTH) a\n    GROUP BY ISSUE_OWNER_ORG, Org_name\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_MAM_USR_TAB": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_MAM_USR_TAB\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_ISM_MAM_USR_TAB\" \n IS TABLE OF MS_ISM_MAM_USR_ROW\n; \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ISSUE_DATE_CHANGE_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_DATE_CHANGE_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ISSUE_DATE_CHANGE_V\" (\"DUMMY1\", \"RTF_ATTACH\", \"STATUS\", \"PREVIOUS_STAGE\", \"ASSIGN_TO\", \"REQ_DUE_DATE\", \"ORG_ISSUE_DUE_DATE\", \"ACT_COMMENTS\", \"ACT_ACTION\", \"NO_OF_CHANGES\", \"OWNED_BY_ORG\", \"ISSUE_TITLE\", \"ISSUE_ID\", \"ISSUE_DESCRIPTION\", \"ISSUE_SOURCE_TYPE\", \"ISSUE_DUE_DATE\", \"ISSUE_IDENTIFIED_ON\", \"ISSUE_PRIORITY\", \"ISSUE_SOURCE\", \"ISSUE_OWNER\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"ISSUE_DESCRIPTION_HTM\") AS \n  select \"DUMMY1\",\"RTF_ATTACH\",\"STATUS\",\"PREVIOUS_STAGE\",\"ASSIGN_TO\",\"REQ_DUE_DATE\",\"ORG_ISSUE_DUE_DATE\",\"ACT_COMMENTS\",\"ACT_ACTION\",\"NO_OF_CHANGES\",\"OWNED_BY_ORG\",\"ISSUE_TITLE\",\"ISSUE_ID\",\"ISSUE_DESCRIPTION\",\"ISSUE_SOURCE_TYPE\",\"ISSUE_DUE_DATE\",\"ISSUE_IDENTIFIED_ON\",\"ISSUE_PRIORITY\",\"ISSUE_SOURCE\",\"ISSUE_OWNER\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\",\"ISSUE_DESCRIPTION_HTM\" from SI_101262_T \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_DETAILED_ACTION_LIST_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_DETAILED_ACTION_LIST_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_DETAILED_ACTION_LIST_V\" (\"ACTION_TITLE\", \"ACTION_STATUS\", \"ACTION_OWNER\", \"PRIORITY\", \"ACTION_DUE_DATE\", \"ACTION_COMPLETED_ON\", \"ISSUE_TITLE\", \"ISSUE_OWNER\", \"ACTION_TYPE\", \"PERCENTAGE_COMPLETED\", \"PID\", \"INSTANCEID\", \"OWNER_ORGANIZATION\", \"OWNER_ORG_ID\", \"ISSUE_ID\", \"APPROVER_ORG\", \"APPROVER_ORG_ID\", \"ACTION_APPROVER\", \"FINAL_APPROVER\", \"ACTION_START_DATE\", \"ACTION_DETAILS\", \"ACTION_CLOSED_ON\", \"ACTION_WORK_DONE\", \"ACTION_RESULTS\", \"NO_OF_EXTENSIONS\", \"REOPEN_ACTION\") AS \n  SELECT\n    (SELECT \u0027\u003ca target\u003d\"_self\" href\u003d\"javascript\u0027\n      || CHR (58)\n      || \u0027try{ callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027\n      --|| ms_apps_utilities.get_infolet_id (\u0027MS_ISM_ACTION\u0027)\n      ||ms_ism_utilities.get_ism_infolet_id(\u0027ACTION\u0027)\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027proc\u003d\u0027\n      || a.dd_process_instance_id\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027flag\u003d1\u0027\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027emd\u003d1\u0027\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027x_edit_mode\u003dYes\u0027\n      || CHR (38) || \u0027amp;\u0027\n      || \u0027wrapper\u003dno\u0027\u0027); }catch(e){}\"\u003e\u0027\n      || REPLACE(REPLACE(a.action_title,\u0027\u003c\u0027, \u0027\u0026\u0027||\u0027lt;\u0027), \u0027\u003e\u0027, \u0027\u0026\u0027||\u0027gt;\u0027)\n      || \u0027\u003c/a\u003e\u0027\n    FROM DUAL\n    )                                                                                                    AS action_title,\n    ms_ism_helper.get_status_display_name(a.action_status)                                               AS action_status,\n    ms_apps_utilities.get_user_full_name (a.action_owner)                                                AS action_owner,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, a.action_priority, 1, NULL, NULL) AS priority,\n    a.action_due_date                                                                                    AS action_due_date,\n    NULL                                                                                                 AS action_completed_on,\n    a.issue_title                                                                                        AS issue_title,\n    ms_apps_utilities.get_user_full_name (a.issue_owner)                                                 AS issue_owner,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Action Item Type\u0027,\n    (SELECT action_type FROM ms_ism_issue_iap WHERE issue_action_id\u003da.action_id\n    ), 1, NULL, NULL) AS Action_Type,\n    --a.action_per_complte AS percentage_completed,\n    a.remove_delegation      AS percentage_completed, --remove delegation has been used to capture percentage complete value as it will get nullified on chnage of actions\n    a.dd_process_instance_id AS pid,\n    a.dd_instance_id         AS instanceid,\n    (SELECT org_entity_name\n    FROM si_org_entities_t\n    WHERE org_entity_id \u003d b.ISSUE_OWNERORG\n    )                AS owner_organization,\n    b.ISSUE_OWNERORG AS owner_organization_id,\n    a.issue_id       AS issue_id,\n    (SELECT org_entity_name\n    FROM si_org_entities_t\n    WHERE org_entity_id \u003d b.issue_approverorg\n    )                                                         AS issue_approverorg,\n    b.issue_approverorg                                       AS issue_approverorg_id,\n    ms_apps_utilities.get_user_full_name (a.ACTION_APPROVER1) AS Action_Approver,\n    ms_apps_utilities.get_user_full_name (a.ACTION_APPROVER2) AS Final_Approver,\n    a.ACTION_START_DATE                                       AS Action_start_date,\n    REGEXP_REPLACE (a.ACTION_DETAILS, \u0027\u003c[^\u003e]+\u003e\u0027, NULL)        AS Action_Details,\n    a.ACTION_CLOSED_ON                                        AS Action_Closed_On,\n    REGEXP_REPLACE (a.ACTION_WORK_DONE, \u0027\u003c[^\u003e]+\u003e\u0027, NULL)      AS Work_Done,\n    REGEXP_REPLACE (a.ACTION_RESULTS, \u0027\u003c[^\u003e]+\u003e\u0027, NULL)        AS Results,\n    a.NO_OF_EXTENSIONS                                        AS No_Of_Extensions,\n    a.REOPEN_ACTION                                           AS Reopen_Action\n  FROM ms_ism_action a,\n    ms_ism_issue b\n  WHERE a.issue_id \u003d b.issue_id\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_AUTO_DELEGATION_ID_S": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_AUTO_DELEGATION_ID_S\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"NOVT_DEV\".\"MS_ISM_AUTO_DELEGATION_ID_S\"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 100000 NOCACHE  NOORDER  NOCYCLE \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_AUTO_DELEGATION_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_AUTO_DELEGATION_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_AUTO_DELEGATION_V\" (\"CONFIG_FLAG\", \"DUMMY_5\", \"DUMMY_4\", \"DUMMY_3\", \"DUMMY_2\", \"DUMMY_1\", \"DEFAULT_INFOLET\", \"DEL_COMMENTS\", \"REMOVE_DELEGATION\", \"DELEGATEE\", \"LOGGED_USER\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\") AS \n  select \"CONFIG_FLAG\",\"DUMMY_5\",\"DUMMY_4\",\"DUMMY_3\",\"DUMMY_2\",\"DUMMY_1\",\"DEFAULT_INFOLET\",\"DEL_COMMENTS\",\"REMOVE_DELEGATION\",\"DELEGATEE\",\"LOGGED_USER\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\" from SI_101260_T \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_ACTIONS_TL_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ACTIONS_TL_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_ACTIONS_TL_V\" (\"CREATED_BY\", \"CREATION_DATE\", \"LAST_UPDATED_BY\", \"LAST_UPDATE_DATE\", \"STAGE_TYPE\", \"ACTION_ID\", \"ACTION_NAME\", \"INTERMEDIATE_STATUS\", \"ACTIVE\", \"LOCALE_ID\", \"LOCALE_STRING\") AS \n  select t.created_by, t.creation_date, t.last_updated_by, t.last_update_date, t.stage_type, t.action_id,\nnvl(tl.action_name,t.action_name), t.intermediate_status, t.active, t.locale_id, t.locale_string\nfrom (select a.created_by, a.creation_date, a.last_updated_by, a.last_update_date, a.stage_type, a.action_id,\na.action_name, a.intermediate_status, a.active, s.locale_id, s.locale_string  \n    FROM ms_ism_actions a, si_enabled_locales s) t, \n    ms_ism_actions_tl tl\n     WHERE t.action_id \u003d tl.action_id(+) AND t.locale_id \u003d tl.locale_id(+) \n \n \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_INTG_ISSUE_PUB_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_INTG_ISSUE_PUB_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_INTG_ISSUE_PUB_V\" (\"ISSUE_ID\", \"ISSUE_TITLE\", \"ISSUE_DETAILS\", \"ISSUE_STATUS\", \"ISSUE_DUE_DATE\", \"PROGRESS_STATUS\", \"ISSUE_SOURCE_TYPE\", \"ISSUE_SOURCE\", \"ISSUE_SOURCE_REF\", \"CREATED_BY\", \"CREATED_ON\", \"LAST_MODIFIED_BY\", \"LAST_MODIFIED_ON\", \"ISSUE_CLOSED_ON\", \"OBJECT_TYPE\", \"ISSUE_OWNER\", \"ISSUE_OWNERORG\", \"ISSUE_DELEGATEE\", \"INITIAL_APPROVER\", \"INITIAL_APPROVER_ORG\", \"ACTION_PLAN_APPROVER\", \"ACT_PLAN_APPROVER_ORG\", \"FINAL_APPROVER\", \"FINAL_APPROVER_ORG\", \"NO_OF_REOPEN\", \"ISSUE_RATING\", \"PRIORITY\", \"ACTION_APPROVAL_BY\", \"NO_OF_ACTIONS_TRIGGERED\", \"IMPACT\", \"LIKELIHOOD\", \"LOSS_GAIN\", \"LOSS_GAIN_AMOUNT\", \"EXCEPTION_TYPE\", \"REACHED_TO_MANAGE\", \"REPEAT_ISSUE\", \"ISSUE_TYPE\", \"RELATED_ISSUES\", \"RELATED_RISKS\", \"RELATED_AREA_OF_COMP\", \"RELATED_CONTROLS\", \"RELATED_CORE_PROCESSES\", \"RELATED_ORGS\", \"RELATED_STANDARD\", \"RELATED_ASSET\", \"RELATED_ASSET_CLASS\", \"RELATED_FINANCIAL_ACOUNT\", \"RELATED_FUNCTION\", \"RELATED_FRAMEWORK_REF\", \"RELATED_OBJECTIVE\", \"RELATED_PROCESS\", \"RELATED_REFERENCE\", \"RELATED_REGULATORY_BODY\", \"RELATED_REQUIREMENT\", \"RELATED_QUESTION_PROCEDURE\", \"RELATED_SUPPLIER\", \"RELATED_AUDITABLE_ENTITY\", \"RELATED_PRODUCT\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"DD_METRIC_ID\", \"ISSUE_LINK\", \"ISSUE_POPUP_LINK\") AS \n  SELECT a.issue_id                                       AS issue_id,\n    A.Issue_Title                                         AS Issue_Title,\n    REGEXP_REPLACE (a.issue_details, \u0027\u003c[^\u003e]+\u003e\u0027, NULL)     AS issue_details,\n    ms_ism_helper.get_status_display_name(a.issue_status) AS issue_status,\n    A.Issue_Due_Date                                      AS Issue_Due_Date,\n    --Decode (A.Issue_Source_Type, 1, \u0027Audit\u0027, 2, \u0027Test Execution\u0027, 3, \u0027Risk Assessment\u0027, 4, \u0027Surveys\u0027, 5, \u0027Internal Loss Event\u0027, 6, \u0027GRC Intelligence\u0027)\n    Ms_Apps_Utilities.Get_Display_Value (100000, \u0027MS ISM Update Status\u0027, A.progress_status, 1, NULL, NULL)                        AS Progress_Status,\n    Ms_Apps_Utilities.Get_Display_Value (100000, \u0027MS ISM Source Type\u0027, A.Issue_Source_Type, 1, NULL, NULL)                        AS Issue_Source_Type,\n    DECODE (a.issue_source_type, \u00270\u0027, a.adhoc_source, MS_ISM_HELPER.get_Issue_Source_Title (a.issue_source, a.issue_source_type)) AS ISSUE_SOURCE,\n    A.Issue_Source_Ref                                                                                                            AS Issue_Source_Ref,\n    A.Dd_Created_By                                                                                                               AS Created_By,\n    A.Dd_Created_On                                                                                                               AS Created_On,\n    A.Dd_Modified_By                                                                                                              AS Last_Modified_By,\n    A.Dd_Modified_On                                                                                                              AS Last_Modified_On,\n    A.Issue_Closed_On                                                                                                             AS Issue_Closed_On,\n    A.Dd_Object_Type                                                                                                              AS Object_Type,\n    A.Issue_Owner                                                                                                                 AS Issue_Owner,\n    (SELECT org_entity_name\n    FROM Si_Org_Entities_T\n    WHERE Org_Entity_Id \u003d A.Issue_Ownerorg\n    )                 AS Issue_Owner_Org,\n    A.Issue_Delegatee AS Issue_Delegatee,\n    A.Issue_Approver  AS Initial_Approver,\n    (SELECT org_entity_name\n    FROM Si_Org_Entities_T\n    WHERE org_entity_id \u003d A.Issue_Approverorg\n    )                      AS Initial_Approver_Org,\n    A.Action_Plan_Approver AS Action_Plan_Approver,\n    (SELECT Org_Entity_Name\n    FROM Si_Org_Entities_T\n    WHERE org_entity_id \u003d A.Act_Plan_Approverorg\n    )                    AS Action_Plan_Approver_Org,\n    A.Final_Iss_Approver AS Final_Approver,\n    (SELECT org_entity_name\n    FROM Si_Org_Entities_T\n    WHERE org_entity_id \u003d A.Final_Iss_Approverorg\n    )                                                                                                      AS Final_Approver_Org,\n    A.Reopen_Issue                                                                                         AS Reopen_Count,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Risk Rating\u0027, a.issue_risk_rating, 1, NULL, NULL) AS issue_rating,\n    ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027, a.issue_risk_priority, 1, NULL, NULL)  AS priority,\n    (SELECT COUNT (1) FROM Ms_Ism_Action WHERE Issue_Id \u003d A.Issue_Id\n    )                                                                                                              AS No_Of_Actions_Added,\n    a.final_action_approval                                                                                        AS final_action_approval,\n    Ms_Apps_Utilities.Get_Display_Value (100000, \u0027MS ISM Impact\u0027, A.Issue_Risk_Impact, 1, NULL, NULL)              AS Issue_Risk_Impact,\n    Ms_Apps_Utilities.Get_Display_Value (100000, \u0027MS ISM Likelihood\u0027, A.Issue_Risk_Liklihood, 1, NULL, NULL)       AS Issue_Risk_Liklihood,\n    Ms_Apps_Utilities.Get_Display_Value (100000, \u0027MS ISM Loss Gain\u0027, A.Issue_Risk_Loss_Gain, 1, NULL, NULL)        AS Issue_Risk_Loss_Gain,\n    a.issue_risk_loss_gain_amt                                                                                     AS loss_gain_amount,\n    Ms_Apps_Utilities.Get_Display_Value (100000, \u0027MS ISM Classification Type\u0027, A.Type_Of_Execution, 1, NULL, NULL) AS Exception_Type,\n    A.Reached_Manage                                                                                               AS Reached_To_Manage_Stage,\n    Ms_Apps_Utilities.Get_Display_Value (100000, \u0027MS_ISM_YES_NO\u0027, A.Repeat_Issue, 1, NULL, NULL)                   AS Repeat_Issue,\n    Ms_Ism_Helper.Get_Comma_Sep_Value_For_Mlov (\u0027ISSUE_TYPE\u0027,\u0027MS_ISM_ISSUE_IST\u0027,A.Issue_Id,\u0027MS ISM Issue Type\u0027)    AS Issue_Type,\n    Ms_Ism_Helper.Get_Comma_Sep_Related_Issues(A.Issue_Id)                                                         AS Related_Issues,\n    ms_ism_helper.get_coma_seprtd_orb_value(\u0027Risks\u0027,a.issue_id)                                                    AS Related_Risks,\n    ms_ism_helper.get_coma_seprtd_orb_value(\u0027Areas of Compliance\u0027,a.issue_id)                                      AS Related_Area_Of_Comp,\n    ms_ism_helper.get_coma_seprtd_orb_value(\u0027Controls\u0027,a.issue_id)                                                 AS Related_Controls,\n    Ms_Ism_Helper.Get_Coma_Seprtd_Orb_Value(\u0027Core Objects\u0027,A.Issue_Id)                                             AS Related_Core_Processes,\n    Ms_Ism_Helper.Get_Coma_Seprtd_Orb_Value(\u0027Organizations\u0027,A.Issue_Id)                                            AS Related_Orgs,\n    Ms_Ism_Helper.Get_Coma_Seprtd_Orb_Value (\u0027Standards\u0027,A.Issue_Id)                                               AS Standard,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Assets\u0027,a.issue_id)                                                  AS Asset,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Asset Classes\u0027,a.issue_id)                                           AS Asset_Class,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Financial Accounts\u0027,a.issue_id)                                      AS Financial_Account,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Functions\u0027,a.issue_id)                                               AS FunctionObj,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Framework References\u0027,a.issue_id)                                    AS Framework_Reference,\n    Ms_Ism_Helper.Get_Coma_Seprtd_Orb_Value (\u0027Objectives\u0027,A.Issue_Id)                                              AS Objective,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Processes\u0027,a.issue_id)                                               AS Process,\n    Ms_Ism_Helper.Get_Coma_Seprtd_Orb_Value (\u0027References\u0027,A.Issue_Id)                                              AS REFERENCE,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Regulatory Bodies\u0027,a.issue_id)                                       AS Regulatory_Bod,\n    Ms_Ism_Helper.Get_Coma_Seprtd_Orb_Value (\u0027Requirements\u0027,A.Issue_Id)                                            AS Requirement,\n    ms_ism_helper.get_coma_seprtd_orb_value (\u0027Questiones/Procedures\u0027,a.issue_id)                                   AS Question_Procedure,\n    Ms_Ism_Helper.Get_Coma_Seprtd_Orb_Value (\u0027Supplier(s)\u0027,A.Issue_Id)                                             AS Supplier,\n    Ms_Ism_Helper.Get_Coma_Seprtd_Orb_Value (\u0027Auditable Entity(s)\u0027,A.Issue_Id)                                     AS Auditable_Entity,\n    Ms_Ism_Helper.Get_Coma_Seprtd_Orb_Value (\u0027Products\u0027,A.Issue_Id)                                                AS Product,\n    Dd_Process_Instance_Id                                                                                         AS Dd_Process_Instance_Id,\n    Dd_Instance_Id                                                                                                 AS Dd_Instance_Id,\n    ms_ism_utilities.get_ism_infolet_id (\u0027ISSUE\u0027)                                                                  AS DD_Metric_ID,\n    \u0027Pushinfolet?id\u003d\u0027\n    || ms_ism_utilities.get_ism_infolet_id (\u0027ISSUE\u0027)\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027proc\u003d\u0027\n    || a.DD_PROCESS_INSTANCE_ID\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027instid\u003d\u0027\n    || a.dd_instance_id\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027flag\u003d1\u0027\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027emd\u003d1\u0027\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027x_edit_mode\u003dNo\u0027\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027wrapper\u003dno\u0027 AS ISSUE_LINK,\n    \u0027Pushinfolet?id\u003d\u0027\n    || ms_ism_utilities.get_ism_infolet_id (\u0027ISSUE\u0027)\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027proc\u003d\u0027\n    || a.DD_PROCESS_INSTANCE_ID\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027instid\u003d\u0027\n    || a.dd_instance_id\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027flag\u003d1\u0027\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027emd\u003d5\u0027\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027x_edit_mode\u003dNo\u0027\n    || CHR (38)\n    || \u0027amp;\u0027\n    || \u0027wrapper\u003dno\u0027 AS ISSUE_POPUP_LINK\n  FROM Ms_Ism_Issue A \n  WHERE NVL (Issue_Source_Type, \u00270\u0027) \u003c\u003e 0\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_STATUS_TL_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_STATUS_TL_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_STATUS_TL_V\" (\"CREATED_BY\", \"CREATION_DATE\", \"LAST_UPDATED_BY\", \"LAST_UPDATE_DATE\", \"STATUS_ID\", \"ACTIVE\", \"STATUS_TYPE\", \"STATUS_NAME\", \"LOCALE_ID\") AS \n  SELECT t.created_by,\n    t.creation_date,\n    t.last_updated_by,\n    t.last_update_date,\n    t.status_id,\n    t.active,\n    t.status_type,\n    NVL (tl.status_name, t.status_name),\n    t.locale_id\n  FROM\n    (SELECT a.created_by,\n      a.creation_date,\n      a.last_updated_by,\n      a.last_update_date,\n      a.status_id,\n      a.active,\n      a.status_type,\n      a.status_name,\n      s.locale_id\n    FROM ms_ism_status a,\n      si_enabled_locales s\n    ) t,\n    ms_ism_status_tl tl\n  WHERE t.status_id \u003d tl.status_id(+)\n  AND t.locale_id   \u003d tl.locale_id(+)\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_CHANGE_HISTORY_ID_S": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_CHANGE_HISTORY_ID_S\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"NOVT_DEV\".\"MS_ISM_CHANGE_HISTORY_ID_S\"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 100000 NOCACHE  NOORDER  NOCYCLE \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_ISM_FLOW_DOWN_ORG_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_FLOW_DOWN_ORG_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_ISM_FLOW_DOWN_ORG_V\" (\"ENTERPRISE_ID\", \"ACTIVITY_ID\", \"ACTIVITY_NAME\", \"ACTIVITY_SHORT_NAME\", \"USER_ID\", \"USER_NAME\", \"ORG_ENTITY_ID\", \"ORG_ENTITY_NAME\", \"ROLE_ID\", \"ROLE_NAME\") AS \n  SELECT a.enterprise_id,\n          d.activity_id,\n          d.activity_name,\n          d.activity_short_name,\n          a.user_id,\n          a.user_name,\n          a.org_entity_id,\n          a.org_entity_name,\n          b.role_id,\n          b.role_name\n     FROM si_activities d,\n          si_role_activities c,\n          si_roles b,\n          /*(SELECT COLUMN_VALUE org_entity_id,\n                  g.role_id,\n                  e.user_id,\n                  f.user_name,\n                  h.org_entity_name,\n                  h.enterprise_id\n             FROM si_org_entities h,\n                  si_org_roles g,\n                  si_users f,\n                  si_user_org_roles e,\n                  TABLE (CAST ( (SELECT ms_ism_utilities.get_child_orgs (f.enterprise_id, g.org_entity_id) FROM DUAL) AS ms_varchar2_array)) tab\n            WHERE     COLUMN_VALUE IS NOT NULL\n                  AND h.org_entity_id \u003d COLUMN_VALUE\n                  AND h.enterprise_id \u003d f.enterprise_id\n                  AND NVL (h.end_date, SYSDATE) \u003e\u003d SYSDATE\n                  AND NVL (f.end_date, SYSDATE) \u003e\u003d SYSDATE\n                  AND f.user_id \u003d e.user_id\n                  AND g.org_role_id \u003d e.org_role_id)*/ ms_ism_list_rep_mv a\n    WHERE     b.role_id \u003d a.role_id\n          AND c.role_id \u003d a.role_id\n          AND NVL (b.end_date, SYSDATE) \u003e\u003d SYSDATE\n          AND NVL (d.end_date, SYSDATE) \u003e\u003d SYSDATE\n          AND d.activity_id \u003d c.activity_id\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
