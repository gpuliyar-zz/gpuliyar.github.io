    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_002_Issue_Risk_Rating\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            Select Stored_Value,\n       Displayed_Value,\n       Lov_Name,\n       Locale_Id,\n       Enterprise_Id,\n       Lov_Id,\n       Lov_Value_Id,\n       Org_Entity_Id,\n       Org_Entity_Name,\n       Start_Date,\n       End_Date\nFrom   Ms_Qs_ISM_Lov_Org_Values_v\nWhere (trim(LOV_NAME) \u003d \u0027MS ISM Risk Rating\u0027)\nand ((trim(START_DATE) \u0026lt;\u003d trunc(sysdate))AND(nvl(end_date,sysdate)\u0026gt;\u003dtrunc(sysdate)))\norder by stored_value\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-f\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Title\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(SUBSTR(Issue_Title,INSTR(Issue_Title,\u0027\u003e\u0027)+1,INSTR(Issue_Title,\u0027\u003c/a\u0027)-1))) like UPPER(\u0027%\u0027||:Issue Title||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Owner\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Issue_Owner) \u003d :Issue Owner)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Rating\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Issue_Rating) \u003d :Issue Rating)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003ePriority\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Priority) \u003d :Priority)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Status\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Issue_Status) \u003d :Issue Status)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eSource Type\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Source_Type) \u003d :Source Type)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eRelated To Core Object\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Core_Objects)) like UPPER(\u0027%\u0027||:Related To Core Object||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eRelated to Organization\u003c/td\u003e\r\n            \u003ctd\u003e((trim(UPPER(Organization)) like UPPER(\u0027%\u0027||:Related To Organization||\u0027%\u0027)))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eRelated To Risks\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Risks)) like UPPER(\u0027%\u0027||:Related to Risks||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eRelated To Compliance Control\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Controls)) like UPPER(\u0027%\u0027||:Related To Compliance Controls||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eRelated To Area of Compliance\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Area_Of_Compleance)) like UPPER(\u0027%\u0027||:Related To Area of Compliance||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOrganization\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Issue_Owner_Org)) like UPPER(\u0027%\u0027||:Owner Organization||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOutstanding_chart_filter\u003c/td\u003e\r\n            \u003ctd\u003e((trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027)) \u003c TO_DATE (sysdate, \u0027DD/MM/RRRR\u0027))AND(trim(Issue_Status) !\u003d \u0027Closed\u0027)AND(trim(Issue_Status) !\u003d \u0027Issue Cancelled\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eOutstanding_chart_filter\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue by Area of Compliance Ongoing\u003c/td\u003e\r\n            \u003ctd\u003e((trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027)) \u003e\u003d TO_DATE (sysdate, \u0027DD/MM/RRRR\u0027))AND(trim(Issue_Status) !\u003d \u0027Closed\u0027)AND(trim(Issue_Status) !\u003d \u0027Issue Cancelled\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eIssue by Area of Compliance Ongoing Chart Filter\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eException Type\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Exception_Type) \u003d :Exception Type)\u003c/td\u003e\r\n            \u003ctd\u003eException Type\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue by Area of Compliance Due Between\u003c/td\u003e\r\n            \u003ctd\u003e((trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027))\u003cTO_DATE (sysdate, \u0027DD/MM/RRRR\u0027)) AND (trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027))\u003e\u003d(trunc(TO_DATE (sysdate, \u0027DD/MM/RRRR\u0027)-ms_apps_utilities.get_parameter_value (1, \u0027MS_ISM_Configuration\u0027, \u0027Enable_ageing_due_in_days_AOC\u0027)))) \nAND (trim(Issue_Status) !\u003d \u0027Closed\u0027)AND(trim(Issue_Status) !\u003d \u0027Issue Cancelled\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eIssue by Area of Compliance Due Between Chart Bar Filter\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue by Area of Compliance Overdue\u003c/td\u003e\r\n            \u003ctd\u003e((trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027))\u003c(trunc(TO_DATE (sysdate, \u0027DD/MM/RRRR\u0027)-ms_apps_utilities.get_parameter_value (1, \u0027MS_ISM_Configuration\u0027, \u0027Enable_ageing_due_in_days_AOC\u0027)))) AND (trim(Issue_Status) !\u003d \u0027Closed\u0027)AND(trim(Issue_Status) !\u003d \u0027Issue Cancelled\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eIssue by Area of Compliance Overdue\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAging by Bizz Unit Ongoing\u003c/td\u003e\r\n            \u003ctd\u003e((trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027)) \u003e\u003d TO_DATE (sysdate, \u0027DD/MM/RRRR\u0027))AND(trim(Issue_Status) !\u003d \u0027Closed\u0027)AND(trim(Issue_Status) !\u003d \u0027Issue Cancelled\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eAging by Bizz Unit Ongoing Report Filter\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAging by Bizz Unit Due Bet 1 and 30\u003c/td\u003e\r\n            \u003ctd\u003e((trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027))\u003cTO_DATE (sysdate, \u0027DD/MM/RRRR\u0027)) AND (trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027))\u003e\u003d(trunc(TO_DATE (sysdate, \u0027DD/MM/RRRR\u0027)-30))) \nAND (trim(Issue_Status) !\u003d \u0027Closed\u0027)AND(trim(Issue_Status) !\u003d \u0027Issue Cancelled\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eAging by Bizz Unit Due Bet 1 and 30 Report Filter\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAging by Bizz Unit Due Bet 30 and X\u003c/td\u003e\r\n            \u003ctd\u003e((trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027))\u003cTO_DATE (sysdate, \u0027DD/MM/RRRR\u0027)-30) AND (trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027))\u003e\u003d(trunc(TO_DATE (sysdate, \u0027DD/MM/RRRR\u0027)-90))) \nAND (trim(Issue_Status) !\u003d \u0027Closed\u0027)AND(trim(Issue_Status) !\u003d \u0027Issue Cancelled\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eAging by Bizz Unit Due Bet 30 and X Report Filter\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAging by Bizz Unit Overdue\u003c/td\u003e\r\n            \u003ctd\u003e((trunc(TO_DATE (Issue_Due_Date, \u0027DD/MM/RRRR\u0027))\u003c(trunc(TO_DATE (sysdate, \u0027DD/MM/RRRR\u0027)-90))) AND (trim(Issue_Status) !\u003d \u0027Closed\u0027)AND(trim(Issue_Status) !\u003d \u0027Issue Cancelled\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eAging by Bizz Unit Overdue Report Filter\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Type\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Issue_Type)) like UPPER(\u0027%\u0027||:Issue Type||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eIssue Type\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eLast One Year Chart\u003c/td\u003e\r\n            \u003ctd\u003eTO_DATE (Created_On, \u0027DD/MM/RRRR\u0027)\u003e\u003dADD_MONTHS(TO_DATE (sysdate, \u0027DD/MM/RRRR\u0027), -12)\u003c/td\u003e\r\n            \u003ctd\u003eFilter for last one year issues chart\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eExclude Closed Status\u003c/td\u003e\r\n            \u003ctd\u003e(trim(ISSUE_STATUS_STORED) !\u003d \u002710\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eExclude Closed Status for Chart\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eExclude Cancelled Status\u003c/td\u003e\r\n            \u003ctd\u003e(trim(ISSUE_STATUS_STORED) !\u003d \u00277\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eExclude Cancelled Status for Chart\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "parameters": "\u003ctable class\u003d\"table table-hover infolet-table-pnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eMandatory\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eLOGIN_ID\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n            \u003ctd\u003eLOGIN_ID\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e"
