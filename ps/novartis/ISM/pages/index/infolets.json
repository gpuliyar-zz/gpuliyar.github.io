    "xml": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_STI_GET_ISSUE_FORM_MID\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"xml\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \u0026lt;?xml version\u003d\u0026quot;1.0\u0026quot;?\u0026gt;\u0026lt;METRIC_DEPLOYMENT\u0026gt;\u0026lt;METRIC       PROCESS\u003d\u0026quot;0\u0026quot;  INSTANCE\u003d\u0026quot;0\u0026quot;  NOTIFY\u003d\u0026quot;YES\u0026quot;   RETURN\u003d\u0026quot;RESULTSET\u0026quot; \u0026gt;\u0026lt;RESULTMAP \u0026gt;\u0026lt;/RESULTMAP\u0026gt;\u0026lt;/METRIC\u0026gt;\u0026lt;/METRIC_DEPLOYMENT\u0026gt;\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "columns": "\u003ctable class\u003d\"table table-hover infolet-table-cnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003e#\u003c/th\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eType\u003c/th\u003e\r\n        \u003cth\u003eResult Column Name\u003c/th\u003e\r\n        \u003cth\u003eUser Column Name\u003c/th\u003e\r\n        \u003cth\u003eDefault Infolet/\u003cbr\u003eInput Infolet\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e1\u003c/td\u003e\r\n            \u003ctd\u003eINFOLETID\u003c/td\u003e            \u003ctd\u003eNumber\u003c/td\u003e\r\n            \u003ctd\u003eINFOLETID\u003c/td\u003e\r\n            \u003ctd\u003eINFOLETID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_STI_GET_ISSUE_FORM_MID\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT  ms_apps_utilities.get_infolet_id(\u0027MS_STI_ISSUE\u0027) as infoletid  from dual\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOrganization\u003c/td\u003e\r\n            \u003ctd\u003e(trim(OWNER_ORG_ID) \u003d :Organization)\u003c/td\u003e\r\n            \u003ctd\u003eOrganization\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Title\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Issue_Title)) like UPPER(\u0027%\u0027||:Issue Title||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eIssue Title\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Title\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(SUBSTR(Action_Title,INSTR(Action_Title,\u0027\u003e\u0027)+1,INSTR(Action_Title,\u0027\u003c/a\u0027)-INSTR(Action_Title,\u0027\u003e\u0027)-1))) like UPPER(\u0027%\u0027||:Action Title||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003ePriority\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Action_Priority) \u003d :Priority)\u003c/td\u003e\r\n            \u003ctd\u003ePriority\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Status\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Action_Status) \u003d :Action Status)\u003c/td\u003e\r\n            \u003ctd\u003eAction Status\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Closed Date\u003c/td\u003e\r\n            \u003ctd\u003e(Action_Closed_Date  \u003d :Action Closed Date)\u003c/td\u003e\r\n            \u003ctd\u003eAction Closed Date\u0