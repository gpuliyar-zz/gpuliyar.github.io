  "MS_ISM_APPROVER_ISSUES_LIST": {
    "references": "\u003ctable class\u003d\"table table-hover infolet-table-rnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eUsed In\u003c/th\u003e\r\n        \u003cth\u003eUsage Type\u003c/th\u003e\r\n        \u003cth\u003eUser\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eList of Approver Issues\u003c/td\u003e\r\n            \u003ctd\u003eReport\u003c/td\u003e\r\n            \u003ctd\u003eSYSTEMI\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
