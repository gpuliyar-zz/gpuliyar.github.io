    "xml": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS ISM List of Actions\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"xml\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \u0026lt;?xml version\u003d\u0026quot;1.0\u0026quot;?\u0026gt;\u0026lt;METRIC_DEPLOYMENT\u0026gt;\u0026lt;METRIC       PROCESS\u003d\u0026quot;0\u0026quot;  INSTANCE\u003d\u0026quot;0\u0026quot;  NOTIFY\u003d\u0026quot;YES\u0026quot;   RETURN\u003d\u0026quot;RESULTSET\u0026quot; \u0026gt;\u0026lt;RESULTMAP \u0026gt;\u0026lt;/RESULTMAP\u0026gt;\u0026lt;/METRIC\u0026gt;\u0026lt;/METRIC_DEPLOYMENT\u0026gt;\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "columns": "\u003ctable class\u003d\"table table-hover infolet-table-cnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003e#\u003c/th\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eType\u003c/th\u003e\r\n        \u003cth\u003eResult Column Name\u003c/th\u003e\r\n        \u003cth\u003eUser Column Name\u003c/th\u003e\r\n        \u003cth\u003eDefault Infolet/\u003cbr\u003eInput Infolet\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e1\u003c/td\u003e\r\n            \u003ctd\u003eCHECKBOX\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eCHECKBOX\u003c/td\u003e\r\n            \u003ctd\u003eCHECKBOX\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e2\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_ID\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_ID\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_ID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e3\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_NAME\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_NAME\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e4\u003c/td\u003e\r\n            \u003ctd\u003eREQUIREMENT_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eREQUIREMENT_NAME\u003c/td\u003e\r\n            \u003ctd\u003eREQUIREMENT_NAME\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e5\u003c/td\u003e\r\n            \u003ctd\u003eREQUIREMENT_ID\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eREQUIREMENT_ID\u003c/td\u003e\r\n            \u003ctd\u003eREQUIREMENT_ID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS ISM ORB Add Area of Compliances\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT checkbox, object_id,object_name, requirement_name, requirement_id\nFROM ((SELECT\u0027\u0026lt;input type\u003dcheckbox id\u003d\u0026quot;\u0027\n|| -1\n|| \u0027\u0026quot; value\u003d\u0026quot;\u0027\n|| substr(Object_Name,1,3600)||\u0027^*^\u0027||object_id||\u0027$#$\u0027||ms_ism_utilities.get_form_url(dd_object_type,object_id) \n|| \u0027\u0026quot; onClick\u003dcheckboxChange(\u0026quot;\u0027\n|| -1\n||\u0027\u0026quot;)\u0026gt;\u0026lt;/input\u0026gt;\u0027 AS checkbox,\nobject_name, object_id, NULL AS requirement_name,\nNULL AS requirement_id\n FROM ms_grc_area_of_complia_pub_v\nWHERE ms_apps_security_pkg.check_object_access (dd_object_type,\nrestrict_access_to,\nowner_organizations,\n:1,\n\u0027VIEW\u0027\n ) \u003d \u0027ACCESS\u0027\nAND TRUNC (SYSDATE) BETWEEN NVL (TRUNC (valid_from),\n TRUNC (SYSDATE)\n)\nAND NVL (TRUNC (valid_until),\n TRUNC (SYSDATE)\n))\nUNION\n(SELECT\u0027\u0026lt;input type\u003dcheckbox id\u003d\u0026quot;\u0027\n|| requirement_id\n|| \u0027\u0026quot; value\u003d\u0026quot;\u0027\n|| substr(a.object_name,1,3000)\n|| \u0027^*^\u0027\n|| a.object_id||\u0027^*^\u0027||requirement_name\n|| \u0027\u0026quot;onClick\u003dcheckboxChange(\u0026quot;\u0027\n|| requirement_id\n|| \u0027\u0026quot;)\u0026gt;\u0026lt;/input\u0026gt;\u0027 AS checkbox,\na.object_name, a.object_id, requirement_name, requirement_id\n FROM ms_grc_area_of_complia_pub_v a,\n(SELECT substr(object_name,1,600) AS requirement_name,\nobject_id AS requirement_id\n FROM ms_grc_requirement_rv\nWHERE ms_apps_security_pkg.check_object_access\n (dd_object_type,\nrestrict_access_to,\nowner_org_ids,\n:1,\n\u0027VIEW\u0027\n ) \u003d \u0027ACCESS\u0027\nAND TRUNC (SYSDATE) BETWEEN NVL (TRUNC (valid_from),\n TRUNC (SYSDATE)\n)\nAND NVL (TRUNC (valid_until),\n TRUNC (SYSDATE)\n)) b\nWHERE ms_apps_security_pkg.check_object_access\n (a.dd_object_type,\na.restrict_access_to,\na.owner_organizations,\n:1,\n\u0027VIEW\u0027\n ) \u003d \u0027ACCESS\u0027\nAND TRUNC (SYSDATE) BETWEEN NVL (TRUNC (a.valid_from),\n TRUNC (SYSDATE)\n)\nAND NVL (TRUNC (a.valid_until),\n TRUNC (SYSDATE))))\nWHERE (REQUIREMENT_ID IS NULL OR REQUIREMENT_id IN\n (select REL_SOURCE_OBJECt_ID from MS_GRC_ORB_rELATIONS_ORB A,MS_GRC_ORB_RELATIONS B \nwhere rel_config_id \u003d (\nSELECT REL_ID FROM MS_gRC_REL_dEFN\nWHERE SRC_OBJ_TYPE\u003d\u0027MS_GRC_REQUIREMENT\u0027\nAND DEST_OBJ_TYPE\u003d\u0027MS_GRC_AREA_OF_COMPLIANCE\u0027\n)\nAND A.REL_SOURCE_ID \u003d B.REL_SOURCE_ID\nAND TGT_OBJ_ID \u003d OBJECT_ID) )\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003ePrev_Stage\u003c/td\u003e\r\n            \u003ctd\u003e(Previous_Stage \u003d :Previous Stage)\u003c/td\u003e\r\n            \u003ctd\u003ePrevious Stage\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eCurr_Stage\u003c/td\u003e\r\n            \u003ctd\u003e(Current_Stage \u003d :Current Stage)\u003c/td\u003e\r\n            \u003ctd\u003eCurrent Stage\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eUser Role\u003c/td\u003e\r\n            \u003ctd\u003e(trim(ROLE_ACTIVITY) IN (SELECT activity_name\n                            FROM ms_qms_flow_down_org_v fdo\n                            Where user_name \u003d nvl(:User_Name,user_name )\n                             And activity_name like \u0027ISM%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eUser Role and Activity\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eCheck_Action\u003c/td\u003e\r\n            \u003ctd\u003e(ms_ism_helper.check_actions(action_id,:p_prev_stage,:p_curr_stage,decode(:p_delegatee,\u0027NIL\u0027,\u0027\u0027,:p_delegatee),nvl(:p_issue_id,\u0027\u0027),nvl(:p_action_id,\u0027\u0027),:p_curr_user,:p_obj_type)\u003d1)\u003c/td\u003e\r\n            \u003ctd\u003eCheck Actions at all the stages.\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
