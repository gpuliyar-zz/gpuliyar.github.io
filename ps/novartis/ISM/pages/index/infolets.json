    "columns": "\u003ctable class\u003d\"table table-hover infolet-table-cnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003e#\u003c/th\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eType\u003c/th\u003e\r\n        \u003cth\u003eResult Column Name\u003c/th\u003e\r\n        \u003cth\u003eUser Column Name\u003c/th\u003e\r\n        \u003cth\u003eDefault Infolet/\u003cbr\u003eInput Infolet\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e1\u003c/td\u003e\r\n            \u003ctd\u003eOTHER_TYPE         \u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eOTHER_TYPE\u003c/td\u003e\r\n            \u003ctd\u003eOther Type\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e2\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_ID         \u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_ID\u003c/td\u003e\r\n            \u003ctd\u003eObject ID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e3\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eOBJECT_NAME\u003c/td\u003e\r\n            \u003ctd\u003eObject Name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_STI_OTHER_OBJECTS\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT  \u0027ACCOUNT\u0027 AS OTHER_TYPE,OBJECT_ID  AS OBJECT_ID,OBJECT_NAME AS OBJECT_NAME \nFROM MS_GRC_FINANCIAL_ACCOU\nWHERE \u0027ACCOUNT\u0027 \u003d :1\nAND SYSDATE BETWEEN NVL (valid_from, SYSDATE) AND NVL (valid_until,SYSDATE)\nAND ms_apps_security_pkg.check_object_access (dd_object_type,restrict_access_to ,owner_organizations,:2,\u0027VIEW\u0027) \u003d  \u0027ACCESS\u0027\n\nUNION \nSELECT  \u0027INFOSYS\u0027 AS OTHER_TYPE,OBJECT_ID AS OBJECT_ID  ,OBJECT_NAME AS OBJECT_NAME\nFROM MS_STI_INFORMATION_SYS\nwhere sysdate between NVL(Valid_from, sysdate) and nvl(valid_until, sysdate)\nAND \u0027INFOSYS\u0027 \u003d :1\nAND ms_apps_security_pkg.check_object_access (dd_object_type, restrict_access_to,owner_organizations,:2,\u0027VIEW\u0027) \u003d \u0027ACCESS\u0027\n\nUNION\nSELECT \u0027PRODUCT\u0027 AS OTHER_TYPE,OBJECT_ID AS OBJECT_ID,OBJECT_NAME AS OBJECT_NAME\nFROM MS_STI_PRODUCT\nWHERE \u0027PRODUCT\u0027 \u003d :1\nand SYSDATE BETWEEN NVL (valid_from, SYSDATE) AND NVL (valid_until,SYSDATE) \nAND ms_apps_security_pkg.check_object_access (dd_object_type,restrict_access_to,owner_organizations,:2,\u0027VIEW\u0027) \u003d \u0027ACCESS\u0027\n   \nUNION\nSELECT \u0027PROJECT\u0027 AS OTHER_TYPE,OBJECT_ID AS OBJECT_ID,PROJECT_NAME AS OBJECT_NAME\nFROM MS_STI_PROJECT\nWHERE \u0027PROJECT\u0027 \u003d :1\nAND SYSDATE BETWEEN NVL (valid_from, SYSDATE) AND NVL (valid_until,SYSDATE) \nAND ms_apps_security_pkg.check_object_access (dd_object_type,restrict_access_to,owner_organizations,:2,\u0027VIEW\u0027) \u003d \u0027ACCESS\u0027\n   \nUNION\nSELECT \u0027QANDP\u0027 AS OTHER_TYPE,OBJECT_ID AS OBJECT_ID,OBJECT_NAME AS OBJECT_NAME\nFROM MS_GRC_QUESTION_PROC\nwhere \u0027QANDP\u0027 \u003d :1\nAND SYSDATE BETWEEN NVL (valid_from, SYSDATE) AND NVL (valid_until,SYSDATE) \nAND ms_apps_security_pkg.check_object_access (dd_object_type,restrict_access_to,owner_organizations,:2,\u0027VIEW\u0027) \u003d \u0027ACCESS\u0027\n   \nUNION  \nSELECT \u0027REGBODY\u0027 AS OTHER_TYPE,OBJECT_ID AS OBJECT_ID,OBJECT_NAME AS OBJECT_NAME \nFROM MS_GRC_REGULATORY_BODY  \nwhere \u0027REGBODY\u0027 \u003d :1\nAND SYSDATE BETWEEN NVL (valid_from, SYSDATE) AND NVL (valid_until,SYSDATE) \nAND ms_apps_security_pkg.check_object_access (dd_object_type,restrict_access_to,owner_organizations,:2,\u0027VIEW\u0027) \u003d \u0027ACCESS\u0027\n\nUNION                                                  \nSELECT \u0027REQMENT\u0027 AS OTHER_TYPE,OBJECT_ID AS OBJECT_ID,OBJECT_NAME AS OBJECT_NAME\nFROM MS_GRC_REQUIREMENT \nwhere \u0027REQMENT\u0027 \u003d :1\nAND SYSDATE BETWEEN NVL (valid_from, SYSDATE) AND NVL (valid_until,SYSDATE)\nAND ms_apps_security_pkg.check_object_access (dd_object_type,restrict_access_to,owner_organizations,:2,\u0027VIEW\u0027) \u003d \u0027ACCESS\u0027 \n                                              \nUNION                                                  \nSELECT \u0027REFERENCE\u0027 AS OTHER_TYPE,OBJECT_ID AS OBJECT_ID,OBJECT_NAME AS OBJECT_NAME \nFROM MS_GRC_REFERENCE \nwhere \u0027REFERENCE\u0027 \u003d :1 \nAND SYSDATE BETWEEN NVL (valid_from, SYSDATE) AND NVL (valid_until,SYSDATE)\nAND ms_apps_security_pkg.check_object_access (dd_object_type,restrict_access_to,owner_organizations,:2,\u0027VIEW\u0027) \u003d \u0027ACCESS\u0027\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
