    "columns": "\u003ctable class\u003d\"table table-hover infolet-table-cnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003e#\u003c/th\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eType\u003c/th\u003e\r\n        \u003cth\u003eResult Column Name\u003c/th\u003e\r\n        \u003cth\u003eUser Column Name\u003c/th\u003e\r\n        \u003cth\u003eDefault Infolet/\u003cbr\u003eInput Infolet\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e1\u003c/td\u003e\r\n            \u003ctd\u003e DISTINCT FDO.USER_ID  \u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_ID\u003c/td\u003e\r\n            \u003ctd\u003eUser ID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e2\u003c/td\u003e\r\n            \u003ctd\u003e FDO.USER_NAME  \u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_LOGIN_NAME\u003c/td\u003e\r\n            \u003ctd\u003eUser Login Name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e3\u003c/td\u003e\r\n            \u003ctd\u003e MS_APPS_UTILITIES.GET_USER_FULL_NAME(FDO.USER_ID) USER_FULL_NAME  \u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_NAME\u003c/td\u003e\r\n            \u003ctd\u003eUser Name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e4\u003c/td\u003e\r\n            \u003ctd\u003e FDO.ROLE_ID  \u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eROLE_ID\u003c/td\u003e\r\n            \u003ctd\u003eRole ID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e5\u003c/td\u003e\r\n            \u003ctd\u003e FDO.ROLE_NAME  \u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_ROLE\u003c/td\u003e\r\n            \u003ctd\u003eUser Role\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e6\u003c/td\u003e\r\n            \u003ctd\u003e FDO.ACTIVITY_ID  \u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eACTIVITY_ID\u003c/td\u003e\r\n            \u003ctd\u003eActivity ID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e7\u003c/td\u003e\r\n            \u003ctd\u003e FDO.ACTIVITY_SHORT_NAME  \u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eACTIVITY_CODE\u003c/td\u003e\r\n            \u003ctd\u003eActivity Code\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e8\u003c/td\u003e\r\n            \u003ctd\u003eFDO.ACTIVITY_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eACTIVITY_NAME\u003c/td\u003e\r\n            \u003ctd\u003eActivity Name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS ISM Org Users\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            WITH  org_list AS (\n  SELECT TRIM(SUBSTR (txt, INSTR (txt, \u0027,\u0027, 1, level ) + 1, INSTR (txt, \u0027,\u0027, 1, level+1) - INSTR (txt, \u0027,\u0027, 1, level) -1)) as org_id\n  FROM ( select \u0027,\u0027||:1||\u0027,\u0027 as TXT from dual )\n\tCONNECT BY level \u0026lt;\u003d LENGTH(txt)-LENGTH(REPLACE(txt,\u0027,\u0027,\u0027\u0027))-1\n)\nSELECT DISTINCT fdo.user_id\n, fdo.user_name\n, ms_apps_utilities.get_user_full_name(fdo.user_id) USER_FULL_NAME\n, NULL \u0026quot;fdo.role_id\u0026quot;\n, NULL \u0026quot;fdo.role_name\u0026quot;\n, NULL \u0026quot;fdo.activity_id\u0026quot;\n, NULL \u0026quot;fdo.activity_short_name\u0026quot;\n, fdo.activity_name\nFROM ms_ism_org_flow_v fdo\nWHERE EXISTS ( SELECT 1\n  FROM org_list\n  WHERE org_list.org_id \u003d fdo.org_entity_id)\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-f\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eInitiate Issue\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Create Issue\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Approver\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Approve Issue\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Approver\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Approve Action\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Plan Approver\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027MS ISM Action Plan Approver\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Owner\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Issue Owner\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Reviewer\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Review Action\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Reviewer\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Review Issue\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Owner\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Implement Action\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Owner Reassigned\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Issue Owner\u0027)  AND (trim(User_Login_Name) !\u003d :ASSIGNEE)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction_Owner_Deligate\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Implement Action\u0027) AND (trim(User_Login_Name) !\u003d :Action_Owner)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eISSUE_DELEGATEE_1\u003c/td\u003e\r\n            \u003ctd\u003e(trim(User_Login_Name) !\u003d :x_issue_owner) AND\n(trim(Activity_Name) \u003d \u0027ISM - Issue Owner\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "parameters": "\u003ctable class\u003d\"table table-hover infolet-table-pnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eMandatory\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eORG_ID\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n            \u003ctd\u003eComma separated List of Organization IDs\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e"
  },
  "MS_ISM_ACTIONS": {
