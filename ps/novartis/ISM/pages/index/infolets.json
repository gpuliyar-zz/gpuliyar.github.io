    "columns": "\u003ctable class\u003d\"table table-hover infolet-table-cnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003e#\u003c/th\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eType\u003c/th\u003e\r\n        \u003cth\u003eResult Column Name\u003c/th\u003e\r\n        \u003cth\u003eUser Column Name\u003c/th\u003e\r\n        \u003cth\u003eDefault Infolet/\u003cbr\u003eInput Infolet\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e1\u003c/td\u003e\r\n            \u003ctd\u003eSTARTDATE\u003c/td\u003e            \u003ctd\u003eDate\u003c/td\u003e\r\n            \u003ctd\u003eSTARTDATE\u003c/td\u003e\r\n            \u003ctd\u003eSTARTDATE\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e2\u003c/td\u003e\r\n            \u003ctd\u003eTITLE\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eTITLE\u003c/td\u003e\r\n            \u003ctd\u003eTITLE\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e3\u003c/td\u003e\r\n            \u003ctd\u003eDESCRIPTION\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eDESCRIPTION\u003c/td\u003e\r\n            \u003ctd\u003eDESCRIPTION\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e4\u003c/td\u003e\r\n            \u003ctd\u003eISMONTHEVENT\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eISMONTHEVENT\u003c/td\u003e\r\n            \u003ctd\u003eISMONTHEVENT\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e5\u003c/td\u003e\r\n     