    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOverDue This Month\u003c/td\u003e\r\n            \u003ctd\u003e(trim(OverDue_This_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eOverDue This Month\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eDue This Month\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Due_This_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eDue This Month\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOverDue Past 3 Months\u003c/td\u003e\r\n            \u003ctd\u003e(trim(OverDue_Past_3_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eOverDue Past 3 Months\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOverDue Past Beyond 3 Months\u003c/td\u003e\r\n            \u003ctd\u003e(trim(OverDue_Beyond_3_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eOverDue Past Beyond 3 Months\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eDue In Next X Months\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Due_In_Next_X_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eDue In Next X Months\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eDue Beyond X Months\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Due_Beyond_X_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eDue Beyond X Months\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \