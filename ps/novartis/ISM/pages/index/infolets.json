    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_STI_ISSUE_PRIORITY_RPT_INFOLET\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT (SELECT    \u0027\u0026lt;a target\u003d\u0026quot;_self\u0026quot; href\u003d\u0026quot;javascript\u0027|| CHR (58)|| \u0027callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027\n|| ms_ism_utilities.get_ism_infolet_id (\u0027ISSUE\u0027) || CHR (38)|| \u0027proc\u003d\u0027 || a.dd_process_instance_id|| CHR (38)|| \u0027flag\u003d1\u0027\n|| CHR (38)|| \u0027emd\u003d1\u0027|| CHR (38) || \u0027x_edit_mode\u003dYes\u0027|| CHR (38)|| \u0027wrapper\u003dno\u0027\u0027)\u0026quot;\u0026gt;\u0027|| a.issue_title|| \u0027\u0026lt;/a\u0026gt;\u0027FROM DUAL) AS issue_title,\nms_ism_helper.get_status_display_name(a.issue_status) AS issue_status,ms_apps_utilities.get_user_full_name (a.issue_owner) AS issue_owner,\nnvl(ms_apps_utilities.get_display_value(100000,\u0027MS ISM Risk Rating\u0027,a.issue_risk_rating,1, NULL,NULL ),\u0027Unknown\u0027) AS issue_rating,\nnvl(ms_apps_utilities.get_display_value(100000,\u0027MS ISM Priority\u0027,a.issue_risk_priority, 1,NULL,NULL),\u0027Unknown\u0027) AS priority,a.issue_due_date AS issue_due_date,\nms_apps_utilities.get_user_full_name(a.dd_created_by)AS issue_initiator, a.issue_identified_on AS first_identified_on, a.issue_id AS issue_id,\nREGEXP_REPLACE (a.issue_details, \u0027\u0026lt;[^\u0026gt;]+\u0026gt;\u0027, NULL) AS issue_details,(SELECT COUNT (1)FROM ms_ism_action WHERE issue_id \u003d a.issue_id) AS no_of_actions,\n(SELECT org_entity_name  FROM si_org_entities_t WHERE org_entity_id \u003d a.issue_ownerorg) AS issue_owner_org,\nms_apps_utilities.get_display_value (100000,\u0027MS ISM Update Status\u0027,a.progress_status,1,NULL,NULL) AS progress_status,\na.dd_process_instance_id AS pid, a.dd_instance_id AS instanceid,a.issue_approverorg AS dummy1, NULL AS dummy2, NULL AS dummy3, NULL AS dummy4,NULL AS dummy5\n  FROM ms_ism_issue a  WHERE a.issue_status not in (\u00271\u0027,\u00275\u0027,\u00276\u0027) \n     AND a.reached_manage \u003d \u0027Y\u0027\n    AND ( 1 \u003d ms_ism_helper.fn_ism_securities(issue_id, :3) )    AND nvl(ms_apps_utilities.get_display_value (100000,\u0027MS ISM Priority\u0027, a.issue_risk_priority, 1,NULL, NULL),\u0027Unknown\u0027) \u003d :1\n   AND DECODE (a.issue_status ,\u002711\u0027, \u0027Open\u0027,\u002714\u0027, \u0027Open\u0027\n              ,\u002715\u0027, \u0027Open\u0027,\u002710\u0027, \u0027Closed\u0027\n              ,\u00277\u0027, \u0027Cancelled\u0027,\u002721\u0027, \u0027Open\u0027 \n              ,\u00274\u0027, \u0027Open\u0027,\u00278\u0027, \u0027Open\u0027\n              ,\u00276\u0027, \u0027Open\u0027 ,\u002717\u0027, \u0027Open\u0027\n              ,\u00275\u0027, \u0027Open\u0027,\u0027Open\u0027  ) \u003d :2\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eArea of Compliance Name\u003c/td\u003e\r\n            \u003ctd\u003e(trim(upper(DISPLAY_NAME)) like upper(:Area of Compliance Name))\u003c/td\u003e\r\n            \u003ctd\u003eArea of Compliance Name\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eInclude Expired?\u003c/td\u003e\r\n            \u003ctd\u003e( EXPIRED_FLAG \u003c\u003d :Include Expired Items)\u003c/td\u003e\r\n            \u003ctd\u003eInclude Expired?\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eArea of Compliance ID\u003c/td\u003e\r\n            \u003ctd\u003e(trim(upper(Area_of_Compliance_ID)) like upper(:Area of Compliance ID))\u003c/td\u003e\r\n            \u003ctd\u003eArea of Compliance ID\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eValidObject\u003c/td\u003e\r\n            \u003ctd\u003e(trim(EXPIRED_FLAG) !\u003d \u00272\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eValidObject\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "parameters": "\u003ctable class\u003d\"table table-hover infolet-table-pnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eMandatory\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eissue_priority\u003c/td\u003e\r\n            \u003ctd\u003eN\u003c/td\u003e\r\n            \u003ctd\u003eissue_priority\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eissue_status\u003c/td\u003e\r\n            \u003ctd\u003eN\u003c/td\u003e\r\n            \u003ctd\u003eissue_status\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eLOGIN_ID\u003c/td\u003e\r\n            \u003ctd\u003eN\u003c/td\u003e\r\n            \u003ctd\u003eLOGIN ID\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e"
