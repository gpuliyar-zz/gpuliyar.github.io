    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_CHANGE_HISTORY\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            select distinct a.issue_id,\nms_ism_helper.fn_get_ism_form_url (\u0027ISSUE\u0027,a.modified_by,b.issue_title,b.dd_process_instance_id,\u0027CHANGE HISTORY\u0027) As Issue_Title,\nms_apps_utilities.get_user_full_name(a.modified_by) As Modified_BY,\na.action_taken,\n\u0027\u0026lt;img title\u003d\u0026quot;Detailed Change History\u0026quot; alt\u003d\u0026quot;Detailed Change History\u0026quot; src\u003d\u0026quot;/images/icons/reports-stack.png\u0026quot;\u0026gt;\u0027 As Detail,\nchange_history_id,\ncast(a.modified_on as timestamp)  As Modified_on ,\nb.issue_title,\nconcat(htf.ESCAPE_SC(b.issue_title), ms_ism_helper.fnGetChangeHisImgLink(a.issue_id,:1)) as Issue_Detail,\nComments\nfrom ms_ism_change_history a, \nms_ism_issue b \nwhere \na.issue_id\u003db.issue_id\nand ( 1 \u003d ms_ism_helper.fn_ism_securities(b.issue_id, :1) ) order by Modified_on desc\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default