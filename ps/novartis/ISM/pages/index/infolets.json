    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_GET_ISSUE_TITLE\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT issue_ownerorg,\n       issue_approverorg,\n       a.issue_id AS issue_id,\n       issue_title,\n       NULL AS col1,\n       NULL AS col2,\n       NULL AS col3,\n       NULL AS col4,\n       NULL AS col5,\n       TRUNC (SYSDATE) AS col6,\n       TRUNC (SYSDATE) AS col7\n  FROM TABLE (ms_ism_helper.get_issue_title (:1, :2, :3)) a                          \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eArea of Compliance Name\u003c/td\u003e\r\n            \u003ctd\u003e(trim(upper(DISPLAY_NAME)) like upper(:Area of Compliance Name))\u003c/td\u003e\r\n            \u003ctd\u003eArea of Compliance Name\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eInclude Expired?\u003c/td\u003e\r\n            \u003ctd\u003e( EXPIRED_FLAG \u003c\u003d :Include Expired Items)\u003c/td\u003e\r\n            \u003ctd\u003eInclude Expired?\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eArea of Compliance ID\u003c/td\u003e\r\n            \u003ctd\u003e(trim(upper(Area_of_Compliance_ID)) like upper(:Area of Compliance ID))\u003c/td\u003e\r\n            \u003ctd\u003eArea of Compliance ID\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eValidObject\u003c/td\u003e\r\n            \u003ctd\u003e(trim(EXPIRED_FLAG) !\u003d \u00272\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eValidObject\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
