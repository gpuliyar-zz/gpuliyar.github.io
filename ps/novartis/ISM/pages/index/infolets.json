       \u003ctd\u003eURL\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eURL\u003c/td\u003e\r\n            \u003ctd\u003eURL\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e6\u003c/td\u003e\r\n            \u003ctd\u003eEND_DATE\u003c/td\u003e            \u003ctd\u003eDate\u003c/td\u003e\r\n            \u003ctd\u003eEND_DATE\u003c/td\u003e\r\n            \u003ctd\u003eEND_DATE\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e7\u003c/td\u003e\r\n            \u003ctd\u003eOBJID\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eOBJID\u003c/td\u003e\r\n            \u003ctd\u003eOBJID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e8\u003c/td\u003e\r\n            \u003ctd\u003eOBJTYPE\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eOBJTYPE\u003c/td\u003e\r\n            \u003ctd\u003eOBJTYPE\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_CALENDAR\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT a.issue_due_date As startdate, a.issue_title As title, a.issue_id As description, 0 As ismonthevent,       ms_ism_helper.get_calendar_form_url(\u0027ISSUE\u0027,a.dd_process_instance_id,a.dd_instance_id,\u00275\u0027) AS url,\n       a.issue_due_date as end_date, NULL As objid, NULL As objtype\n  FROM ms_ism_issue a\n  where issue_status NOT IN (\u00277\u0027, \u002710\u0027)\nand TO_DATE (a.ISSUE_DUE_DATE, \u0027DD/MM/RRRR\u0027)\u0026gt;\u003dTO_DATE (sysdate, \u0027DD/MM/RRRR\u0027)\n  and exists (select b.process_instance_id \n        from si_event_assignments b \n        where user_id\u003dms_apps_utilities.get_user_id(:1) and b.process_instance_id\u003da.dd_process_instance_id)\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-f\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Title\u003c/td\u003e\r\n            \u003ctd\u003e(trim(upper(DISPLAY_NAME)) like upper(\u0027%\u0027||:Issue Title||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Owner\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(USER_FULL_NAME)) \u003d upper(:Issue Owner))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOwner Organization\u003c/td\u003e\r\n            \u003ctd\u003e(trim(upper(OWNER_ORG)) \u003d upper(:Owner Organization))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eSource Type\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(SOURCE_TYPE)) \u003d UPPER(:Source Type))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Rating\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Issue_Rating)) \u003d UPPER(:Issue Rating))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003ePriority\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Priority)) \u003d UPPER(:Priority))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Status\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(ISSUE_STATUS)) \u003d UPPER(:Issue Status))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eRelated to Core Objects\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Core_Objects)) like UPPER(\u0027%\u0027||:Related To Core Object||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eRelated To Organization\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(ORG)) like UPPER(\u0027%\u0027||:Related To Organization||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eRelated to Compliance Controls\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Controls)) like UPPER(\u0027%\u0027||:Related To Compliance Controls||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eRelated to Area of Compliance\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Area_of_Compliance)) like UPPER(\u0027%\u0027||:Related To Area of Compliance||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "parameters": "\u003ctable class\u003d\"table table-hover infolet-table-pnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eMandatory\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eLOGIN_ID\u003c/td\u003e\r\n            \u003ctd\u003eN\u003c/td\u003e\r\n            \u003ctd\u003eLOGIN_ID\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e"
  },
