    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eArea of Compliance Name\u003c/td\u003e\r\n            \u003ctd\u003e(trim(upper(DISPLAY_NAME)) like upper(:Area of Compliance Name))\u003c/td\u003e\r\n            \u003ctd\u003eArea of Compliance Name\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eInclude Expired?\u003c/td\u003e\r\n            \u003ctd\u003e( EXPIRED_FLAG \u003c\u003d :Include Expired Items)\u003c/td\u003e\r\n            \u003ctd\u003eInclude Expired?\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eArea of Compliance ID\u003c/td\u003e\r\n            \u003ctd\u003e(trim(upper(Area_of_Compliance_ID)) like upper(:Area of Compliance ID))\u003c/td\u003e\r\n            \u003ctd\u003eArea of Compliance ID\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eValidObject\u003c/td\u003e\r\n            \u003ctd\u003e(trim(EXPIRED_FLAG) !\u003d \u00272\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eValidObject\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
