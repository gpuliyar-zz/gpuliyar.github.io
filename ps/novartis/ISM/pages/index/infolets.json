    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue ID\u003c/td\u003e\r\n            \u003ctd\u003e(trim(ISSUE_ID) \u003d :Issue ID)\u003c/td\u003e\r\n            \u003ctd\u003eIssue ID\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eChange History ID\u003c/td\u003e\r\n            \u003ctd\u003e(CHANGE_HISTORY_ID \u003d :Change History ID)\u003c/td\u003e\r\n            \u003ctd\u003eChange History ID Filter\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eField Label\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Field_Label)) like UPPER(\u0027%\u0027||(:Field Label)||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eField Label\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eChanged From\u003c/td\u003e\r\n            \u003ctd\u003e( dbms_lob.instr(UPPER(Changed_From),UPPER(:Changed From))!\u003d0)\u003c/td\u003e\r\n            \u003ctd\u003eChanged From\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eChanged To\u003c/td\u003e\r\n            \u003ctd\u003e( dbms_lob.instr(UPPER(Changed_To),UPPER(:Changed To))!\u003d0)\u003c/td\u003e\r\n            \u003ctd\u003eChanged To\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eChanged By\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Changed_By)) like UPPER(\u0027%\u0027||(:Changed By)||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eChanged By\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ct