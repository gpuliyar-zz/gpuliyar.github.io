    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_Get_Open_Issue_List\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT a.action_title  AS Action_Title,ms_apps_utilities.get_user_full_name (a.action_owner) AS Action_Owner,\n          to_char(a.action_due_date,\u0027Mon-dd-yyyy\u0027) AS Action_Due_Date,\n          a.action_status AS Status,\n          \u0027Normal\u0027 AS Type,\n          ms_apps_utilities.get_display_value (100000,\n                                               \u0027MS ISM Risk Rating\u0027,\n                                               a.action_priority,\n                                               1,\n                                               NULL,\n                                               NULL) AS Priority,\n          a.remove_delegation AS Perct_Complete,\n          a.action_comments AS Comments\n     FROM ms_ism_action_v a\nWHERE a.issue_id \u003d :1\nAND a.instance_id \u003d (select max(instance_id) from ms_ism_action_v where action_id \u003d a.action_id)\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eIssue Owner\u003c/td\u003e\r\n            \u003ctd\u003e(User_ID in (SELECT user_id FROM MS_ISM_ORG_FLOW_V WHERE activity_name \u003d \u0027ISM - Issue Owner\u0027 and org_entity_id in (select org_entity_id from MS_ISM_ORG_FLOW_V where user_name \u003d (\u0027@LOGIN_ID@\u0027))))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
