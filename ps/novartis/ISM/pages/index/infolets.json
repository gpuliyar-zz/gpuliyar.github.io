    "columns": "\u003ctable class\u003d\"table table-hover infolet-table-cnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003e#\u003c/th\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eType\u003c/th\u003e\r\n        \u003cth\u003eResult Column Name\u003c/th\u003e\r\n        \u003cth\u003eUser Column Name\u003c/th\u003e\r\n        \u003cth\u003eDefault Infolet/\u003cbr\u003eInput Infolet\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e1\u003c/td\u003e\r\n            \u003ctd\u003eDISTINCTFDO.USER_ID\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_ID\u003c/td\u003e\r\n            \u003ctd\u003eUser ID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e2\u003c/td\u003e\r\n            \u003ctd\u003eFDO.USER_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_NAME\u003c/td\u003e\r\n            \u003ctd\u003eUser Name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e3\u003c/td\u003e\r\n            \u003ctd\u003eFDO.USER_FULL_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_FULL_NAME\u003c/td\u003e\r\n            \u003ctd\u003eUser Full Name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS ISM Org Review Users\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            WITH  org_list AS (\n  SELECT COLUMN_VALUE AS ORG_ID FROM TABLE(MS_APPS_UTILITIES.SPLIT_STRING(:1,\u0027,\u0027))\n)\nSELECT DISTINCT fdo.user_id\n, fdo.user_name\n, fdo.user_full_name\nFROM ms_ism_org_flow_v fdo\nWHERE fdo.activity_name \u003d \u0027ISM - Review Issue\u0027\n  AND EXISTS ( SELECT 1\n  FROM org_list\n  WHERE org_list.org_id \u003d fdo.org_entity_id)\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003ePrev_Stage\u003c/td\u003e\r\n            \u003ctd\u003e(Previous_Stage \u003d :Previous Stage)\u003c/td\u003e\r\n            \u003ctd\u003ePrevious Stage\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eCurr_Stage\u003c/td\u003e\r\n            \u003ctd\u003e(Current_Stage \u003d :Current Stage)\u003c/td\u003e\r\n            \u003ctd\u003eCurrent Stage\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eUser Role\u003c/td\u003e\r\n            \u003ctd\u003e(trim(ROLE_ACTIVITY) IN (SELECT activity_name\n                            FROM ms_qms_flow_down_org_v fdo\n                            Where user_name \u003d nvl(:User_Name,user_name )\n                             And activity_name like \u0027ISM%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003eUser Role and Activity\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eCheck_Action\u003c/td\u003e\r\n            \u003ctd\u003e(ms_ism_helper.check_actions(action_id,:p_prev_stage,:p_curr_stage,decode(:p_delegatee,\u0027NIL\u0027,\u0027\u0027,:p_delegatee),nvl(:p_issue_id,\u0027\u0027),nvl(:p_action_id,\u0027\u0027),:p_curr_user,:p_obj_type)\u003d1)\u003c/td\u003e\r\n            \u003ctd\u003eCheck Actions at all the stages.\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
