    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ORB_Add_Question_Proc\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            select \u0027\u0026lt;input type\u003dcheckbox id\u003d\u0026quot;\u0027 || Object_Id ||\n       \u0027\u0026quot; value\u003d\u0026quot;\u0027 || substr(htf.ESCAPE_SC(Object_Name),1,3600)||\u0027$#$\u0027||ms_grc_utilities.get_form_url(dd_object_type,object_id) || \u0027\u0026quot;onClick\u003dcheckboxChange(\u0026quot;\u0027 ||\n       htf.ESCAPE_SC(Object_Id) || \u0027\u0026quot;)\u0026gt;\u0026lt;/input\u0026gt;\u0027 As Checkbox,\n       object_id, object_name \nFROM MS_GRC_QUESTION_PROC_PUB_V\nwhere \nms_apps_security_pkg.check_object_access(dd_object_type, restrict_access_to, OWNER_ORGANIZATIONS, :1, \u0027VIEW\u0027) \u003d \u0027ACCESS\u0027\nand \nTRUNC(sysdate) BETWEEN  NVL(TRUNC(valid_from),TRUNC(sysdate)) AND NVL(TRUNC(valid_until),TRUNC(sysdate)) order by object_name,object_id\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003