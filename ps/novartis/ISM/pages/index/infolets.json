    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_001_Issue_List_Report\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            select issue_title,issue_status,issue_owner,issue_rating,priority,source_type,issue_due_date,\n     issue_initiator,first_identified_on,issue_id,\n      issue_details,no_of_actions,issue_owner_org,issue_approverorg,\ndecode(issue_status_stored,\u002710\u0027,\u0027\u0027,progress_status) as progress_status,Org,     Area_of_Compliance,Controls,Risks,Core_Objects,PID,EXCEPTION_TYPE,Standard,\nAsset,\nAsset_Class,\nFinancial_Account,\nFunctionObj,\nFramework_Reference,\nObjective,\nProcess,\nReference,\nRegulatory_Bod,\nRequirement,\nQuestion_Procedure,\nExceptionObj,\nSupplier,\nAuditable_Entity,\nIssue_Type,\ndd_created_on,\nissue_source_stored As issue_source_stored,\nissue_status_stored AS issue_status_stored \nfrom MS_ISM_DETAILED_ISSUE_LIST a    \nWHERE \n issue_status_stored NOT IN (\u00271\u0027, \u00275\u0027, \u00276\u0027)      \nAND reached_manage \u003d \u0027Y\u0027\nand (1\u003dms_ism_helper.fn_ism_securities(a.issue_id,:1))\nORDER BY org_issue_title\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027