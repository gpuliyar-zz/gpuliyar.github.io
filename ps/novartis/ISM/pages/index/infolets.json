    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_MY_ACTIONS_RPT\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT ms_ism_helper.\n               get_link_url_for_my_actions (\u0027ACTION\u0027,\n                                            A.DD_PROCESS_INSTANCE_ID,\n                                            A.ACTION_STATUS,\n                                            A.ACTION_TITLE)  AS ACTION_TITLE,\n       ms_ism_helper.get_status_display_name(A.ACTION_STATUS) AS ACTION_STATUS,\n       MS_ISM_UTILITIES.GET_USER_FULL_NAME (A.ACTION_OWNER, \u0027\u0027) AS ACTION_OWNER,\n       B.ISSUE_TITLE AS ISSUE_TITLE,\n       A.ACTION_DUE_DATE,\n       B.ISSUE_DUE_DATE,\n       (TO_DATE (SYSDATE, \u0027DD/MM/RRRR\u0027)- TO_DATE (A.ACTION_DUE_DATE, \u0027DD/MM/RRRR\u0027))\n          AS Days_past_due,\n       A.NO_OF_EXTENSIONS AS NO_OF_EXTENSIONS\nFROM MS_ISM_ACTION A, MS_ISM_ISSUE B\n WHERE A.ISSUE_ID \u003d B.ISSUE_ID AND (A.ACTION_OWNER\u003d:1 OR A.ACTION_DELEGATEE\u003d:1)\nAND A.ACTION_STATUS IN (\u00272\u0027,\u00275\u0027,\u00276\u0027,\u002716\u0027,\u002721\u0027)\n ORDER BY A.DD_CREATED_ON desc \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"Select