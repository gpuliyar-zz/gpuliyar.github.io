    "xml": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_Get_Reassign_Owner_Link_Rpt\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"xml\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \u0026lt;?xml version\u003d\u0026quot;1.0\u0026quot;?\u0026gt;\u0026lt;METRIC_DEPLOYMENT\u0026gt;\u0026lt;METRIC       PROCESS\u003d\u0026quot;0\u0026quot;  INSTANCE\u003d\u0026quot;0\u0026quot;  NOTIFY\u003d\u0026quot;YES\u0026quot;   RETURN\u003d\u0026quot;RESULTSET\u0026quot; \u0026gt;\u0026lt;RESULTMAP \u0026gt;\u0026lt;/RESULTMAP\u0026gt;\u0026lt;/METRIC\u0026gt;\u0026lt;/METRIC_DEPLOYMENT\u0026gt;\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "columns": "\u003ctable class\u003d\"table table-hover infolet-table-cnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003e#\u003c/th\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eType\u003c/th\u003e\r\n        \u003cth\u003eResult Column Name\u003c/th\u003e\r\n        \u003cth\u003eUser Column Name\u003c/th\u003e\r\n        \u003cth\u003eDefault Infolet/\u003cbr\u003eInput Infolet\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e1\u003c/td\u003e\r\n            \u003ctd\u003eRADIO\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eRADIO\u003c/td\u003e\r\n            \u003ctd\u003eRADIO\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e2\u003c/td\u003e\r\n            \u003ctd\u003eF.USER_ID\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_ID\u003c/td\u003e\r\n            \u003ctd\u003eUSER_ID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e3\u003c/td\u003e\r\n            \u003ctd\u003eF.USER_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_NAME\u003c/td\u003e\r\n            \u003ctd\u003eUSER_NAME\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e4\u003c/td\u003e\r\n            \u003ctd\u003eMS_APPS_UTILITIES.GET_USER_FULL_NAME(F.USER_ID)USER_FULL_NAMEFR\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_FULL_NAME\u003c/td\u003e\r\n            \u003ctd\u003eUser_full_name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_Get_Reassign_Owner_Link_Rpt\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n             With Sub_Qry As (Select  E.User_Id, \n   a.activity_id,\n   a.activity_short_name,\n   a.activity_name\n  FROM si_activities_t a,\n    si_role_activities b,\n    si_org_roles c,\n    si_org_entities_t d,\n    si_user_org_roles e\n  WHERE a.activity_id \u003d b.activity_id\n  AND b.role_id       \u003d c.role_id\n  And C.Org_Entity_Id \u003d D.Org_Entity_Id\n  And C.Org_Role_Id   \u003d E.Org_Role_Id\n  And C.Org_Entity_Id \u003d :1\n  and a.activity_Name \u003d \u0027ISM - Issue Owner\u0027)\n Select   \n\tMS_ISM_HELPER.get_select_url_for_rpt (\n\t  Ms_Apps_Utilities.Get_User_Full_Name(f.User_Id),\n\t  F.USER_NAME,\n\t  \u0027RADIO\u0027)\n\t  AS Radio,\n\tF.User_Id,\n    F.User_Name,\n    Ms_Apps_Utilities.Get_User_Full_Name(f.User_Id) User_Full_Name\n  From Si_Users_T F\n  WHERE EXISTS (SELECT 1 FROM SUB_QRY\n  Where F.User_Id \u003d Sub_Qry.User_Id) and F.User_Name\u0026lt;\u0026gt;:2\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Approver\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Approve Action\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Owner\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Activity_Name) \u003d \u0027ISM - Implement Action\u0027)\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
