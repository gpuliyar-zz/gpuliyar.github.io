    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_AGEING_ISSUELISTRPT_DRILL_INF\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT issue_title,\n  issue_status,\n  issue_owner,\n  issue_rating,\n  priority,\n  source_type AS issue_source_type,\n  issue_due_date,\n  issue_initiator,\n  FIRST_IDENTIFIED_ON AS issue_identified_on,\n  no_of_actions,\n  issue_id,\n  issue_owner_org   AS issue_ownerorg_disp,\n  issue_approverorg AS issue_approverorg_disp,\n  OWNERORG          AS issue_ownerorg,\n  approverorg       AS issue_approverorg,\n  progress_status,\n  org,\n  area_of_compliance,\n  controls,\n  risks,\n  core_objects,\n  OVERDUE_THIS_MONTH    AS OverDue_This_Month_Flag,\n  DUE_THIS_MONTH        AS Due_This_Month_Flag,\n  OD_PAST_THREE_MONTH   AS OverDue_Past_3_Months_Flag,\n  OD_BEYOND_THREE_MONTH AS OverDue_Beyond_3_Months_Flag,\n  DUE_WITHIN_X_MONTH    AS Due_In_Next_X_Months_Flag,\n  DUE_BEYOND_X_MONTH    AS Due_Beyond_X_Months_Flag,\n  NULL                  AS Col1,\n  NULL                  AS Col2\nFROM MS_ISM_DETAILED_ISSUE_LIST_TBL a\n where ( :1 is null or OWNERORG in (:1)\n)\nand exists (select 1 from ms_grc_novt_user_org_flow_tbl where to_char(ORG_ENTITY_ID) \u003d OWNERORG and user_name \u003d :2) \nAND EXISTS\n  (SELECT 1 FROM MS_ISM_ISSUE_RPT_TBL WHERE issue_id\u003da.issue_id\n  )\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOverDue This Month\u003c/td\u003e\r\n            \u003ctd\u003e(trim(OverDue_This_Month) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eOverDue This Month\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eDue This Month\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Due_This_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eDue This Month\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOverDue Past 3 Months\u003c/td\u003e\r\n            \u003ctd\u003e(trim(OverDue_Past_3_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eOverDue Past 3 Months\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOverDue Past Beyond 3 Months\u003c/td\u003e\r\n            \u003ctd\u003e(trim(OverDue_Beyond_3_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eOverDue Past Beyond 3 Months\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eDue In Next X Months\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Due_In_Next_X_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eDue In Next X Months\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eDue Beyond X Months\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Due_Beyond_X_Month_Flag) \u003d \u0027Y\u0027)\u003c/td\u003e\r\n            \u003ctd\u003eDue Beyond X Months\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c