    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_006_Action_List_Report\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            select action_title,action_status,action_owner,priority,action_due_date,\naction_completed_on,issue_title,issue_owner,\ndecode(action_status, \u0027Closed\u0027, \u0027\u0027, percentage_completed) as percentage_completed,pid,\ninstanceid,owner_organization from MS_ISM_DETAILED_ACTION_LIST a\nwhere  (1\u003dms_ism_helper.fn_ism_securities(a.issue_id,:1))\nORDER BY action_title\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003ePriority\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Priority)) \u003d UPPER(:Priority))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eStatus\u003c/td\u003e\r\n            \u003ctd\u003e(trim(Action_Status) \u003d (:Action Status))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eissue Title\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Issue_Title)) like UPPER(\u0027%\u0027||:Issue Title||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eAction Title\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(SUBSTR(Action_Title,INSTR(Action_Title,\u0027\u003e\u0027)+1,INSTR(Action_Title,\u0027\u003c/a\u0027)-1))) like UPPER(\u0027%\u0027||:Action Title||\u0027%\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOrganization\u003c/td\u003e\r\n            \u003ctd\u003e(trim(UPPER(Owner_Organization)) \u003d UPPER(:Organization))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
