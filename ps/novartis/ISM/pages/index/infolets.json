    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_STI_ISSUE_RATING_RPT_INFOLET280320150848\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT (SELECT    \u0027\u0026lt;a target\u003d\u0026quot;_self\u0026quot; href\u003d\u0026quot;javascript\u0027\n               || CHR (58)\n               || \u0027callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027\n               || ms_apps_utilities.get_infolet_id (\u0027MS_STI_ISSUE\u0027)\n               || CHR (38)\n               || \u0027proc\u003d\u0027\n               || a.dd_process_instance_id\n               || CHR (38)\n               || \u0027flag\u003d1\u0027\n               || CHR (38)\n               || \u0027emd\u003d1\u0027\n               || CHR (38)\n               || \u0027x_edit_mode\u003dYes\u0027\n               || CHR (38)\n               || \u0027wrapper\u003dno\u0027\u0027)\u0026quot;\u0026gt;\u0027\n               || a.issue_title\n               || \u0027\u0026lt;/a\u0026gt;\u0027\n          FROM DUAL) AS issue_title,\n       a.issue_status AS issue_status,\n       ms_apps_utilities.get_user_full_name (a.issue_owner) AS issue_owner,\n       ms_apps_utilities.get_display_value\n                                       (100000,\n                                        \u0027MS ISM STI Risk Rating\u0027,\n                                        a.issue_risk_rating,\n                                        1,\n                                        NULL,\n                                        NULL\n                                       ) AS issue_rating,\n       ms_apps_utilities.get_display_value\n                                          (100000,\n                                           \u0027MS ISM Priority\u0027,\n                                           a.issue_risk_priority,\n                                           1,\n                                           NULL,\n                                           NULL\n                                          ) AS priority,\n       a.issue_due_date AS issue_due_date,\n       ms_apps_utilities.get_user_full_name\n                                          (a.dd_created_by)\n                                                           AS issue_initiator,\n       a.issue_identified_on AS first_identified_on, a.issue_id AS issue_id,\n       a.issue_details AS issue_details,\n       (SELECT COUNT (1)\n          FROM ms_ism_action\n         WHERE issue_id \u003d a.issue_id) AS no_of_actions,\n       (SELECT org_entity_name\n          FROM si_org_entities_t\n         WHERE org_entity_id \u003d a.issue_ownerorg) AS issue_owner_org,\n       ms_apps_utilities.get_display_value\n                                  (100000,\n                                   \u0027MS ISM Update Status\u0027,\n                                   a.progress_status,\n                                   1,\n                                   NULL,\n                                   NULL\n                                  ) AS progress_status,\n       a.dd_process_instance_id AS pid, a.dd_instance_id AS instanceid,\n       NULL AS dummy1, NULL AS dummy2, NULL AS dummy3, NULL AS dummy4,\n       NULL AS dummy5\n   FROM ms_ism_issue a, ms_sti_issue b\nWHERE a.issue_id \u003d b.issue_id and\n  ms_apps_utilities.get_display_value\n                                       (100000,\n                                        \u0027MS ISM STI Risk Rating\u0027,\n                                        a.issue_risk_rating,\n                                        1,\n                                        NULL,\n                                        NULL\n                                       )\u003d:1\n     AND DECODE (a.issue_status,\n                \u0027Issue Cancelled\u0027, \u0027Cancelled\u0027,\n                 \u0027New - Issue Cancelled\u0027, \u0027Cancelled\u0027,\n                 \u0027Mon - Issue Cancelled\u0027,\u0027Cancelled\u0027,\n                 \u0027Man - Issue Cancelled\u0027,\u0027Cancelled\u0027,\n                 \u0027Issue Closed\u0027, \u0027Closed\u0027,\n                 \u0027Man - Issue Closed\u0027, \u0027Closed\u0027,\n                 \u0027Mon - Issue Closed\u0027,\u0027Closed\u0027,\n                 \u0027Unknown\u0027, \u0027Unknown\u0027,\n                 \u0027Open\u0027\n                ) \u003d :2\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOwner Org\u003c/td\u003e\r\n            \u003ctd\u003e(trim(ORG_ENTITY_NAME) IN ( select org_entity_name from MS_QMS_FLOW_DOWN_ORG_V\nwhere user_name \u003d (\u0027@LOGIN_ID@\u0027)))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
