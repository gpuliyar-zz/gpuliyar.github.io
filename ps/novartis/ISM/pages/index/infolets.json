    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ISSUE_AGEING_RPT_INF\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT Org_id          AS Org_id,\r\n  Org_name             AS Org_name,\r\n  ISSUE_SOURCE_TYPE    AS ISSUE_SOURCE_TYPE,\r\n  OVER_DUE             AS OVER_DUE,\r\n  DUE_THIS_MONTH       AS DUE_THIS_MONTH,\r\n  OVERDUE_PAST_3_MONTH AS OVERDUE_PAST_3_MONTH,\r\n  DUE_BEYOND_3_MONTH   AS DUE_BEYOND_3_MONTH,\r\n  DUE_NEXT_X_MONTH     AS DUE_NEXT_X_MONTH,\r\n  DUE_BEYOND_X_MONTH   AS DUE_BEYOND_X_MONTH,\r\n  total                AS total,\r\n  Owner_Org            AS Owner_Org,\r\n  col1                 AS Col1,\r\n  col2                 AS Col2,\r\n  col3                 AS COL3\r\nFROM  ms_ism_issue_ageing_rpt_v a\r\n WHERE  exists (select 1 from ms_grc_novt_user_org_flow_tbl where ORG_ENTITY_ID \u003d a.Org_id and user_name \u003d :2) AND\r\n (:1 IS NULL or Org_id in (:1))\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/di