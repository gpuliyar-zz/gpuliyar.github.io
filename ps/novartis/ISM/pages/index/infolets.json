    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_Action_List_Report_Infolet280320150848\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT  (SELECT \u0027\u0026lt;a target\u003d\u0026quot;_self\u0026quot; href\u003d\u0026quot;javascript\u0027||CHR(58)||\u0027callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027||ms_apps_utilities.get_infolet_id(\u0027MS_ISM_ACTION\u0027)||CHR(38)||\u0027proc\u003d\u0027||a.dd_process_instance_id|| CHR (38)|| \u0027flag\u003d1\u0027|| CHR (38)|| \u0027emd\u003d1\u0027|| CHR (38)|| \u0027x_edit_mode\u003dYes\u0027 || CHR (38) || \u0027wrapper\u003dno\u0027\u0027)\u0026quot;\u0026gt;\u0027||a.action_title|| \u0027\u0026lt;/a\u0026gt;\u0027 from dual)  as Action_Title\n       ,a.action_status as Action_Status\n       ,ms_apps_utilities.get_user_full_name(a.action_owner) as Action_Owner\n       ,MS_APPS_UTILITIES.GET_DISPLAY_VALUE(100000,\u0027MS ISM Risk Rating\u0027,a.action_priority,1,null,null) as Priority \n       ,a.action_due_date as Action_Due_Date\n       ,NULL as Action_Completed_On\n       ,a.issue_title as Issue_Title\n       ,ms_apps_utilities.get_user_full_name(a.issue_owner) as Issue_Owner\n       ,a.action_per_complte as Percentage_Completed\n       ,a.dd_process_instance_id as pid\n       ,a.dd_instance_id as instanceid\n       ,a.REMOVE_DELEGATION as Dummy1\n       ,NULL as Dummy2\n       ,NULL as Dummy3\n       ,NULL as Dummy4\n        FROM ms_ism_action a,ms_ism_issue b\n WHERE a.issue_id \u003d b.issue_id and\n a.issue_owner_org \u003d NVL (:1,issue_owner_org)\n    and (\n          (:2 is null)\n             or\n          (:2 is not null and upper(a.issue_title) like \u0027%\u0027 || upper(:2) || \u0027%\u0027)   \n       )\n    and (\n          (:3 is null)\n             or\n          (:3 is not null and upper(a.action_title) like \u0027%\u0027 || upper(:3) || \u0027%\u0027)   \n       )\n    and (\n          (:4 is null)\n             or\n          (:4 is not null and a.action_priority \u003d :4)   \n       ) \n    and (\n          (:5 is null)\n             or\n          (:5 is not null and a.action_status \u003d :5)   \n       )\n        AND ms_apps_security_pkg.check_object_access (b.dd_object_type,\n                                                   b.restrict_access_to,\n                                                   b.owner_organizations,\n                                                   :6,\n                                                   \u0027VIEW\u0027\n                                                  ) \u003d \u0027ACCESS\u0027 \n        order by a.action_title\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e\r\n",
    "filters": "\u003ctable class\u003d\"table table-hover infolet-table-fnr\"\u003e\r\n    \u003cthead\u003e\r\n        \u003cth\u003eName\u003c/th\u003e\r\n        \u003cth\u003eExpression\u003c/th\u003e\r\n        \u003cth\u003eDescription\u003c/th\u003e\r\n    \u003c/thead\u003e\r\n    \u003ctbody\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003eOrganization\u003c/td\u003e\r\n            \u003ctd\u003e(trim(User_Name) \u003d (\u0027@LOGIN_ID@\u0027))\u003c/td\u003e\r\n            \u003ctd\u003e\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
