    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ACTION_LIST_RPT_DRILL_DOWN_INF\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT action_title as Action_Title\r\n       ,ms_ism_helper.get_status_display_name(action_status) as Action_Status\r\n       ,ms_apps_utilities.get_user_full_name(action_owner) as Action_Owner\r\n       ,MS_APPS_UTILITIES.GET_DISPLAY_VALUE(100000,\u0027MS ISM Risk Rating\u0027,action_priority,1,null,null) as Priority\r\n,action_per_complte as Percentage_Completed\r\n       ,action_due_date as Action_Due_Date\r\n       ,Null as Action_Completed_On\r\n       ,issue_title as Issue_Title\r\n       ,ms_apps_utilities.get_user_full_name(issue_owner) as Issue_Owner\r\n       ,dd_process_instance_id as pid\r\n       ,dd_instance_id as instanceid\r\n       ,(select MS_CONCAT((Select Displayed_Value From Ms_Qs_Lov_Org_Values_v where LOV_NAME\u003d\u0027MS_GRC_CONTROL_OWNER\u0027 and stored_value\u003dperformed_by))\r\nfrom ms_ism_issue_apb where ISSUE_ID\u003d:1 and ISSUE_ACTION_ID\u003daction_id)  as performed_by\r\n  FROM ms_ism_action\r\n  WHERE issue_id\u003d:1\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u