    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_Closed_Issue_By_Src_Priority\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            select  issue_source_type,\n      ms_ism_helper.get_lov_count(\u00271\u0027,a.issue_source_type,:1,\u0027MS_ISM_CLOSED_ISSUE\u0027) As High,\n      ms_ism_helper.get_lov_count(\u00272\u0027,a.issue_source_type,:1,\u0027MS_ISM_CLOSED_ISSUE\u0027) As Medium,\n      ms_ism_helper.get_lov_count(\u00273\u0027,a.issue_source_type,:1,\u0027MS_ISM_CLOSED_ISSUE\u0027) As Low,\n      ms_ism_helper.get_lov_count(\u00274\u0027,a.issue_source_type,:1,\u0027MS_ISM_CLOSED_ISSUE\u0027) As Dummy1,\n      ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027,\u00271\u0027, 1, NULL, NULL)  AS priority1,\n      ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027,\u00272\u0027, 1, NULL, NULL)  AS priority2,\n      ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027,\u00273\u0027, 1, NULL, NULL)  AS priority3,\n      ms_apps_utilities.get_display_value (100000, \u0027MS ISM Priority\u0027,\u00274\u0027, 1, NULL, NULL)  AS priority4,\nms_ism_helper.get_status_display_name(\u002710\u0027)                               AS param1\n      from MS_ISM_CLOSED_ISSUE a  where a.issue_risk_priority is not null\n        group by issue_source_type\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e