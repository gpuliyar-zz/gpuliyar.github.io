        \u003ctd\u003e8\u003c/td\u003e\r\n            \u003ctd\u003eFDO.ACTIVITY_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eACTIVITY_NAME\u003c/td\u003e\r\n            \u003ctd\u003eActivity Name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e1\u003c/td\u003e\r\n            \u003ctd\u003eDISTINCTFDO.USER_ID\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_ID\u003c/td\u003e\r\n            \u003ctd\u003eUser ID\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e2\u003c/td\u003e\r\n            \u003ctd\u003eFDO.USER_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_STORED_NAME\u003c/td\u003e\r\n            \u003ctd\u003eUser Stored Name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n        \u003ctr\u003e\r\n            \u003ctd\u003e3\u003c/td\u003e\r\n            \u003ctd\u003eFDO.USER_FULL_NAME\u003c/td\u003e            \u003ctd\u003eVarchar2\u003c/td\u003e\r\n            \u003ctd\u003eUSER_NAME\u003c/td\u003e\r\n            \u003ctd\u003eUser Name\u003c/td\u003e            \u003ctd\u003e--/ --\u003c/td\u003e\r\n        \u003c/tr\u003e\r\n    \u003c/tbody\u003e\r\n\u003c/table\u003e",
    "query": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_ISM_ACTION_DELEGATEE\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            SELECT DISTINCT fdo.user_id\n, fdo.user_name\n, fdo.user_full_name\n, NULL As \u0026quot;role_id\u0026quot;\n, NULL As \u0026quot;role_name\u0026quot;\n, NULL As \u0026quot;activity_id\u0026quot;\n, NULL As \u0026quot;activity_short_name\u0026quot;\n, fdo.activity_name\nFROM ms_ism_org_flow_v fdo\nWHERE (trim(activity_name) \u003d \u0027ISM - Implement Action\u0027) AND \n(trim(user_name) !\u003d :1) AND\n:2\u003d \u00271\u0027 AND\nfdo.org_entity_id \u003d:3\nunion\nSELECT DISTINCT fdo.user_id\n, fdo.user_name\n, fdo.user_full_name \n, NULL As \u0026quot;role_id\u0026quot;\n, NULL As \u0026quot;role_name\u0026quot;\n, NULL As \u0026quot;activity_id\u0026quot;\n, NULL As \u0026quot;activity_short_name\u0026quot;\n, fdo.activity_name\nFROM ms_ism_org_flow_v fdo\nWHERE (trim(activity_name) \u003d \u0027ISM - Implement Action\u0027) AND\n(trim(user_name) !\u003d :1) AND\n(trim(user_name) !\u003d :2) AND\nfdo.org_entity_id \u003d:3\r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r