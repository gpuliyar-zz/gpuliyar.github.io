  "MS_RSK_SCNR_ID_S": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_SCNR_ID_S\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"NOVT_DEV\".\"MS_RSK_SCNR_ID_S\"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 100000 NOCACHE  NOORDER  NOCYCLE \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_ONGOING_ASSMNT_RPT_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_ONGOING_ASSMNT_RPT_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_ONGOING_ASSMNT_RPT_V\" (\"PLAN_ID\", \"PLAN_NAME\", \"ASSMNT_ID\", \"ASSMNT_NAME\", \"RISK_ID\", \"RISK_NAME\", \"ORGANIZATION_ID\", \"ORGANIZATION_NAME\", \"CORE_OBJECT_ID\", \"CORE_OBJECT_NAME\", \"TREND\", \"INHERENT_SCORE\", \"INHERENT_RATING\", \"RESIDUAL_SCORE\", \"RESIDUAL_RATING\", \"OVERALL_CONTROL_SCORE\", \"OVERALL_CONTROL_EFFECTIVENESS\", \"LAST_ASSESSED_ON\", \"LAST_ASSESSED_BY\", \"LAST_APPROVED_ON\", \"LAST_APPROVED_BY\", \"INHERENT_SCORE_CORE\", \"INHERENT_RATING_CORE\", \"INHERENT_SCORE_ORG\", \"INHERENT_RATING_ORG\", \"INHERENT_SCORE_RSK\", \"INHERENT_RATING_RSK\", \"RESIDUAL_SCORE_ORG\", \"RESIDUAL_RATING_ORG\", \"RESIDUAL_SCORE_RSK\", \"RESIDUAL_SCORE_CORE\", \"RESIDUAL_RATING_RSK\", \"RESIDUAL_RATING_CORE\", \"PLAN_PID\", \"PLAN_INSTANCE_ID\", \"PLAN_METRIC_ID\", \"ASSMNT_PID\", \"ASSMNT_INSTANCE_ID\", \"ASSMNT_METRIC_ID\", \"SCENARIO_ID\", \"ASSMNT_TYPE\", \"SCH_ATS_PK\", \"RESTRICT_ACCESS_TO\", \"PLAN_OWNER_ORGS\", \"DD_OBJECT_TYPE\", \"CURRENT_STAGE\", \"RISK_OWNER\", \"ASSESS\", \"APPROVE\", \"DUMMY1\", \"DUMMY2\", \"DUMMY3\", \"DUMMY4\", \"DUMMY5\", \"DUMMY6\", \"DUMMY7\", \"DUMMY8\", \"DUMMY9\", \"DUMMY10\", \"TOTAL_CONTROL\", \"DISP_RSK_SCOR\", \"ADC_LIB_RISK_ADDED\", \"ORG_SEQ\", \"PRC_SEQ\", \"RSK_SEQ\") AS \n  SELECT plan_id,\n    --ms_rsk_utils.get_edit_url (\u0027MS_RSK_SCHEDULE_RISK_ASSMN\u0027, plan_pid, plan_instance_id, \u0027undefined\u0027, plan_name ) AS plan_name,\n    ms_rsk_rpt_utils.build_form_url (\u0027MS_RSK_SCHEDULE_RISK_ASSMN\u0027, plan_pid, plan_instance_id, \u0027N\u0027, plan_name ) AS plan_name,\n    assmnt_id,\n    -- ms_rsk_utils.get_asmt_url (\u0027MS_RSK_RISK_ASSESSMENT\u0027, assmnt_pid, assmnt_instance_id, \u0027undefined\u0027, assmnt_name ) as assmnt_name,\n    ms_rsk_rpt_utils.build_form_url (\u0027MS_RSK_RISK_ASSESSMENT\u0027, assmnt_pid, assmnt_instance_id, \u0027N\u0027, assmnt_name ) AS assmnt_name,\n    risk_id,\n    risk_url AS risk_name,\n    organization_id,\n    ms_rsk_utils.get_organizations (organization_id) AS organization_name,\n    core_object_id,\n    core_object_url                                                                                             AS core_object_name,\n    ms_rsk_rpt_utils.build_report_url (\u0027ONG_TRND\u0027, \u0027Assessment Trend\u0027, NULL, \u0027NULL\u0027, \u0027View\u0027, SCH_ATS_PK, NULL ) AS trend,\n    --ms_cmp_rpt_utils.build_report_url (\u0027GRCCONLR\u0027, \u0027MS_GRC_Control_Report\u0027, null, \u0027STATRISK\u0027, max (no_of_objects), risk_name, null ) as no_of_objects,\n    NULL AS inherent_score,\n    NULL AS inherent_rating,\n    NULL AS residual_score,\n    NULL AS residual_rating,\n    overall_control_score,\n    overall_control_effectiveness,\n    last_assessed_on,\n    ms_apps_utilities.get_user_full_name (last_assessed_by),\n    last_approved_on,\n    ms_apps_utilities.get_user_full_name (last_approved_by),\n    inherent_score_core,\n    inherent_rating_core,\n    inherent_score_org,\n    inherent_rating_org,\n    inherent_score_rsk,\n    inherent_rating_rsk,\n    residual_score_org,\n    residual_rating_org,\n    residual_score_rsk,\n    residual_score_core,\n    residual_rating_rsk,\n    residual_rating_core,\n    plan_pid,\n    plan_instance_id,\n    plan_metric_id,\n    assmnt_pid,\n    assmnt_instance_id,\n    assmnt_metric_id,\n    a.scenario_id,\n    assmnt_type,\n    sch_ats_pk,\n    restrict_access_to,\n    plan_owner_orgs,\n    dd_object_type,\n    current_stage,\n    ms_rsk_utils.get_risk_owners (risk_id)                                                                        AS risk_owner,\n    ms_rsk_utils.get_asmt_url (\u0027MS_RSK_RISK_ASSESSMENT\u0027, assmnt_pid, assmnt_instance_id, \u0027undefined\u0027, \u0027Assess\u0027 )  AS assess,\n    ms_rsk_utils.get_asmt_url (\u0027MS_RSK_RISK_ASSESSMENT\u0027, assmnt_pid, assmnt_instance_id, \u0027undefined\u0027, \u0027Approve\u0027 ) AS approve,\n    a.assessor                                                                                                    AS dummy1,\n    a.enable_assess_approve                                                                                       AS dummy2,\n    NULL                                                                                                          AS dummy3,\n    NULL                                                                                                          AS dummy4,\n    a.disp_overall_rsk_rat                                                                                                          as dummy5,\n    a.DISP_RES_RATING                                                                                                          AS dummy6,\n    a.assmnt_instance_rec_num                                                                                     AS dummy7,\n    NULL                                                                                                          AS dummy8,\n    NULL                                                                                                          AS dummy9,\n    NULL                                                                                                          AS dummy10,\n    a.total_control,\n    a.disp_rsk_scor,\n    a.adc_lib_risk_added,\n    a.org_seq,\n    a.prc_seq,\n    a.rsk_seq\n  FROM ms_rsk_ongoing_assmnt_rpt a,\n    ms_rsk_schedule_risk_a_ats ats\n    /*WHERE a.assmnt_id \u003d\n    (SELECT MAX(assmnt_id)\n    FROM ms_rsk_ongoing_assmnt_rpt b\n    WHERE a.sch_ats_pk \u003d b.sch_ats_pk\n    )*/\n  WHERE TRUNC(NVL(a.plan_valid_until,sysdate)) \u003e\u003d TRUNC(sysdate)\n  AND TRUNC(NVL(a.plan_valid_from,sysdate))     \u003c \u003d TRUNC(sysdate)\n  AND TRUNC(NVL(a.available_from,sysdate))     \u003c\u003d TRUNC(sysdate)\n  AND TRUNC(NVL(a.available_until,Sysdate))     \u003e \u003d TRUNC(Sysdate)\n  AND ats.allow_ad_hoc_assmnt                   \u003d \u00271\u0027\n  AND ats.ats_pk                                \u003d a.sch_ats_pk\n  AND a.assmnt_id                               \u003d\n    (SELECT MAX(assmnt_id)\n    FROM ms_rsk_ongoing_assmnt_rpt\n    where sch_ats_pk\u003da.sch_ats_pk\n    )\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
