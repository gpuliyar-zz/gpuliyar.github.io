  "MS_RSK_ORG_FLOW_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_ORG_FLOW_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_ORG_FLOW_V\" (\"ACTIVITY_ID\", \"ACTIVITY_NAME\", \"ACTIVITY_SHORT_NAME\", \"ROLE_ID\", \"ORG_ENTITY_ID\", \"ORG_ENTITY_NAME\", \"ORG_ROLE_ID\", \"USER_ID\", \"USER_NAME\", \"ROLE_NAME\", \"ORG_HIERARCHY_ID\", \"ORG_LEVEL\") AS \n  SELECT a.activity_id,\n          a.activity_name,\n          a.activity_short_name,\n          b.role_id,\n          c.org_entity_id,\n          d.org_entity_name,\n          e.org_role_id,\n          e.user_id,\n          f.user_name,\n          g.role_name,\n          d.org_hierarchy_id,\n          h.org_level\n     FROM si_activities_t a,\n          si_role_activities b,\n          si_org_roles c,\n          si_org_entities_t d,\n          si_user_org_roles e,\n          si_users_t f,\n          si_roles_t g,\n          SI_ORG_HIERARCHY_LEVELS_T h\n    WHERE     a.activity_id \u003d b.activity_id\n          AND b.role_id \u003d c.role_id\n          AND b.role_id \u003d g.role_id\n          AND c.org_entity_id \u003d d.org_entity_id\n          AND c.org_role_id \u003d e.org_role_id\n          AND e.user_id \u003d f.user_id\n          AND D.ORG_HIERARCHY_ID \u003d H.ORG_HIERARCHY_ID\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \