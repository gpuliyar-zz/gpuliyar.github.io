EW_DUE_DATE\",\"MNEW_APPROVER\",\"MNEW_ASSESSOR\",\"MR_ROLES\",\"MR_USERS\",\"MR_AVAILABLE_TO\",\"RISKS\",\"CORE_OBJECT\",\"ORGANIZATIONS\",\"DUE_DATE\",\"REVIEWER\",\"ASSESSOR\",\"NMR_ROLES\",\"NMR_USERS\",\"NMR_AVAILABLE_TO\",\"INHERIT_ASSMNT_SCOPE\",\"ASSESSMENT_TYPE\",\"SCENARIO_NAME\",\"OBJECT_ID\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\" from SI_102066_T \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_CORE_OBJECTS_PR_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_CORE_OBJECTS_PR_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_CORE_OBJECTS_PR_V\" (\"PROCESS_NAME\", \"PROCESS_ID\", \"OWNER_ORGANIZATIONS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DD_OBJECT_TYPE\") AS \n  SELECT case\n         when b.object_name is null then\n          case\n         when c.object_name is null then\n          d.object_name\n         else\n          c.object_name\n       end else b.object_name end as process_name,\n       a.object_id AS process_id,\n       A.ato_organization AS OWNER_ORGANIZATIONS,\n       b.valid_until AS valid_until,\n       B.VALID_FROM AS VALID_FROM,\n       case\n         when B.DD_OBJECT_TYPE is null then\n          case\n         when c.dd_object_type is null then\n          d.dd_object_type\n         ELSE\n          C.DD_OBJECT_TYPE\n       end else b.dd_object_type end as DD_OBJECT_TYPE\n  FROM ms_grc_core_object_org_pr_vw a,\n       ms_grc_core_object        b,\n       ms_grc_objectives         c,\n       ms_grc_product            d\n WHERE a.object_id \u003d b.object_id(+)\n   AND A.OBJECT_ID \u003d C.OBJECT_ID(+)\n   AND A.OBJECT_ID \u003d D.OBJECT_ID(+)\nUNION\n SELECT a.object_name as process_name,\n       a.object_id AS process_id,\n       b.OWNER_ORGANIZATIONS AS OWNER_ORGANIZATIONS,\n       a.valid_until AS valid_until,\n       a.VALID_FROM AS VALID_FROM,\n        a.DD_OBJECT_TYPE AS DD_OBJECT_TYPE\n  FROM MS_GRC_CORE_OBJECT        A ,\n  MS_GRC_CORE_OBJECT_OGN B\nWHERE A.OBJECT_ID \u003d B.OBJECT_ID(+)\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
