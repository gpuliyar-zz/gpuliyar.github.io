  "MS_RSK_RATNG_COREOBJ_REPORT": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_RATNG_COREOBJ_REPORT\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_RSK_RATNG_COREOBJ_REPORT\" AS OBJECT (\n core_object_id            VARCHAR2 (400),\n  core_object_name            VARCHAR2 (400),\n  SCENARIO_ID                VARCHAR2 (400),\n  SCENARIO_NAME                VARCHAR2 (800),\n  ASSESSMENT_TYPE            VARCHAR2 (400),\n  INHERENT_ROLLUP_RATING    VARCHAR2 (4000),\n  RESIDUAL_ROLLUP_RATING    VARCHAR2 (4000),\n  OBJECT_LEVEL_NUM            NUMBER(5),\n VIEW_ASSESSMENT             VARCHAR2 (200)\n);   \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_SL_DECODE_FORMULA": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_SL_DECODE_FORMULA\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_RSK_SL_DECODE_FORMULA\" AS OBJECT \n( \n  FACTOR_TYPE VARCHAR2(4000),\n  SUMOF VARCHAR2(4000),\n  PRODOF VARCHAR2(4000),\n  AVERAGE VARCHAR2(4000)\n)  \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_RISK_CATEGORY_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_RISK_CATEGORY_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_RISK_CATEGORY_V\" (\"RISK_CATEGORY\", \"CATEGORY_NAME\", \"RISK_ID\", \"RISK_NAME\") AS \n  SELECT C.CATEGORY_RISK               AS RISK_CATEGORY ,\n    MS_RSK_LND_REPORTING_UTILS.GET_CATEGORY_NAME(C.CATEGORY_RISK) AS CATEGORY_NAME,\n    B.OBJECT_ID                        AS RISK_ID,\n    B.OBJECT_NAME                      AS RISK_NAME\n  FROM MS_GRC_RISK B,\n    MS_GRC_RISK_CAT C\n  WHERE B.OBJECT_ID\u003dC.OBJECT_ID\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_CORE_OBJECTS_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_CORE_OBJECTS_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_CORE_OBJECTS_V\" (\"PROCESS_NAME\", \"PROCESS_ID\", \"OWNER_ORGANIZATIONS\", \"VALID_UNTIL\", \"VALID_FROM\", \"DD_OBJECT_TYPE\") AS \n  SELECT case\n         when b.object_name is null then\n          case\n         when c.object_name is null then\n          d.object_name\n         else\n          c.object_name\n       end else b.object_name end as process_name,\n       a.object_id AS process_id,\n       A.ato_organization AS OWNER_ORGANIZATIONS,\n       b.valid_until AS valid_until,\n       B.VALID_FROM AS VALID_FROM,\n       case\n         when B.DD_OBJECT_TYPE is null then\n          case\n         when c.dd_object_type is null then\n          d.dd_object_type\n         ELSE\n          C.DD_OBJECT_TYPE\n       end else b.dd_object_type end as DD_OBJECT_TYPE\n  FROM ms_grc_core_object_org_vw a,\n       ms_grc_core_object        b,\n       ms_grc_objectives         c,\n       ms_grc_product            d\n WHERE a.object_id \u003d b.object_id(+)\n   AND A.OBJECT_ID \u003d C.OBJECT_ID(+)\n   AND A.OBJECT_ID \u003d D.OBJECT_ID(+)\nUNION\n SELECT a.object_name as process_name,\n       a.object_id AS process_id,\n       b.OWNER_ORGANIZATIONS AS OWNER_ORGANIZATIONS,\n       a.valid_until AS valid_until,\n       a.VALID_FROM AS VALID_FROM,\n        a.DD_OBJECT_TYPE AS DD_OBJECT_TYPE\n  FROM MS_GRC_CORE_OBJECT        A,\n  MS_GRC_CORE_OBJECT_OGN B\nWHERE A.OBJECT_ID \u003d B.OBJECT_ID(+)\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_RATNG_ORG_REPORT": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_RATNG_ORG_REPORT\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_RSK_RATNG_ORG_REPORT\" AS OBJECT (\n ORGANIZATION_NAME            VARCHAR2 (400),\n  ORGANIZATION_ID            VARCHAR2 (400),\n  SCENARIO_ID                VARCHAR2 (400),\n  SCENARIO_NAME                VARCHAR2 (800),\n  ASSESSMENT_TYPE            VARCHAR2 (400),\n  INHERENT_ROLLUP_RATING    VARCHAR2 (4000),\n  RESIDUAL_ROLLUP_RATING    VARCHAR2 (4000),\n  OBJECT_LEVEL_NUM            NUMBER(5),\n  VIEW_ASSESSMENT             VARCHAR2 (200)\n);   \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_ORG_REPORT_TAB": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_ORG_REPORT_TAB\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_RSK_ORG_REPORT_TAB\" IS TABLE OF MS_RSK_org_Report   \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_SCHAMN_ID_S": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_SCHAMN_ID_S\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"NOVT_DEV\".\"MS_RSK_SCHAMN_ID_S\"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 100000 NOCACHE  NOORDER  NOCYCLE \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_LND_ASSMNT_STATUS_RPT_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_LND_ASSMNT_STATUS_RPT_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_LND_ASSMNT_STATUS_RPT_V\" (\"PLAN_NAME\", \"ASSMNT_ID\", \"ASSMNT_STATUS\", \"ASSMNT_ASSESSOR\", \"ASSMNT_APPROVER\", \"ASSMNT_ASSIGNED_ON\", \"ASSMNT_ASSESSED_ON\", \"ASSMNT_APPROVED_ON\", \"ASSMNT_DUE_DATE\", \"ASSMNT_PUBLISHED_ON\", \"SCENERIO\", \"ORGANIZATIONS\", \"CORE_OBJECTS\", \"RISKS\", \"VIEW_LINK\", \"ASSMNT_ID_VALUE\", \"ASSMNT_TYPE\", \"DUMMY1\", \"DUMMY2\", \"DUMMY3\", \"NO_OF_DAYS_DELAYED\", \"NEW_PLAN_ID\", \"NEW_PLAN_NAME\", \"ASSMNT_ASSESSOR_NAME\", \"ASSMNT_APPROVER_NAME\", \"FINAL_APPROVER_NAME\", \"ASSESSMENT_NAME\", \"FINAL_APPROVER_APPROVED_ON\", \"ASSMNT_TRIGD_FROM\", \"SECURITY_OWNER_ORGS\", \"RESTRICT_ACCESS_TO\", \"FINAL_APPROVER\", \"REVIEWER\", \"RESP_OWNER\", \"I_ASSMNT_STATUS\", \"I_PLAN_NAME\", \"I_ASSMNT_NAME\", \"PLAN_OWNER_ORG\") AS \n  SELECT\n    (SELECT MS_RSK_LND_REPORTING_UTILS.BUILD_FORM_URL (\u0027MS_RSK_SCHEDULE_RISK_ASSMN\u0027, PLAN_ID,\u0027PLAN\u0027,\u0027Y\u0027,PLAN_NAME,PLAN_PID,PLAN_INSTANCE_ID)\n    FROM DUAL\n    ) AS PLAN_NAME,\n    (SELECT MS_RSK_LND_REPORTING_UTILS.BUILD_FORM_URL(\u0027MS_RSK_RISK_ASSESSMENT\u0027,ASSMNT_ID,\u0027ASSMNT\u0027,\u0027Y\u0027,ASSMNT_ID,ASSMNT_PID , ASSMNT_INSTANCE_ID )\n    FROM DUAL\n    ) AS ASSMNT_ID,\n    ASSMNT_STATUS,\n    assmnt_assessor,\n    ASSMNT_APPROVER,\n    TRUNC(ASSMNT_ASSIGNED_ON)  AS ASSMNT_ASSIGNED_ON,\n    TRUNC(ASSMNT_ASSESSED_ON)  AS ASSMNT_ASSESSED_ON,\n    TRUNC(ASSMNT_APPROVED_ON)  AS ASSMNT_APPROVED_ON,\n    TRUNC(ASSMNT_DUE_DATE)     AS ASSMNT_DUE_DATE,\n    TRUNC(assmnt_published_on) AS assmnt_published_on,\n    SCENERIO,\n    SUBSTR(ORGANIZATIONS,instr(ORGANIZATIONS,\u0027,\u0027)+1,LENGTH(ORGANIZATIONS)) AS ORGANIZATIONS,\n    SUBSTR(CORE_OBJECTS,instr(CORE_OBJECTS,\u0027,\u0027)  +1,LENGTH(CORE_OBJECTS))  AS CORE_OBJECTS,\n    SUBSTR(RISKS,INSTR(RISKS,\u0027,\u0027)                +1,LENGTH(RISKS))         AS RISKS,\n    \u0027View\u0027                                                                 AS VIEW_LINK,\n    ASSMNT_ID                                                              AS ASSMNT_ID_VALUE,\n    ASSMNT_TYPE ,\n    (\n    CASE\n      WHEN(TRUNC(assmnt_due_date) \u003e TRUNC(assmnt_published_on))\n      THEN \u0027GREEN\u0027\n      WHEN(TRUNC(assmnt_due_date) \u003c TRUNC(assmnt_published_on))\n      THEN \u0027RED\u0027\n      WHEN(TRUNC(assmnt_due_date) \u003d TRUNC(assmnt_published_on))\n      THEN \u0027GREEN\u0027\n      ELSE NULL\n    END) AS DUMMY1,\n    NULL AS DUMMY2,\n    NULL AS Dummy3,\n    CASE\n      WHEN TRUNC(assmnt_due_date)     - TRUNC(assmnt_published_on) \u003c 0\n      THEN ABS(TRUNC(assmnt_due_date) - TRUNC(assmnt_published_on))\n    END       AS NO_OF_DAYS_DELAYED,\n    PLAN_ID   AS NEW_PLAN_ID,\n    PLAN_NAME AS NEW_PLAN_NAME,\n    (SELECT MS_RSK_LND_REPORTING_UTILS.GET_COMMA_SEPARATED_FULL_NAME(ASSMNT_ASSESSOR)\n    FROM DUAL\n    ) AS ASSMNT_ASSESSOR_NAME,\n   /* (SELECT MS_RSK_LND_REPORTING_UTILS.GET_FINAL_APPROVER_FULL_NAME(ASSMNT_ID,\u0027FA_NAME\u0027,ASSMNT_PID,ASSMNT_INSTANCE_ID)\n    FROM DUAL\n    ) AS ASSMNT_APPROVER_NAME,*/\n      MS_RSK_LND_REPORTING_UTILS.GET_COMMA_SEPARATED_FULL_NAME(ASSMNT_APPROVER) AS ASSMNT_APPROVER_NAME,\n    (\n    SELECT MS_RSK_LND_REPORTING_UTILS.GET_FINAL_APPROVER_FULL_NAME(ASSMNT_ID,\u0027FINAL_APPROVER\u0027,ASSMNT_PID,ASSMNT_INSTANCE_ID)\n    FROM DUAL\n    ) AS FINAL_APPROVER_NAME,\n    (SELECT MS_RSK_LND_REPORTING_UTILS.GET_FINAL_APPROVER_FULL_NAME(ASSMNT_ID,\u0027ASSMNT_NAME\u0027,ASSMNT_PID,ASSMNT_INSTANCE_ID)\n    FROM DUAL\n    )                          AS ASSESSMENT_NAME,\n    TRUNC(ASSMNT_PUBLISHED_ON) AS FINAL_APPROVER_APPROVED_ON,\n    ASSMNT_TRIGD_FROM,\n    (SELECT MS_RSK_LND_REPORTING_UTILS.MS_RSK_GET_PLAN_SECURITY_ORGS(PLAN_ID)\n    FROM DUAL\n    ) AS SECURITY_OWNER_ORGS,\n    (SELECT MS_RSK_LND_REPORTING_UTILS.MS_RSK_GET_PLAN_SECURITY_RAT(PLAN_ID)\n    FROM DUAL\n    ) AS RESTRICT_ACCESS_TO,\n    (SELECT MS_RSK_LND_REPORTING_UTILS.GET_FINAL_APPROVER_FULL_NAME(ASSMNT_ID,\u0027FA_NAME\u0027,ASSMNT_PID,ASSMNT_INSTANCE_ID)\n    FROM DUAL\n    ) AS FINAL_APPROVER,\n    (SELECT MS_RSK_LND_REPORTING_UTILS.GET_FINAL_APPROVER_FULL_NAME(ASSMNT_ID,\u0027REVIEWER\u0027,ASSMNT_PID,ASSMNT_INSTANCE_ID)\n    FROM DUAL\n    ) AS REVIEWER,\n    (SELECT MS_RSK_LND_REPORTING_UTILS.GET_FINAL_APPROVER_FULL_NAME(ASSMNT_ID,\u0027RESP_ONWER\u0027,ASSMNT_PID,ASSMNT_INSTANCE_ID)\n    FROM DUAL\n    )             AS RESP_OWNER,\n    ASSMNT_STATUS AS I_ASSMNT_STATUS,\n    PLAN_NAME     AS I_PLAN_NAME,\n    (SELECT MS_RSK_LND_REPORTING_UTILS.GET_FINAL_APPROVER_FULL_NAME(ASSMNT_ID,\u0027ASSMNT_NAME\u0027,ASSMNT_PID,ASSMNT_INSTANCE_ID)\n    FROM DUAL\n    ) as I_ASSMNT_NAME,\n    PLAN_OWNER_ORG\n  FROM MS_RSK_ASSMNT_RPT_DTLS\n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_CORE_RISK_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_CORE_RISK_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_CORE_RISK_V\" (\"OBJECT_ID\", \"OBJECT_NAME\") AS \n  SELECT distinct rn.object_id, rn.object_name\n     FROM ms_grc_risk rn , ms_grc_risk_org_vw ro\n     where rn.object_id \u003d ro.object_id(+)\n        AND SYSDATE BETWEEN NVL (valid_from, SYSDATE)\n                        AND NVL (valid_until, SYSDATE)\n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_GRC_CONTROL_INLINE_RPT": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_GRC_CONTROL_INLINE_RPT\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_GRC_CONTROL_INLINE_RPT\" (\"CHECKBOX\", \"OBJID_ROWNUM\", \"OBJECT_ID\", \"OBJECT_NAME\", \"C_TYPE\", \"C_KEY\", \"C_CATEGORY\") AS \n  SELECT    \u0027\u003cinput type\u003dcheckbox id\u003d\"\u0027\n          || object_id\n          || \u0027$\u0027\n          || ROWNUM\n          || \u0027\" value\u003d\"\u0027\n          || object_name\n          || \u0027\"onClick\u003djavascript:checkboxChange(\"\u0027\n          || object_id\n          || \u0027$\u0027\n          || ROWNUM\n          || \u0027\")\u003e\u003c/input\u003e\u0027 AS checkbox,\n          object_id || \u0027$\u0027 || ROWNUM AS objid_rownum, object_id,\n          object_name,\n          ms_apps_utilities.get_display_value\n                                         (\u0027100000\u0027,\n                                          \u0027MS_GRC_COMPLIANCE_TYPE\u0027,\n                                          compliance_type\n                                         ) AS c_type,\n          ms_apps_utilities.get_display_value\n                                    (\u0027100000\u0027,\n                                     \u0027MS_GRC_COMPLIANCE_KeyControl\u0027,\n                                     key_compliance\n                                    ) AS c_key,\n          ms_apps_utilities.get_display_value\n                                 (\u0027100000\u0027,\n                                  \u0027MS_GRC_COMPLIANCE_CATEGORY\u0027,\n                                  compliance_category\n                                 ) AS c_category\n     FROM ms_grc_compliance_obje \n \n \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_ORG_REPORT": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_ORG_REPORT\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_RSK_ORG_REPORT\" AS OBJECT (\n        organization_id           VARCHAR2 (4000),\n      Organization_name         VARCHAR2 (4000),\n      scenario_name            VARCHAR2 (4000),\n      assessment_type          VARCHAR2 (100),\n      scenario_id              VARCHAR2 (1000),\n      residual_rollup_rating   VARCHAR2 (1000),\n      level_num                NUMBER (3)\n);   \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_QUANT_ASSMNT_FACTOR_AV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_QUANT_ASSMNT_FACTOR_AV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_QUANT_ASSMNT_FACTOR_AV\" (\"CONFIGURE\", \"PREVIOUS_STAGE\", \"ATTACHMENTS\", \"LEVEL_1_APPROVER\", \"LEVEL_2_APPROVER\", \"OBJECT_ACTION\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"PROGRESS_STATUS\", \"RESTRICT_ACCESS_TO\", \"VALID_FROM\", \"VALID_UNTIL\", \"ACRONYM\", \"RTF_MULTI_ATTACHMENT\", \"ACTION_COMMENTS\", \"DEFAULT_RESPONSE\", \"DEFAULT_SCORE\", \"DEFAULT_WEIGHTING_FACTOR\", \"DESCRIPTION\", \"FACTOR_CONTRIBUTION\", \"FACTOR_TYPE\", \"MNR_SCORE\", \"OBJECT_ID\", \"OBJECT_NAME\", \"RESPONSE\", \"RISK_CATEGORY\", \"SCORING_TYPE\", \"SCR_SCORE\", \"VALUE_1\", \"VALUE_2\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\") AS \n  SELECT \"CONFIGURE\", \"PREVIOUS_STAGE\", \"ATTACHMENTS\",\n          (SELECT ms_apps_utilities.get_user_full_name\n                     (ms_apps_utilities.get_user_id (level_1_approver)\n                     )\n             FROM DUAL) AS level_1_approver,\n          (SELECT ms_apps_utilities.get_user_full_name\n                     (ms_apps_utilities.get_user_id (level_2_approver)\n                     )\n             FROM DUAL) AS level_2_approver,\n          (SELECT DISTINCT display_value\n                      FROM ms_rsk_quan_actions_v\n                     WHERE stored_value \u003d object_action) object_action,\n          \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"PROGRESS_STATUS\",\n          ms_apps_utilities.get_display_value\n                             (100000,\n                              \u0027MS RSK Restrict Access To\u0027,\n                              restrict_access_to\n                             ) restrict_access_to,\n          \"VALID_FROM\", \"VALID_UNTIL\", \"ACRONYM\", \"RTF_MULTI_ATTACHMENT\",\n          \"ACTION_COMMENTS\", \"DEFAULT_RESPONSE\", \"DEFAULT_SCORE\",\n          \"DEFAULT_WEIGHTING_FACTOR\", \"DESCRIPTION\",\n          ms_apps_utilities.get_display_value\n                 (100000,\n                  \u0027MS RSK List of Factor Categorization\u0027,\n                  factor_contribution\n                 ) factor_contribution,\n          ms_apps_utilities.get_display_value\n                                        (100000,\n                                         \u0027MS RSK Type of Factor\u0027,\n                                         factor_type\n                                        ) factor_type,\n          \"MNR_SCORE\", \"OBJECT_ID\", \"OBJECT_NAME\", \"RESPONSE\",\n          ms_apps_utilities.get_display_values\n                             (100000,\n                              \u0027MS RSK List of Risk Categories\u0027,\n                              risk_category,\n                              \u0027,\u0027\n                             ) risk_category,\n          ms_apps_utilities.get_display_value\n                                (100000,\n                                 \u0027MS RSK List of Scoring Rules\u0027,\n                                 scoring_type\n                                ) scoring_type,\n          \"SCR_SCORE\", \"VALUE_1\", \"VALUE_2\", \"DD_CURRENT_STAGE\",\n          \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\",\n          \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\",\n          \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\",\n          \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\",\n          \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\",\n          \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"DESCRIPTION_HTM\"\n     FROM ms_rsk_quant_assmnt_factor_v \n \n \n \n \n \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_HM_ALL": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_HM_ALL\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_RSK_HM_ALL\" as object(\n  ASSESSMENT_TYPE VARCHAR2(100 CHAR),  \n    ORGANIZATION_ID VARCHAR2(100 CHAR), \n    ORGANIZATION_NAME VARCHAR2(4000 CHAR),\n  CORE_OBJECT_ID VARCHAR2(100 CHAR), \n    CORE_OBJECT_NAME VARCHAR2(4000 CHAR),\n  RISK_ID VARCHAR2(100 CHAR), \n    RISK_NAME VARCHAR2(4000 CHAR),\n    PREV_INHERENT_SCORE NUMBER, \n    CURR_INHERENT_SCORE NUMBER,   \n    CURR_RESIDUAL_SCORE NUMBER, \n    PREV_RESIDUAL_SCORE NUMBER, \n    LEVEL_NUM NUMBER(2,0), \n    INHERENT_RATING VARCHAR2(100 CHAR), \n    RESIDUAL_RATING VARCHAR2(100 CHAR), \n    SCENARIO_ID VARCHAR2(100 CHAR), \n    PREV_RESIDUAL_RATING VARCHAR2(100 CHAR), \n    PREV_INHERENT_RATING VARCHAR2(100 CHAR),\n  SHORT_NAME VARCHAR2(100 CHAR),\n  FLAG VARCHAR2(100 CHAR),\n  VERSION_NO NUMBER,\n  RISK_CATEGORIES VARCHAR2(4000 CHAR),\n  OBJECT_OWNERS VARCHAR2(4000 CHAR))\n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_LOG_KEY": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_LOG_KEY\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n   CREATE SEQUENCE  \"NOVT_DEV\".\"MS_RSK_LOG_KEY\"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 102120 CACHE 20 NOORDER  NOCYCLE \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_GET_RISK_RELATIONSHIP": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_GET_RISK_RELATIONSHIP\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_GET_RISK_RELATIONSHIP\" (\"RISK_NAME\", \"RISK_ID\", \"RISK_CATEGORIES\", \"RELATED_PROCESS\", \"RELATED_ORGANIZATIONS\") AS \n  select distinct a.OBJECT_NAME as RISK_NAME,\n  a.OBJECT_ID AS RISK_ID,\n  (SELECT ms_concat(category_risk)\n  FROM MS_GRC_RISK_CAT C\n  WHERE a.object_id \u003d c.object_id\n  ) RISK_CATEGORIES,\n  MS_GRC_UTILITIES.GET_RELATIONSHIPS (OBJECT_ID, DD_OBJECT_TYPE, \u0027MS_GRC_PROCESS\u0027,\u0027OBJECT_ID\u0027 )       as RELATED_PROCESS,\n  ms_grc_utilities.get_relationships (object_id, DD_OBJECT_TYPE, \u0027MS_GRC_ORGANIZATION\u0027, \u0027OBJECT_ID\u0027 ) AS rELATED_ORGANIZATIONS\nFROM ms_grc_risk a\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_GRC_CTL_INLINE_RPT_VIEW": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_GRC_CTL_INLINE_RPT_VIEW\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_GRC_CTL_INLINE_RPT_VIEW\" (\"CHECKBOX\", \"OBJID_ROWNUM\", \"OBJECT_ID\", \"OBJECT_NAME\", \"C_TYPE\", \"C_KEY\", \"C_CATEGORY\") AS \n  SELECT     \u0027\u003cinput type\u003d\"radio\" name\u003d\"selectCheck\" value\u003d\u0027\n            || \u0027\"\u0027\n            ||a.object_name\n            || \u0027\"\u0027\n            || \u0027 id \u003d\u0027\n            || \u0027\"\u0027\n            || a.object_id\n            || \u0027\"\u0027\n            || \u0027 onClick\u003d\"javascript:populateValues(\u0027\u0027\u0027\n            || a.object_id\n            || \u0027\u0027\u0027)\"\u003e\u0027   AS checkbox,\n          a.object_id || \u0027$\u0027 || ROWNUM AS objid_rownum, a.object_id, a.object_name,\n          ms_apps_utilities.get_display_values\n                                         (\u0027100000\u0027,\n                                          \u0027MS_GRC_COMPLIANCE_TYPE\u0027,\n                                          a.compliance_type,\u0027,\u0027\n                                         ) AS c_type,\n                ms_apps_utilities.get_display_value\n                                    (\u0027100000\u0027,\n                                     \u0027MS_GRC_COMPLIANCE_KeyControl\u0027,\n                                     decode(a.key_compliance,\u0027yes\u0027,\u00271\u0027, \u0027no\u0027,\u00272\u0027,null)\n                                    ) AS c_key,\n         MS_APPS_UTILITIES.GET_DISPLAY_VALUES\n                                 (\u0027100000\u0027,\n                                  \u0027MS_GRC_COMPLIANCE_CATEGORY\u0027,\n                                  (SELECT MS_CONCAT(P.PURPOSE)  FROM ms_grc_compliance_obje O, MS_GRC_CONTROL_PUR P\nwhere  O.OBJECT_ID\u003dP.OBJECT_ID and O.OBJECT_ID\u003da.OBJECT_ID),\u0027,\u0027\n                                 ) AS c_category\n     FROM ms_grc_compliance_obje a where nvl(VALID_UNTIL,sysdate)\u003e\u003dsysdate\n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
