  "MS_RSK_SCENARIOS_DV": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_SCENARIOS_DV\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_SCENARIOS_DV\" (\"INITIATOR\", \"GROUP_NAME\", \"RTF_MULTI_ATTACHMENT\", \"OBJECT_ID\", \"DUMMY_REF\", \"CALCULATE_RESIDUAL_SCORES\", \"SCN_OBJECT_ID\", \"SCENARIO_ID\", \"CONTINUOUS_ASSESSMENT\", \"VALID_UNTIL\", \"VALID_FROM\", \"MANUAL_REFRESH\", \"OPERAND_FOR_STAN_SCORIN_SCENAR\", \"RAM_PROFILE\", \"SCENARIO_ALGORITHM\", \"RISK_ASSESSMENT_METHODOLOGY\", \"ADDITIONAL_INFORMATION\", \"OWNER_ORGANIZATION\", \"ASSESSED_BY\", \"RISK_CATEGORIES\", \"ASSESSABLE_ELEMENT\", \"ASSESSMENT_TYPE\", \"DEFAULT_SCENARIO\", \"OBJECT_NAME\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\", \"ADDITIONAL_INFORMATION_HTM\") AS \n  select \"INITIATOR\",\"GROUP_NAME\",\"RTF_MULTI_ATTACHMENT\",\"OBJECT_ID\",\"DUMMY_REF\",\"CALCULATE_RESIDUAL_SCORES\",\"SCN_OBJECT_ID\",\"SCENARIO_ID\",\"CONTINUOUS_ASSESSMENT\",\"VALID_UNTIL\",\"VALID_FROM\",\"MANUAL_REFRESH\",\"OPERAND_FOR_STAN_SCORIN_SCENAR\",\"RAM_PROFILE\",\"SCENARIO_ALGORITHM\",\"RISK_ASSESSMENT_METHODOLOGY\",\"ADDITIONAL_INFORMATION\",\"OWNER_ORGANIZATION\",\"ASSESSED_BY\",\"RISK_CATEGORIES\",\"ASSESSABLE_ELEMENT\",\"ASSESSMENT_TYPE\",\"DEFAULT_SCENARIO\",\"OBJECT_NAME\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\",\"ADDITIONAL_INFORMATION_HTM\" from SI_102076_T \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_LND_RATNG_ORG_REPORT": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_LND_RATNG_ORG_REPORT\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_RSK_LND_RATNG_ORG_REPORT\" AS OBJECT\n        (\n            organization_name           VARCHAR2 (400),\n            organization_id             VARCHAR2 (400),\n            scenario_id                 VARCHAR2 (400),\n            scenario_name               VARCHAR2 (800),\n            assessment_type             VARCHAR2 (400),\n            inherent_rollup_rating      VARCHAR2 (4000),\n            residual_rollup_rating      VARCHAR2 (4000),\n            object_level_num            NUMBER   (5),\n            view_assessment             VARCHAR2 (200),\n            residual_score              VARCHAR2 (4000),\n            inherent_score              VARCHAR2 (4000),\n            previous_residual_rating    VARCHAR2 (4000),\n            previous_residual_score     VARCHAR2 (4000),\n            inherent_weightage          VARCHAR2 (4000), --Added for Bug 212229 on 16-JUN-2014\n            residual_weightage          VARCHAR2 (4000), --Added for Bug 212229 on 16-JUN-2014\n            --Addition for all methodology calculation\n            risk_assessment_methodology NUMBER   (5),\n            weightage                   VARCHAR2 (4000),\n            inherent_x                  VARCHAR2 (4000),\n            inherent_y                  VARCHAR2 (4000),\n            weightage_inherent_x        VARCHAR2 (4000),\n            weightage_inherent_y        VARCHAR2 (4000),\n            weightage_inherent_rating   VARCHAR2 (4000),\n            residual_x                  VARCHAR2 (4000),\n            residual_y                  VARCHAR2 (4000),\n            weightage_residual_x        VARCHAR2 (4000),\n            weightage_residual_y        VARCHAR2 (4000),\n            weightage_residual_rating   VARCHAR2 (4000),\n            dummy_10                    VARCHAR2 (4000),\n            dummy_11                    VARCHAR2 (4000),\n            dummy_12                    VARCHAR2 (4000)\n            --End of Addition for all methodology calculation\n        )\n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_SCHEDULE_ACTIONS_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_SCHEDULE_ACTIONS_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_SCHEDULE_ACTIONS_V\" (\"STORED_VALUE\", \"DISPLAY_VALUE\", \"CURRENT_STAGE\", \"ROLE_ACTIVITY\", \"ACTION_ID\") AS \n  SELECT act.action_name stored_value, act.intermediate_status display_value,\n          stgact.current_stage current_stage, role_activity,\n          act.action_id action_id\n     FROM ms_rsk_actions act,\n          (SELECT *\n             FROM ms_rsk_stage_actions_mappings\n            WHERE stage_type \u003d 3 AND active \u003d \u0027Y\u0027) stgact\n    WHERE stgact.action_id \u003d act.action_id\n      AND stgact.active \u003d \u0027Y\u0027 \n \n \n \n \n \n \n \n \n \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_RATNG_PIE_ORG": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_RATNG_PIE_ORG\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE TYPE \"NOVT_DEV\".\"MS_RSK_RATNG_PIE_ORG\" AS\n  OBJECT (\n    RATING          VARCHAR2 (4000),\n    ASSESSMENT_TYPE VARCHAR2 (4000),\n    SCENARIO_ID     VARCHAR2 (4000),\n    ORGANIZATION_ID VARCHAR2 (4000),\n    RATING_COUNT    NUMBER(9),\n    ORGANIZATION_NAME VARCHAR2 (4000),\n    DUMMY1          VARCHAR2 (4000),\n    DUMMY2          VARCHAR2 (4000),\n    DUMMY3          VARCHAR2 (4000),\n    DUMMY4          VARCHAR2 (4000)\n   )  \n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_RISK_ASSMNT_TASK_REP_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_RISK_ASSMNT_TASK_REP_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_RISK_ASSMNT_TASK_REP_V\" (\"STATUS\", \"RISK_ASSESSMNT_PLAN\", \"PERSPECTIVE\", \"DUMMY_4\", \"DUMMY_3\", \"DUMMY_2\", \"DUMMY_1\", \"DUE_BEFORE\", \"ASSESSOR\", \"ASSESSMENT_TYPE\", \"ASSESSED_BY_ORG\", \"ASSESSED_AFTER\", \"ASSEMNT_TASK_REPORT_ID\", \"APPROVER\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\") AS \n  select \"STATUS\",\"RISK_ASSESSMNT_PLAN\",\"PERSPECTIVE\",\"DUMMY_4\",\"DUMMY_3\",\"DUMMY_2\",\"DUMMY_1\",\"DUE_BEFORE\",\"ASSESSOR\",\"ASSESSMENT_TYPE\",\"ASSESSED_BY_ORG\",\"ASSESSED_AFTER\",\"ASSEMNT_TASK_REPORT_ID\",\"APPROVER\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\" from SI_102071_T \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_RISK_GRC_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_RISK_GRC_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_RISK_GRC_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"OBJECT_TYPE\", \"OBJECT_SEQUENCE\", \"DESCRIPTION\", \"DESCRIPTION_HTM\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJ_STATUS\", \"VALID_FROM\", \"VALID_UNTIL\", \"OBJECT_LEVEL\", \"OBJECT_PARENT\", \"CATEGORY_RISK\", \"TYPE_RISK\", \"RTF_ATTACH\", \"DD_OBJECT_TYPE\", \"ATO_OWNER\", \"ATO_COMMENTS\", \"ATO_ORGANIZATION\") AS \n  select \nCa.OBJECT_ID,\nCa.OBJECT_NAME,\n\u0027Risk Object\u0027 Object_Type,\n\u00272\u0027 Object_Sequence,\nDESCRIPTION,\nDESCRIPTION_HTM,\nOWNERS,\nOWNER_ORGANIZATIONS,\nRESTRICT_ACCESS_TO,\nOBJ_STATUS,\nVALID_FROM,\nVALID_UNTIL,\nOBJECT_LEVEL,\nOBJECT_PARENT,\nCATEGORY_RISK,\nTYPE_RISK,\nRTF_ATTACH,\nDD_OBJECT_TYPE,\nAto_Owner,\nAto_Comments,\nAto_Organization\nfrom Ms_Grc_Risk_Pub_v Ca\n\nLEFT OUTER JOIN\n    (\n    SELECT\n      /*+ Index(b MS_GRC_COMPLIANCE_OBJE_ORG_IX)*/\n      (Xmlagg(Xmlelement(a,Nvl2(Ato_Owner, Ato_Owner\n      || \u0027,\u0027, Ato_Owner))).Extract(\u0027//text()\u0027)).GetClobVal() Ato_Owner,\n      (Xmlagg(Xmlelement(a,Nvl2(Ato_Comments,Ato_Comments\n      || \u0027,\u0027,Ato_Comments))).Extract(\u0027//text()\u0027)).GetClobVal() Ato_Comments,\n      (Xmlagg(Xmlelement(a,Nvl2(Ato_Organization,Ato_Organization\n      || \u0027,\u0027,Ato_Organization))).Extract(\u0027//text()\u0027)).GetClobVal() Ato_Organization,\n      b.Object_Id\n    FROM\n      (SELECT SRC_OBJ_ID AS object_id ,\n        TGT_OBJ_id       AS ATO_ORGANIZATION ,\n        NULL             AS Ato_Owner ,\n        NULL             AS Ato_Comments\n      FROM MS_GRC_ORB_RELATIONS_PUB_V\n      WHERE SRC_OBJ_TYPE\u003d\u0027MS_GRC_RISK\u0027\n      AND TGT_OBJ_TYPE  \u003d\u0027MS_GRC_ORGANIZATION\u0027\n      ) b\n    GROUP BY b.Object_Id\n    ) Cb\n  ON CA.OBJECT_ID \u003d CB.OBJECT_ID\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
