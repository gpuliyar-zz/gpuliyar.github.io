dbProgramsAndObjectsScriptData = {
  "MS_RSK_METRICS_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_METRICS_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_METRICS_V\" (\"NAME\", \"OBJECT_ID\", \"OBJECT_NAME\", \"ATI_OBJECT_ID\", \"FREQUENCY\", \"EXPECTED_VALUE\", \"PREVIOUS_VALUE\", \"CURRENT_VALUE\", \"CURRENT_DATE_VALUE\", \"CHANGE\", \"LAST_3ASMNT\", \"LAST_6ASMNT\", \"LAST_12ASMNT\", \"EVENT_ID\") AS \n  select b.object_name AS NAME,--\n    a.object_id,--\n    c.object_name AS object_name,--\n    a.ati_object_id,--\n    ms_apps_utilities.get_display_value (ms_grc_kpi_kri_utilities.get_ent_id (), \u0027MS GRC KI Frequency\u0027, frequency ) frequency,--\n    a.expected_value,--\n    a.previous_value,--\n    a.current_value,--\n    a.current_date_value,--\n    ms_grc_kpi_kri_utilities.for_breached_rpt (current_value, previous_value )                AS CHANGE,--\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 3 )  AS last_3asmnt,--\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 6 )  AS last_6asmnt,--\n    ms_grc_kpi_kri_utilities.fn_avg (a.current_date_value, a.ati_object_id, a.object_id, 12 ) AS last_12asmnt,--\n    --applies_to,--\n    event_id\n     FROM ms_grc_kpi_kri_data_en_pub_v a\n  join ms_grc_kpi_kri_defn_pub_v b on a.object_id \u003d b.object_id\n  join ms_grc_risk_pub_v c on c.object_id \u003d a.ati_object_id\n  join ms_grc_kpi_kri_defn_ati_pub_v t on t.object_id\u003d a.object_id and t.ati_obj_id \u003d a.ati_object_id\n -- left join ms_grc_kpi_kri_defn_aor g on g.ati_object_id_pk \u003d t.ati_object_id_pk\n  WHERE current_date_value IN\n    (SELECT MAX (current_date_value)\n    FROM ms_grc_kpi_kri_data_en_pub_v\n    WHERE object_id   \u003d a.object_id\n    AND ati_object_id \u003d a.ati_object_id\n    )\n\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_CMP_RPT": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_CMP_RPT\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_CMP_RPT\" (\"ISSUE_TITLE\", \"ISSUE_STATUS\", \"ISSUE_OWNER\", \"ISSUE_RATING\", \"PRIORITY\", \"SOURCE_TYPE\", \"ISSUE_DUE_DATE\", \"SSUE_INITIATOR\", \"FIRST_IDENTIFIED_ON\", \"ISSUE_ID\", \"ISSUE_DETAILS\", \"NO_OF_ACTIONS\", \"ISSUE_OWNER_ORG\", \"ISSUE_APPROVERORG_DIS\", \"ISSUE_APPROVERORG\", \"ISSUE_OWNERORG\", \"PROGRESS_STATUS\", \"PID\", \"INSTANCEID\") AS \n  SELECT (SELECT    \u0027\u003ca target\u003d\"_self\" href\u003d\"javascript\u0027\n                  || CHR (58)\n                  || \u0027callreportdetails(\u0027\u0027Pushinfolet?id\u003d\u0027\n                  || ms_apps_utilities.get_infolet_id (\u0027MS_ISM_ISSUE\u0027)\n                  || CHR (38)\n                  || \u0027proc\u003d\u0027\n                  || c.process_instance_id\n                  || CHR (38)\n                  || \u0027flag\u003d1\u0027\n                  || CHR (38)\n                  || \u0027emd\u003d1\u0027\n                  || CHR (38)\n                  || \u0027x_edit_mode\u003dYes\u0027\n                  || CHR (38)\n                  || \u0027wrapper\u003dno\u0027\u0027)\"\u003e\u0027\n                  || a.issue_title\n                  || \u0027\u003c/a\u003e\u0027\n             FROM DUAL) AS issue_title,\n          a.issue_status AS issue_status,\n          ms_apps_utilities.get_user_full_name (a.issue_owner) AS issue_owner,\n          ms_apps_utilities.get_display_value\n                                       (100000,\n                                        \u0027MS ISM Risk Rating\u0027,\n                                        a.issue_risk_rating,\n                                        1,\n                                        NULL,\n                                        NULL\n                                       ) AS issue_rating,\n          ms_apps_utilities.get_display_value\n                                          (100000,\n                                           \u0027MS ISM Priority\u0027,\n                                           a.issue_risk_priority,\n                                           1,\n                                           NULL,\n                                           NULL\n                                          ) AS priority,\n          ms_apps_utilities.get_display_value\n                                        (100000,\n                                         \u0027MS ISM Source Type\u0027,\n                                         a.issue_source_type,\n                                         1,\n                                         NULL,\n                                         NULL\n                                        ) AS source_type,\n          a.issue_due_date AS issue_due_date,\n          ms_apps_utilities.get_user_full_name\n                                          (a.dd_created_by)\n                                                           AS issue_initiator,\n          a.issue_identified_on AS first_identified_on,\n          a.issue_id AS issue_id, a.issue_details AS issue_details,\n          (SELECT COUNT (1)\n             FROM ms_ism_action\n            WHERE issue_id \u003d a.issue_id) AS no_of_actions,\n          (SELECT org_entity_name\n             FROM si_org_entities_t\n            WHERE org_entity_id \u003d a.issue_ownerorg) AS issue_owner_org,\n          (SELECT org_entity_name\n             FROM si_org_entities_t\n            WHERE org_entity_id \u003d\n                                 a.issue_approverorg)\n                                                     AS issue_approverorg_dis,\n          a.issue_ownerorg, a.issue_approverorg,\n          ms_apps_utilities.get_display_value\n                                  (100000,\n                                   \u0027MS ISM Update Status\u0027,\n                                   a.progress_status,\n                                   1,\n                                   NULL,\n                                   NULL\n                                  ) AS progress_status,\n          a.dd_process_instance_id AS pid, a.dd_instance_id AS instanceid\n     FROM ms_ism_issue a, ms_ism_issue_v c\n    WHERE a.issue_id \u003d c.issue_id\n      AND c.instance_id \u003d (SELECT MAX (instance_id)\n                             FROM ms_ism_issue_v\n                            WHERE issue_id \u003d a.issue_id)\n      AND c.instance_rec_num \u003d 1\n      AND a.issue_status NOT IN\n              (\u0027New\u0027, \u0027Issue Reviewed\u0027, \u0027Pending Review\u0027, \u0027Pending Approval\u0027)\n      AND EXISTS (SELECT previous_stage\n                    FROM ms_ism_issue_v\n                   WHERE issue_id \u003d a.issue_id AND dd_current_stage \u003d 4)\n      AND a.issue_action NOT IN (\u00271\u0027, \u00273\u0027) \n \n \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_ONGOING_TREND_RPT_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_ONGOING_TREND_RPT_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_ONGOING_TREND_RPT_V\" (\"VERSION_NO\", \"PUBLISHED_ON\", \"ASSMNT_ID\", \"ORGANIZATION_ID\", \"ORGANIZATION_NAME\", \"CORE_OBJECT_ID\", \"RISK_ID\", \"RISK_OWNER\", \"INHERENT_SCORE\", \"INHERENT_RATING\", \"INH_RAT_TREND\", \"INH_RAT_TREND_COMMENTS\", \"TOTAL_NO_OF_CONTROLS\", \"NO_OF_NEW_CONTROLS\", \"NO_OF_CONTROLS_ASSESSED\", \"OVERALL_CONTROL_SCORE\", \"OVERALL_CONTROL_EFFECTIVENESS\", \"RESIDUAL_SCORE\", \"RESIDUAL_RATING\", \"RES_RAT_TREND\", \"RES_RAT_TREND_COMMENTS\", \"LAST_ASSESSED_ON\", \"LAST_ASSESSED_BY\", \"LAST_APPROVED_ON\", \"LAST_APPROVED_BY\", \"NO_OF_ISSUES\", \"NO_OF_OPEN_ISSUES\", \"NO_OF_OVERDUE_ISSUES\", \"PLAN_ID\", \"ASSMNT_NAME\", \"PLAN_NAME\", \"CORE_OBJECT_NAME\", \"RISK_NAME\", \"ASSMNT_PID\", \"ASSMNT_INSTANCE_ID\", \"INHERENT_SCORE_ORG\", \"INHERENT_RATING_ORG\", \"INHERENT_SCORE_CORE\", \"INHERENT_RATING_CORE\", \"INHERENT_SCORE_RSK\", \"INHERENT_RATING_RSK\", \"RESIDUAL_SCORE_ORG\", \"RESIDUAL_RATING_ORG\", \"RESIDUAL_SCORE_CORE\", \"RESIDUAL_RATING_CORE\", \"RESIDUAL_SCORE_RSK\", \"RESIDUAL_RATING_RSK\", \"SCENARIO_ID\", \"ASSMNT_TYPE\", \"SCH_ATS_PK\", \"RESTRICT_ACCESS_TO\", \"PLAN_OWNER_ORGS\", \"DD_OBJECT_TYPE\", \"CURRENT_STAGE\", \"DUMMY1\", \"DUMMY2\", \"DUMMY3\", \"DUMMY4\", \"DUMMY5\", \"DUMMY6\", \"DUMMY7\", \"DUMMY8\", \"DUMMY9\", \"DUMMY10\", \"DUMMY11\", \"DUMMY12\", \"DUMMY13\", \"DUMMY14\", \"DUMMY15\", \"DUMMY16\", \"DUMMY17\", \"DUMMY18\", \"DUMMY19\", \"DUMMY20\", \"DUMMY21\", \"DUMMY22\", \"DISP_RSK_SCOR\", \"ADC_LIB_RISK_ADDED\", \"ORG_SEQ\", \"PRC_SEQ\", \"RSK_SEQ\", \"RSK_OVERRIDE_SCORE_INH\", \"RSK_OVERRIDE_SCORE_RES\") AS \n  SELECT a.version_no,\n    a.PUBLISHED_ON,\n    a.ASSMNT_ID,\n    a.ORGANIZATION_ID,\n    MS_RSK_UTILS.get_organizations(a.ORGANIZATION_ID) AS ORGANIZATION_NAME,\n    a.CORE_OBJECT_ID,\n    a.RISK_ID,\n    ms_rsk_utils. get_risk_owners(a.risk_id) AS RISK_OWNER,\n    NULL                                     AS inherent_score,\n    NULL                                     AS inherent_rating,\n    a.INH_RAT_TREND,\n    a.INH_RAT_TREND_COMMENTS,\n    a.TOTAL_NO_OF_CONTROLS,\n    a.NO_OF_NEW_CONTROLS,\n    a.NO_OF_CONTROLS_ASSESSED,\n    a.OVERALL_CONTROL_SCORE,\n    a.OVERALL_CONTROL_EFFECTIVENESS,\n    NULL AS RESIDUAL_SCORE,\n    NULL AS RESIDUAL_RATING,\n    a.RES_RAT_TREND,\n    a.RES_RAT_TREND_COMMENTS,\n    a.LAST_ASSESSED_ON,\n    Ms_apps_utilities.get_user_full_name(a.LAST_ASSESSED_BY) AS LAST_ASSESSED_BY,\n    a.LAST_APPROVED_ON,\n    Ms_apps_utilities.get_user_full_name(a.LAST_APPROVED_BY) AS LAST_APPROVED_BY,\n    a.NO_OF_ISSUES,\n    a.NO_OF_OPEN_ISSUES,\n    a.NO_OF_OVERDUE_ISSUES,\n    NULL AS PLAN_ID,\n    (SELECT DISTINCT assmnt_name\n    FROM ms_rsk_ongoing_assmnt_rpt e\n    WHERE a.sch_ats_pk \u003d e.sch_ats_pk\n    AND a.ASSMNT_ID    \u003de.assmnt_id\n    AND rownum         \u003d1\n    ) AS ASSMNT_NAME,\n    (SELECT DISTINCT plan_name\n    FROM ms_rsk_ongoing_assmnt_rpt e\n    WHERE a.sch_ats_pk \u003d e.sch_ats_pk\n    AND a.ASSMNT_ID    \u003de.assmnt_id\n    AND rownum         \u003d1\n    ) AS plan_name,\n    (SELECT DISTINCT core_object_url\n    FROM ms_rsk_ongoing_assmnt_rpt c\n    WHERE a.core_object_id \u003d c.core_object_id\n    AND a.sch_ats_pk       \u003d c.sch_ats_pk\n    AND a.ASSMNT_ID        \u003dc.assmnt_id\n    AND rownum             \u003d1\n    )\n    /*b.core_object_name*/\n    AS CORE_OBJECT_NAME,\n    (SELECT DISTINCT risk_url\n    FROM ms_rsk_ongoing_assmnt_rpt d\n    WHERE a.risk_id  \u003d d.risk_id\n    AND a.sch_ats_pk \u003dd.sch_ats_pk\n    AND a.ASSMNT_ID  \u003dd.assmnt_id\n    AND rownum       \u003d1\n    )\n    /*b.risk_name*/\n         AS RISK_NAME,\n    NULL AS ASSMNT_PID,\n    a.ASSMNT_INSTANCE_ID,\n    --    a.INHERENT_SCORE_ORG,\n    --    a.INHERENT_RATING_ORG,\n    --    a.INHERENT_SCORE_CORE,\n    --    a.INHERENT_RATING_CORE,\n    --    a.INHERENT_SCORE_RSK,\n    --    a.INHERENT_RATING_RSK,\n    --    a.RESIDUAL_SCORE_ORG,\n    --    a.RESIDUAL_RATING_ORG,\n    --    a.RESIDUAL_SCORE_CORE,\n    --    a.RESIDUAL_RATING_CORE,\n    --    a.RESIDUAL_SCORE_RSK,\n    --    a.RESIDUAL_RATING_RSK,\n    NVL(a.inherent_score_org,0) ,\n    ms_rsk_assessments_task.fn_get_previous_range(NVL(a.inherent_score_org,0)),\n    --a.INHERENT_RATING_ORG,\n    NVL(a.inherent_score_core,0),\n    ms_rsk_assessments_task.fn_get_previous_range(NVL(a.inherent_score_core,0)),\n    -- a.INHERENT_RATING_CORE,\n    NVL(a.inherent_score_rsk,0),\n    ms_rsk_assessments_task.fn_get_previous_range(NVL(a.inherent_score_rsk,0)),\n    --a.INHERENT_RATING_RSK,\n    NVL(a.residual_score_org,0),\n    ms_rsk_assessments_task.fn_get_previous_range(NVL(a.residual_score_org,0)),\n    --  a.RESIDUAL_RATING_ORG,\n    NVL(a.residual_score_core,0),\n    ms_rsk_assessments_task.fn_get_previous_range(NVL(a.residual_score_core,0)),\n    --   a.RESIDUAL_RATING_CORE,\n    NVL(a.residual_score_rsk,0),\n    ms_rsk_assessments_task.fn_get_previous_range(NVL(a.residual_score_rsk,0)),\n    --a.RESIDUAL_RATING_RSK,\n    a.SCENARIO_ID,\n    a.ASSMNT_TYPE,\n    a.SCH_ATS_PK,\n    a.RESTRICT_ACCESS_TO,\n    a.PLAN_OWNER_ORGS,\n    a.DD_OBJECT_TYPE,\n    a.CURRENT_STAGE,\n    a.prv_inherent_score_org      AS dummy1,\n    a.prv_residual_score_org      AS dummy2,\n    a.prv_inherent_score_core     AS dummy3,\n    a.prv_residual_score_core     AS dummy4,\n    a.prv_inherent_score_risk     AS dummy5,\n    a.prv_residual_score_risk     AS dummy6,\n    NULL                          AS dummy7,\n    NULL                          AS dummy8,\n    assmnt_instance_rec_num       AS dummy9,\n    NULL                          AS dummy10,\n    a.inherent_justification_org  AS dummy11,\n    a.residual_justification_org  AS dummy12,\n    a.INHERENT_JUSTIFICATION_CORE AS dummy13,\n    a.residual_justification_core AS dummy14,\n    a.inherent_justification_risk AS dummy15,\n    a.RESIDUAL_JUSTIFICATION_RISK AS dummy16,\n    disp_overall_rsk_rat                          AS dummy17,\n    DISP_RES_RATING                          AS dummy18,\n    NULL                          AS dummy19,\n    NULL                          AS dummy20,\n    NULL                          AS dummy21,\n    NULL                          AS dummy22,\n    a.disp_rsk_scor,\n    a.adc_lib_risk_added AS adc_lib_risk_added,\n    a.org_seq,\n    a.prc_seq,\n    a.rsk_seq,\n    a.rsk_override_score_inh,\n    a.rsk_override_score_res\n  FROM ms_rsk_ongoing_trend_rpt a\n    /*,\n    ms_rsk_ongoing_assmnt_rpt_v b*/\n    /*where a.sch_ats_pk \u003d b.sch_ats_pk\n    and   a.plan_id    \u003d b.plan_id*/\n    /*and   a.organization_id \u003d b.organization_id\n    and   a.core_object_id \u003d b.core_object_id\n    and a.risk_id \u003d b.risk_id*/\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_CONTROL_REPORT_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_CONTROL_REPORT_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_CONTROL_REPORT_V\" (\"OBJECT_ID\", \"ORGANIZATION_ID\", \"ASSESSABLE_ENTITY\", \"RISK_ID\", \"CONTROL_ID\", \"RISK_ASSESSMENT_TYPE\", \"PERSPECTIVE\") AS \n  select max(object_id) as Object_id,organization_id,process_id as Assessable_Entity,risk_id,ctl_id as Control_ID,risk_assessment_type,scenario_name as Perspective from\n(\nSELECT\n  a.scenario_name,\n  a.object_id,\n  b.risk_id,\n  c.process_id,\n  d.organization_id,\n  f.ctl_id,\n  a.risk_assessment_type\nFROM\n  ms_rsk_risk_assessment a,\n  ms_rsk_risk_assessment_rsk b,\n  ms_rsk_risk_assessment_prc c,\n  ms_rsk_risk_assessment_org d,\n  ms_rsk_risk_assessment_ctl e,\n(select distinct object_id,ctl_id,ctrl_risk_id,ctrl_org_id,ctrl_proc_id from\n  ms_rsk_risk_assessment_ctl a) f \nWHERE \n  a.object_id\u003db.object_id\n  and  a.objecT_id\u003dc.object_id\n  and a.object_id\u003dd.object_id\n  and a.object_id\u003de.object_id\n  and a.object_id\u003df.object_id\n  and e.ctl_id \u003d f.ctl_id\n --and a.scenario_name\u003d\u0027SCN-000005\u0027\n -- and a.risk_assessment_type\u003d3\n  )\n  GROUP BY ORGANIZATION_ID,process_id,RISK_ID,ctl_id,risk_assessment_type,scenario_name\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_SF_CONFIGURATION_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_SF_CONFIGURATION_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_SF_CONFIGURATION_V\" (\"CFG_SF_ID\", \"CFG_PK\", \"DEFAULT_PROFILE\", \"SF_Y_AXIS\", \"SF_X_AXIS\", \"PROFILE_CONFIG\", \"PERSPECTIVE\", \"SF_ID\", \"DD_PROCESS_CODE\", \"DD_CURRENT_STAGE\", \"DD_ENTERPRISE_INFO\", \"DD_EVENT_USER_NAME\", \"DD_CURRENT_USER_NAME\", \"DD_OBJECT_TYPE\", \"INSTANCE_REC_NUM\", \"INSTANCE_ID\", \"METRIC_ID\", \"METRIC_RUN_DATE\", \"METRIC_COMPLETED_DATE\", \"LATEST_FLAG\", \"CREATED_BY\", \"CREATION_DATE\", \"PROCESS_FLOW_STATUS\", \"PROCESS_INSTANCE_ID\", \"MULTIROW_GROUP_NAME\", \"ELECTRONICALLY_SIGNED\", \"MULTIROW_REGION_ID\") AS \n  select \"CFG_SF_ID\",\"CFG_PK\",\"DEFAULT_PROFILE\",\"SF_Y_AXIS\",\"SF_X_AXIS\",\"PROFILE_CONFIG\",\"PERSPECTIVE\",\"SF_ID\",\"DD_PROCESS_CODE\",\"DD_CURRENT_STAGE\",\"DD_ENTERPRISE_INFO\",\"DD_EVENT_USER_NAME\",\"DD_CURRENT_USER_NAME\",\"DD_OBJECT_TYPE\",\"INSTANCE_REC_NUM\",\"INSTANCE_ID\",\"METRIC_ID\",\"METRIC_RUN_DATE\",\"METRIC_COMPLETED_DATE\",\"LATEST_FLAG\",\"CREATED_BY\",\"CREATION_DATE\",\"PROCESS_FLOW_STATUS\",\"PROCESS_INSTANCE_ID\",\"MULTIROW_GROUP_NAME\",\"ELECTRONICALLY_SIGNED\",\"MULTIROW_REGION_ID\" from SI_102080_T \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_RISK_AS_OBJ_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_RISK_AS_OBJ_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_RISK_AS_OBJ_V\" (\"OBJECT_ID\", \"OBJECT_NAME\", \"OWNERS\", \"OWNER_ORGANIZATIONS\", \"RESTRICT_ACCESS_TO\", \"OBJECT_PARENT\", \"CATEGORY_RISK\", \"TYPE_RISK\", \"DD_PROCESS_INSTANCE_ID\", \"DD_INSTANCE_ID\", \"ASSOCIATED_OBJECTS\", \"OBJECT_SEQUENCE\", \"OBJECT_TYPE\", \"ASSOCIATED_REPORT\", \"DD_OBJECT_TYPE\", \"NO_OF_OBJECTS\", \"RISK_LEVEL\") AS \n  SELECT Risk.\"OBJECT_ID\",\n    Risk.\"OBJECT_NAME\",\n    Risk.\"OWNERS\",\n    Risk.\"OWNER_ORGANIZATIONS\",\n    Risk.\"RESTRICT_ACCESS_TO\",\n    Risk.\"OBJECT_PARENT\",\n    Risk.\"CATEGORY_RISK\",\n    Risk.\"TYPE_RISK\",\n    Risk.\"DD_PROCESS_INSTANCE_ID\",\n    Risk.\"DD_INSTANCE_ID\",\n    Risk.\"ASSOCIATED_OBJECTS\",\n    Risk.\"OBJECT_SEQUENCE\",\n    Risk.\"OBJECT_TYPE\",\n    Risk.\"ASSOCIATED_REPORT\",\n    Risk.\"DD_OBJECT_TYPE\",\n    (LENGTH(associated_objects)-LENGTH(REPLACE(associated_objects,\u0027,\u0027)))+1 AS no_of_objects,\n    Risk.Risk_Level\n  FROM\n    (SELECT Rsk.object_id ,\n      OBJECT_NAME,\n      OWNERS ,\n      OWNER_ORGANIZATIONS ,\n      RESTRICT_ACCESS_TO ,\n      object_parent,\n      C.CATEGORY_RISK ,\n      TYPE_RISK ,\n      DD_PROCESS_INSTANCE_ID,\n      DD_INSTANCE_ID ,\n      associated_objects ,\n      OBJECT_SEQUENCE,\n      object_type,\n      associated_report ,\n      dd_object_type,\n      Risk_Level\n    FROM\n      ( WITH Z AS\n      (SELECT\n        /*+ materialize */\n        V.object_id ,\n        OBJECT_NAME,\n        OWNERS ,\n        OWNER_ORGANIZATIONS ,\n        RESTRICT_ACCESS_TO ,\n        object_parent,\n        CATEGORY_RISK ,\n        TYPE_RISK ,\n        DD_PROCESS_INSTANCE_ID,\n        DD_INSTANCE_ID ,\n        dd_object_type,\n        object_level\n      FROM ms_grc_risk_pub_v v\n      )\n    SELECT object_id ,\n      OBJECT_NAME,\n      OWNERS ,\n      OWNER_ORGANIZATIONS ,\n      RESTRICT_ACCESS_TO ,\n      OBJECT_PARENT,\n      CATEGORY_RISK ,\n      TYPE_RISK ,\n      DD_PROCESS_INSTANCE_ID,\n      DD_INSTANCE_ID ,\n      MS_RSK_LND_REPORTING_UTILS.ms_rsk_get_orb_relations(dd_object_type,\u0027MS_GRC_CONTROL\u0027,OBJECT_ID ) AS associated_objects ,\n      2                                                                                     AS OBJECT_SEQUENCE,\n      \u0027Compliance Object\u0027                                                                   AS object_type,\n      \u0027MS_GRC_Control_Report\u0027                                                               AS associated_report ,\n      dd_object_type,\n      Object_Level AS Risk_Level\n    FROM Z\n    UNION ALL\n    SELECT object_id ,\n      OBJECT_NAME,\n      OWNERS ,\n      OWNER_ORGANIZATIONS ,\n      RESTRICT_ACCESS_TO ,\n      OBJECT_PARENT,\n      CATEGORY_RISK ,\n      TYPE_RISK ,\n      DD_PROCESS_INSTANCE_ID,\n      DD_INSTANCE_ID ,\n      MS_RSK_LND_REPORTING_UTILS.ms_rsk_get_orb_relations(dd_object_type,\u0027MS_GRC_AREA_OF_COMPLIANCE\u0027,OBJECT_ID) AS associated_objects ,\n      1                                                                                               AS OBJECT_SEQUENCE,\n      \u0027Area Of Compliance\u0027                                                                            AS object_type,\n      \u0027MS_GRC_Area_Of_Compliance_Report\u0027                                                              AS ASSOCIATED_REPORT ,\n      dd_object_type,\n      Object_Level AS Risk_Level\n    FROM Z\n    UNION ALL\n    SELECT object_id ,\n      OBJECT_NAME,\n      OWNERS ,\n      OWNER_ORGANIZATIONS ,\n      RESTRICT_ACCESS_TO ,\n      OBJECT_PARENT,\n      CATEGORY_RISK ,\n      TYPE_RISK ,\n      DD_PROCESS_INSTANCE_ID,\n      DD_INSTANCE_ID ,\n      MS_RSK_LND_REPORTING_UTILS.ms_rsk_get_orb_relations(dd_object_type,\u0027MS_GRC_PROCESS\u0027,OBJECT_ID) AS associated_objects ,\n      3                                                                                    AS OBJECT_SEQUENCE ,\n      \u0027Core Object\u0027                                                                        AS object_type,\n      \u0027MS_GRC_Process_Report\u0027                                                              AS associated_report ,\n      dd_object_type,\n      Object_Level AS Risk_Level\n    FROM Z\n    UNION ALL\n    SELECT object_id ,\n      OBJECT_NAME,\n      OWNERS ,\n      OWNER_ORGANIZATIONS ,\n      RESTRICT_ACCESS_TO ,\n      OBJECT_PARENT,\n      CATEGORY_RISK ,\n      TYPE_RISK ,\n      DD_PROCESS_INSTANCE_ID,\n      DD_INSTANCE_ID ,\n      MS_RSK_LND_REPORTING_UTILS.ms_rsk_get_orb_relations(dd_object_type,\u0027MS_GRC_REQUIREMENT\u0027,OBJECT_ID) AS associated_objects ,\n      4                                                                                        AS OBJECT_SEQUENCE ,\n      \u0027Requirement\u0027                                                                            AS object_type,\n      \u0027MS_GRC_Requirement_Report\u0027                                                              AS associated_report ,\n      dd_object_type,\n      object_level AS risk_level\n    FROM z\n      ) rsk,\n      ms_grc_risk_cat c\n    where c.object_id \u003d rsk.object_id\n    ) RISK\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
  "MS_RSK_USER_ORG_ROLES_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_USER_ORG_ROLES_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_USER_ORG_ROLES_V\" (\"USER_ID\", \"ORG_ENTITY_ID\", \"ROLE_ID\") AS \n  SELECT b.user_id, a.org_entity_id, a.role_id\n       FROM si_org_roles a, si_user_org_roles b\n      WHERE A.ORG_ROLE_ID \u003d B.ORG_ROLE_ID\n   ORDER BY user_id, org_entity_id, role_id\n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --\u003e",
