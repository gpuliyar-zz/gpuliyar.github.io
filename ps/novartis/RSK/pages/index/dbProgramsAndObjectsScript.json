  "MS_RSK_QUAN_ACTIONS_V": "\u003cdiv class\u003d\"modal-dialog\" style\u003d\"width:1000px;overflow:auto;\"\u003e\r\n    \u003cdiv class\u003d\"modal-content\"\u003e\r\n        \u003cdiv class\u003d\"modal-header\"\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"close\" data-dismiss\u003d\"modal\" aria-hidden\u003d\"true\"\u003e\u0026times;\u003c/button\u003e\r\n            \u003ch4 class\u003d\"modal-title\" id\u003d\"myModalLabel\"\u003eMS_RSK_QUAN_ACTIONS_V\u003c/h4\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-body\" \u003e\r\n            \u003cpre\u003e\r\n            \u003ccode id\u003d\"selectme\" class\u003d\"sql\" style\u003d\"height:350px;overflow:auto;\"\u003e\r\n            \n  CREATE OR REPLACE FORCE VIEW \"NOVT_DEV\".\"MS_RSK_QUAN_ACTIONS_V\" (\"STORED_VALUE\", \"DISPLAY_VALUE\", \"CURRENT_STAGE\", \"ROLE_ACTIVITY\", \"ACTION_ID\") AS \n  SELECT act.action_name stored_value, act.intermediate_status display_value,\n          stgact.current_stage current_stage, role_activity,\n          act.action_id action_id\n     FROM ms_rsk_actions act,\n          (SELECT *\n             FROM ms_rsk_stage_actions_mappings\n            WHERE stage_type \u003d 2 AND active \u003d \u0027Y\u0027) stgact\n    WHERE stgact.action_id \u003d act.action_id\n      AND stgact.active \u003d \u0027Y\u0027 \n \n \n \n \n \n \n \n \n \n \r\n            \u003c/code\u003e\r\n            \u003c/pre\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class\u003d\"modal-footer\"\u003e\r\n        \r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" onclick\u003d\"SelectText(\u0027selectme\u0027)\"\u003eSelect All\u003c/button\u003e\r\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-default\" data-dismiss\u003d\"modal\"\u003eClose\u003c/button\u003e\r\n        \u003c/div\u003e\r\n    \u003c/div\u003e\r\n    \u003c!-- /.modal-content --\u003e\r\n\u003c/div\u003e\r\n\u003c!-- /.modal-dialog --