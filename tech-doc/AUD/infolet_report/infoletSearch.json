,dd_instance_id,\u0027edit_flag\u003d\u0027||htf.escape_sc(dd_edit_flag)||chr(38)||\u0027flag\u003d1\u0027||chr(38)||\u0027emd\u003d1\u0027||chr(38))) as report_json,\nowners_stored,\ncase when  dummy1.relation_exists is  null and  dummy2.relation_exists is null then\n    \u0027yes\u0027\n    end orphan,level_1_approver as level_1_approver,\n       level_2_approver as level_2_approver,level_1_approver_stored,level_2_approver_stored\nfrom ms_aud_grc_process_rv\nleft join\n   ( select distinct rel_source_object_id,\u0027exist\u0027 \n    as relation_exists from ms_grc_orb_relations rel\n      inner join\n       ms_grc_orb_relations_orb orb\n      on  rel.rel_source_id\u003dorb.rel_source_id      \n      where  rel.dd_object_type  \u003d \u0027ms_aud_auditable_entity\u0027\n      and tgt_obj_type not like  \u0027applies to organization%\u0027) dummy1\n on object_id \u003ddummy1.rel_source_object_id \n left join\n   (select distinct tgt_obj_id,\u0027exist\u0027 \n    as relation_exists from ms_grc_orb_relations rel\n      inner join\n       ms_grc_orb_relations_orb orb\n      on  rel.rel_source_id\u003dorb.rel_source_id      \n      where  rel.dd_object_type  \u003d \u0027ms_aud_auditable_entity\u0027\n      and tgt_obj_type not like \u0027applies to organization%\u0027) dummy2\n on object_id \u003ddummy2.tgt_obj_id\nwhere ms_apps_security_pkg.check_object_access (dd_object_type,\n                                                   restrict_access_to,\n                                                   owner_org_ids,\n                                                   :1,\n                                                   \u0027view\u0027\n                                                  ) \u003d \u0027access\u0027\nand (:2\u003d1 or (nvl(:2,\u00272\u0027)\u003d2 and object_id in (select max(b.object_id) from ms_aud_grc_core_object_pub_v a, ms_aud_auditable_entit b \nwhere a.object_id \u003d b.object_id \ngroup by b.internal_object_id)))"
