  "MS_AUD_Plan_Trail": [
    "MS AUD Plan Trail",
    "1",
    "M",
    "1",
    "select \ncase when rownum \u003d1 then x.plan_title else null end as plan_title, \ncase when rownum \u003d1 then x.plan_start_date else null end as plan_start_date, \ncase when rownum \u003d1 then x.plan_end_date else null end as plan_end_date, \ncase when rownum \u003d1 then x.organization else null end as organization, \ncase when rownum \u003d1 then x.plantype else null end as plantype,y.status as status, y.date_time as date_time, y.user_name as user_name, y.comments as comments,y.electronic_signatures as electronic_signatures, y.plan_id as plan_id, y.process_instance_id as process_instance_id, y.report_json as report_json \nfrom\n(select distinct plan_title as plan_title,\n         trunc (start_date) as plan_start_date,\n         trunc (end_date) as plan_end_date,\n         ms_grc_common_utils.fn_get_org_display_names (hdr_organization) as organization,\n         (select displayed_value \n          from ms_qs_list_of_values lovvalue,ms_qs_lov_names lovname,si_metrics_t m\n          where lovvalue.lov_id \u003d lovname.lov_id\n          and lovvalue.org_entity_id \u003d 1\n          and lovvalue.enterprise_id \u003d m.enterprise_id\n\t\t  and m.metric_name \u003d dd_object_type\n          and lovvalue.stored_value \u003d plan_type\n          and lovname.lov_name \u003d \u0027ms aud plan types\u0027 --i_lov_name         \n          and lovvalue.locale_id \u003d nvl(cast(si_db_locale_sv.getvalue as number),1009)\n\t\t  and (lovname.end_date is null or lovname.end_date \u003e sysdate)\n          and lovname.start_date \u003c\u003d sysdate\n\t\t  and (lovvalue.end_date is null or lovvalue.end_date \u003e sysdate)) as plantype\n\t\t from ms_aud_plan_audit_v \n\t\t where  internal_plan_id \u003d :1) x,\n\t\t( select  (select b.status_name from ms_aud_status_tl_v b\n          where b.locale_id\u003dnvl(cast(si_db_locale_sv.getvalue as number),1009)\n          and a.status\u003db.status_id) as status,\n         to_char (a.creation_date,\u0027mm/dd/yyyy hh24:mm:ss\u0