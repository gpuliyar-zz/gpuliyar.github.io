  "MS_AUD_Get_Audits_List": [
    "MS AUD Get Audits List",
    "1",
    "M",
    "1",
    "select  a.audit_id                                                             as audit_id ,\n  a.audit_title                                                          as audit_title ,\n  (select status_name\n  from ms_aud_status_tl_v\n  where locale_id \u003d\n    (select b.locale_id\n    from si_users_t a,\n      si_locales b\n    where a.locale  \u003d b.locale_string\n    and a.user_name \u003d :1\n    )\n  and status_id \u003d a.audit_status\n  )                                                                                                                                                                                            as audit_status ,\n  nvl(ms_apps_utilities.get_user_full_name (a.lead_auditor),ms_aud_report_utils.fn_get_mlov_lead_aud_disp(ms_grc_common_utils.fn_get_val_by_id(audit_id,\u0027ms_aud_audit_alm\u0027,\u0027lead_auditor_mlov\u0027,\u0027audit_id\u0027)))  as lead_auditor ,\n  nvl(ms_apps_utilities.get_user_full_name (a.audit_manager),ms_aud_report_utils.fn_get_mlov_aud_man_disp(ms_grc_common_utils.fn_get_val_by_id(audit_id,\u0027ms_aud_audit_amm\u0027,\u0027audit_manager_mlov\u0027,\u0027audit_id\u0027))) as audit_manager ,\n  ms_aud_execution_rpt_utils.fn_get_task_count(a.audit_id,:1)                                                                                                                               as view_tasks ,\n  audit_start_date                                                                                                                                                                             as audit_start_date ,\n  (select s.status_name\n  from ms_aud_plan_scope ps ,\n    ms_aud_status_tl_v s\n  where ps.audit_id\u003d a.audit_id\n  and ps.form_type \u003d\u0027p\u0027\n  and s.locale_id  \u003d\n    (select b.locale_id\n    from si_users_t a,\n      si_locales b\n    where a.locale  \u003d b.locale_string\n    and a.user_name \u003d :1\n    )\n  and ps.status \u003d s.status_id\n  )                                                                                                                                                                           as plan_scope ,\n  ms_aud_reporting_rpt_utils.fn_get_draft_issue_count(a.audit_id)                                                                                                                       as draft_issues ,\n  ms_aud_f011_helper.fn_get_attachment_count(a.audit_id)                                                                                                                      as attachment_no,\n  ms_aud_f011_helper.fn_get_current_audit_urls_mod(\u0027ms_aud_manage_audit\u0027,:1,a.audit_id,a.audit_title,a.dd_process_instance_id)                                         as report_json_audit_title,\n  ms_aud_f011_helper.fn_get_current_audit_urls_mod(\u0027ms_aud_plan_scope\u0027,:1,a.audit_id,\u0027x\u0027,a.dd_process_instance_id)                                                     as report_json_audit_plan,\n  ms_aud_f011_helper.fn_get_current_audit_urls_mod(\u0027ms_aud_draft_issues\u0027,:1,a.audit_id,ms_aud_reporting_rpt_utils.fn_get_draft_issue_count(a.audit_id),a.dd_process_instance_id) as report_json_draft_issue,\n  ms_aud_f011_helper.fn_get_attachment_details(a.audit_id)  as attachments,a.audit_end_date as audit_end_date\nfrom ms_aud_audit a\nwhere a.status                     in (\u00273\u0027,\u00278\u0027)\nand to_char(audit_start_date,\u0027rrrr\u0027)\u003d nvl(:2,to_char(audit_start_date,\u0027rrrr\u0027))\nand audit_id                        \u003d nvl(:3, audit_id)\nand conducted_by_org                \u003d nvl(:4,conducted_by_org)\nand (nvl(lead_auditor,\u00271\u0027)          \u003d nvl(:5,nvl(lead_auditor,\u00271\u0027))\nor :5                 in\n  ( select lead_auditor_mlov from ms_aud_audit_alm where audit_id \u003d a.audit_id\n  ))\nand (nvl(audit_manager,\u00271\u0027) \u003d nvl(:6,nvl(audit_manager,\u00271\u0027))\nor :6        in\n  (select audit_manager_mlov from ms_aud_audit_amm where audit_id \u003d a.audit_id\n  ))\nand ms_aud_audit_security.check_view_access (audit_id,dd_object_type,restrict_access_to,conducted_by_org,:1,\u0027view\u0027) \u003d \u0027access\u0027\norder by a.dd_process_instance_id desc"
  ],
