  "MS_AUD_Audit_By_Month": [
    "MS AUD Audit By Month",
    "1",
    "M",
    "1",
    "select number_audits_start, number_audits_end, audits_started, audits_ended,\n       organization_name  from (select   nvl (no_aud_start, 0) number_audits_start, nvl (no_aud_end, 0) number_audits_end,nvl (conducted_by_org, org) organisation,\n nvl (month11, month12) aud_month, ms_aud_report_utils.fn_get_audits_month_start\n  (nvl (t11.conducted_by_org, t22.org), nvl (t11.month11, t22.month12), :1,:2,:3 ) audits_started,\n ms_aud_report_utils.fn_get_audits_month_end (nvl (t11.conducted_by_org, t22.org), nvl (t11.month11, t22.month12),:1,:2,:3 ) audits_ended, ms_grc_common_utils.fn_get_org_display_names\n  (nvl (conducted_by_org, org) ) || \u0027 - \u0027|| nvl (month11, month12) as organization_name\n    from (select   count (audit_id) no_aud_start, to_char (audit_start_date,\u0027mon-yyyy\u0027  ) month11,  conducted_by_org\n    from ms_aud_audit where audit_status in (33,34,35,37,38) and ms_aud_audit_security.check_view_access\n (audit_id, dd_object_type, restrict_access_to,  conducted_by_org, :3,\u0027view\u0027 ) \u003d\u0027access\u0027\nand trunc (audit_start_date) between :1+1 and :2-1 group by to_char (audit_start_date,\u0027mon-yyyy\u0027 ),\n  conducted_by_org     order by month11) t11 full outer join\n (select   count (aud_id) no_aud_end, org, month12 from (select a.audit_id aud_id, b.conducted_by_org org,\n a.month12  from ms_aud_audit b, (select   r.audit_id,\n to_char(max (r.published_date), \u0027mon-yyyy\u0027) month12 from ms_aud_audit_published_reports r,\n ms_aud_audit m where r.audit_id \u003d m.audit_id and ms_aud_audit_security.check_view_access\n(m.audit_id, m.dd_object_type,  m.restrict_access_to, m.conducted_by_org,  :3,  \u0027view\u0027) \u003d \u0027access\u0027  group by r.audit_id having max(r.published_date)\n     between :1+1 and :2-1) a  where a.audit_id \u003d b.audit_id)\n  group by month12, org) t22 on t11.month11 \u003d t22.month12 and t11.conducted_by_org \u003d t22.org\n  order by to_date (aud_month, \u0027mm-yy\u0027),ms_grc_common_utils.fn_get_org_display_names\n                                 (nvl (conducted_by_org, org)))"
  ],
  "MS_AUD_Report_AE_Risk_Infolet": [
    "MS AUD Report AE Risk Infolet",
    "1",
    "M",
    "1",
    "select a.object_id as object_id, \n   (select displayed_value\n       from ms_qs_list_of_values lovvalue,ms_qs_lov_names lovname,si_metrics_t m\n       where lovvalue.lov_id \u003d lovname.lov_id\n       and lovvalue.org_entity_id \u003d 1\n       and lovname.lov_name in(\u0027ms_aud_ae_process_type\u0027,\u0027ms grc process type\u0027)\n       and lovvalue.locale_id \u003d nvl(cast(si_db_locale_sv.getvalue as number),1009) \n       and stored_value\u003da.core_type\n       and lovvalue.enterprise_id\u003dm.enterprise_id\n       and m.metric_name \u003d a.dd_object_type) object_type,\n              a.object_name  as object_name ,\n               trim(substr(ms_aud_advisor_utilities.fn_get_rating(ms_aud_planning_rpt_utils.get_risk_score (a.object_id,:1),a.object_id,:1,\u0027c\u0027,1),1,\n              instr(ms_aud_advisor_utilities.fn_get_rating(ms_aud_planning_rpt_utils.get_risk_score (a.object_id,:1),a.object_id,:1,\u0027c\u0027,1),\u0027 \u0027))) as risk_rating,\n              (select max(audit_start_date) keep (dense_rank first order by to_number(audit_status)) from ms_aud_audit where audit_id in (\n               select b.audit_id from ms_aud_audit_aue b,ms_aud_audit ad\n\t\t\t\twhere b.core_objects \u003d a.object_id\n\t\t\t\tand ad.audit_id\u003db.audit_id\n\t\t\t\tand ad.audit_status not in (4)) and audit_start_date is not null) as last_audited_on,\n                (select trunc (min (audit_start_date)) from ms_aud_audit where audit_id in (select audit_id from ms_aud_audit_aue b\n                            where b.core_objects \u003d a.object_id)\n                    and audit_status \u003d \u002733\u0027) as next_audit_date,\n                ms_aud_planning_rpt_utils.get_plan_years (a.object_id) as plan_years,\n                nvl((select count(b.audit_id) from ms_aud_audit_aue b,ms_aud_audit aud\n                     where b.core_objects \u003d a.object_id and aud.status not in (4,8) and aud.audit_id\u003db.audit_id),0) as no_of_audits_till_date,\n                 ms_aud_advisor_utilities.fn_get_open_issue(a.object_id,\u0027y\u0027) as open_issues,\n                (select displayed_value\n          from ms_qs_list_of_values lovvalue,ms_qs_lov_names lovname ,si_metrics_t m\n          where lovvalue.lov_id \u003d lovname.lov_id\n              and lovvalue.org_entity_id \u003d 1\n              and lovname.lov_name in(\u0027ms grc core category\u0027,\u0027ms_aud_ae_category_type\u0027)\n              and lovvalue.locale_id \u003d nvl(cast(si_db_locale_sv.getvalue as number),1009) \n              and stored_value\u003da.core_category\n              and lovvalue.enterprise_id\u003dm.enterprise_id\n              and m.metric_name \u003d a.dd_object_type) as core_category ,\n                ms_apps_utilities.get_user_full_name(d.obj_created_by) as created_by,trunc (d.dd_created_on) as creation_date,\n            ms_aud_report_utils.fn_get_report_json(\n            (select metric_id from si_metrics_t where metric_name \u003d a.dd_object_type),\n              a.dd_process_instance_id,a.dd_instance_id,\n              \u0027edit_flag\u003dn\u0027 || chr (38)|| \u0027flag\u003d2\u0027|| chr (38)|| \u0027emd\u003d1\u0027,\u0027y\u0027) as report_json,d.obj_created_by  as created_by_id, decode(d.dd_object_type,\u0027ms_aud_auditable_entity\u0027,\u0027ms_aud_auditable_entit\u0027,d.dd_object_type) as core_obj_type\n             from ms_aud_grc_core_object_pub_v a, ms_grc_core_object_pub_v d\n          where a.object_id \u003d d.object_id\n          and ((d.object_id in (\n                select max(a.object_id) from ms_aud_auditable_entit a,ms_grc_core_object_pub_v b \n                where a.object_id\u003db.object_id\n                and b.obj_status not in (\u0027inact\u0027,\u0027exp\u0027)\n                group by internal_object_id) \n          and d.dd_object_type\u003d\u0027ms_aud_auditable_entity\u0027) or d.dd_object_type\u003c\u003e\u0027ms_aud_auditable_entity\u0027) \n         and ms_apps_security_pkg.check_object_access(a.dd_object_type, a.restrict_access_to, a.owner_organizations, :2, \u0027view\u0027) \u003d \u0027access\u0027\n         and  trunc(nvl(a.valid_until,sysdate)) \u003e\u003d trunc(sysdate)   \n         order by ms_aud_planning_rpt_utils.get_risk_score (a.object_id,:1) desc nulls last"
  ],
