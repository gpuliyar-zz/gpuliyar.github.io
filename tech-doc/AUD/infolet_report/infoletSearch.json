    "1",
    "select process_name,\n       process_id,\n       core_type,\n       risk_name,\n       risk_id,\n       org_entity_name,\n       org_entity_id,\n       process_level,\n       version_no\n  from (select process_name,\n               process_id,\n               core_type,\n               risk_name,\n               risk_id,\n               org_entity_name,\n               org_entity_id,\n               process_level,\n               version_no\n          from ms_aud_tmp_core_objects_v\n         where (:1 \u003d 5 or :1 \u003d 1)\n           and ((object_id in\n               (select max(b.object_id)\n                    from ms_aud_grc_core_object_pub_v a,\n                         ms_aud_auditable_entit       b\n                   where a.object_id \u003d b.object_id\n                     and nvl(trunc(a.valid_from), trunc(sysdate)) \u003c\u003d\n                         trunc(sysdate)\n                     and nvl(trunc(a.valid_until), trunc(sysdate)) \u003e\u003d\n                         trunc(sysdate)\n                   group by b.internal_object_id) and\n               dd_object_type \u003d \u0027ms_aud_auditable_entity\u0027) or\n               dd_object_type \u003c\u003e \u0027ms_aud_auditable_entity\u0027 or\n               object_id in\n               (select tmp_core_objects\n                   from ms_aud_audit_plan_cro\n                  where plan_id in\n                        (select plan_id\n                           from ms_aud_audit_plan\n                          where internal_plan_id \u003d :2)))\n           and ms_apps_security_pkg.check_object_access(dd_object_type,\n                                                        restrict_access_to,\n                                                        owner_organizations,\n                                                        :3,\n                                                        \u0027view\u0027) \u003d \u0027access\u0027\n        union\n        select null as process_name,\n               process_id||ms_aud_constants.scope_delimiter()||ato_organizations as process_id,\n               core_type,\n               risk_name,\n               risk_id,\n               org_entity_name,\n               org_entity_id,\n               process_level,\n               version_no\n          from ms_aud_ac_core_objects_v\n         where ato_organizations in\n               (select column_value\n                  from table(ms_grc_str_common_utilities.split_string_stored(nvl(:4, 1))))\n           and :1 \u003d 3\n           and ((object_id in\n               (select max(b.object_id)\n                    from ms_aud_grc_core_object_pub_v a,\n                         ms_aud_auditable_entit       b\n                   where a.object_id \u003d b.object_id\n                     and nvl(trunc(a.valid_from), trunc(sysdate)) \u003c\u003d\n                         trunc(sysdate)\n                     and nvl(trunc(a.valid_until), trunc(sysdate)) \u003e\u003d\n                         trunc(sysdate)\n                   group by b.internal_object_id) and\n               dd_object_type \u003d \u0027ms_aud_auditable_entity\u0027) or\n               dd_object_type \u003c\u003e \u0027ms_aud_auditable_entity\u0027 or\n               object_id in\n               (select tmp_core_objects\n                   from ms_aud_audit_plan_cro\n                  where plan_id in\n                        (select plan_id\n                           from ms_aud_audit_plan\n                          where internal_plan_id \u003d :2)))\n           and ms_apps_security_pkg.check_object_access(dd_object_type,\n                                                        restrict_access_to,\n                                                        owner_organizations,\n                                                        :3,\n                                                        \u0027view\u0027) \u003d \u0027access\u0027)\n order by process_name"
