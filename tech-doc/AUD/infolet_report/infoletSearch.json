  "MS_AUD_Issues_Under_Auditee_Review": [
    "MS AUD Issues Under Auditee Review",
    "1",
    "M",
    "1",
    "select distinct \n      b.audit_title as audit_title,\n         a.management_response_due_date as mgmt__response_due_date,\n       ms_aud_report_utils.fn_get_mlov_aud_man_disp\n          (nvl (a.lead_auditor,\n                (ms_aud_report_utils.mlov_values (a.audit_id,\n                                               \u0027audit_id\u0027,\n                                               \u0027lead_auditor_mlov\u0027,\n                                               \u0027ms_aud_audit_alm\u0027\n                                              )))) as lead_auditor,\n       ms_aud_report_utils.fn_get_mlov_aud_man_disp\n          (nvl (a.audit_manager,\n                (ms_aud_report_utils.mlov_values (a.audit_id,\n                                               \u0027audit_id\u0027,\n                                               \u0027audit_manager_mlov\u0027,\n                                               \u0027ms_aud_audit_amm\u0027\n                                              )))) as audit_manager,\n       ms_aud_report_utils.fn_get_mlov_aud_man_disp(issue_reviewer) as auditee,(select status_name from ms_aud_status_tl_v where locale_id \u003d (select b.locale_id from si_users_t a, si_locales b \n       where a.locale \u003d b.locale_string and a.user_name \u003d :1) and status_id \u003d a.status) as current_status,\n       case\n      when trim(ms_aud_reporting_rpt_utils.fn_send_issues_rev_edit (:1,a.process_instance_id)) \u003c\u003e \u0027n\u0027\n        then ms_aud_report_utils.fn_get_report_json(\n  (select metric_id from si_metrics_t where metric_name\u003d\u0027ms_aud_audit_issue_rev_task\u0027\n  ),\n  a.process_instance_id,\n  null,\n  \u0027flag\u003d1\u0027\n  ||chr(38)\n  ||\u0027emd\u003d4\u0027\n  ||chr(38)\n  ||\u0027x_edit_mode\u003dyes\u0027\n  || chr (38)\n  || \u0027audit_id\u003d\u0027\n  || a.audit_id)\n      else           \t\t\t\t \n      ms_aud_report_utils.fn_get_report_json(\n  (select metric_id from si_metrics_t where metric_name\u003d\u0027ms_aud_audit_issue_rev_task\u0027\n  ),\n  a.process_instance_id,\n  null,\n  \u0027flag\u003d1\u0027\n  ||chr(38)\n  ||\u0027emd\u003d2\u0027\n  ||chr(38)\n  ||\u0027x_edit_mode\u003dno\u0027\n  || chr (38)\n  || \u0027audit_id\u003d\u0027\n  || a.audit_id) \n                 end  as report_json, (nvl (a.lead_auditor,\n                (ms_aud_report_utils.mlov_values (a.audit_id,\u0027audit_id\u0027,\u0027lead_auditor_mlov\u0027,\u0027ms_aud_audit_alm\u0027)))) as lead_auditor_stored,\n          (nvl (a.audit_manager,\n                (ms_aud_report_utils.mlov_values (a.audit_id,\u0027audit_id\u0027,\u0027audit_manager_mlov\u0027,\u0027ms_aud_audit_amm\u0027)))) as audit_manager_stored, issue_reviewer as auditor_stored\n  from ms_aud_audit_issue_rev_task_v a,\n                ms_aud_audit b\n where instance_id \u003d (select max (instance_id)\n                        from ms_aud_audit_issue_rev_task_v\n                       where audit_id \u003d a.audit_id and process_instance_id\u003da.process_instance_id)\n                       and a.audit_id\u003db.audit_id\n   and a.instance_rec_num \u003d 1\n   and review_type \u003d 2\n   and a.status in (26,29)\n   and b.status not in (4)\n   and (:1\u003dissue_reviewer or :1 in (select column_value  from table (ms_grc_str_common_utilities.split_string_stored (nvl(a.lead_auditor,a.lead_auditor_mlov))))  or :1 in (select column_value  from table (ms_grc_str_common_utilities.split_string_stored (nvl(a.audit_manager,a.audit_manager_mlov)))) or :1\u003da.delegatee)"
  ],
  "MS_AUD_Search_Auditors_Profile": [
    "MS AUD Search Auditors Profile",
    "1",
    "M",
    "1",
    "select auditor_name, title, email_address, auditor_groups, professional_qualifications, training, proficiency, subject_matter_expert, \nlanguage_skills, a.cost_per_hour,a.report_json,email_address_url,auditor_user_name\nfrom ms_aud_search_auditors_prof_v a\nwhere (:1 is null\nor (:1    is not null\nand (lower(ms_apps_utilities.get_user_full_name(a.auditor_user_name)) like \u0027%\u0027\n  ||lower(:1)\n  ||\u0027%\u0027)))\nand (:2                  is null\nor (:2                   is not null\nand (a.auditor_user_name in\n  (select user_name\n  from si_users_t\n  where lower(first_name) like \u0027%\u0027\n    ||lower(:2)\n    ||\u0027%\u0027\n  ))))\nand (:3                  is null\nor (:3                   is not null\nand (a.auditor_user_name in\n  (select user_name\n  from si_users_t\n  where lower(last_name) like \u0027%\u0027\n    ||lower(:3)\n    ||\u0027%\u0027\n  ))))\nand (:4 is null\nor (:4  is not null\nand (lower(email_address) like \u0027%\u0027\n  ||lower(:4)\n  ||\u0027%\u0027) ))\nand (:5 is null\nor (:5  is not null\nand (lower(phone_number) like \u0027%\u0027\n  ||lower(:5)\n  ||\u0027%\u0027)))\nand a.auditor_title\u003dnvl(:6,a.auditor_title)\nand (:7           is null\nor (:7            is not null\nand (ms_aud_report_utils.mlov_values(a.auditor_user_name,\u0027auditor_user_name\u0027,\u0027training\u0027,\u0027ms_aud_auditor_profile_trn\u0027) like \u0027%\u0027\n  ||:7\n  ||\u0027%\u0027) ))\nand (:8               is null\nor (:8                is not null\nand (a.profiency_level \u003d:8)))\nand (:9               is null\nor (:9                is not null\nand (ms_aud_report_utils.mlov_values(a.auditor_user_name,\u0027auditor_user_name\u0027,\u0027professional_qual\u0027,\u0027ms_aud_auditor_profile_pro\u0027) like \u0027%\u0027\n  ||:9\n  ||\u0027%\u0027)))\nand (:10 is null\nor (:10  is not null\nand (ms_aud_report_utils.mlov_values(a.auditor_user_name,\u0027auditor_user_name\u0027,\u0027language_skills\u0027,\u0027ms_aud_auditor_profile_lns\u0027) like \u0027%\u0027\n  ||:10\n  ||\u0027%\u0027)))\nand (:11 is null\nor (:11  is not null\nand (ms_aud_report_utils.mlov_values(a.auditor_user_name,\u0027auditor_user_name\u0027,\u0027subject_matter_expert\u0027,\u0027ms_aud_auditor_profile_sme\u0027) like \u0027%\u0027\n  ||:11\n  ||\u0027%\u0027)))"
