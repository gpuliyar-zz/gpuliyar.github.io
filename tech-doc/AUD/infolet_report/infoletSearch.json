    "1",
    "select distinct a.action_id     as action_id,\n                d.action_name   as action,\n                a.flow_id       as flow,\n                d.locale_id     as locale_id,\n                d.display_order as disp_order\n  from ms_aud_flow_stage_action a,\n       ms_aud_stages b,\n       ms_aud_stages c,\n       ms_aud_actions_tl_v d,\n       (select ms_aud_utilities.get_parameter_value(\u0027is_plan_reviewer_required\u0027) as param_val from dual) prm,\n       (select ms_aud_f001_helper.fn_get_plan_approver(\u0027aud - approve plan\u0027, :1) as apr_val from dual) apr\n where a.previous_stage \u003d b.stage_id\n   and a.current_stage \u003d c.stage_id\n   and a.action_id \u003d d.action_id\n   and a.active \u003d \u0027y\u0027\n   and b.active \u003d \u0027y\u0027\n   and d.active \u003d \u0027y\u0027\n   and a.current_stage \u003d to_number(:2)\n   and a.previous_stage \u003d to_number(:3)\n   and a.activity in (select activity_name\n                        from ms_aud_flow_down_org_v fdo\n                       where user_name \u003d nvl(:4, user_name)\n                         and activity_name like \u0027aud%\u0027)\n   and (case\n         when prm.param_val \u003d \u0027y\u0027 and a.action_id \u003d \u002711\u0027 and\n              :2 in (\u00271\u0027, \u00275\u0027) then\n          1\n         when prm.param_val \u003d \u0027n\u0027 and a.action_id \u003d \u002711\u0027 and\n              :2 in (\u00271\u0027, \u00275\u0027) then\n          0\n         else\n          1\n       end) \u003d 1\n   and (case\n         when apr.apr_val like \u0027%\u0027 || :4 || \u0027%\u0027 and\n              a.action_id \u003d \u002735\u0027 and :2 in (\u00271\u0027, \u00275\u0027) then\n          1\n         when apr.apr_val like \u0027%\u0027 || :4 || \u0027%\u0027 and\n              a.action_id \u003d \u002735\u0027 and :2 in (\u00271\u0027, \u00275\u0027) then\n          0\n         else\n          1\n       end) \u003d 1\n order by disp_order"
  ],
