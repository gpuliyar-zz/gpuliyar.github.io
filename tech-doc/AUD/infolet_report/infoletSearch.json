  "MS_AUD_Top_5_NonCompliant_Audit_Class": [
    "MS AUD Top 5 NonCompliant Audit Class",
    "1",
    "M",
    "1",
    "select p.conducted_by_org,p.orgdisp,p.audit_classification,p.audit_classification_disp,p.orgclass,p.activecnt,p.finalcnt,nvl(q.ineffcount,0) as ineffcount,(p.activecnt+p.finalcnt+nvl(q.ineffcount,0)) as tot,p.active,p.finalaud,p.startafter,p.startbefore,\u00273\u0027 as audit_rating\n  from \n  (select conducted_by_org,ms_grc_common_utils.fn_get_org_display_names(conducted_by_org) as orgdisp,audit_classification,\n    ms_aud_report_utils.get_code_description(\u0027ms aud classification\u0027,audit_classification) as audit_classification_disp,\n    ms_grc_common_utils.fn_get_org_display_names(conducted_by_org)||\u0027-  \u0027||ms_aud_report_utils.get_code_description(\u0027ms aud classification\u0027,audit_classification)\n    as orgclass,activecnt, finalcnt,total,\u0027start\u0027 as active,\u0027final\u0027 as finalaud,:1 as startafter, :2 as startbefore from  \n         (select conducted_by_org,audit_classification,sum(activecount) as activecnt, sum(finalreportcount) as finalcnt, sum(activecount+finalreportcount) total from\n           (\n             select  \n                  conducted_by_org,\n                  audit_classification,\n                  decode(status, 3, 1, 0) activecount, \n                  decode(audit_status,38, 1,0 ) finalreportcount\n                  from ms_aud_audit where\n\t\t\t           audit_start_date between :1+1 and :2 and\n                            ms_aud_audit_security.check_view_access \n                     (audit_id,dd_object_type,restrict_access_to,conducted_by_org,:3,\u0027view\u0027) \u003d \n                     \u0027access\u0027 and audit_classification is not null       \n         )     \n         group by conducted_by_org,audit_classification having sum(activecount+finalreportcount) \u003e 0\n         order by sum(activecount+finalreportcount) desc)\n         where rownum \u003c\u003d5) p,\n         (select count(audit_id) as ineffcount,conducted_org,audit_classification from \n    (select a.audit_id as audit_id , a.conducted_by_org as conducted_org,a.audit_classification as audit_classification from ms_aud_audit a  where \n          a.audit_id in (select a.audit_id \n            from ms_aud_audit_report a,ms_aud_audit_published_reports b\n            where a.task_id\u003db.task_id and\n            a.audit_id\u003db.audit_id\n            and to_char(b.published_date,\u0027dd/mm/yyyy hh24-mi-ss\u0027)\u003dto_char(ms_aud_report_utils.fn_chrt_max_date(a.audit_id),\u0027dd/mm/yyyy hh24-mi-ss\u0027)\n            and a.audit_rating\u003d\u00273\u0027 )\n            and a.audit_status\u003d\u002738\u0027\n         and ms_aud_audit_security.check_view_access \n            (a.audit_id,a.dd_object_type,a.restrict_access_to,a.conducted_by_org,:3,\u0027view\u0027) \u003d \u0027access\u0027\n         and a.audit_start_date between :1+1 and :2\n         and a.audit_classification is not null\n         )\n         group by conducted_org,audit_classification) q\n         where p.conducted_by_org\u003dq.conducted_org(+)\n         and p.audit_classification\u003dq.audit_classification(+)\n        order by p.activecnt+p.finalcnt+nvl(q.ineffcount,0) desc"
  ],
