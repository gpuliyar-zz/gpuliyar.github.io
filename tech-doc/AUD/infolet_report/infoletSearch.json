  "MS_AUD_AE_Scope_Coverage_DD_Inf": [
    "MS_AUD_AE_Scope_Coverage_DD_Inf",
    "1",
    "M",
    "1",
    "select a.object_id as object_id,    \n              a.object_name  as object_name ,\n              ot.stored_value as core_type,\n               trim(substr(ms_aud_advisor_utilities.fn_get_rating(ms_aud_planning_rpt_utils.get_risk_score (a.object_id,:1),a.object_id,:1,\u0027r\u0027,1),1,\n              instr(ms_aud_advisor_utilities.fn_get_rating(ms_aud_planning_rpt_utils.get_risk_score (a.object_id,:1),a.object_id,:1,\u0027r\u0027,1),\u0027 \u0027))) as risk_rating,\n              (select max(audit_start_date) keep (dense_rank first order by to_number(audit_status))\n                   from ms_aud_audit where audit_id in (\n                           select b.audit_id from ms_aud_audit_aue b,ms_aud_audit ad\n                            where b.core_objects \u003d a.object_id\n                            and ad.audit_id\u003db.audit_id\n                            and ad.audit_status not in (4))\n                    and audit_start_date is not null) as last_audited_on,\n                (select trunc (min (audit_start_date))\n                   from ms_aud_audit where audit_id in (\n                           select audit_id from ms_aud_audit_aue b\n                            where b.core_objects \u003d a.object_id)\n                    and audit_status \u003d \u002733\u0027) as next_audit_date,\n                ms_aud_planning_rpt_utils.get_plan_years (a.object_id) as plan_years,\n                nvl((select count(b.audit_id) from ms_aud_audit_aue b,ms_aud_audit aud\n                     where b.core_objects \u003d a.object_id and aud.status not in (4) and aud.audit_id\u003db.audit_id),0) as no_of_audits_till_date,\n                 ms_aud_advisor_utilities.fn_get_open_issue(a.object_id,\u0027y\u0027) as open_issues,               \n                ms_apps_utilities.get_user_full_name(d.obj_created_by) as created_by,\n                trunc (d.dd_created_on) as creation_date,\nms_aud_report_utils.fn_get_report_json(\n            (select metric_id from si_metrics_t where metric_name \u003d a.dd_object_type),\n              a.dd_process_instance_id,\n              null,\n              \u0027edit_flag\u003dn\u0027 || chr (38)|| \u0027flag\u003d2\u0027|| chr (38)|| \u0027emd\u003d1\u0027 ||chr(38)) as report_json,(select ms_concat(b.audit_id) from ms_aud_audit_aue b,ms_aud_audit aud\nwhere b.core_objects \u003d a.object_id \nand aud.status not in (4) \nand aud.audit_id\u003db.audit_id) as aud_list,\ndecode(a.dd_object_type,\u0027ms_aud_auditable_entity\u0027,\u0027ms_aud_auditable_entit\u0027,a.dd_object_type) as core_obj_type\n             from ms_aud_grc_core_object_pub_v a, ms_grc_core_object_pub_v d,(select lovv.stored_value\n            from ms_qs_list_of_values lovv,ms_qs_lov_names lovn\n            where lovv.lov_id \u003d lovn.lov_id\n                and lovv.org_entity_id \u003d 1\n                and lovn.lov_name in (\u0027ms_aud_ae_process_type\u0027,\u0027ms grc process type\u0027)\n                and lovv.locale_id \u003d nvl(cast(si_db_locale_sv.getvalue as number),1009)                             \n        ) ot\n          where a.object_id \u003d d.object_id\n         and ms_apps_security_pkg.check_object_access(a.dd_object_type, a.restrict_access_to, a.owner_organizations, :2, \u0027view\u0027) \u003d \u0027access\u0027\n         and  trunc(nvl(a.valid_until,sysdate)) \u003e\u003d trunc(sysdate)   \n         and ot.stored_value \u003d a.core_type\n         and a.core_type is not null         \n         and a.core_type\u003d:3         \n         and ((:4\u003d\u0027si\u0027 \n         and  nvl((select count(b.audit_id) from ms_aud_audit_aue b,ms_aud_audit aud\n                     where b.core_objects \u003d a.object_id and aud.status not in (4) and aud.audit_id\u003db.audit_id  and extract(year from aud.audit_start_date) \u003d nvl(:5, ms_aud_constants.fn_year)),0)\u003e0)\n\t\t\t\t\t or :4\u003d\u0027so\u0027)\n         order by ms_aud_planning_rpt_utils.get_risk_score (a.object_id,:1) desc nulls last\n"
  ],
