gned_to_stored,approver_stored,final_approver_stored from ms_aud_controltsk_wrkpaper_v c, ms_aud_audit_task a, (select aud.audit_id, aud.audit_title,aud.scope_based_on, case when (lead_auditor\u003d:1 or audit_manager \u003d:1) then \u0027mgr_lead\u0027 when (select 1 from ms_aud_audit_tmr tmm where tmm.audit_id \u003daud.audit_id and tmm.tmr_user_name\u003d:1)\u003d1 then \u0027tmm\u0027 when (select 1 from ms_aud_audit_tem tem where tem.audit_id \u003daud.audit_id and tem.rsm_team_member\u003d:1)\u003d1 then \u0027tem\u0027 when (select 1 from ms_aud_audit_amm amm where amm.audit_id \u003daud.audit_id and amm.audit_manager_mlov\u003d:1)\u003d1 then \u0027amm\u0027 when (select 1 from ms_aud_audit_alm alm where alm.audit_id \u003daud.audit_id and alm.lead_auditor_mlov\u003d:1)\u003d1 then \u0027alm\u0027 end privs from ms_aud_audit aud where status not in (\u00278\u0027,\u00274\u0027) ) b where a.audit_id\u003db.audit_id and b.privs is not null and a.audit_id \u003d c.audit_id and a.task_id \u003d c.task_id"
