\u003d1)) aev,ms_aud_report_utils.fn_get_report_json((select metric_id from si_metrics_t where metric_name\u003d\u0027ms_aud_manage_audit\u0027),a.dd_process_instance_id,null,\u0027flag\u003d1\u0027||chr(38)||\u0027emd\u003d2\u0027||chr(38)||\u0027x_edit_mode\u003dno\u0027|| chr (38)||\u0027audit_id\u003d\u0027||a.audit_id )as ajson,\nms_aud_report_utils.fn_get_report_json((select metric_id from si_metrics_t where metric_name\u003d\u0027ms_aud_plan_scope\u0027),(select ps.dd_process_instance_id from ms_aud_plan_scope ps where ps.audit_id\u003d a.audit_id and ps.form_type\u003d\u0027p\u0027),(select ps.dd_instance_id from ms_aud_plan_scope ps where ps.audit_id\u003d a.audit_id and ps.form_type\u003d\u0027p\u0027),\n\u0027flag\u003d1\u0027||chr(38)||\u0027emd\u003d51\u0027||chr(38)||\u0027x_edit_mode\u003dno\u0027|| chr (38)|| \u0027audit_id\u003d\u0027|| a.audit_id) sjson from ms_aud_audit a\nleft outer join (select alm.audit_id,\nms_concat(lead_auditor_mlov) lead_auditor_mlov,\nms_concat(user_full_name) la_disp_name\nfrom ms_aud_audit_alm alm,\nms_aud_users_v u\nwhere u.user_name \u003d alm.lead_auditor_mlov\ngroup by audit_id\n)la on (a.audit_id \u003d la.audit_id)\nleft outer join (select amm.audit_id,\nms_concat(audit_manager_mlov) audit_manager_mlov,\nms_concat(user_full_name) ma_disp_name\nfrom ms_aud_audit_amm amm,\nms_aud_users_v u\nwhere u.user_name \u003d amm.audit_manager_mlov\ngroup by audit_id\n)ma\non (a.audit_id\u003d ma.audit_id)\nwhere\n (nvl (a.lead_auditor,\u00271\u0027)\u003d nvl(:4, nvl (a.lead_auditor,\u00271\u0027))\nor instr(\u0027,\u0027\n||la.lead_auditor_mlov\n||\u0027,\u0027, \u0027,\u0027\n||:4\n||\u0027,\u0027)\u003e0)\nand (nvl(a.audit_manager,\u00271\u0027) \u003d nvl(:3, nvl (a.audit_manager,\u00271\u0027))\nor instr(\u0027,\u0027\n||ma.audit_manager_mlov\n||\u0027,\u0027,\u0027,\u0027\n||:3\n||\u0027,\u0027)\u003e0)\nand ms_aud_audit_security.check_view_access (a.audit_id,a.dd_object_type,a.restrict_access_to,a.conducted_by_org,:1,\u