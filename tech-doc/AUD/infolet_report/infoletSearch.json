  "MS_AUD_Schedule_Type_Frequency": [
    "MS AUD Schedule Type Frequency",
    "1",
    "M",
    "1",
    "select frequencey,year,no \n  from (select distinct :2 as plan_type, :4 as end_date, \u0027quarter \u0027||to_char((last_day(to_date(td.end_date+1-rownum))),\u0027q\u0027)||\u0027 \u0027||to_char((last_day(to_date(td.end_date+1-rownum))),\u0027yyyy\u0027) as frequencey,\n                        to_char((last_day(to_date(td.end_date+1-rownum))),\u0027yyyy\u0027) as year,\n                        to_char((last_day(to_date(td.end_date +1-rownum))),\u0027q\u0027) as no\n                   from all_objects,\n                        (select :3 start_date,\n                                to_date(\u002731-12-\u0027||to_char(to_date(add_months(trunc(:3),12)),\u0027yyyy\u0027),\u0027dd-mm-yyyy\u0027) end_date\n                           from dual) td\n                  where trunc(td.end_date+1-rownum,\u0027mm\u0027)\u003e\u003dtrunc(td.start_date,\u0027mm\u0027))\n   where :1\u003d\u0027quarter\u0027\nunion\nselect frequencey,year,no\n    from (select distinct :2 as plan_type, :4 as end_date, to_char((last_day(to_date(td.end_date+1-rownum))),\u0027mon\u0027)||\u0027 \u0027||to_char((last_day(to_date(td.end_date+1-rownum))),\u0027yyyy\u0027) as frequencey,\n                          to_char((last_day(to_date(td.end_date+1-rownum))),\u0027yyyy\u0027) as year,\n                          to_char((last_day(to_date(td.end_date+1-rownum))),\u0027mm\u0027) as no\n                     from all_objects,\n                          (select :3 start_date,\n                                  to_date (\u002731-12-\u0027||to_char(to_date(add_months(trunc(:3),12)),\u0027yyyy\u0027),\u0027dd-mm-yyyy\u0027) end_date\n                             from dual) td\n                    where trunc(td.end_date+1-rownum,\u0027mm\u0027)\u003e\u003dtrunc(td.start_date,\u0027mm\u0027))\n   where :1\u003d\u0027month\u0027\nunion\nselect \u0027time window \u0027||ms_aud_planning_rpt_utils.fn_get_time_window(q)||\u0027 \u0027||to_char(to_date(e),\u0027yyyy\u0027) as frequencey,to_char(to_date (e),\u0027yyyy\u0027) as year,to_char(ms_aud_planning_rpt_utils.fn_get_time_window(q)) as no\n  from (select :2 as plan_type, :4 as end_date, rownum q,to_char((td.start_date+ms_aud_planning_rpt_utils.fn_get_time_window_days(rownum - 1)),\u0027dd-mon-yyyy\u0027) as s,to_char((td.start_date+ms_aud_planning_rpt_utils.fn_get_time_window_days(rownum))-1,\u0027dd-mon-yyyy\u0027) as e\n          from all_objects,\n               (select to_date(\u00271-1-\u0027||to_char(:3,\u0027yyyy\u0027),\u0027dd-mm-yyyy\u0027) as start_date,\n                       to_date(\u002731-12-\u0027||to_char(to_date(add_months(trunc(:3),12)),\u0027yyyy\u0027),\u0027dd-mm-yyyy\u0027) end_date\n                    from dual) td\n         where trunc (td.end_date+1-rownum,\u0027mm\u0027)\u003e\u003dtrunc(td.start_date,\u0027mm\u0027))\n   where to_date (e,\u0027dd-mon-yyyy\u0027)\u003c\u003dtrunc(to_date(\u002731-12-\u0027||to_char(to_date(add_months(trunc(:3),12)),\u0027yyyy\u0027),\u0027dd-mm-yyyy\u0027))\n     and ((to_date(s,\u0027dd-mon-yyyy\u0027)\u003eto_date(trunc(:3))) or to_date(trunc(:3)) between to_date(s,\u0027dd-mon-yyyy\u0027) and to_date(e,\u0027dd-mon-yyyy\u0027))\n     and :1 \u003d \u0027tw\u0027\norder by 2, 3"
  ],
