    "1",
    "M",
    "1",
    "select a.audit_id as audit_id,\na.audit_title as audit_title,\na.audit_title_link as audit_title_link,\na.plan_title as plan_title,\na.audit_type as audit_type,\na.audit_category as audit_category,\na.audit_status as audit_status,\na.core_objects as core_objects,\nrelated_risks as risks,\na.organizations as organizations,\ncase\n when a.lead_auditor is null then\n  la.la_disp_name\n else\n  (select user_full_name\n   from ms_aud_users_v\n  where user_name \u003d a.lead_auditor)\nend as lead_auditor,\ncase\n when a.audit_manager is null then\n  ma.ma_disp_name\n else\n  (select user_full_name\n   from ms_aud_users_v\n  where user_name \u003d a.audit_manager)\nend as audit_manager,\na.created_by as created_by,\na.audit_start_date as audit_start_date,\na.view_tasks as view_tasks,\na.restrict_access_to as restrict_access_to,\na.team_member as team_member,\nms_aud_report_utils.fn_get_report_json((select metric_id\n from si_metrics_t\n where metric_name \u003d\n \u0027ms_aud_manage_audit\u0027),\n a.dd_process_instance_id,\n a.dd_instance_id,\n a.audit_id) as report_json,\n   a.lead_auditor as lead_auditor_id,\n       case\n         when a.audit_status \u003d \u0027final report\u0027 then\n          (select url_link\n             from (select url_link,\n                          audit_id,\n                          dense_rank() over(partition by audit_id order by published_date desc) as rnk\n                     from ms_aud_audit_published_reports) c\n            where c.audit_id \u003d a.audit_id\n              and rnk \u003d 1)\n       end as download_final_report,\ncase\n when a.lead_auditor is null then\n  la.lead_auditor_mlov\n else\n  (select user_name\n   from ms_aud_users_v\n  where user_name \u003d a.lead_auditor)\nend as lead_auditor_stored,\ncase\n when a.audit_manager is null then\n  ma.audit_manager_mlov\n else\n  (select user_name\n   from ms_aud_users_v\n  where user_name \u003d a.audit_manager)\nend as audit_manager_stored,a.audit_end_date as audit_end_date\n  from ms_aud_audit_list_report_v a\n  left outer join (select audit_id,\n                          ms_join_for_display(lead_auditor_mlov) lead_auditor_mlov,\n                          ms_join_for_display(user_full_name) la_disp_name\n                     from ms_aud_audit_alm alm, ms_aud_users_v u\n                    where u.user_name \u003d alm.lead_auditor_mlov\n                    group by audit_id) la\n    on (a.audit_id \u003d la.audit_id)\n  left outer join (select audit_id,\n                          ms_join_for_display(audit_manager_mlov) audit_manager_mlov,\n                          ms_join_for_display(user_full_name) ma_disp_name\n                     from ms_aud_audit_amm amm, ms_aud_users_v u\n                    where u.user_name \u003d amm.audit_manager_mlov\n                    group by audit_id) ma\n    on (a.audit_id \u003d ma.audit_id)\n where a.audit_id in\n       (select org.audit_id\n          from ms_aud_audit_org org\n         where coalesce(substr(trim(org.manage_organizations),\n                               0,\n                               instr(trim(org.manage_organizations),\n                                     ms_aud_constants.scope_delimiter(),\n                                     1,\n                                     1) - 1),\n                        trim(org.manage_organizations)) in\n               (select trim(column_value)\n                  from table(ms_grc_str_common_utilities.split_string_stored(:1))))\n   and ms_aud_audit_security.check_view_access(a.audit_id,\n                                               a.dd_object_type,\n                                               a.restrict_access_to,\n                                               a.conducted_by_org,\n                                               :2,\n                                               \u0027view\u0027) \u003d \u0027access\u0027\n   and a.status not in (\u00272\u0027, \u002731\u0027, \u002744\u0027, \u002751\u0027, \u002759\u0027)\n   and a.approve_flag \u003d \u00271\u0027\n order by a.audit_title"
