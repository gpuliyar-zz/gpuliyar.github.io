t_utils.mlov_values (a.audit_id,  \n                                       \u0027audit_id\u0027,  \n                                       \u0027team_member\u0027,  \n                                       \u0027ms_aud_audit_tmm\u0027  \n                                      ) as team_member,\n    ms_aud_report_utils.fn_get_report_json(\n  (select metric_id from si_metrics_t where metric_name\u003d\u0027ms_aud_manage_audit\u0027\n  ),\n  a.dd_process_instance_id,\n  a.dd_instance_id,\n  \u0027flag\u003d1\u0027\n  ||chr(38)\n  ||\u0027emd\u003d2\u0027\n  ||chr(38)\n  ||\u0027x_edit_mode\u003dno\u0027\n  || chr (38)  \n  || \u0027audit_id\u003d\u0027  \n  || a.audit_id \n  ) as report_json   ,a.lead_auditor as lead_auditor_id,\na.audit_manager as audit_manager_id,\na.dd_created_by as created_by_id,\nnvl(a.lead_auditor,ms_grc_common_utils.fn_get_val_by_id(a.audit_id,\u0027ms_aud_audit_alm\u0027,\u0027lead_auditor_mlov\u0027,\u0027audit_id\u0027)) as   \nlead_auditor_stored,\nnvl(a.audit_manager,ms_grc_common_utils.fn_get_val_by_id(a.audit_id,\u0027ms_aud_audit_amm\u0027,\u0027audit_manager_mlov\u0027,\u0027audit_id\u0027)) as   \naudit_manager_stored,a.audit_end_date as audit_end_date\n     from ms_aud_audit a  \n where  \n    a.audit_id in (select column_value from table (ms_grc_str_common_utilities.split_string_stored(:1))) and   \n    ms_aud_audit_security.check_view_access(a.audit_id,a.dd_object_type,a.restrict_access_to,a.conducted_by_org,:2,\u0027view\u0027) \u003d \u0027access\u0027   \n  order by a.audit_title"
