  "MS_AUD_PLAN_SCOPE_RISK": [
    "MS_AUD_PLAN_SCOPE_RISK",
    "1",
    "M",
    "1",
    "select risk_id,\n  risk_name,\n  object_level,\n  object_parent\nfrom\n  (select object_name as risk_name,\n    object_id         as risk_id,\n    object_level,\n    ms_aud_notification_utilities.fn_get_risk_display_values (object_parent) as object_parent\n  from ms_grc_risk_pub_v\n    start with object_id in\n    (select a.object_id\n    from ms_grc_risk_pub_v a,\n      ms_aud_grc_risk_process_v b\n    where a.object_id \u003d b.tgt_obj_id\n    and b.src_obj_id in\n      (select column_value\n      from table (ms_grc_str_common_utilities.split_string_stored (:1) )\n      )\n    and :3                                                                                                                                   \u003d 1\n    and ms_aud_utilities.get_parameter_value_for_id (:2, \u0027is_ae_risks_relation_required\u0027 )                                             \u003c\u003e \u0027n\u0027\n    and nvl (trunc (a.valid_until), trunc (sysdate))                                                                                            \u003e\u003d trunc (sysdate)\n    and nvl (trunc(a.valid_from),trunc(sysdate)) \u003c\u003d trunc(sysdate)\n    and ms_apps_security_pkg.check_object_access (a.dd_object_type, a.restrict_access_to, a.owner_organizations, :4, \u0027view\u0027 ) \u003d \u0027access\u0027\n    )\n    connect by prior object_id \u003d object_parent\n  union\n  select object_name as risk_name,\n    object_id        as risk_id,\n    object_level,\n    ms_aud_notification_utilities.fn_get_risk_display_values (object_parent) as object_parent\n  from ms_grc_risk_pub_v\n    start with object_id in\n    (select a.object_id\n    from ms_grc_risk_pub_v a\n    where :3                                                                                                                                 \u003d 1\n    and ms_aud_utilities.get_parameter_value_for_id (:2, \u0027is_ae_risks_relation_required\u0027 )                                              \u003d \u0027n\u0027\n    and nvl (trunc (a.valid_until), trunc (sysdate))                                                                                            \u003e\u003d trunc (sysdate)\n    and nvl (trunc(a.valid_from),trunc(sysdate)) \u003c\u003d trunc(sysdate)\n    and ms_apps_security_pkg.check_object_access (a.dd_object_type, a.restrict_access_to, a.owner_organizations, :4, \u0027view\u0027 ) \u003d \u0027access\u0027\n    )\n    connect by prior object_id \u003d object_parent\n  union\n  select distinct object_name as risk_name,\n    object_id                 as risk_id,\n    object_level,\n    ms_aud_notification_utilities.fn_get_risk_display_values (object_parent) as object_parent\n  from ms_grc_risk_pub_v\n  where object_id in\n    (select ro.risk_id as object_id\n    from ms_aud_grc_risk_v ro\n    where ato_organizations in\n      (select column_value\n      from table (ms_grc_str_common_utilities.split_string_stored (:1) )\n      )\n    and :3                                         \u003d 2\n    and nvl (trunc (ro.valid_until), trunc (sysdate)) \u003e\u003d trunc (sysdate)\n    and nvl (trunc(ro.valid_from),trunc(sysdate)) \u003c\u003d trunc(sysdate)\n    )\n  union\n  select object_name as risk_name,\n    object_id        as risk_id,\n    object_level,\n    ms_aud_notification_utilities.fn_get_risk_display_values (object_parent) as object_parent\n  from ms_grc_risk_pub_v\n  where object_id in\n    (select object_id\n    from ms_grc_risk_pub_v\n      start with object_id in\n      (select tgt_obj_id as object_id\n      from ms_aud_grc_process_risk_v\n      where src_obj_id in\n        (select column_value\n        from table (ms_grc_str_common_utilities.split_string_stored (:1) )\n        )\n      and :3 \u003d 3\n      )\n      connect by prior object_id \u003d object_parent\n    )\n  and nvl (trunc (valid_until), trunc (sysdate)) \u003e\u003d trunc (sysdate)\n  and nvl (trunc(valid_from),trunc(sysdate)) \u003c\u003d trunc(sysdate)\n  )\norder by rownum asc"
