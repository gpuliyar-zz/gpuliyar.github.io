  "MS_AUD_AE_Risk_Assessmnt_Compltd": [
    "MS AUD AE Risk Assessmnt Compltd",
    "1",
    "M",
    "1",
    "select plan_name_link,\n  assmnt_id,\n  assmnt_name_link,\n  plan_name,\n  assmnt_status,\n  assmnt_assessed_on,\n  core_objects,\n  assmnt_assessor_name,\n  assmnt_approver_name,\n  final_approver_name,\n  inherent_rating,\n  residual_rating,\n  inherent_score,\n  residual_score,\n  open_issues,\n  scenario,\n  scenario_name,\n  assmnt_assigned_on,\n  assmnt_approved_on,\n  version_no,\n  (select ms_aud_f011_helper.fn_get_lov_disp_val(ae_risk_asses_regulatory, \u0027ms aud yes no\u0027)\n  from ms_aud_auditable_entit\n  where risk_assessment_id\u003d:4\n  and rownum              \u003d1\n  ) as regulatory_requirement,\n  (select ms_aud_f011_helper.fn_get_lov_disp_val(ae_risk_asses_override,\u0027ms aud audit task issue rating\u0027)\n  from ms_aud_auditable_entit\n  where risk_assessment_id\u003d:4\n  and rownum              \u003d1\n  ) as override\nfrom table(ms_aud_f014_helper.fn_get_risk_assessment_dtls(:1,:2,:3))\nwhere assmnt_status\u003d\u0027completed\u0027\nand rownum         \u003d1"
  ],
  "MS_AUD_PLAN_SCOPE_RISK_DU": [
    "MS_AUD_PLAN_SCOPE_RISK_DU",
    "1",
    "M",
    "1",
    "select risk_id, risk_name, object_level, object_parent from (select object_name as risk_name, object_id as risk_id, object_level, ms_aud_report_utils.fn_get_risk_display_values(object_parent) as object_parent from ms_grc_risk_pub_v start with object_id in (select a.object_id from ms_grc_risk_pub_v a, ms_aud_grc_risk_process_v b where a.object_id \u003d b.tgt_obj_id and b.src_obj_id in (select column_value from table(ms_apps_utilities.parse_string(:1,\u0027,\u0027))) and :3 \u003d 1 and ms_aud_utilities.get_parameter_value_for_id(:5, \u0027is_ae_risks_relation_required\u0027)\u003c\u003e\u0027n\u0027 and nvl(trunc(a.valid_until), trunc(sysdate))\u003e\u003dtrunc(sysdate) and ms_apps_security_pkg.check_object_access(a.dd_object_type, a.restrict_access_to, a.owner_organizations, :2,\u0027view\u0027)\u003d\u0027access\u0027) connect by prior object_id \u003d object_parent union select object_name as risk_name, object_id as risk_id, object_level, ms_aud_report_utils.fn_get_risk_display_values(object_parent) as object_parent from ms_grc_risk_pub_v start with object_id in (select a.object_id from ms_grc_risk_pub_v a where :3 \u003d 1 and ms_aud_utilities.get_parameter_value_for_id(:5, \u0027is_ae_risks_relation_required\u0027)\u003d\u0027n\u0027 and nvl(trunc(a.valid_until), trunc(sysdate))\u003e\u003dtrunc(sysdate) and ms_apps_security_pkg.check_object_access(a.dd_object_type, a.restrict_access_to, a.owner_organizations, :2,\u0027view\u0027)\u003d\u0027access\u0027) connect by prior object_id\u003dobject_parent union select distinct object_name as risk_name, object_id as risk_id, object_level, ms_aud_report_utils.fn_get_risk_display_values(object_parent) as object_parent from ms_grc_risk_pub_v where object_id in (select ro.risk_id as object_id from ms_aud_grc_risk_v ro where ato_organizations in (select column_value from table(ms_apps_utilities.parse_string(:4,\u0027,\u0027))) and :3\u003d2 and nvl(trunc(ro.valid_until),trunc(sysdate))\u003e\u003dtrunc(sysdate)) union select object_name as risk_name, object_id as risk_id, object_level, ms_aud_report_utils.fn_get_risk_display_values(object_parent) as object_parent from ms_grc_risk_pub_v where object_id in (select object_id from ms_grc_risk_pub_v start with object_id in (select tgt_obj_id as object_id from ms_aud_grc_process_risk_v where src_obj_id in (select column_value from table(ms_apps_utilities.parse_string(:1,\u0027,\u0027))) and :3 \u003d 3) connect by prior object_id\u003dobject_parent) and nvl(trunc(valid_until), trunc(sysdate))\u003e\u003dtrunc(sysdate)) order by rownum asc"
  ],
  "MS_AUD_Aud_Manager": [
    "MS AUD Aud Manager",
    "1",
    "Y",
    "1",
    "select distinct a1.user_name,\n                ms_apps_utilities.get_user_full_name (a1.user_name)\n           from ms_aud_flow_down_org_v a1, si_users_t b1\n          where a1.user_id \u003d b1.user_id\n            and a1.activity_name \u003d \u0027aud - manages the audit\u0027"
  ],
  "MS_AUD_Audit_Project_Tasks_Inf": [
    "MS AUD Audit Project Tasks Inf",
    "1",
    "M",
    "1",
    "select a.task_name as task_name_link, nvl \n(ms_aud_report_utils.fn_get_status_display_name (a.status), \u0027none\u0027 ) as status, \nnvl(a.total_open_comments,\u00270\u0027) as \nopen_queries, ms_apps_utilities.get_user_full_name (a.task_owner) as auditor, \nnvl(ms_apps_utilities.get_user_full_name (a.approver),\u0027na\u0027) as approver,nvl(ms_apps_utilities.get_user_full_name(a.final_approver),\u0027na\u0027) as final_approver, \na.task_start_date as start_date, a.task_completion_date as end_date, \nms_apps_utilities.get_display_value (ms_apps_utilities.get_enterprise_id \n(ms_apps_utilities.get_infolet_id (\u0027ms_aud_manage_audit\u0027) ), \u0027ms aud task type\u0027, \na.task_type ) as task_type, null as action, case when (ms_aud_execution_rpt_utils.fn_get_coaching_icon \n(:1, a.audit_id) \u003d \u0027yes\u0027 ) then case when ms_aud_execution_rpt_utils.fn_get_offline_status \n(a.dd_process_instance_id) \u003d \u0027y\u0027 then   null  when \na.status in (\u002715\u0027,\u002716\u0027,\u002719\u0027,\u002752\u0027,\u002753\u0027) then null else  ms_aud_execution_rpt_utils.fn_get_view_task_coa_urls(decode(a.task_type,4,\u0027ms_aud_control_test_task\u0027,2, \u0027ms_aud_audit_task\u0027,3, \n\u0027ms_aud_checklist_task\u0027,null), :1, a.audit_id, a.task_id, a.task_type, a.dd_process_instance_id) end else null end as \n coaching_notes, \ncase when a.status in (\u002715\u0027,\u002753\u0027) then  ms_aud_execution_rpt_utils.fn_get_view_task_urls_reopen(decode(a.task_type,4,\u0027ms_aud_control_test_task\u0027,2,\u0027ms_aud_audit_task\u0027,3, \n\u0027ms_aud_checklist_task\u0027,a.task_type ), :1, a.audit_id, a.task_id, a.task_type, a.dd_process_instance_id,a.status,\u0027x\u0027) end as reopen_task, \ncase when ms_aud_execution_rpt_utils.fn_get_offline_status (a.dd_process_instance_id) \u003d \u0027y\u0027 \nthen  null when a.status in (\u002711\u0027,\u002749\u0027) then ms_aud_execution_rpt_utils.fn_get_view_task_urls_reopen(decode(a.task_type,4,\n \u0027ms_aud_control_test_task\u0027,2,\u0027ms_aud_audit_task\u0027,3, \n\u0027ms_aud_checklist_task\u0027,a.task_type), :1, a.audit_id, a.task_id,\n a.task_type, a.dd_process_instance_id,a.status,\u0027x\u0027) end  as reassign_task, \nnull as update_task, a.audit_id, b.audit_title,  ms_aud_execution_rpt_utils.fn_get_task_details(a.audit_id,a.task_id,a.task_type) as details, a.budgeted_hours, a.task_name,decode(a.task_type,\u00272\u0027,ms_aud_report_utils.fn_get_mlov_lead_aud_disp(ms_aud_report_utils.mlov_values (a.task_id,\u0027task_id\u0027,\u0027co_auditors_at\u0027,\u0027ms_aud_audit_task_cad\u0027)),\u00273\u0027,ms_aud_report_utils.fn_get_mlov_lead_aud_disp(ms_aud_report_utils.mlov_values (a.task_id,\u0027task_id\u0027,\u0027co_auditors\u0027,\u0027ms_aud_checklist_task_car\u0027)),\u00274\u0027,ms_aud_report_utils.fn_get_mlov_lead_aud_disp(ms_aud_report_utils.mlov_values (a.task_id,\u0027task_id\u0027,\u0027co_auditors\u0027,\u0027ms_aud_ctrl_test_task_car\u0027))) as co_auditors,\ncase when a.status in (\u002711\u0027,\u002749\u0027) then ms_aud_execution_rpt_utils.fn_get_view_task_urls_reopen(decode(a.task_type,4,\u0027ms_aud_control_test_task\u0027,2,\u0027ms_aud_audit_task\u0027, 3,\u0027ms_aud_checklist_task\u0027,a.task_type),:1,a.audit_id,a.task_id,a.task_type,a.dd_process_instance_id,a.status,\u0027update_co_aud\u0027) end as update_co_auditors, ms_aud_report_utils.fn_get_report_json(decode(a.task_type,\u00274\u0027,(select metric_id from si_metrics_t where metric_name\u003d\u0027ms_aud_control_test_task\u0027),\u00272\u0027,(select metric_id from si_metrics_t where metric_name\u003d\u0027ms_aud_audit_task\u0027),\u00273\u0027,(select metric_id from si_metrics_t where metric_name\u003d\u0027ms_aud_checklist_task\u0027),null),a.dd_process_instance_id,null,\u0027flag\u003d1\u0027||chr(38)||\u0027emd\u003d7\u0027||chr(38)||\u0027x_edit_mode\u003dno\u0027||chr(38)||\u0027mode\u003dview\u0027||chr(38)||\u0027task_id\u003d\u0027||a.task_id) as report_json,a.task_owner as auditor_id,\na.approver as approver_id,\na.final_approver as final_approver_id from \nms_aud_audit_task a, ms_aud_audit b where a.audit_id \u003d b.audit_id and a.audit_id \n\u003d :2 and a.task_type in (\u00272\u0027, \u00273\u0027, \u00274\u0027) and\n(ms_aud_audit_security.check_view_access (b.audit_id, b.dd_object_type, b.restrict_access_to, \u0027\u0027||b.conducted_by_org||\u0027\u0027, :1, \u0027view\u0027 ) like \u0027access\u0027 \nor ms_aud_audit_security.check_team_member(b.audit_id,:1)\u003e0) \norder by auditor"
  ],
  "MS_AUD_Rvw_Iss_New_Usr": [
    "MS AUD Rvw Iss New Usr",
    "1",
    "Y",
    "1",
    "select out_user_id\n,out_user_name                 \n,out_user_fullname\n,out_roles\n,out_orgs\nfrom table(ms_aud_reassgn_utilities.fn_get_mam_new_users(:1,:2,\u0027issue review task\u0027,\u0027aud - review issues\u0027))"
  ],
  "MS_AUD_Top_5_Question_Drilldown": [
    "MS AUD Top 5 Question Drilldown",
    "1",
    "M",
    "1",
    "select distinct b.checklist_id, b.checklist_name, b.version_no,\n                ms_apps_utilities.get_display_value\n                   (ms_apps_utilities.get_enterprise_id\n                       (ms_apps_utilities.get_infolet_id\n                                                    (\u0027ms_chk_checklist_master\u0027)\n                       ),\n                    \u0027ms chk checklist type\u0027,\n                    b.checklist_type,\n                    1,\n                    null,\n                    \u0027ms_apps_utilities.get_enterprise_name(ms_apps_utilities.get_enterprise_id \n(ms_apps_utilities.get_infolet_id (\u0027\u0027ms_chk_checklist_master\u0027\u0027) ))\u0027\n                   ) as checklist_type,\n                ms_apps_utilities.get_display_value\n                   (ms_apps_utilities.get_enterprise_id\n                       (ms_apps_utilities.get_infolet_id\n                                                    (\u0027ms_chk_checklist_master\u0027)\n                       ),\n                    \u0027ms chk scoring method\u0027,\n                    b.scoring_method,\n                    1,\n                    null,\n                    \u0027ms_apps_utilities.get_enterprise_name(ms_apps_utilities.get_enterprise_id \n(ms_apps_utilities.get_infolet_id (\u0027\u0027ms_chk_checklist_master\u0027\u0027) ))\u0027\n                   ) as scoring_method,\n                ms_grc_common_utils.fn_get_org_display_names\n                   (ms_grc_common_utils.fn_get_val_by_id\n                                                  (c.checklist_id,\n                                                   \u0027ms_chk_chklist_master_org\u0027,\u0027owner_organizations\u0027,\u0027checklist_id\u0027\n                                                  )\n                   ) as owner_organizations,\n                ms_chk_utilities.fn_get_user_disp_val\n                   (ms_grc_common_utils.fn_get_val_by_id\n                                                  (d.checklist_id,\n                                                  \u0027ms_chk_chklist_master_own\u0027,\n                                                   \u0027owners\u0027,\n                                                   \u0027checklist_id\u0027\n                                                  )\n                   ) as owners,\n                ms_apps_utilities.get_user_full_name\n                                               (b.dd_created_by)\n                                                                as created_by,\n               \u0027edit\u0027 as edit,\n                ms_chk_utilities.include_expired\n                                       (trunc (b.valid_from),\n                                        trunc (b.valid_untill)\n                                       ) as expired_flag,\n           ms_aud_report_utils.fn_get_report_json(\n  (select metric_id from si_metrics_t where metric_name\u003d\u0027ms_chk_checklist_master\u0027\n  ),\n  b.dd_process_instance_id,\n  b.dd_instance_id,\n  \u0027flag\u003d1\u0027\n  ||chr(38)\n  ||\u0027emd\u003d1\u0027\n  ||chr(38)\n  ||\u0027edit_flag\u003dno\u0027\n  ) as report_json,\n                ms_grc_common_utils.fn_get_val_by_id(d.checklist_id,\u0027ms_chk_chklist_master_own\u0027 ,\u0027owners\u0027,\u0027checklist_id\u0027) as owners_stored,\n                b.dd_created_by as created_by_stored\n           from ms_chk_chklist_master b,\n                ms_chk_chklist_master_org c,\n                ms_chk_chklist_master_own d,\n                ms_aud_chk_question_qst e\n            where\n                e.question_id\u003d:1 \n            and e.qst_checklist_id\u003db.checklist_id and e.qst_checklist_id\u003dc.checklist_id\n            and e.qst_checklist_id\u003dd.checklist_id  and b.checklist_id \u003d c.checklist_id\n            and b.checklist_id \u003d d.checklist_id and d.checklist_id \u003d c.checklist_id\nand ms_apps_security_pkg.check_object_access (b.dd_object_type, b.restrict_access_to,\nms_grc_common_utils.fn_get_val_by_id (c.checklist_id,\u0027ms_chk_chklist_master_org\u0027,\u0027owner_organizations\u0027,\u0027checklist_id\u0027),:2,\u0027view\u0027)\u003d\u0027access\u0027"
  ],
  "MS_AUD_Get_Co_Auditors_For_Task": [
    "MS_AUD_Get_Co_Auditors_For_Task",
    "1",
    "M",
    "1",
    "select \n          user_full_name as user_full_name, \n          user_name as user_name,\n          locale_id\nfrom\n(\nselect user_name as user_name,\n       user_full_name as user_full_name\n  from ms_aud_users_v b\n where exists (select 1\n                       from ms_aud_audit_tmr a\n                      where a.audit_id \u003d :1 and a.tmr_user_name \u003d b.user_name)\nunion\nselect user_name as user_name,\n       user_full_name as user_full_name\n  from ms_aud_users_v a\n where exists (select 1\n                        from ms_aud_audit b\n                       where b.audit_id \u003d :1 and b.lead_auditor \u003d a.user_name\n               union\n               select 1\n                        from ms_aud_audit b\n                       where b.audit_id \u003d :1 and b.audit_manager \u003d a.user_name)\n and \u00272\u0027 \u003d :2\nunion\nselect user_name as user_name,\n       user_full_name as user_full_name\n  from ms_aud_users_v a\n where exists ((select 1\n                        from ms_aud_audit_alm b\n                       where b.audit_id \u003d :1 and a.user_name \u003d b.lead_auditor_mlov)\n                     union all\n                     (select 1\n                        from ms_aud_audit_amm b\n                       where b.audit_id \u003d :1 and a.user_name \u003d b.audit_manager_mlov))\n   and \u00271\u0027 \u003d :2),si_enabled_locales where user_name \u003c\u003e :3\n    and exists (select 1 from ms_aud_audit where audit_id\u003d:1 and setup_enable_collaboration\u003d\u00271\u0027)"
  ],
  "MS_AUD_Get_Rltd_Ctrl": [
    "MS AUD Get Rltd Ctrl",
    "1",
