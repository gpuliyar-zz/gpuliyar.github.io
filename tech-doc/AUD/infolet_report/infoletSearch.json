    "select distinct \n a.audit_title\n  as audit_title,\n  (select lovvalue.displayed_value as displayed_value\n  from ms_qs_list_of_values lovvalue,\n    ms_qs_lov_names lovname\n  where lovvalue.lov_id      \u003d lovname.lov_id\n  and lovvalue.org_entity_id \u003d 1\n  and lovname.lov_name       \u003d \u0027md aud report type\u0027\n  and lovvalue.locale_id     \u003d nvl(cast(si_db_locale_sv.getvalue as number),1009)\n  and stored_value           \u003db.report_type\n  and lovvalue.enterprise_id \u003d(select enterprise_id from si_ent)\n  and (lovname.end_date     is null\n  or lovname.end_date        \u003e sysdate)\n  and lovname.start_date    \u003c\u003d sysdate\n  and (lovvalue.end_date    is null\n  or lovvalue.end_date       \u003e sysdate)\n  ) as report_type,\n                ms_aud_report_utils.fn_get_mlov_aud_man_disp\n                   (nvl (a.lead_auditor,\n                         (ms_aud_utilities.mlov_values (b.audit_id,\n                                                        \u0027audit_id\u0027,\n                                                        \u0027lead_auditor_mlov\u0027,\n                                                        \u0027ms_aud_audit_alm\u0027\n                                                       )\n                         )\n                        )\n                   ) as lead_auditor,\n                ms_aud_report_utils.fn_get_mlov_aud_man_disp\n                   (nvl (a.audit_manager,\n                         (ms_aud_utilities.mlov_values (a.audit_id,\n                                                        \u0027audit_id\u0027,\n                                                        \u0027audit_manager_mlov\u0027,\n                                                        \u0027ms_aud_audit_amm\u0027\n                                                       )\n                         )\n                        )\n                   ) as audit_manager,\n                ms_aud_report_utils.fn_get_mlov_aud_man_disp\n                                                   (decode(b.ar_reviewer,\u00270\u0027,b.ar_auditee,null,b.ar_auditee,b.ar_reviewer))\n                                                                  as reviewer,\n(select status_name from ms_aud_status_tl_v where locale_id \u003d (select b.locale_id from si_users_t a, si_locales b where a.locale \u003d b.locale_string and a.user_name \u003d :1) and status_id \u003d b.status) as current_status,\nms_aud_report_utils.fn_get_report_json(\n        (select metric_id from si_metrics_t where metric_name\u003d\u0027ms_aud_audit_report\u0027),\n        b.process_instance_id,\n        null,decode(ms_aud_reporting_rpt_utils.fn_send_issues_rev_edit (:1,b.process_instance_id),\u0027y\u0027,\n        \u0027flag\u003d1\u0027\t|| chr (38)\t|| \u0027emd\u003d4\u0027\t|| chr (38)\t|| \u0027x_edit_mode\u003dyes\u0027\t|| chr (38)\t|| \u0027audit_id\u003d\u0027\t|| b.audit_id,\n        \u0027flag\u003d1\u0027\t|| chr (38)\t|| \u0027emd\u003d2\u0027\t|| chr (38)\t|| \u0027x_edit_mode\u003dno\u0027\t|| chr (38)\t|| \u0027audit_id\u003d\u0027\t|| b.audit_id) ) as report_json,\n           nvl (a.lead_auditor,(ms_aud_utilities.mlov_values (b.audit_id,\u0027audit_id\u0027,\u0027lead_auditor_mlov\u0027,\u0027ms_aud_audit_alm\u0027)))as lead_auditor_stored,nvl (a.audit_manager,\n (ms_aud_utilities.mlov_values (a.audit_id,\u0027audit_id\u0027,\u0027audit_manager_mlov\u0027,\u0027ms_aud_audit_amm\u0027))) as audit_manager_stored,\ndecode(b.ar_reviewer,\u00270\u0027,b.ar_auditee,null,b.ar_auditee,b.ar_reviewer) as ar_reviewer_stored\n           from ms_aud_audit a,\n                ms_aud_audit_report_v b\n           where b.instance_id \u003d (select max (instance_id)\n                        from ms_aud_audit_report_v\n                       where audit_id \u003d b.audit_id and process_instance_id\u003db.process_instance_id)\n   and a.audit_id\u003db.audit_id\n   and b.instance_rec_num \u003d 1\n and ( ms_aud_audit_security.check_team_member(a.audit_id,:1) \u003e 0 or b.ar_auditee\u003d:1 or b.ar_approver\u003d:1 or b.ar_reviewer \u003d:1 or b.pfr_management_support\u003d:1)"
