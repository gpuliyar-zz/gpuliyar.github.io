    "select a.audit_id as audit_id,a.audit_title as audit_title,a.audit_title_link as audit_title_link,a.plan_title as plan_title,a.audit_type as audit_type,\na.audit_category as audit_category,a.audit_status as audit_status ,a.core_objects as core_objects , rsk_object_name as risks,\na.organizations as organizations,case when a.lead_auditor is null then la.la_disp_name else (select user_full_name from ms_aud_users_v where user_name\u003da.lead_auditor) end as lead_auditor,case when a.audit_manager is null then ma.ma_disp_name else (select user_full_name from ms_aud_users_v where user_name\u003da.audit_manager) end audit_manager \n,a.created_by as created_by,\na.audit_start_date as audit_start_date,a.view_tasks as view_tasks,a.restrict_access_to as restrict_access_to,a.team_member as team_member,ms_aud_report_utils.fn_get_report_json((select metric_id from si_metrics_t where metric_name \u003d \u0027ms_aud_manage_audit\u0027),a.dd_process_instance_id,a.dd_instance_id,\u0027flag\u003d1\u0027||chr(38)||\u0027emd\u003d2\u0027||chr(38)||\u0027x_edit_mode\u003dno\u0027||chr(38)||\u0027audit_id\u003d\u0027||a.audit_id) as report_json,\na.lead_auditor as lead_auditor_id\nfrom ms_aud_audit_list_report_v a left outer join (select audit_id,ms_join_for_display(lead_auditor_mlov) lead_auditor_mlov,ms_join_for_display(user_full_name) la_disp_name \nfrom ms_aud_audit_alm alm,ms_aud_users_v u where u.user_name \u003d alm.lead_auditor_mlov group by audit_id)la on (a.audit_id \u003d la.audit_id) left outer join  \n(select audit_id,ms_join_for_display(audit_manager_mlov) audit_manager_mlov,ms_join_for_display(user_full_name) ma_disp_name from ms_aud_audit_amm amm,ms_aud_users_v u where u.user_name \u003d amm.audit_manager_mlov group by audit_id)ma on (a.audit_id \u003d ma.audit_id) left outer join (select audit_id,ms_join_for_display(object_name) rsk_object_name from(select audit_id,object_name from ms_grc_risk_pub_v rp,ms_aud_audit_ris r where rp.object_id\u003dr.related_risks union select rp.audit_id,rs_risk_name_adhoc object_name from ms_aud_plan_scope_rsk rp,ms_aud_audit_ris r where r.audit_id\u003drp.audit_id and r.related_risks\u003drp.risk_id and rp.rs_risk_name_adhoc is not null) group by audit_id)prsk on (a.audit_id \u003d prsk.audit_id) \nwhere (:2 is null or (:2 is not null and (lower(a.audit_title) like \u0027%\u0027 || lower(:2) || \u0027%\u0027) ) )and (:7 is null or (:7 is not null and (trunc(a.audit_start_date) \u003e:7))) and (:8 is null or (:8 is not null and (trunc (a.audit_closure_date) \u003c :8) ) ) and nvl (a.conducted_by_org,1)\u003dnvl(:1,nvl(a.conducted_by_org,\u00271\u0027)) and nvl(a.audit_type,\u00271\u0027)\u003dnvl(:3,nvl(a.audit_type,\u00271\u0027)) and nvl(a.audit_classification,\u00271\u0027)\u003dnvl(:4,nvl (a.audit_classification,\u00271\u0027)) and (nvl (a.lead_auditor,\u00271\u0027) \u003d nvl(:5, nvl (a.lead_auditor,\u00271\u0027)) or instr(\u0027,\u0027||la.lead_auditor_mlov||\u0027,\u0027, \u0027,\u0027||:5||\u0027,\u0027)\u003e0) and (nvl(a.audit_manager,\u00271\u0027) \u003d nvl(:6, nvl (a.audit_manager,\u00271\u0027)) or instr(\u0027,\u0027||ma.audit_manager_mlov||\u0027,\u0027,\u0027,\u0027||:6||\u0027,\u0027)\u003e0)and ms_aud_audit_security.check_view_access (a.audit_id,a.dd_object_type,a.restrict_access_to,a.conducted_by_org,:9,\u0027view\u0027)\u003d\u0027access\u0027 and a.status not in (\u00272\u0027,\u002731\u0027,\u002744\u0027,\u002751\u0027,\u002759\u0027) and a.approve_flag \u003d \u00271\u0027 order by a.audit_title"
