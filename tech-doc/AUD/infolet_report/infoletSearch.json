  "MS_AUD_Flow_Actions": [
    "MS AUD Flow Actions",
    "1",
    "M",
    "1",
    "with prm as\n  (select pvalue.parameter_value as param_val \n     from ms_qs_parameter_values pvalue,\n           ms_qs_parameter_types ptype,\n           ms_qs_parameter_names pname\n    where upper(ptype.parameter_type)    \u003d \u0027ms_aud_system_parameters\u0027\n    and pname.parameter_id             \u003d pvalue.parameter_id\n    and ptype.parameter_type_id        \u003d pvalue.parameter_type_id\n    and ptype.parameter_type_id        \u003d pname.parameter_type_id\n    and pname.parameter_name           \u003d \u0027is_plan_reviewer_required\u0027\n    and pvalue.org_entity_id           \u003d 1),\n    apr as\n    (select ms_aud_f001_helper.fn_get_plan_approver(\u0027aud - approve plan\u0027, :5) as apr_val from dual)\nselect distinct a.action_id  as action_id\n        ,d.action_name as action, a.flow_id as flow ,\n        null  as custom_column1,\n        null  as custom_column2,\n        null  as custom_column3,\n        null  as custom_column4,\n        d.locale_id as locale_id\n   from ms_aud_flow_stage_action a\n        ,ms_aud_stages b\n        ,ms_aud_stages c\n        ,ms_aud_actions_tl_v d,\n        prm,\n        apr\n   where a.previous_stage \u003d b.stage_id\n          and a.current_stage \u003d c.stage_id\n           and a.action_id \u003d d.action_id\n     and a.active \u003d\u0027y\u0027\n     and b.active \u003d \u0027y\u0027\n     and d.active \u003d \u0027y\u0027\n          and a.current_stage \u003d to_number(:1)\n     and a.previous_stage \u003d  to_number(:2)\nand exists(select 1\n                from si_user_org_roles uor,\n                     si_org_roles orl,\n                     si_role_activities rat,\n                     si_activities_t act\n               where uor.user_id     \u003d (select usr.user_id from si_users_t usr where usr.user_name \u003d :3)\n                 and orl.org_role_id \u003d uor.org_role_id \n                 and rat.role_id     \u003d orl.role_id\n                 and act.activity_id \u003d rat.activity_id\n                 and act.activity_name like \u0027aud%\u0027 )\nand (case when prm.param_val \u003d \u0027y\u0027 and a.action_id \u003d\u002711\u0027 and :1 \u003d\u00271\u0027 then\n              1\n         when  prm.param_val \u003d \u0027n\u0027 and a.action_id \u003d\u002711\u0027 and :1 \u003d\u00271\u0027 then\n              0\n         else 1 end)\u003d1\n and (case when apr.apr_val like \u0027%\u0027 ||:3 ||\u0027%\u0027 and a.action_id \u003d\u002735\u0027 and :1 \u003d\u00271\u0027 then\n              1\n         when  apr.apr_val like \u0027%\u0027 ||:3 ||\u0027%\u0027 and a.action_id \u003d\u002735\u0027 and :1 \u003d\u00271\u0027 then\n              0\n         else 1 end)\u003d1         \nand ms_aud_utilities.fn_actfilter_by_audit_id (a.action_id,:4,:5,:1,:2)\u003d1 \nand a.action_id \u003d nvl(:6,a.action_id) order by action"
