  "MS_AUD_Risk_Link_Report": [
    "MS AUD Risk Link Report",
    "1",
    "M",
    "1",
    "select a.object_id||ms_aud_constants.scope_delimiter()||b.src_obj_id as risk_id,null as risk_name,object_level,\nms_aud_report_utils.fn_get_risk_display_values (object_parent) as object_parent,\nnull as process_name,null as process_id,null as org_entity_name,\nnull as org_entity_id\n  from ms_grc_risk_pub_v a,\n    ms_aud_grc_risk_process_v b\n  where a.object_id \u003d b.tgt_obj_id\n  and b.src_obj_id in\n    (select column_value\n    from table (ms_grc_str_common_utilities.split_string_stored (:3) )\n    )\n  and :1\u003d 1\n  and ms_aud_utilities.get_parameter_value (\u0027is_ae_risks_relation_required\u0027) \u003c\u003e \u0027n\u0027\n  and nvl(trunc(a.valid_until),trunc(sysdate))\u003e\u003d trunc(sysdate)\n  and ms_apps_security_pkg.check_object_access (a.dd_object_type, a.restrict_access_to, a.owner_organizations, :2, \u0027view\u0027 ) \u003d \u0027access\u0027\n  union\n  select object_id as risk_id,object_name as risk_name, \n    object_level,ms_aud_report_utils.fn_get_risk_display_values (object_parent) as object_parent,\n    null as process_name,\n    null as process_id,\n    null as org_entity_name,\n    null as org_entity_id\n  from ms_grc_risk_pub_v\n    start with object_id in\n    (select a.object_id\n    from ms_grc_risk_pub_v a\n    where :1 \u003d 1\n    and ms_aud_utilities.get_parameter_value (\u0027is_ae_risks_relation_required\u0027)\u003d \u0027n\u0027\n    and nvl(trunc(a.valid_until),trunc(sysdate)) \u003e\u003d trunc(sysdate)\n    and ms_apps_security_pkg.check_object_access (a.dd_object_type, a.restrict_access_to, a.owner_organizations, :2, \u0027view\u0027 ) \u003d \u0027access\u0027\n    )\n    connect by prior object_id \u003d object_parent\n  union\n  select\n    a.risk_id||ms_aud_constants.scope_delimiter()||a.ato_organizations as risk_id,\n    null as risk_name,\n    a.object_level,\n    ms_aud_report_utils.fn_get_risk_display_values (a.object_parent) as object_parent,\n    null as process_name,\n    null as process_id,\n    null as org_entity_name,\n    null as org_entity_id\n  from ms_aud_grc_risk_v a\n  where a.ato_organizations in\n    (select column_value\n    from table (ms_grc_str_common_utilities.split_string_stored (:4) )\n    )\n  and :1 \u003d 2\n  and nvl(trunc(a.valid_until),trunc(sysdate)) \u003e\u003d trunc(sysdate)\n  union\n  select   b.object_id\n    ||ms_aud_constants.scope_delimiter()\n    ||a.proc_org_id as risk_id,\n    b.object_name   as risk_name,\n    b.object_level,\n    ms_aud_report_utils.fn_get_risk_display_values (b.object_parent) as object_parent,\n    null as process_name,\n    null as process_id,\n    null as org_entity_name,\n    null as org_entity_id\n  from  (select column_value as proc_org_id\n    from table (ms_grc_str_common_utilities.split_string_stored (:3))\n    ) a,\n    ms_grc_risk_pub_v b,\n    ms_aud_grc_process_risk_v c\n    where b.object_id   \u003d c.tgt_obj_id\n  and substr(a.proc_org_id, 0, instr(a.proc_org_id, ms_aud_constants.scope_delimiter(), 1, 1) -1)\u003dc.src_obj_id\n  and :1 \u003d 3\n  and nvl(trunc(b.valid_until),trunc(sysdate)) \u003e\u003d trunc(sysdate)"
