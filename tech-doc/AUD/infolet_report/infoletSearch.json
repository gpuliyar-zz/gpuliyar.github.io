  ],
  "MS_AUD_Audits_by_Audit_Classification_2years_Last_Year": [
    "MS_AUD_Audits_by_Audit_Classification_2years_Last_Year",
    "1",
    "M",
    "1",
    "select   a.audit_id as audit_id, \n    a.audit_title as audit_title,\n    ms_aud_report_utils.fn_get_report_json((select metric_id from si_metrics_t where metric_name \u003d \u0027ms_aud_manage_audit\u0027),\n    a.dd_process_instance_id, a.dd_instance_id,\n    \u0027flag\u003d1\u0027 || chr(38) || \u0027emd\u003d2\u0027 ||chr(38) || \u0027x_edit_mode\u003dno\u0027 ||chr(38) || \u0027audit_id\u003d\u0027 || a.audit_id) as audit_title_link,\n    nvl(ms_aud_planning_rpt_utils.fn_get_plan_title(a.plan_title), \u0027adhoc\u0027) as plan_title,\n    ms_aud_execution_rpt_utils.get_code_description(\u0027ms aud audit type\u0027, a.audit_type) as audit_type,\n    ms_aud_execution_rpt_utils.get_code_description(\u0027ms aud classification\u0027, a.audit_classification) as audit_category,\n    ms_aud_report_utils.fn_get_status_display_name(a.audit_status) as audit_status,\n    ms_aud_report_utils.fn_get_audit_core_objects(a.audit_id, a.scope_based_on) as core_objects,\n    ms_aud_report_utils.fn_get_audit_related_risks(a.audit_id, a.scope_based_on) as risks,\n    ms_aud_report_utils.fn_get_audit_organizations(a.audit_id, a.scope_based_on) as organizations,\n    ms_grc_common_utils.fn_get_org_display_names(conducted_by_org) as conducted_org,\n    nvl(ms_apps_utilities.get_user_full_name(a.lead_auditor),\n           ms_aud_report_utils.fn_get_mlov_lead_aud_disp(ms_grc_common_utils.fn_get_val_by_id(a.audit_id,\n                                                                                                \u0027ms_aud_audit_alm\u0027,\u0027lead_auditor_mlov\u0027,\u0027audit_id\u0027))) as lead_auditor,\n    nvl(ms_apps_utilities.get_user_full_name(a.audit_manager),\n           ms_aud_report_utils.fn_get_mlov_aud_man_disp(ms_grc_common_utils.fn_get_val_by_id(a.audit_id,\n                                                                                               \u0027ms_aud_audit_amm\u0027,\u0027audit_manager_mlov\u0027,\u0027audit_id\u0027))) as audit_manager,\n    ms_apps_utilities.get_user_full_name(a.dd_created_by) as created_by,\n    a.audit_start_date as  audit_start_date,\n    ms_aud_execution_rpt_utils.fn_get_task_count(a.audit_id, :2) as view_tasks,\n    a.restrict_access_to as restrict_access_to,\n    ms_aud_utilities.mlov_values(a.audit_id,\n                                    \u0027audit_id\u0027,\n                                    \u0027team_member\u0027,\n                                    \u0027ms_aud_audit_tmm\u0027) as team_member,\n    decode(status, 3, \u0027start\u0027, null) as starts,\n    decode(status, 20, \u0027sch\u0027, null) as sch,\n    a.audit_end_date as audit_end_date,\n    s.issues as issues,\n    case\n      when a.audit_status in (\u002738\u0027,\u00278\u0027)\n      then\n        (select url_link\n          from\n          (select url_link,\n            audit_id,\n            dense_rank() over(partition by audit_id order by published_date desc) as rnk\n            from ms_aud_audit_published_reports\n          ) c\n        where c.audit_id\u003da.audit_id\n        and rnk \u003d1\n        )\n    end as download,\nnvl(a.lead_auditor,ms_grc_common_utils.fn_get_val_by_id(a.audit_id,\u0027ms_aud_audit_alm\u0027,\u0027lead_auditor_mlov\u0027,\u0027audit_id\u0027)) as lead_auditor_stored,\n    nvl(a.audit_manager,ms_grc_common_utils.fn_get_val_by_id(a.audit_id,\u0027ms_aud_audit_amm\u0027,\u0027audit_manager_mlov\u0027,\u0027audit_id\u0027)) as audit_manager_stored,\n    a.dd_created_by as created_by_stored\n  from ms_aud_audit a\n  left outer join\n  (select audit_id,\n    count(1) as issues\n  from table (ms_aud_reporting_rpt_utils.get_issue_data)\n  group by audit_id\n  )s\n  on(a.audit_id        \u003ds.audit_id)\n where ms_aud_audit_security.check_view_access(a.audit_id,\n                                               a.dd_object_type,\n                                               a.restrict_access_to,\n                                               a.conducted_by_org,\n                                               :2,\n                                               \u0027view\u0027) \u003d \u0027access\u0027\n   and a.audit_classification \u003d :1\n   and extract(year from audit_start_date) \u003d\n       nvl(:3, ms_aud_constants.fn_year)\n   and status not in (44, 51, 2, 31)"
  ],
