  "MS_AUD_Process_Mlov": [
    "MS AUD Process Mlov",
    "1",
    "M",
    "1",
    "select    process_id as processid,\n          process_name as processname,process_level as prlevel ,core_type,\n          version_no\n          from (select object_name as process_name, object_id as process_id,\n                 ms_aud_utilities.fn_get_ae_obj_name(dd_object_type) as core_type,\n                  object_level as process_level,\n                 (select ae_version from ms_aud_auditable_entit where object_id\u003dms_aud_grc_core_object_pub_v.object_id) as version_no\n             from ms_aud_grc_core_object_pub_v\n           where (:2 \u003d 5 or :2 \u003d 1) \n            and (( ms_aud_grc_core_object_pub_v.object_id in (select max(b.object_id) from ms_aud_grc_core_object_pub_v a, \n                ms_aud_auditable_entit b where a.object_id \u003d b.object_id \n                and nvl(trunc(a.valid_from),trunc(sysdate)) \u003c\u003d trunc(sysdate) \n                and nvl(trunc(a.valid_until),trunc(sysdate)) \u003e\u003d trunc(sysdate) \n                group by b.internal_object_id)\n                  and dd_object_type\u003d\u0027ms_aud_auditable_entity\u0027) or dd_object_type \u003c\u003e \u0027ms_aud_auditable_entity\u0027\n                  or ms_aud_grc_core_object_pub_v.object_id in (select core_objects from ms_aud_audit_aue where audit_id\u003d:3 and :3\u003c\u003e\u00270\u0027))\n             and  nvl(trunc(valid_from),trunc(sysdate)) \u003c\u003d trunc(sysdate) and nvl(trunc(valid_until),trunc(sysdate)) \u003e\u003d trunc(sysdate)\n             and ms_apps_security_pkg.check_object_access\n                                                         (dd_object_type,\n                                                          restrict_access_to,\n                                                          owner_organizations,\n                                                          :1,\n                                                          \u0027view\u0027\n                                                         ) \u003d \u0027access\u0027\n             union\n            select distinct po.process_name as process_name,\n                        po.process_id||ms_aud_constants.scope_delimiter()|| po.ato_organizations  as process_id,\n                         ms_aud_utilities.fn_get_ae_obj_name(pub.dd_object_type) as core_type,\n                         object_level as process_level,\n                        (select ae_version from ms_aud_auditable_entit where object_id\u003dpo.process_id) as version_no\n          from ms_aud_grc_process_v po, ms_aud_grc_core_object_pub_v pub\n         where po.ato_organizations in (select column_value from table(ms_grc_str_common_utilities.split_string_stored(nvl(:4, 1))))\n           and :2 \u003d 3\n           and po.process_id \u003d pub.object_id\n           and (( pub.object_id in (select max(b.object_id) from ms_aud_grc_core_object_pub_v a, ms_aud_auditable_entit b \n          where a.object_id \u003d b.object_id \n          and nvl(trunc(a.valid_from),trunc(sysdate)) \u003c\u003d trunc(sysdate) \n          and nvl(trunc(a.valid_until),trunc(sysdate)) \u003e\u003d trunc(sysdate) \n          group by b.internal_object_id)\n                    and pub.dd_object_type\u003d\u0027ms_aud_auditable_entity\u0027) or pub.dd_object_type \u003c\u003e \u0027ms_aud_auditable_entity\u0027\n                    or pub.object_id in (select core_objects from ms_aud_audit_aue where audit_id\u003d:3 and :3\u003c\u003e\u00270\u0027))\n                     and nvl(trunc(pub.valid_from), trunc(sysdate)) \u003c\u003d trunc(sysdate)\n                     and nvl(trunc(pub.valid_until), trunc(sysdate)) \u003e\u003d trunc(sysdate)\n                     and ms_apps_security_pkg.check_object_access(dd_object_type,\n                                                                  restrict_access_to,\n                                                                  pub.owner_organizations,\n                                                                  :1,\n                                                                  \u0027view\u0027) \u003d \u0027access\u0027\n             )"
  ],
  "MS_AUD_FLOW_ACTIONS_NEW": [
    "MS_AUD_FLOW_ACTIONS_NEW",
    "1",
    "M",
    "1",
    "select action,\n  custom_column4,\n  custom_column2,\n  custom_column3,\n  locale_id,\n  custom_column1,\n  action_id,\n  flow\nfrom\n  (select action_id,\n    action_name as action,\n    null        as flow,\n    null        as custom_column1,\n    null        as custom_column2,\n    null        as custom_column3,\n    null        as custom_column4,\n    locale_id   as locale_id\n  from ms_aud_actions_tl_v\n  where action_id\u003d41\n  and action_id  \u003dnull\n  union\n  select distinct a.action_id as action_id ,\n    d.action_name             as action,\n    a.flow_id                 as flow ,\n    null                      as custom_column1,\n    null                      as custom_column2,\n    null                      as custom_column3,\n    null                      as custom_column4,\n    d.locale_id               as locale_id\n  from ms_aud_flow_stage_action a ,\n    ms_aud_stages b ,\n    ms_aud_stages c ,\n    ms_aud_actions_tl_v d ,\n    (select ms_aud_utilities.get_parameter_value(\u0027is_plan_reviewer_required\u0027) as param_val\n    from dual\n    ) prm ,\n    (select ms_aud_f001_helper.fn_get_plan_approver(\u0027aud - approve plan\u0027, :2) as apr_val\n    from dual\n    ) apr,(select distinct a.activity_name\n                     from si_activities_t a,\n                          si_role_activities b,\n                          si_org_roles c,\n                          si_org_entities_t d,\n                          si_user_org_roles e,\n                          si_users_t f,\n                          si_roles_t g\n                    where a.activity_id \u003d b.activity_id\n                      and b.role_id \u003d c.role_id\n                      and b.role_id \u003d g.role_id\n                      and c.org_entity_id \u003d d.org_entity_id\n                      and c.org_role_id \u003d e.org_role_id\n                      and e.user_id \u003d f.user_id\n                      and f.user_name \u003d :1\n                      and a.activity_name like \u0027aud%\u0027) t\n  where a.previous_stage \u003d b.stage_id\n  and a.current_stage    \u003d c.stage_id\n  and a.action_id        \u003d d.action_id\n  and a.active           \u003d\u0027y\u0027\n  and b.active           \u003d \u0027y\u0027\n  and d.active           \u003d \u0027y\u0027\n  and a.current_stage    \u003d to_number(:3)\n  and a.previous_stage   \u003d to_number(:4)\n  and a.activity\u003dt.activity_name\n  and (\n    case\n      when prm.param_val \u003d \u0027y\u0027\n      and a.action_id    \u003d\u002711\u0027\n      and :3          \u003d\u00271\u0027\n      then 1\n      when prm.param_val \u003d \u0027n\u0027\n      and a.action_id    \u003d\u002711\u0027\n      and :3          \u003d\u00271\u0027\n      then 0\n      else 1\n    end)\u003d1\n  and (\n    case\n      when apr.apr_val like \u0027%\u0027\n        ||:1\n        ||\u0027%\u0027\n      and a.action_id \u003d\u002735\u0027\n      and :3       \u003d\u00271\u0027\n      then 1\n      when apr.apr_val like \u0027%\u0027\n        ||:1\n        ||\u0027%\u0027\n      and a.action_id \u003d\u002735\u0027\n      and :3       \u003d\u00271\u0027\n      then 0\n      else 1\n    end)                                                                                                \u003d1\n  and ms_aud_utilities.fn_actfilter_by_audit_id (a.action_id,:5,:2,:3,:4)\u003d1\n  and a.action_id                                                                                       \u003d nvl(null,a.action_id)\n  order by action\n  )\nwhere locale_id \u003d nvl(si_db_locale_sv.getvalue, si_i18n_sv.getdefaultlocaleid)"
  ],
