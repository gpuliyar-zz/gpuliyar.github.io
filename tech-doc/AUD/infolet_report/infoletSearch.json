data={
  "MS_AUD_AUDIT_AES_DRILLDOWN": [
    "MS AUD AUDIT AES DRILLDOWN",
    "1",
    "M",
    "1",
    "select object_name display_name,\n to_char (object_id) object_id, decode(dd_object_type,\u0027ms_aud_auditable_entity\u0027,\u0027ms_aud_auditable_entit\u0027,dd_object_type) object_type,\n null object_level, \u0027n\u0027 as over_ride_action,\n null as edit_action_parameters\nfrom ms_grc_core_object_pub_v a\n where exists (\nwith ae_list as\n (select trim (substr (txt,\n instr (txt, \u0027,\u0027, 1, level) + 1,\n instr (txt, \u0027,\u0027, 1, level + 1)\n - instr (txt, \u0027,\u0027, 1, level)\n - 1\n)\n) as ae_id\nfrom (select \u0027,\u0027 || ms_aud_utilities.mlov_values_data_browser (audit_id,\u0027audit_id\u0027,\u0027core_objects\u0027,\u0027ms_aud_audit_aue\u0027) || \u0027,\u0027 as txt\nfrom ms_aud_audit aud\n where audit_id \u003d :1)\nconnect by level \u003c\u003d\nlength (txt) - length (replace (txt, \u0027,\u0027, \u0027\u0027))\n- 1)\nselect distinct ae_id\n from ae_list\nwhere ae_id \u003d a.object_id)"
  ],
  "MS_AUD_Test_ID_Sequence": [
    "MS_AUD_Test_ID_Sequence",
