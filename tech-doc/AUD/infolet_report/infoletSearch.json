  "MS_AUD_Project_Status": [
    "MS AUD Project Status",
    "1",
    "M",
    "1",
    "select distinct a1.audit_id, a1.audit_title ||\u0027 [\u0027 ||a1.audit_id ||\u0027]\u0027 as aud_title, case a1.status when \u00273\u0027 then \u0027in process\u0027 when \u00274\u0027 then \u0027canceled\u0027 when \u00278\u0027 then \u0027closed\u0027 when \u002720\u0027 then \u0027future audit\u0027 else \u0027status not found\u0027 end km_status, trim(c1.km_milestones) as km_milestones, nvl (to_char (trunc (c1.km_proposed_date), \u0027mm/dd/yyyy\u0027), \u0027-\u0027) as proposed_date, \u0027planned\u0027 as proposed, case when trunc (c1.km_proposed_date)\u003ctrunc(c1.km_actual_date) then \u0027delayed\u0027 when trunc (c1.km_proposed_date)\u003e\u003dtrunc(c1.km_actual_date) then \u0027on-time\u0027 else \u0027not-applicable\u0027 end as km_category, c1.km_event_in_system, nvl((trunc(c1.km_actual_date))-trunc((c1.km_proposed_date)),0) date_color from ms_aud_audit a1, ms_aud_audit_kms c1 where a1.audit_id \u003dc1.audit_id and nvl(trim(a1.plan_title),\u00271\u0027) \u003dnvl(trim(:1),nvl(trim(a1.plan_title),\u00271\u0027)) and nvl(trim(a1.conducted_by_org),\u00270\u0027) \u003dnvl(trim (:2),nvl(trim(a1.conducted_by_org),\u00270\u0027)) and nvl (trim (a1.audit_type), \u00270\u0027) \u003d nvl (trim (:4), nvl (trim (a1.audit_type), \u00270\u0027)) and nvl (trim (a1.audit_classification), \u00270\u0027) \u003d nvl (trim (:3), nvl (trim (a1.audit_classification), \u00270\u0027)) and trunc (a1.audit_start_date) \u003c\u003d trunc (sysdate) and trunc (a1.audit_start_date) \u003e trunc(to_date (nvl(:5,(add_months(:6,-12))))) and trunc (a1.audit_start_date) \u003c trunc(to_date (:6)) and ( :7 is null or case a1.status when \u00273\u0027 then \u0027in process\u0027 when \u00274\u0027 then \u0027canceled\u0027 when \u00278\u0027 then \u0027closed\u0027 when \u002720\u0027 then \u0027future audit\u0027 else \u0027status not found\u0027 end \u003d :7) and ms_aud_audit_security.check_view_access(a1.audit_id,a1.dd_object_type,a1.restrict_access_to, a1.conducted_by_org,:9,\u0027view\u0027) \u003d \u0027access\u0027 and ( (:8 is null and a1.milestone_template \u003d \u0027100\u0027) or ( :8 is not null and a1.milestone_template \u003d :8) ) and a1.status not in (\u00272\u0027,\u002731\u0027,\u002751\u0027) and a1.approve_flag \u003d \u00271\u0027 union select distinct a1.audit_id, a1.audit_title ||\u0027 [\u0027 ||a1.audit_id ||\u0027]\u0027 as aud_title, case a1.status when \u00273\u0027 then \u0027in process\u0027 when \u00274\u0027 then \u0027canceled\u0027 when \u00278\u0027 then \u0027closed\u0027 when \u002720\u0027 then \u0027future audit\u0027 else \u0027status not found\u0027 end km_status, trim(c1.km_milestones), nvl (to_char (trunc (c1.km_actual_date), \u0027mm/dd/yyyy\u0027), \u0027-\u0027) as proposed_date , \u0027actual\u0027 as proposed, case when trunc (c1.km_proposed_date) \u003c trunc (c1.km_actual_date) then \u0027delayed\u0027 when trunc (c1.km_proposed_date) \u003e\u003d trunc (c1.km_actual_date) then \u0027on-time\u0027 else \u0027not-applicable\u0027 end as km_category , c1.km_event_in_system , nvl((trunc(c1.km_actual_date))-trunc((c1.km_proposed_date)),0) date_color from ms_aud_audit a1, ms_aud_audit_kms c1 where a1.audit_id \u003d c1.audit_id and nvl (trim (a1.plan_title), \u00271\u0027) \u003d nvl (trim (:1), nvl (trim (a1.plan_title), \u00271\u0027)) and nvl (trim (a1.conducted_by_org), \u00270\u0027) \u003d nvl (trim (:2), nvl (trim (a1.conducted_by_org), \u00270\u0027)) and nvl (trim (a1.audit_type), \u00270\u0027) \u003d nvl (trim (:4), nvl (trim (a1.audit_type), \u00270\u0027)) and nvl (trim (a1.audit_classification), \u00270\u0027) \u003d nvl (trim (:3), nvl (trim (a1.audit_classification), \u00270\u0027)) and trunc (a1.audit_start_date) \u003c\u003d trunc (sysdate) and trunc (a1.audit_start_date) \u003e trunc(to_date (nvl(:5,(add_months(:6,-12))))) and trunc (a1.audit_start_date) \u003c trunc(to_date (:6)) and ( :7 is null or case a1.status when \u00273\u0027 then \u0027in process\u0027 when \u00274\u0027 then \u0027canceled\u0027 when \u00278\u0027 then \u0027closed\u0027 when \u002720\u0027 then \u0027future audit\u0027 else \u0027status not found\u0027 end \u003d :7) and ms_aud_audit_security.check_view_access(a1.audit_id,a1.dd_object_type,a1.restrict_access_to, a1.conducted_by_org,:9,\u0027view\u0027) \u003d \u0027access\u0027 and ( (:8 is null and a1.milestone_template \u003d \u0027100\u0027) or ( :8 is not null and a1.milestone_template \u003d :8) ) and a1.status not in (\u00272\u0027,\u002731\u0027,\u002751\u0027) and a1.approve_flag \u003d \u00271\u0027 order by aud_title, km_event_in_system, proposed desc"
