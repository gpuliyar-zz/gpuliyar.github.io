    "select user_id,\n       user_name,\n       user_full_name,\n       activity_id,\n       activity_short_name,\n       activity_name,\n       locale_id as local_id\n  from (select /*+use_hash(a b c) use_nl(b d name_format)*/a.user_id,\n               b.user_name,\n               (case name_format.parameter_value\n                    when \u00272\u0027 then \n                         b.last_name || \u0027 \u0027 || b.first_name\n                    when \u00273\u0027 then \n                         b.last_name || \u0027,\u0027 || \u0027 \u0027 || b.first_name || \u0027 \u0027 || \u0027(\u0027 || b.user_name || \u0027)\u0027\n                    when \u00271\u0027 then \n                         b.first_name || \u0027 \u0027 || b.last_name\n                    when \u00274\u0027 then \n                         b.first_name || \u0027,\u0027 || \u0027 \u0027 || b.last_name || \u0027 \u0027 || \u0027(\u0027 || b.user_name || \u0027)\u0027\n                    else \n                         b.last_name || \u0027 \u0027 || b.first_name\n                end)\n                   as user_full_name,\n               c.activity_id,\n               c.activity_short_name,\n               c.activity_name,\n               d.locale_id\n          from (select /*+ index (si_user_activities si_user_activityname_idx) */activity_name, user_id from si_user_activities where upper (activity_name) in (\u0027ism - implement action\u0027, \u0027ism - approve action\u0027)) a,\n               si_users_t b,\n               si_activities_t c,\n               si_locales d,\n               (select locale from si_users_t where user_name \u003d :1) usr_loc,\n               (select pvalue.parameter_value\n                  from ms_qs_parameter_types ptype,\n                       ms_qs_parameter_values pvalue,\n                       ms_qs_parameter_names pname \n                 where upper (ptype.parameter_type) \u003d upper (\u0027ms apps user name display format\u0027)\n                   and ptype.parameter_type_id \u003d pvalue.parameter_type_id\n                   and pvalue.org_entity_id \u003d 1\n                   and ptype.parameter_type_id \u003d pname.parameter_type_id\n                   and pvalue.parameter_id \u003d pname.parameter_id\n                   and upper (pname.parameter_name) \u003d upper (\u0027display_format\u0027)) name_format\n         where a.user_id \u003d b.user_id\n           and a.activity_name \u003d c.activity_name  \n           and usr_loc.locale \u003d d.locale_string)"
