data={
  "Ms_GRC_Reference_Category": [
    "Ms GRC Reference Category",
    "1",
    "M",
    "1",
    "select stored_value,\n       displayed_value,\n       lov_name,\n       locale_id,\n       enterprise_id,\n       lov_id,\n       lov_value_id,\n       org_entity_id,\n       org_entity_name,\n       start_date,\n       end_date\nfrom   ms_grc_lov_org_values_v\nwhere(trim(lov_name) \u003d \u0027ms grc reference category\u0027) and locale_id\u003d (select locale_id from si_locales a,si_users_t b where locale\u003dlocale_string and upper(user_name) \u003d upper(:1))\norder by display_order asc"
  ],
  "MS_GRC_OBJC_Entp_List_Rpt_Inf": [
    "MS GRC OBJC Entp List Rpt Inf",
    "1",
    "M",
    "1",
    "select object_id  as objective_id,\n  object_name     as objective_name,\n  object_type,\n  object_name as objective_name_url,\n  objective_type,\n  key_objective,\n  coso_type,\n  object_level,\n  object_parent,\n  object_status,\n  created_date,\n  created_by,\n  updated_date,\n  updated_by,\n  valid_from,\n  valid_until,\n  description,\n  owner_org_names,\n  owner_name,\n  level_1_approver,\n  level_2_approver,\n  rel_data_link as rel_organization_names,\n  rel_data_link as rel_aoc_obj_name_agg,\n  rel_data_link as rel_asset_obj_name_agg,\n  rel_data_link as rel_asset_class_obj_name_agg,\n  rel_data_link as rel_ctrl_obj_name_agg,\n  rel_data_link as rel_fin_act_obj_name_agg,\n  rel_data_link as rel_function_obj_name_agg,\n  rel_data_link as rel_objective_obj_name_agg,\n  rel_data_link as rel_process_obj_name_agg,\n  rel_data_link as rel_product_name_agg,\n  rel_data_link as rel_refrences_obj_name_agg,\n  rel_data_link as rel_requirement_obj_name_agg,\n  rel_data_link as rel_risk_obj_name_agg,\n  (ms_grc_utilities.fn_get_report_json((select metric_id from si_metrics_t where metric_name\u003d\u0027ms_grc_objectives\u0027),rv.dd_process_instance_id,(select dd_instance_id from ms_grc_objectives where object_id\u003drv.object_id),\n\u0027\u0027\u0027flag\u003d1\u0027||chr(38)||\u0027emd\u003d1\u0027||chr(38)||\u0027x_edit_mode\u003dyes\u0027||chr (38)|| \u0027wrapper\u003d no\u0027|| chr (38)|| \u0027mode\u003dview\u0027\u0027\u0027)) as report_json\n,\n object_parent_id\nfrom ms_grc_objc_entp_rv rv\nwhere locale \u003d\n  (select locale from si_users_t where user_name \u003d :36\n  )\nand nvl(is_orphan, 1)\u003dnvl(:6,nvl(is_orphan,1))\nand exists\n    (select column_value\n    from table(cast(ms_grc_rep_browser_fil_denorm.fn_obj_report_filter(ms_grc_array_type(:1,:2,:3,:4,:5, :7,:8,to_char(:9,\u0027yyyy-mm-dd\u0027),to_char(:10,\u0027yyyy-mm-dd\u0027),:11,:12,:13,:14,to_char(:15,\u0027yyyy-mm-dd\u0027),to_char(:16,\u0027yyyy-mm-dd\u0027), to_char(:17,\u0027yyyy-mm-dd\u0027),to_char(:18,\u0027yyyy-mm-dd\u0027),:19,:20,:21,:22, :23,:24,:25,:26,null,:27,:28,null,:29,null,:30,null,:31,:32,null,:33,:34, :35,null,\u0027ms_grc_objectives\u0027,:36)) as ms_varchar2_array))\n  where column_value \u003d object_id) order by upper_object_name"
  ],
  "MS_GRC_MODEL_REFERENCE_DFLT_STRUCTURED_INFOLET": [
    "MS_GRC_MODEL_REFERENCE_DFLT_STRUCTURED_INFOLET",
    "11",
    "M",
    "1",
    null
  ],
  "MS_GRC_ACCT_ORB_Rpt_Inf": [
    "MS_GRC_ACCT_ORB_Rpt_Inf",
    "1",
    "M",
    "1",
    "select \n  fin_summ.object_id  as object_id,\n  fin_summ.object_name as object_name ,\n  mls_summ.fin_type as fin_type,\n  mls_summ.fin_category as fin_category,\n  \u0027ms_grc_financial_accounts\u0027 as additional_column3,\nnull as form_url,\n\u0027ms_grc_financial_accou\u0027 as additional_column4,\nfin_summ.upper_object_name as upper_object_name\nfrom ms_grc_fin_acc_summary fin_summ,ms_grc_fin_acc_mls_summary mls_summ\nwhere fin_summ.object_id \u003d mls_summ.object_id\nand mls_summ.locale_id \u003d (select locale_id from si_users_t t1 ,si_locales t2 where user_name \u003d :1 and t1.locale \u003d t2.locale_string)\nand exists\n    (select column_value\n    from table(cast(ms_grc_orb_report_filter.fn_financial_acc_orb_filters( :1, :2, :3, :4,:5) as ms_varchar2_array))\n    where column_value \u003d fin_summ.object_id\n    )\nand trunc(sysdate) between nvl(trunc(fin_summ.valid_from),trunc(sysdate)) and nvl(trunc(fin_summ.valid_until),trunc(sysdate))"
  ],
  "MS_GRC_FETCH_PROCESS_PARENT": [
    "MS GRC FETCH PROCESS PARENT",
    "1",
    "M",
    "1",
    "select distinct a.object_name as object_name,\n  a.object_id as object_id,\n  \u0027level \u0027\n  || a.object_level as object_level,c.locale_id as locale_id\nfrom ms_grc_core_object a,\n  ms_grc_core_object_ogn b,si_locales c,\nsi_users_t d where \nc.locale_string \u003d d.locale\nand d.user_name \u003d :2\nand object_level                                                                                          \u003d :1 - 1\nand dd_object_type                                                                                          \u003d \u0027ms_grc_process\u0027\nand trunc(sysdate)                                                                                         \u003e\u003d trunc(nvl(valid_from,sysdate))\nand trunc(sysdate)                                                                                         \u003c\u003d trunc(nvl(valid_until,sysdate))\nand ms_grc_utilities.ms_grc_security(dd_object_type, restrict_access_to, b.owner_organizations, :2, \u0027view\u0027) \u003d \u0027access\u0027\nand a.object_id                                                                                             \u003db.object_id\nand a.dd_object_type                                                                                        \u003d \u0027ms_grc_process\u0027\n"
  ],
  "MS_GRC_GET_METRIC_ID_SCHEDULED_ID_BY_METRIC_NAME": [
    "MS GRC GET METRIC ID SCHEDULED ID BY METRIC NAME",
    "1",
    "Y",
    "1",
    "select metric_id, metric_schedule_id from si_metric_schedules where metric_id in \n(select metric_id from si_metrics_t where metric_name \u003d :1) and rownum \u003d 1"
  ],
  "MS_GRC_GET_DATA_PRODUCT": [
    "MS GRC GET DATA PRODUCT",
    "1",
    "M",
    "1",
    "select object_id,\n  object_name,\n  valid_from ,\n  valid_until,\n  restrict_access_to,\n  dd_object_type as object_type ,\n  dd_process_instance_id,\n  dd_instance_id,\n  owner_organizations,\n  display_name,\n  edit_action_parameters\nfrom\n  (select object_id ,\n    object_name,\n    substr(object_name,1,30) display_name,\n    dd_object_type object_type ,\n    valid_from ,\n    valid_until,\n    restrict_access_to,\n    dd_object_type,\n    dd_process_instance_id,\n    \u0027proc\u003d\u0027\n    || to_char(dd_process_instance_id)\n    ||\u0027|instid\u003d\u0027\n    ||to_char(dd_instance_id)\n    ||\u0027|edit_flag\u003d\u0027\n    ||nvl(dd_edit_flag,\u0027y\u0027) as edit_action_parameters,\n    dd_instance_id,\n    (select ms_concat (d.owner_organizations)\n    from ms_grc_product_ogn d\n    where a.object_id \u003d d.object_id\n    ) as owner_organizations\n  from ms_grc_product a\n  ) t1\nwhere ms_grc_utilities.ms_grc_security(dd_object_type, restrict_access_to, t1.owner_organizations, :1, \u0027view\u0027) \u003d \u0027access\u0027\nand trunc(sysdate) between nvl(trunc(valid_from),trunc(sysdate)) and nvl(trunc(valid_until),trunc(sysdate))\norder by object_name asc"
  ],
  "MS_GRC_Control_Frequency": [
    "MS GRC Control Frequency",
    "1",
    "M",
    "1",
    "select c.stored_value as stored_value,\n  c.displayed_value as displayed_value,\n  c.lov_name as lov_name\nfrom ms_grc_lov_org_values_v c,\n  si_locales a,\n  si_users_t b\nwhere c.locale_id      \u003d a.locale_id\nand a.locale_string    \u003d b.locale\nand b.user_name \u003d :1\nand (trim(lov_name)    \u003d\u0027ms_grc_frequency\u0027)\nand (c.end_date is null or c.end_date \u003e sysdate) \n        and c.start_date \u003c\u003dsysdate\norder by stored_value asc"
  ],
  "MS_GRC_61_SP2_ASSETCLASS_BROWSER_INF": [
    "MS GRC 61 SP2 ASSETCLASS BROWSER INF",
    "1",
    "M",
    "1",
    "select\nobject_name as display_name,\ndd_object_type as object_type,\nover_ride_action,\nedit_action_parameters as view_action_parameters,\nobject_id as object_id\nfrom ms_grc_asset_class_denorm_drv\nwhere nvl(is_orphan, 1)\u003dnvl(:5,nvl(is_orphan,1))\nand exists\n  (select column_value\n    from table(cast(ms_grc_rep_browser_fil_denorm.fn_asst_class_report_filters(ms_grc_array_type(:1,:2,:3,:4, :6,to_char(:7,\u0027yyyy-mm-dd\u0027),to_char(:8,\u0027yyyy-mm-dd\u0027),:9,:10,:11,:12,to_char(:13,\u0027yyyy-mm-dd\u0027),to_char(:14,\u0027yyyy-mm-dd\u0027),to_char(:15,\u0027yyyy-mm-dd\u0027),to_char(:16,\u0027yyyy-mm-dd\u0027),:17,:18,:19,:20,:21,:22,:23,null,null,:24,null,null,null,null,null,null,null,:25, null,:26,:27,:28,null,\u0027ms_grc_asset_class\u0027,:29)) as ms_varchar2_array)) \n  where column_value \u003d object_id\n  ) order by upper_object_name"
  ],
  "MS_GRC_GET_DATA_REFERENCE_dbf": [
    "MS GRC GET DATA REFERENCE dbf",
    "1",
    "Y",
    "1",
    "select object_id, object_name, valid_from\n , valid_until,\n restrict_access_to,\n dd_object_type object_type ,reference_type,\n dd_process_instance_id,\n dd_instance_id, owner_organizations,\n display_name,edit_action_parameters\n,over_ride_action,expired_flag,object_id from\n(select\n object_id\n , object_name,reference_type,\n  substr(object_name,1,30)  display_name, dd_object_type object_type,   \u0027y\u0027  as over_ride_action,  ms_grc_utilities.include_expired(valid_from,valid_until) as expired_flag\n , valid_from\n , valid_until,\n restrict_access_to,\n dd_object_type,\n dd_process_instance_id, \u0027proc\u003d\u0027|| to_char(dd_process_instance_id)||\u0027|instid\u003d\u0027||to_char(dd_instance_id)||\u0027|edit_flag\u003d\u0027||nvl(dd_edit_flag,\u0027y\u0027)  as edit_action_parameters,\n dd_instance_id,\n (select ms_concat (d.owner_organizations)\n               from    ms_grc_reference_org d\n              where   a.object_id \u003d d.object_id) as owner_organizations\n  from ms_grc_reference a) t1\n  where\nms_grc_utilities.get_object_access(dd_object_type,\n                                                   object_id,\n                                                   :1)  \u003d \u0027access\u0027\n and trunc(sysdate) between nvl(trunc(valid_from),trunc(sysdate)) and nvl(trunc(valid_until),trunc(sysdate)) \norder by object_name asc"
  ],
  "MS_GRC_Req_Impact": [
    "MS GRC Req Impact",
    "1",
    "M",
    "1",
    "select c.stored_value as stored_value,\n       c.displayed_value as displayed_value,\n       c.lov_name as lov_name,\n       c.locale_id as locale_id,\n       c.enterprise_id as enterprise_id,\n       c.lov_id as lov_id,\n       c.lov_value_id as lov_value_id,\n       c.org_entity_id as org_entity_id,\n       c.org_entity_name as org_entity_name,\n       c.start_date as start_date,\n       c.end_date as end_date\n  from ms_grc_lov_org_values_v c,\n       si_locales a,\n       si_users_t b \n where c.locale_id\u003d a.locale_id\n   and a.locale_string \u003d b.locale\n   and b.user_name \u003d :1\n   and lov_name \u003d \u0027ms grc requirement impact\u0027\norder by c.displayed_value"
  ],
  "MS_GRC_Stakeholders_OBJ_Infolet": [
    "MS GRC Stakeholders OBJ Infolet",
    "1",
    "M",
    "1",
    "select user_id,\n   user_name,\n   user_full_name \nfrom table(ms_grc_utilities.get_stakeholders(:1,\u0027ms_grc_objectives\u0027,:2))"
  ],
  "MS_GRC_ORB_Add_Asset": [
    "MS GRC ORB Add Asset",
    "1",
    "M",
    "1",
    "select checkbox, object_id, object_name,business_criticality,object_parent,asset_type\nfrom table ( cast( ms_grc_report_filters.get_filters_asset\n(:1,:2,:3,:4,:5,:6,:7) as ms_grc_asset_tab) )\norder by upper(object_name)"
  ],
  "MS_GRC_Product_Check_Initiator_Access": [
    "MS GRC Product Check Initiator Access",
    "1",
    "M",
    "1",
    "select ms_grc_utilities.get_heirarchy_activity(:1,:2,:3) as access_flag from dual"
  ],
  "MS_GRC_Bus_Criticality": [
    "MS GRC Bus Criticality",
    "1",
    "M",
    "1",
    "select c.stored_value as stored_value,\n       c.displayed_value as displayed_value,\n       c.lov_name as lov_name,\n       c.locale_id as locale_id,\n       c.enterprise_id as enterprise_id,\n       c.lov_id as lov_id,\n       c.lov_value_id as lov_value_id,\n       c.org_entity_id as org_entity_id,\n       c.org_entity_name as org_entity_name,\n       c.start_date as start_date,\n       c.end_date as end_date\n  from ms_grc_lov_org_values_v c,\n       si_locales a,\n       si_users_t b \n where c.locale_id\u003d a.locale_id\n   and a.locale_string \u003d b.locale\n   and b.user_name \u003d :1\n   and lov_name \u003d \u0027ms_grc_business_criticality\u0027\norder by c.display_order"
  ],
  "MS_GRC_CURRENCY_INF": [
    "MS GRC CURRENCY INF",
    "1",
    "Y",
    "1",
    "select object_id,country,iso,currency_symbol,currency,rate from ms_grc_currency_upload_cry"
  ],
  "MS_GRC_61_SP2_CIF_FTECH_API_RUN_INF": [
    "MS GRC 61 SP2 CIF FTECH API RUN INF",
    "5",
    "Y",
    "1",
    null
  ],
  "MS_GRC_Function_Approvers": [
    "MS GRC Function Approvers",
    "1",
    "M",
    "1",
    "select a.user_id as user_id,\n  a.user_name as user_name,\n  a.user_full_name as user_full_name,\n  a.activity_id as activity_id,\n  a.activity_short_name as activity_short_name,\n  a.activity_name as activity_name,\n  c.locale_id as locale_id\nfrom table(ms_grc_utilities.get_owner_org_users(:1,\u0027ms_grc_function\u0027,\u0027a\u0027,null)) a\n,si_locales c,\nsi_users_t d where \nc.locale_string \u003d d.locale\nand d.user_name \u003d :2"
  ],
  "MS_GRC_CURRENCY": [
    "MS GRC CURRENCY",
    "1",
    "M",
    "1",
    "select c.stored_value as stored_value,\n       c.displayed_value as displayed_value ,\n       c.lov_name as lov_name,\n       a.locale_id as  locale_id ,\n       c.enterprise_id as enterprise_id ,\n       c.lov_id as lov_id ,\n       c.lov_value_id as lov_value_id ,\n       c.org_entity_id as org_entity_id ,\n       c.org_entity_name as  org_entity_name,\n       c.start_date as  start_date,\n       c.end_date\n as end_date\nfrom   ms_grc_lov_org_values_v c,\nsi_locales a,si_users_t b \nwhere  c.locale_id\u003d a.locale_id\nand a.locale_string \u003d b.locale and b.user_name \u003d :1 and lov_name \u003d\u0027ms_grc_currency\u0027"
  ],
  "MS_GRC_61_SP2_CONTROL_PARENT_INF": [
    "MS GRC 61 SP2 CONTROL PARENT INF",
    "1",
    "M",
    "1",
    "select object_id,  \nobject_name \nfrom ms_grc_control_denorm_rv rv\nwhere exists\n(select column_value\nfrom table(cast(ms_grc_rep_browser_fil_denorm.fn_control_rpt_filters(ms_grc_array_type(null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,null,\u0027ms_grc_control\u0027,null,:1)) as ms_varchar2_array))\nwhere column_value \u003d rv.object_id\n)  and locale_id\u003d(select locale_id from si_users_t t1 ,si_locales t2 where user_name \u003d :1  and t1.locale \u003d t2.locale_string) \norder by upper_object_name"
  ],
  "MS_GRC_Objectives_Type": [
    "MS GRC Objectives Type",
    "1",
    "M",
    "1",
    "select c.stored_value as stored_value,\n       c.displayed_value as displayed_value,\n       c.lov_name as lov_name\nfrom   ms_grc_lov_org_values_v c,\nsi_locales a,si_users_t b \nwhere \nc.locale_id\u003d a.locale_id\nand a.locale_string \u003d b.locale and b.user_name \u003d :1 and  (trim(lov_name)\u003d\u0027ms_grc_objective_type\u0027)\nand c.stored_value in (select distinct \u00271\u0027 from si_user_org_act_view where user_name \u003d:1\nand activity_name in (\u0027grc - view business objective\u0027,ms_grc_utilities.get_type_activity(\u0027ms_grc_objectives\u0027,:2,1,\u0027e\u0027),\u0027grc - approve business objective\u0027)\nunion all \nselect distinct \u00272\u0027 from si_user_org_act_view where user_name \u003d:1\nand activity_name in (\u0027grc - view control objective\u0027,ms_grc_utilities.get_type_activity(\u0027ms_grc_objectives\u0027,:2,2,\u0027e\u0027),\u0027grc - approve control objective\u0027)\nunion all \nselect distinct \u00273\u0027 from si_user_org_act_view where user_name \u003d:1\nand activity_name in (\u0027grc - view process objective\u0027,ms_grc_utilities.get_type_activity(\u0027ms_grc_objectives\u0027,:2,3,\u0027e\u0027),\u0027grc - approve process objective\u0027))\norder by stored_value asc"
  ],
  "MS_GRC_CURRENCY_TEMPLATE": [
    "MS GRC CURRENCY TEMPLATE",
    "1",
    "Y",
    "1",
    "select  success_template\n,error_template\nfrom (\nselect  success_template, error_template,upload_process_instance_id, max(upload_process_instance_id) over(partition by form_name) as max_id\n  from ms_apps_du_upload_status_log\n where form_name \u003d \u0027ms_grc_currency_upload\u0027\n   and success_template is not null\n   and error_template is not null\n   )\n   where upload_process_instance_id \u003d max_id\nand rownum \u003d 1"
  ],
  "MS_GRC_Requirement_Parent": [
    "MS GRC Requirement Parent",
    "1",
    "M",
    "1",
    "select distinct a.object_name as obj_name,\n  \u0027level \u0027\n  ||a.object_level as obj_level,\n  a.dd_object_type as obj_type,\n  a.object_id    as obj_id,c.locale_id as locale_id\nfrom ms_grc_requirement a,\n  ms_grc_requirement_org b,si_locales c,\nsi_users_t d where \nc.locale_string \u003d d.locale\nand d.user_name \u003d :2\nand a.object_id          \u003db.object_id\nand object_level                                                                                            \u003d (:1 -1)\nand trunc(sysdate)                                                                                         \u003e\u003d trunc(nvl(valid_from,sysdate))\nand trunc(sysdate)                                                                                         \u003c\u003d trunc(nvl(valid_until,sysdate))\nand ms_grc_utilities.ms_grc_security(dd_object_type, restrict_access_to, b.owner_organizations, :2, \u0027view\u0027) \u003d \u0027access\u0027"
  ],
  "MS GRC Control Infolet": [
    "MS GRC Control Infolet",
    "1",
    "M",
    "1",
    "select summ.object_id  as control_id,\nsumm.object_name as control_name,\nsumm.object_name as edit,\nmls.key_control,\nmls.control_purpose,\nmls.control_type,\nmls.control_nature,\nmls.control_source,\nmls.control_priority,\nmls.object_level,\n(select object_name from ms_grc_compliance_obje comp where comp.object_id\u003dsumm.parent and comp.dd_object_type \u003d \u0027ms_grc_control\u0027) as object_parent,\nmls.object_status,\nmls.control_frequency,\nsumm.created_date,\n (select user_full_name from ms_grc_user_full_name_v where user_name \u003d  summ.created_by) as created_by,\nsumm.updated_date,\n (select user_full_name from ms_grc_user_full_name_v where user_name \u003d   summ.updated_by) as updated_by,\nsumm.valid_from,\nsumm.valid_until,\nsumm.description,\nsumm.owner_org_names as owner_organization,\nsumm.owner_name as owner_names,\nsumm.level_1_approver,\nsumm.level_2_approver,\nsumm.rel_organization_names as applies_to_organizations,\nsumm.rel_aoc_obj_name_agg as related_areas_of_compliance,\nmls.assertions as related_assertions,\nsumm.rel_asset_obj_name_agg as related_assets,\nsumm.rel_ctrl_obj_name_agg as related_controls,\nsumm.rel_asset_class_obj_name_agg as related_asset_classes,\nsumm.rel_evidence_obj_name_agg as related_evidences,\nsumm.rel_mod_ref_obj_name_agg as related_framework_references,\nsumm.rel_function_obj_name_agg as related_functions,\nsumm.rel_objective_obj_name_agg as related_objectives,\nsumm.rel_process_obj_name_agg as related_processes,\nsumm.rel_product_name_agg as related_products,\nsumm.rel_que_proc_obj_name_agg as realted_ques_procs,\nsumm.rel_refrences_obj_name_agg as related_references,\nsumm.rel_requirement_obj_name_agg as related_requirements,\nsumm.rel_risk_obj_name_agg as related_risks,\nsumm.report_json as report_json\n,( select usr.level_1_approver from ms_grc_compliance_obje usr where summ.object_id\u003dusr.object_id ) as level_1_appr,\n(select usr.level_2_approver from ms_grc_compliance_obje usr where summ.object_