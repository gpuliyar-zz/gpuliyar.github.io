f_impact,\n(select user_name from ms_grc_user_full_name_v where user_full_name \u003d to_char(rsk_summ.owner_name))  as ownr_names\nfrom ms_grc_risk_obj_summary rsk_summ,\nms_grc_risk_obj_mls_summary mls_summ\nwhere mls_summ.object_id\u003drsk_summ.object_id and mls_summ.object_id in (select column_value\nfrom table(cast(ms_grc_rep_browser_fil_denorm.fn_risk_rpt_filter(ms_grc_array_type(:1,:2,:3,:5,:7,:6,:8,:9,to_char(:10,\u0027yyyy-mm-dd\u0027),to_char(:11,\u0027yyyy-mm-dd\u0027),:12,:13,to_char(:14,\u0027yyyy-mm-dd\u0027),to_char(:15,\u0027yyyy-mm-dd\u0027),to_char(:16,\u0027yyyy-mm-dd\u0027),to_char(:17,\u0027yyyy-mm-dd\u0027),:18,:19,:20,:21,:22,:23,:24,null,null,:25,null,null,:26,null,:27,null,:28,:29,:30,:31,null,:32,:33,\u0027ms_grc_risk\u0027,:34))as ms_varchar2_array))\n)  and mls_summ.locale_id \u003d (select locale_id from si_users_t t1 ,si_locales t2 where user_name \u003d :34  and t1.locale \u003d t2.locale_string) \nand nvl(is_orphan, 1)\u003dnvl(:4,nvl(is_orphan,1)) order by upper_object_name"
