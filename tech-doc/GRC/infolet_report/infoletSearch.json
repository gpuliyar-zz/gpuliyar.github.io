    "select\nmodsumm.object_id as framework_reference_id,\nmodsumm.object_name as framework_reference_name,\nmodmlssumm.mod_type as framework_reference_type,\nmodmlssumm.object_status as status,\nmodsumm.created_on as created_on,\n(select user_full_name from ms_grc_user_full_name_v where user_name \u003dmodsumm.created_by) as created_by,\nmodsumm.updated_on as modified_on,\n(select user_full_name from ms_grc_user_full_name_v where user_name \u003dmodsumm.updated_by) as modification_requested_by,\nmodsumm.valid_from as valid_from,\nmodsumm.valid_until as valid_until ,\nmodsumm.description as description,\nmodsumm.owner_org_names as owner_organizations,\nmodsumm.owner_name as owners,\n(select user_full_name from ms_grc_user_full_name_v where user_name \u003dmodsumm.level_1_approver) as level_1_approver,\n(select user_full_name from ms_grc_user_full_name_v where user_name \u003dmodsumm.level_2_approver) as level_2_approver,\nrel_mod_ref_obj_name_agg as framework_reference,\nrel_refrences_obj_name_agg as references,\nattachments as attached_files\n,modmlssumm.mod_source as source\n,modsumm.report_json as report_json\n,modsumm.level_1_approver as level_1_appr,\nmodsumm.level_2_approver as level_2_appr,\nmodsumm.upper_object_name as upper_object_name\nfrom ms_grc_mod_ref_summary modsumm,\nms_grc_mod_ref_mls_summary modmlssumm\nwhere modmlssumm.object_id \u003d modsumm.object_id\nand modmlssumm.object_id in  (select column_value\nfrom table( cast(ms_grc_rep_browser_fil_denorm.fn_mod_ref_report_filters(ms_grc_array_type(:1,:2,:3,:4,:6,to_char(:7,\u0027yyyy-mm-dd\u0027),to_char(:8,\u0027yyyy-mm-dd\u0027), :9,:10,to_char(:11,\u0027yyyy-mm-dd\u0027),to_char(:12,\u0027yyyy-mm-dd\u0027),to_char(:13,\u0027yyyy-mm-dd\u0027),to_char(:14,\u0027yyyy-mm-dd\u0027),\n:15,:16,:17,:18,null,null,null,null,null,null,null,\n:19,null,null,null,null,null,null,null,null,:20,\nnull,null,\u0027ms_grc_model_reference\u0027,:21)) as ms_varchar2_array)) \n)\nand modmlssumm.locale_id \u003d (select locale_id from si_users_t t1 ,si_locales t2 where user_name \u003d :21 and t1.locale \u003d t2.locale_string)\nand nvl(is_orphan, 1)\u003dnvl(:5,nvl(is_orphan,1))"
